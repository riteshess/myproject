    <SQLQuery><![CDATA[SELECT         
CLD_ID,       
SLOC_ID,       
ORG_ID,       
ORG_DESC,       
HO_ORG_ID,       
FY_ID,       
DOC_ID,       
RCD_ID,       
RCD_DT,       
PRJ_ID,       
EO_ID,       
EO_NM,       
SEL_BASIS,       
SEL_BASIS_NM,       
REF_COA_ID,       
COA_NM,       
AMT_SP,       
AMT_BS,       
CURR_ID_SP,       
CURR_DESC,       
CURR_CONV_RATE,       
USR_ID_CREATE,       
USR_NAME,       
USR_ID_CREATE_DT,       
USR_ID_MOD,       
USR_ID_MOD_DT,    
RCD_STAT,    
RCD_STAT_NM    
from         
SEARCH$PMS$SITE$RCD_VW      
WHERE       
CLD_ID = :cldIdBind      
and sloc_id = :slocIdBind      
AND org_id = :orgIdBind      
and ho_org_id = :hoOrgIdBind      
and nvl(eo_id,-1) = nvl(:eoIdBind,nvl(eo_id,-1))      
and REF_COA_ID = nvl(:coaIdBind,REF_COA_ID)      
and upper(RCD_ID) LIKE upper('%'||:rcdIdBind||'%')      
AND trunc(rcd_dt)  >= NVL(:strtDtBind,trunc(rcd_dt))                 
AND trunc(rcd_dt)  <= NVL(:endDtBind,trunc(rcd_dt))    
and rcd_stat = nvl(:rcdStatBind, rcd_stat)  
order by RCD_ID desc]]></SQLQuery>
