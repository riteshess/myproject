<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SvcTopCustInvcVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="svcdashboardapp.model.views.SvcTopCustInvcVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="CountBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT EO_ID EOID,EO_NM Supplier,AMOUNT AMOUNT 
FROM (SELECT A.EO_ID, A.EO_NM,SUM(A.TOT_AMT_BS) AMOUNT  
      FROM SVC.SVC$MIS$TOP$CUST$INVC$VW A  
       WHERE A.CLD_ID = :CldIdBind  
      AND A.HO_ORG_ID = :HoOrgIdBind  
      GROUP BY EO_ID , EO_NM  
      ORDER BY AMOUNT DESC)  
WHERE ROWNUM<= :CountBind]]></SQLQuery>
  <ViewAttribute
    Name="Eoid"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="EOID"
    Expression="EOID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Supplier"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPLIER"
    Expression="SUPPLIER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Amount"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="AMOUNT"
    Expression="AMOUNT"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="svcdashboardapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
