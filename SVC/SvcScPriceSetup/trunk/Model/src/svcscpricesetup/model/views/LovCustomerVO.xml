  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
    <DesignTime>
        <Attr Name="_isExpertMode" Value="true"/>
    </DesignTime>
    <Properties>
        <SchemaBasedProperties>
            <LABEL
              ResId="LovCustomerVO_LABEL"/>
        </SchemaBasedProperties>
    </Properties>
    <Variable
