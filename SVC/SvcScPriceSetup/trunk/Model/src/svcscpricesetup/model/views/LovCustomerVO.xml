    <ViewAttribute
      Name="DfltBillAdds"
      IsUpdateable="false"
      IsPersistent="false"
      PrecisionRule="true"
      Precision="20"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="DFLT_BILL_ADDS"
      Expression="DFLT_BILL_ADDS"
      SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="20"/>
        </DesignTime>
    </ViewAttribute>
    <ViewAttribute
      Name="EmailId"
      IsUpdateable="false"
      IsPersistent="false"
      PrecisionRule="true"
      Precision="100"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="EMAIL_ID"
