  <SQLQuery><![CDATA[select a.cld_id,           
a.sloc_id,           
a.org_id,           
a.ho_org_id,           
a.doc_id,           
a.pmt_dt,    
to_char(a.pmt_dt,'dd-MM-yyyy' ) pmt_Dt_disp    
from svc$sc$pmt$schdl a          
where          
a.pmt_dt not in (select b.pmt_schdl_dt        
from svc$invc$src b       
where B.doc_id_src = a.doc_id       
and a.cld_id = b.cld_id       
and a.org_id = b.org_id       
and a.ho_org_id = B.ho_org_id       
and a.sloc_id = b.sloc_id       
)       
and a.cld_id=:bindCldId          
and a.org_id=:bindOrgId         
and a.sloc_id=:bindSlocId         
and a.doc_id=:bindDocId        
and a.ho_org_id=:bindHoOrgId]]></SQLQuery>
