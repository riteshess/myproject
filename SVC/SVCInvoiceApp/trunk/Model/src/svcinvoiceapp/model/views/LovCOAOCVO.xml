<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovCOAOCVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="svcinvoiceapp.model.views.LovCOAOCVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindCoaId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select coa_id ,            
coa_nm,            
ORG_ID,            
SLOC_ID,            
CLD_ID            
from fin.FIN$COA$DTL$VW             
where             
ACC_TYPE = 0          
and org_id=:bindOrgId            
and cld_id=:bindCldId            
and sloc_id=:bindSlocId   
and coa_id =nvl(:bindCoaId, coa_id)]]></SQLQuery>
