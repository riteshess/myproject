<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovScDocIdVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="svcinvoiceapp.model.views.LovScDocIdVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindEoId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT DISTINCT A.CLD_ID,                   
A.SLOC_ID,                   
A.HO_ORG_ID,                   
A.ORG_ID, A.DOC_ID,                   
A.SC_NO, A.EO_ID,                    
A.COA_ID,                       
A.CURR_ID_SP,                   
A.DOC_DT                  
FROM SVC.SVC$SC A  ,       svc$sc$pmt$schdl C            
where TRUNC(C.pmt_dt) not in (select TRUNC(b.pmt_schdl_dt)          
                      from svc$invc$src b         
                      where B.doc_id_src=C.doc_id         
                      and B.cld_id= C.cld_id         
                      and B.org_id = C.org_id         
                      and B.ho_org_id = C.ho_org_id         
                      and B.sloc_id= C.sloc_id         
                      )         
AND A.DOC_ID = C.DOC_ID     
AND A.DOC_STAT = 60                  
AND A.AUTH_STAT = 'Y'   
AND A.SC_TYPE IN ( 21, 24)  
AND A.CLD_ID =:BindCldId                    
AND A.SLOC_ID =:BindSlocId                    
AND A.ORG_ID =:BindOrgId                    
AND A.EO_ID =:BindEoId]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="HoOrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="HO_ORG_ID"
    Expression="HO_ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DocId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOC_ID"
    Expression="DOC_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ScNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SC_NO"
    Expression="SC_NO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="EoId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="EO_ID"
    Expression="EO_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="CoaId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="COA_ID"
    Expression="COA_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="CurrIdSp"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="CURR_ID_SP"
    Expression="CURR_ID_SP"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="DocDt"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Timestamp"
    ColumnType="TIMESTAMP"
    AliasName="DOC_DT"
    Expression="DOC_DT"
    SQLType="DATE"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="CldId"/>
    <Item Value="SlocId"/>
    <Item Value="HoOrgId"/>
    <Item Value="OrgId"/>
    <Item Value="DocId"/>
    <Item Value="ScNo"/>
    <Item Value="EoId"/>
    <Item Value="CoaId"/>
    <Item Value="DocDt"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="svcinvoiceapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
