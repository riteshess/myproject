<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovUsrVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="svcinvoiceapp.model.views.LovUsrVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindUsrId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select sloc_id,usr_id,usr_name from app.app$sec$usr       
where sloc_id=:bindSlocId      
and usr_id=nvl(:bindUsrId,usr_id)      
and usr_actv='Y']]></SQLQuery>
  <ViewAttribute
    Name="SlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="UsrId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="USR_ID"
    Expression="USR_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="UsrName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="USR_NAME"
    Expression="USR_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="svcinvoiceapp.view.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
