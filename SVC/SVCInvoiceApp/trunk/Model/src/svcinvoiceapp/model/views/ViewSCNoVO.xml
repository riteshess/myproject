    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindEoId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT DISTINCT A.CLD_ID,                   
A.SLOC_ID,                   
A.HO_ORG_ID,                   
A.ORG_ID, A.DOC_ID,                   
A.SC_NO, A.EO_ID,                    
A.COA_ID,                       
A.CURR_ID_SP,                   
A.DOC_DT                  
FROM SVC.SVC$SC A  ,       svc$sc$pmt$schdl C            
where A.DOC_ID = C.DOC_ID     
AND A.DOC_STAT = 60                  
AND A.AUTH_STAT = 'Y'       
AND A.CLD_ID =:BindCldId                    
AND A.SLOC_ID =:BindSlocId                    
AND A.ORG_ID =:BindOrgId                    
AND A.EO_ID =:BindEoId]]></SQLQuery>
