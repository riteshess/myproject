  <SQLQuery><![CDATA[select a.cld_id,a.sloc_id,a.ho_org_id,a.org_id,           
a.doc_id,a.Doc_dt,a.fy_id,a.sc_no,a.sc_type,a.eo_id,a.doc_type,           
b.eo_nm,a.quot_no,a.doc_stat,(select att_nm from svc$ds$att where att_type_id=13 and att_id=a.doc_stat) as doc_nm from svc$sc a ,app.app$eo b           
 where              
 b.eo_cld_id=a.cld_id           
 and b.eo_sloc_id=a.sloc_id           
 and b.eo_ho_org_id=a.ho_org_id           
 and b.eo_id=a.eo_id           
 and ( (UPPER(a.sc_no) LIKE UPPER('%' || nvl(:bindscno,a.sc_no) || '%')))              
      And Trunc(a.Doc_Dt) Between Nvl(:binfFrmdt,Trunc(a.Doc_Dt)) And Nvl(:bindTodt,Trunc(a.Doc_Dt))             
and a.eo_id=nvl(:bindeoId,a.eo_id)   
and a.doc_stat=nvl(:bindDocStat,a.doc_stat)   
and a.sc_type=nvl(:bindScType,a.Sc_type)  
and a.sloc_id=:bindSlocId              
and a.cld_id=:bindCldId             
and a.org_id=:bindOrgId             
and a.ho_org_id=:bindHoOrgId]]></SQLQuery>
