  <SQLQuery><![CDATA[select a.cld_id,           
a.sloc_id,           
a.ho_org_id,           
a.org_id,           
a.eo_id,       
(select eo_nm from app.app$eo c where c.eo_cld_id=a.cld_id       
and c.eo_sloc_id=a.sloc_id      
and c.eo_ho_org_id=a.ho_org_id       
and c.eo_id=a.eo_id) eo_nm,    
loc_ref_id,   
loc_ref_nm,   
a.adds_id,          
(select adds           
from app.app$adds$vw b           
where b.cld_id=a.cld_id           
and b.sloc_id=a.sloc_id           
and b.adds_id=a.adds_id) adds           
from            
app.APP$EO$ADDS a         
where a.actv='Y'            
and a.svc_loc='Y'        
and a.org_id=:bindOrgId        
and a.ho_org_id=:bindHoOrgId        
and a.sloc_id=:bindSlocId        
and a.cld_id=:bindCldId        
and a.adds_id=nvl(:bindAddsId,a.adds_id)    
and a.eo_id=nvl(:bindEoId,a.eo_id)]]></SQLQuery>
