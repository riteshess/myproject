org_id,                    
sc_no,                    
quot_no,                    
eo_id,          
doc_id,          
doc_type,    
doc_dt,    
prd_to    
from svc.svc$sc  a                
where     
a.cld_id=:bindcldId                     
and a.sloc_id=:bindSlocId                     
and a.ho_org_id=:bindHoOrgId                    
and a.org_id=:bindOrgId                 
and a.sc_type=nvl(:bindScType,sc_type)                
and a.eo_id=:bindEoId              
and a.doc_stat = 60              
and a.auth_stat = 'Y'            
and a.DOC_ID NOT IN (SELECT NVL(B.DOC_ID_PREV,'X') FROM SVC$SC B      
where a.cld_id = b.cld_id             
and a.sloc_id = b.sloc_id            
and a.org_id = b.org_id            
and a.sc_type = b.sc_type            
and a.eo_id = b.eo_id )     
and trunc(prd_to) > trunc(sysdate)]]></SQLQuery>
