  <SQLQuery><![CDATA[SELECT * 
FROM 
  (SELECT 
    /*+ FIRST_ROWS(n) */ 
    A.*, 
    ROWNUM RNUM 
  FROM 
    ( --SELECT * FROM ( 
    SELECT A.CLD_ID, 
      A.SLOC_ID, 
      A.HO_ORG_ID, 
      A.ORG_ID, 
      A.DOC_ID, 
      A.DOC_DT, 
      A.QUOT_ID, 
      A.EO_NM, 
      A.QUOT_AMT_SP, 
      A.QUOT_AMT_BS, 
      A.CURR_DESC, 
      A.USR_ID, 
      A.WF_TYPE, 
      B.USR_NAME, 
      A.REMARKS 
    FROM SLS$QUOT$WF$DET A,APP.APP$SEC$USR B 
    WHERE A.CLD_ID  = :CldIdBind 
    AND A.ORG_ID    = :OrgIdBind 
    AND A.HO_ORG_ID = :HoOrgIdBind 
    AND A.SLOC_ID   = :SlocIdBind 
    AND A.WF_TYPE   = :WfTypeBind 
    AND A.USR_ID    = :UsrIdBind 
    AND A.USR_ID = B.USR_ID 
    AND A.SLOC_ID = B.SLOC_ID 
    ORDER BY A.QUOT_ID DESC 
       
  ) A 
WHERE ROWNUM <= :RowsLowerLimit ) WHERE RNUM >= :RowsUpperLimit]]></SQLQuery>
