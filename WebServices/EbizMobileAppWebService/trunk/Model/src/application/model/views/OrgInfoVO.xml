  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select o.org_id, 
o.org_type, 
o.org_desc, 
o.org_id_parent, 
(select org_desc 
from org 
where org_id = o.org_id_parent 
and org_cld_id = o.org_cld_id 
and org_create_ref_sloc_id = o.org_create_ref_sloc_id) prnt_org_desc, 
o.org_fy_st_dt, 
c.curr_desc, 
cc.cntry_desc, 
d.org_add_id1, 
ad1.ADDS ADDRESS_1, 
d.org_add_id2, 
ad2.ADDS ADDRESS_2, 
d.org_phone1, 
d.org_phone2, 
d.org_mobile1, 
d.org_mobile2, 
d.org_fax1, 
d.org_fax2, 
d.org_website1, 
d.org_website2, 
d.org_email1, 
d.org_email2 
from app.org o,  
app.org$oth$dtl d,  
app.app$curr c,  
app.app$cntry cc, 
APP.APP$ADDS$VW AD1, 
APP.APP$ADDS$VW AD2 
where o.org_cld_id = d.org_cld_id 
and o.org_id = d.org_id 
and o.org_cld_id = c.cld_id 
and o.org_curr_id_bs = c.curr_id 
and o.org_cld_id = cc.cntry_cld_id 
and o.org_country_id = cc.cntry_id 
and ad1.CLD_ID(+) = d.org_cld_id 
and ad1.ADDS_ID(+)= d.org_add_id1 
and ad2.CLD_ID(+) = d.org_cld_id 
and ad2.ADDS_ID(+)= d.org_add_id2 
and o.org_id = :BindOrgId 
and o.ORG_CLD_ID = :BindCldId]]></SQLQuery>
