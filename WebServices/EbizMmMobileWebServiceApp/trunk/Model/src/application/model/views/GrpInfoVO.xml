      <LABEL
        ResId="application.model.views.GrpInfoVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindHorgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select grp_id,grp_nm from app.APP$GRP  
where cld_id=:BindCldId   
and sloc_id=:BindSlocId   
and ho_org_id=:BindHorgId   
and org_id=:BindOrgId]]></SQLQuery>
  <ViewAttribute
