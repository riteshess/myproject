  <SQLQuery><![CDATA[SELECT    
a.itm_id ITM_ID,          
a.itm_desc ITM_DESC,          
a.PRICE_BASIC PRICE_BASIC,    
a.PRICE_PUR,    
a.PRICE_SLS,    
a.SRVC_ITM_FLG,    
a.STOCKABLE_FLG,    
a.SLS_ITM_FLG,    
a.PUR_ITM_FLG,    
a.WIP_ITM_FLG,    
a.TAX_EXMPT_FLG,    
a.CAPITAL_GD_FLG,    
a.QC_REQD_FLG,    
a.CONSUMABLE_FLG,    
a.CASH_PUR_FLG,    
a.SERIALIZED_FLG,    
a.SAMPL_ITM_FLG,    
uB.uom_nm uomBAsic,    
uP.uom_nm uomPurchase,    
uS.uom_nm uomSales,       
app.fn_get_itm_att(a.cld_id,a.sloc_id,a.ho_org_id,a.itm_id,'c',',') att    
FROM app.APP$ITM$PRF a,            
app.APP$GRP g ,    
app.APP$UOM$CONV$STD uB,    
app.APP$UOM$CONV$STD uP,    
app.APP$UOM$CONV$STD uS    
WHERE a.ACTV='Y'    
and a.UOM_BASIC = uB.uom_id    
and a.UOM_PUR =uP.uom_id    
and a.UOM_SLS=uS.uom_id    
AND a.grp_id=g.grp_id      
AND a.sloc_id=:BindSlocId          
AND a.cld_id=:BindCldId          
AND a.ho_org_id=:BindHorgId    
AND a.itm_id = :BindItemId]]></SQLQuery>
