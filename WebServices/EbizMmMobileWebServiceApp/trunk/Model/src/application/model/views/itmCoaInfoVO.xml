    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT     
A.CLD_ID,     
A.SLOC_ID,     
A.ORG_ID,     
A.ITM_ID,     
A.COA_FOR,     
B.ATT_NM COA_FOR_DESC,     
A.COA_ID,     
C.COA_NM    
FROM APP.APP$ITM$COA A, APP.APP$DS$ATT B, FIN.FIN$COA C    
WHERE A.COA_FOR = B.ATT_ID    
AND A.CLD_ID = C.COA_CLD_ID    
AND A.SLOC_ID = C.COA_SLOC_ID    
AND A.HO_ORG_ID = C.COA_HO_ORG_ID    
AND A.COA_ID = C.COA_ID 
AND A.CLD_ID  = :CldIdBind    
AND A.SLOC_ID   = :SlocIdBind    
AND A.ORG_ID    = :OrgIdBind    
AND A.ITM_ID = :ItmIdBind]]></SQLQuery>
