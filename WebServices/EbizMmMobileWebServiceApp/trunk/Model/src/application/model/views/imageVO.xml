<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="imageVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="application.model.views.imageVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindItmId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindHoId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT  
    ITM_ID,  
    IMG_PATH 
FROM  
    APP.APP$ITM$IMG 
    WHERE ACTV = 'Y' 
    AND ITM_ID =:bindItmId 
    AND CLD_ID =:bindCldId 
    AND SLOC_ID =:bindSlocId 
    AND HO_ORG_ID =:bindHoId]]></SQLQuery>
  <ViewAttribute
    Name="ItmId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_ID"
    Expression="ITM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ImgPath"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="IMG_PATH"
    Expression="IMG_PATH"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="application.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
