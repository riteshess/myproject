  <SQLQuery><![CDATA[SELECT *     
FROM     
  (SELECT     
    A.*,     
    ROWNUM RNUM     
  FROM     
    (     
    SELECT      
    A.CLD_ID,     
    A.Sloc_Id,     
    --A.HO_ORG_ID,     
    A.ORG_ID,     
    A.Doc_Id,     
    A.PO_Id,     
    A.PO_DT,     
    A.EO_NM,     
    A.PO_AMT_SP,     
    A.PO_AMT_BS,     
    A.CURR_DESC,     
    A.USR_ID,     
    A.OP_TYPE,     
    B.USR_NAME,     
    A.REMARKS,  
    a.po_type_nm, 
    a.usr_id_create_nm 
FROM     
    MM.MM$PO$PND$MOB$VW A,APP.APP$SEC$USR B     
WHERE     
    A.CLD_ID = :CldIdBind AND     
    A.Org_Id = :Orgidbind And      
    A.SLOC_ID = :SlocIdBind AND     
    A.OP_TYPE = :WfTypeBind AND     
    A.USR_ID = :UsrIdBind     
    AND A.USR_ID = B.USR_ID     
    And A.Sloc_Id = B.Sloc_Id     
       
   ORDER BY A.PO_ID DESC     
  ) A     
WHERE ROWNUM <= :RowsUpperLimit) WHERE RNUM >= :RowsLowerLimit]]></SQLQuery>
