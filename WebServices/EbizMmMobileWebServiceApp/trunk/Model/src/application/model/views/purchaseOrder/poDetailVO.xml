<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="poDetailVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="application.model.views.purchaseOrder.poDetailVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="OrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="PoDocIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT A.DOC_ID,    
NVL(A.AUTH_PO_NO, A.PO_ID) PO_ID,    
A.PO_DT,    
A.EO_ID,    
(SELECT EO_NM    
FROM APP.APP$EO B    
WHERE A.CLD_ID  = B.EO_CLD_ID    
AND A.SLOC_ID   = B.EO_SLOC_ID    
AND MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID) = B.EO_HO_ORG_ID    
AND A.EO_ID    = B.EO_ID    
) EO_NM,    
A.CURR_ID_SP,    
(SELECT CURR_DESC    
FROM APP.APP$CURR C    
WHERE C.CLD_ID = A.CLD_ID    
AND C.CURR_ID  = A.CURR_ID_SP    
) CURR_DESC,    
A.PO_TYPE,    
(SELECT ATT_NM    
FROM APP.APP$DS$ATT D    
WHERE D.ATT_ID  = A.PO_TYPE    
) ORDR_TYP_DESC,    
A.PO_AMT_SP, 
APP.WF_GET_ID(A.SLOC_ID,A.CLD_ID, A.ORG_ID,18504,A.PO_TYPE) WF_NO 
FROM MM$DRFT$PO A    
WHERE A.CLD_ID  = :CldIdBind    
AND A.SLOC_ID   = :SlocIdBind    
AND A.ORG_ID    = :OrgIdBind    
AND A.DOC_ID    = :PoDocIdBind]]></SQLQuery>
  <ViewAttribute
    Name="DocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOC_ID"
    Expression="DOC_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="PoId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PO_ID"
    Expression="PO_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="PoDt"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="PO_DT"
    Expression="PO_DT"
