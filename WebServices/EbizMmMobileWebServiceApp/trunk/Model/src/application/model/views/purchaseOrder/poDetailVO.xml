    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="EO_ID"
    Expression="EO_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="EoNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EO_NM"
    Expression="EO_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="CurrIdSp"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Scale="0"
    Type="java.lang.Long"
    ColumnType="NUMBER"
    AliasName="CURR_ID_SP"
    Expression="CURR_ID_SP"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="CurrDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CURR_DESC"
    Expression="CURR_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="PoType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="PO_TYPE"
    Expression="PO_TYPE"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="OrdrTypDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORDR_TYP_DESC"
    Expression="ORDR_TYP_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="PoAmtSp"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="26"
    Scale="6"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="PO_AMT_SP"
    Expression="PO_AMT_SP"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="WfNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4000"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="WF_NO"
    Expression="WF_NO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4000"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="application.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
