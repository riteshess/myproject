  <SQLQuery><![CDATA[SELECT 
distinct
A.HO_ORG_ID HO_ORG_ID,
    a.SLOC_ID EO_SLOC_ID, 
    a.EO_ID EO_ID, 
    a.EO_NM EO_NM ,
    a.CLD_ID EO_CLD_ID,
    a.EO_TYPE,
A.SUPP_CURR_ID,
A.CONV_FCTR,
A.org_Id
FROM 
    app.APP$SUPP_VW a
where 
 a.EO_TYPE=69
and  
sloc_id = :SlocIdBindVar
and cld_id=:CldIdBindVar
and ho_org_id=:hoOrg
and org_id=:orgIdBindVar
order by a.eo_nm asc]]></SQLQuery>
