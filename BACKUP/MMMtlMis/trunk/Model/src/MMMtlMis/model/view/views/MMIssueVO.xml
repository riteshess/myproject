A.CLD_ID,A.SLOC_ID,A.ORG_ID,A.DOC_ID_ISSU,A.ISSU_NO,A.ISSU_DT,
A.itm_id,A.ITM_DESC,A.ITM_UOM,A.UOM_NM,SUM(A.ISSU_QTY) ISSU_QTY,
A.ISSU_SRC_ID,A.ISSU_SRC_NM,A.DOC_TYPE_SRC, A.DOC_TYPE_SRC_NM,A.DOC_ID_SRC,
A.DOC_NO_SRC DOC_NO,A.DOC_DT_SRC DOC_DT
FROM MM$ISSU$RPT$DTL$VW A
WHERE  A.CLD_ID = NVL(:BindCldId,A.CLD_ID)
AND A.SLOC_ID = NVL(:BindSlocId,A.SLOC_ID)
AND A.ORG_ID = NVL(:BindOrgId,A.ORG_ID)
AND A.ISSU_SRC_ID = NVL(:BindEoId,A.ISSU_SRC_ID)
AND A.ITM_ID = NVL(:BindItmId,A.ITM_ID)
GROUP BY
A.CLD_ID,A.SLOC_ID,A.ORG_ID,A.DOC_ID_ISSU,A.ISSU_NO,A.ISSU_DT,
A.itm_id,A.ITM_DESC,A.ITM_UOM,A.UOM_NM,
A.ISSU_SRC_ID,A.ISSU_SRC_NM,A.DOC_TYPE_SRC, A.DOC_TYPE_SRC_NM,A.DOC_ID_SRC,
A.DOC_NO_SRC ,A.DOC_DT_SRC]]></SQLQuery>
