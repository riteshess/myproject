    A.WH_NM,
    A.ITM_ID,
    A.ITM_DESC,
    A.ITM_UOM,
    A.UOM_NM,
    SUM(A.BAL_QTY) QTY
    From Mm$mis$stk$req_Vw A
    Where
    A.Cld_Id=:Bindcldid
    And A.Org_Id=:Bindorgid
    And A.Sloc_Id=:Bindslocid
    And a.fy_id=:BindFyId
    and A.Itm_Id=nvl(:Binditmid,a.itm_id)
    GROUP BY A.CLD_ID,
    A.SLOC_ID,
    A.ORG_ID,
    A.FY_ID,
    A.STK_RESV_SRC_TYPE,
    A.STK_RESV_SRC_TYPE_NM,
