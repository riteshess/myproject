<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVKitWorkShopVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="inventoryreportapp.model.views.LOVKitWorkShopVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindWhId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindFyId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select DOC_ID,KIT_PROD_NO from MM$KIT$PROD
where CLD_ID = :bindCldId
and SLOC_ID=:bindSlocId
and ORG_ID=:bindOrgId
and WH_ID = NVL(:bindWhId,WH_ID)
and FY_ID=NVL(:bindFyId,FY_ID)]]></SQLQuery>
  <ViewAttribute
    Name="DocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOC_ID"
    Expression="DOC_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="KitProdNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="KIT_PROD_NO"
    Expression="KIT_PROD_NO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="inventoryreportapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
