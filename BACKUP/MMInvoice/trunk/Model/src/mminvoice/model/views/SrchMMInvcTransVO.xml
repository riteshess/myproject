<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SrchMMInvcTransVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mminvoice.model.views.SrchMMInvcTransVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindOrgSrcId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindWhSrcId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindCurrSpId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindItmId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindFrmDt"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <Variable
    Name="bindToDt"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <Variable
    Name="bindFyId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[Select
distinct
A.Cld_Id,
A.Sloc_Id,
A.Org_Id,
A.Doc_Id,
A.Invc_No,
A.Invc_Dt
From Mm.Mm$invc A, Mm.Mm$invc$itm B
Where A.ORG_ID_SRC  Is Not Null
AND A.WH_ID_SRC Is Not Null
And A.Cld_Id = B.Cld_Id
And A.Sloc_Id=B.Sloc_Id
And A.Org_Id=B.Org_Id
AND A.DOC_ID=B.Doc_Id
And A.Cld_Id = :bindCldId
And A.Sloc_Id = :bindSlocId
And A.Org_Id = :bindOrgId
And A.ORG_ID_SRC =:bindOrgSrcId
And A.WH_ID_SRC =:bindWhSrcId
AND A.CURR_ID_SP =:bindCurrSpId
And B.Itm_Id = Nvl(:bindItmId,B.Itm_Id)
And To_Date(A.Invc_Dt,'DD-MON-YY')  >= Nvl(To_Date(:bindFrmDt,'DD-MON-YY'),To_Date(A.Invc_Dt,'DD-MON-YY'))
AND TO_DATE(A.INVC_DT,'DD-MON-YY')  <= NVL(TO_DATE(:bindToDt,'DD-MON-YY'),TO_DATE(A.INVC_DT,'DD-MON-YY'))
AND A.AUTH_STAT = 'Y'
AND A.INVC_TYPE=456
AND A.Fy_Id = NVL(:bindFyId,A.FY_ID)]]></SQLQuery>
