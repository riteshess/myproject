<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="IssuVwVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mminvoice.model.views.IssuVwVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindDocId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select
a.ISSU_NO,
a.doc_id
from MM.MM$MTL$ISSU a
