  <SQLQuery><![CDATA[SELECT  
OUTPUT_ITM_ID,
ITM_DESC,
UOM_BASIC,
UOM_NM,
INPUT_ITM,
TOTAL_ITM,
CREATE_DT,
LAST_MOD_DT,
SLOC_ID,
CLD_ID,
ORG_ID,
 MM.MM_get_ho_org(CLD_ID,SLOC_ID,ORG_ID) HO_ORG_ID
FROM
APP.APP$KIT$SEARCH$VW
          WHERE 
           (ITM_DESC LIKE ('%' || NVL(:ItmDescBindVar,ITM_DESC) || '%') )
          AND UOM_NM=NVL(:UomBindVar,UOM_NM)
          AND INPUT_ITM LIKE ('%' ||  NVL(:InputItmBindVar,INPUT_ITM) || '%')
     AND  trunc(CREATE_DT) between (NVL(:FromCreateDtBindVar,trunc(CREATE_DT))) and  (NVL(:ToCreateDtBindVar,trunc(CREATE_DT)))
AND SLOC_ID = :SlocIdBind
and CLD_ID = :CldIdBind
AND ORG_ID = :OrgIdBind
            
          order by 2]]></SQLQuery>
