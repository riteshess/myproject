a.ho_org_id,
a.SLOC_ID,
a.ITM_ID,
a.ITM_DESC,
a.UOM_BASIC,
a.PRICE_BASIC,
a.CLD_ID,
x.schm_id
FROM mm.mm$itm$vw a, mm.mm$schm x
where (case when a.actv is null then 'Y' else a.actv end)='Y'	
and a.sloc_id = x.sloc_id
and a.cld_id = x.cld_id
and a.org_id=x.org_id
and a.ITM_ID not in (select  b.itm_id from mm$schm$itm b where b.schm_id=nvl(x.schm_id,b.schm_id)
and b.org_id=x.org_id
and b.sloc_id=x.sloc_id
and b.cld_id=x.cld_id)
and a.CLD_ID=:CldIdBindVar
and a.org_id=:orgVar
and a.ho_org_id=:hoOrgVar
and a.SLOC_ID=:SlocIdBindVar]]></SQLQuery>
