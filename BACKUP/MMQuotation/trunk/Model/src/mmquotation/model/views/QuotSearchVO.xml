SLOC_ID,
   ORG_ID, 
   DOC_ID,
   QUOT_ID,
   QUOT_DT,
   EO_ID,
   EO_NM,
   TOT_ITM,
   QUOT_AMT_BS,
   QUOT_AMT_SP,
   CURR_NOTATION_ALIAS,
   ORG_DESC,
   QUOT_STATUS,
   QUOT_STAT_DESC
FROM
MM.MM$QUOT_VW
    WHERE
    QUOT_ID =nvl(upper(:QuotIdBindVar),QUOT_ID)
   AND TRUNC(QUOT_DT) BETWEEN (NVL(:FromQuotDtBindVar,TRUNC(QUOT_DT))) AND (NVL(:ToQuotDtBindVar,TRUNC(QUOT_DT)))
    And Org_Desc = Nvl(:OrgDescBindVar,Org_Desc) 
    AND TOT_ITM = NVL(:TotItmBindVar,TOT_ITM) 
    AND QUOT_AMT_BS  between (NVL(:FromQuotAmtBsBindVar,QUOT_AMT_BS ) )   AND (NVL(:ToQuotAmtBsBindVar,QUOT_AMT_BS ))
    AND SLOC_ID =:SlocIdBind
    And Org_Id =:OrgIdBindVar
    And Quot_Status = Nvl(:QuotStatusBindVar,Quot_Status)
        AND EO_ID =NVL(:EoNmBindVaR,EO_ID)]]></SQLQuery>
