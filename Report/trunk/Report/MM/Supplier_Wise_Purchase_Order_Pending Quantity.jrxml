	<queryString>
		<![CDATA[SELECT a.doc_id,a.org_id, A.AUTH_PO_NO,PO_DT,A.EO_ID,B.ITM_ID,A.PO_AMT_BS, A.PO_AMT_SP,
B.ITM_UOM,   B.ORD_QTY,  (B.ORD_QTY * B.ITM_PRICE)GROSS_PRICE,  B.DISC_VAL,  B.ITM_AMT_SP,
B.BAL_QTY,  (B.ITM_PRICE * A.CURR_CONV_FCTR) BASIC_PRICE,
C.ORG_DESC,D.EO_NM,E.ITM_DESC,F.UOM_NM UOM_DESC,G.CURR_NOTATION_ALIAS
FROM MM$DRFT$PO A,MM$DRFT$PO$ITM B,APP.ORG C,APP.APP$EO D,APP.APP$ITM$PRF E,APP.APP$UOM$CONV$STD F, APP.APP$CURR_VW G
WHERE A.CLD_ID =B.CLD_ID
AND A.SLOC_ID =B.SLOC_ID
AND A.ORG_ID =B.ORG_ID
AND A.DOC_ID =B.DOC_ID
And A.Cld_Id =C.Org_Cld_Id
AND A.SLOC_ID = C.ORG_CREATE_REF_SLOC_ID
AND A.ORG_ID =C.ORG_ID
And A.Cld_Id=D.Eo_Cld_Id
And A.Sloc_Id=D.Eo_Sloc_Id
And MM.MM_GET_HO_ORG(A.CLD_ID,A.SLOC_ID, A.ORG_ID)=D.EO_HO_ORG_ID
AND A.EO_ID =D.EO_ID
And B.Cld_Id=E.Cld_Id
AND B.SLOC_ID=E.SLOC_ID
And MM.MM_GET_HO_ORG(B.CLD_ID,B.SLOC_ID, B.ORG_ID)=E.HO_ORG_ID
And B.Itm_Id =E.Itm_Id
And B.Cld_Id=F.Cld_Id
And B.Sloc_Id=F.Sloc_Id
AND B.ITM_UOM = F.UOM_ID
AND PO_STATUS =217
AND PO_MODE =231
and B.BAL_QTY >0
AND A.CURR_ID_SP = G.CURR_ID
And A.Cld_Id =  nvl($P{CldId},A.Cld_Id)
AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
And A.Org_Id = nvl($P{OrgId},A.Org_Id)
And A.Eo_Id = nvl($P{EoId},A.Eo_Id)
And B.Itm_Id = nvl($P{ItmId},B.Itm_Id)
And A.doc_id = nvl($P{DocId},A.doc_id)
And TRUNC(A.Po_Dt) >=nvl(to_date($P{FromDate},'yyyy-mm-dd'), TRUNC(A.po_dt))
AND TRUNC(A.PO_DT) <=NVL(TO_DATE($P{ToDate},'yyyy-mm-dd'),TRUNC(A.PO_DT))
order by a.doc_id, a.po_dt, A.AUTH_PO_NO,B.ITM_ID]]>
	</queryString>
