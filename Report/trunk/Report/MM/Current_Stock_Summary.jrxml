NVL(A.RWK_STK,0) RWK_STK,(NVL(A.RWK_STK,0) * NVL(MM.MM_GET_ITM_STK_VAL(A.CLD_ID,A.SLOC_ID,A.ORG_ID,A.WH_ID,A.FY_ID,A.ITM_ID,'W'),0)) RWK_VAL,
