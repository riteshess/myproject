FROM MM$MTL$ISSU A,MM$MTL$ISSU$ITM B,APP.APP$ITM$PRF C,
APP.APP$UOM$CONV$STD D,APP.APP$RQMT$AREA E,APP.ORG F,MM$MTL$ISSU$LOT G,MM$STK$SUMM$LOT H ,APP.APP$WH$ORG I
WHERE A.CLD_ID =B.CLD_ID
