	<queryString>
		<![CDATA[Select  V.Org_Id,  V.Org_Desc,  R.Itm_Id,   S.Itm_Desc,  S.Uom_Basic,  T.UOM_NM
,S.GRP_ID,   U.GRP_NM,   app.GET_ITM_GRP_PATH(R.SLOC_ID,R.CLD_ID,R.ORG_ID,S.GRP_ID)GRP_PATH,
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,1) GRP1,
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,2) GRP2,
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,3) GRP3,
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,4) GRP4,
sum(NVL(R.AVL_STK,0))AVL_STK,  sum(Nvl(R.Scrp_Stk,0))Scrp_Stk,  sum(Nvl(R.Rwk_Stk,0))Rwk_Stk,
sum(Nvl(R.Op_Stk,0))Op_Stk  ,sum(nvl(P.Rcpt_Qty,0)) Rcpt_Qty,
sum(Nvl(P.Issu_Qty,0)) Issu_Qty,sum(Nvl(R.Rej_QTY,0))Rej_Qty,
sum(NVL(R.REQ_STK,0))RESV_QTY,
sum((NVL(R.OP_STK,0)+(nvl(P.Rcpt_Qty,0) - nvl(P.ISSU_QTY,0))))CLOSING_STK

from

(SELECT X.CLD_ID, X.ORG_ID, X.SLOC_ID, X.WH_ID, X.ITM_ID, X.ITM_UOM_BS, SUM(X.RCPT_QTY) RCPT_QTY, SUM(X.ISSU_QTY) ISSU_QTY
    FROM
    (SELECT A.CLD_ID,A.ORG_ID,A.SLOC_ID,B.WH_ID,B.ITM_ID,B.ITM_UOM_BS, SUM(B.FINAL_RCPT_QTY_BS) RCPT_QTY,0 ISSU_QTY
        FROM MM$MTL$RCPT A,MM$MTL$RCPT$ITM B
        WHERE A.CLD_ID = NVL($P{CldId} ,A.CLD_ID)
        AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
        AND A.ORG_ID = NVL($P{OrgId} ,A.ORG_ID)
        AND A.FY_ID =  NVL($P{FyId} ,A.FY_ID)
        AND B.ITM_ID = NVL($P{ItmId},B.ITM_ID)
        AND TRUNC(A.RCPT_DT) >=NVL(TO_DATE($P{FromDate},'yyyy-mm-dd'),TRUNC(A.RCPT_DT))
        AND TRUNC(A.RCPT_DT) <=NVL(TO_DATE($P{ToDate} ,'yyyy-mm-dd'),TRUNC(A.RCPT_DT))
        AND A.CLD_ID = B.CLD_ID
        AND A.SLOC_ID = B.SLOC_ID
        And A.Org_Id = B.Org_Id
        And A.Doc_Id = B.Doc_Id
        and a.wh_id = b.wh_id
        AND A.RCPT_SRC_TYPE <> 396
        --AND A.AUTH_STAT = 'Y'
        AND A.RCPT_STAT IN (383, 384)
    GROUP BY A.CLD_ID,A.ORG_ID,A.SLOC_ID,B.WH_ID,B.ITM_ID,B.ITM_UOM_BS
  UNION
  SELECT A.CLD_ID,A.ORG_ID,A.SLOC_ID,B.WH_ID,B.ITM_ID,B.ITM_UOM,0 RCPT_QTY, SUM(issu_qty_bs) ISSU_QTY
    FROM MM$MTL$ISSU A,MM$MTL$ISSU$ITM B
    WHERE A.CLD_ID = NVL( $P{CldId},A.CLD_ID)
    AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
    AND A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
    AND A.FY_ID =  NVL( $P{FyId},A.FY_ID)
    AND B.ITM_ID = NVL($P{ItmId},B.ITM_ID)
    AND TRUNC(A.issu_DT) >=NVL(TO_DATE($P{FromDate} ,'yyyy-mm-dd'),TRUNC(A.ISSU_DT))
    AND TRUNC(A.ISSU_DT) <=NVL(TO_DATE( $P{ToDate},'yyyy-mm-dd'),TRUNC(A.ISSU_DT))
    AND  A.CLD_ID = B.CLD_ID
    AND A.CLD_ID = B.CLD_ID
    AND A.SLOC_ID = B.SLOC_ID
    And A.Org_Id = B.Org_Id
    AND A.DOC_ID =B.DOC_ID
    and a.wh_id = b.wh_id
    AND A.ISSU_STAT = 369
    GROUP BY A.CLD_ID,A.ORG_ID,A.SLOC_ID,B.WH_ID,B.ITM_ID,B.ITM_UOM)X
    Group By X.Cld_Id,X.Org_Id,X.Sloc_Id,X.Itm_Id,X.Itm_Uom_BS,X.Wh_Id)P,

(SELECT R.CLD_ID, R.SLOC_ID, R.ORG_ID, R.WH_ID, R.FY_ID, R.ITM_ID, R.ITM_UOM_BS, SUM(NVL(R.TOT_STK,0))TOT_STK, SUM(NVL(R.AVL_STK,0))AVL_STK,
SUM(NVL(R.REQ_STK,0))REQ_STK, SUM(NVL(R.SCRP_STK,0))SCRP_STK, SUM(NVL(R.RWK_STK,0))RWK_STK, SUM(NVL(R.OP_STK,0))OP_STK
,SUM(NVL(R.REJ_QTY,0))REJ_QTY
FROM
(SELECT R.CLD_ID, R.SLOC_ID, R.ORG_ID, R.WH_ID,  R.FY_ID,  R.ITM_ID, R.ITM_UOM_BS, R.TOT_STK,  R.AVL_STK,  R.REQ_STK,
Nvl(R.Scrp_Stk,0)Scrp_Stk,  Nvl(R.Rwk_Stk,0)Rwk_Stk,
Nvl(R.Op_Stk,0)Op_Stk  , Nvl(R.Rej_Stk,0)Rej_Qty
FROM MM$STK$SUMM$ITM R
WHERE  R.CLD_ID = NVL($P{CldId},R.CLD_ID)
AND R.SLOC_ID = NVL($P{SlocId},R.SLOC_ID)
AND R.ORG_ID  = NVL($P{OrgId},R.ORG_ID)
AND R.ITM_ID = NVL($P{ItmId}, R.ITM_ID)
and R.fy_id = NVL($P{FyId},R.FY_ID)
UNION
SELECT R.CLD_ID, R.SLOC_ID, R.ORG_ID, R.WH_ID,  R.FY_ID,  R.ITM_ID, R.ITM_UOM_BS, (R.RCPT_STK - R.CONSM_STK + R.RET_STK)TOT_STK,
R.AVL_STK,  R.REQ_STK, 0 Scrp_Stk,  0 Rwk_Stk,
0 Op_Stk  , 0 Rej_Qty
FROM MM$STK$SUMM$ITM$RQMT R
WHERE  R.CLD_ID = NVL($P{CldId},R.CLD_ID)
AND R.SLOC_ID = NVL($P{SlocId},R.SLOC_ID)
AND R.ORG_ID  = NVL($P{OrgId},R.ORG_ID)
AND R.ITM_ID = NVL($P{ItmId}, R.ITM_ID)
AND nvl(R.STK_OWNERSHIP_TYPE,841) in (841, 842)
and R.fy_id = NVL($P{FyId},R.FY_ID))R
GROUP BY R.CLD_ID, R.SLOC_ID, R.ORG_ID, R.WH_ID, R.FY_ID, R.ITM_ID,R.ITM_UOM_BS)R
, App.App$itm$prf S ,APP.APP$UOM$CONV$STD T ,APP.APP$GRP U, APP.ORG V, APP.APP$WH$ORG W
Where P.Cld_Id(+) = R.Cld_Id
And P.Sloc_Id(+) = R.Sloc_Id
And P.Org_Id(+) = R.Org_Id
And P.Itm_Id(+) = R.Itm_Id
AND P.WH_ID(+) =R.WH_ID
and R.cld_id = s.cld_id
And R.Sloc_Id = S.Sloc_Id
And MM.MM_GET_HO_ORG(R.CLD_ID,R.SLOC_ID,R.Org_Id) = S.Ho_Org_Id
And R.Itm_Id = S.Itm_Id
And S.Cld_Id = U.Cld_Id
And S.Sloc_Id = U.Sloc_Id
AND S.HO_ORG_ID = U.HO_ORG_ID
and s.grp_id = u.grp_id
AND R.ITM_UOM_BS = T.UOM_ID
AND R.CLD_ID = NVL($P{CldId},R.CLD_ID)
AND R.SLOC_ID = NVL($P{SlocId},R.SLOC_ID)
AND R.ORG_ID  = NVL($P{OrgId},R.ORG_ID)
AND R.ITM_ID = NVL($P{ItmId}, R.ITM_ID)
and R.fy_id = NVL($P{FyId},R.FY_ID)
and R.cld_id = v.org_cld_id
and R.org_id = v.org_id
and R.cld_id = W.Cld_id
and R.sloc_id = w.sloc_id
And R.Org_Id =W.Org_Id
and R.wh_id = w.wh_id
AND S.GRP_ID IN (SELECT GRP_ID FROM APP.APP$GRP CONNECT BY PRIOR GRP_ID = GRP_ID_PARENT
                  Start With Grp_Id = Nvl($P{GrpId},S.Grp_Id))
Group by   V.Org_Id,  V.Org_Desc,  R.Itm_Id,   S.Itm_Desc,  S.Uom_Basic,  T.UOM_NM
,S.GRP_ID,   U.GRP_NM,   app.GET_ITM_GRP_PATH(R.SLOC_ID,R.CLD_ID,R.ORG_ID,S.GRP_ID),
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,1),
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,2) ,
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,3) ,
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,4)
order by
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,1),
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,2) ,
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,3) ,
regexp_substr(app.GET_ITM_GRP_PATH(S.SLOC_ID,S.CLD_ID,S.HO_ORG_ID,S.GRP_ID),'[^>>]+',1,4),
R.itm_id]]>
