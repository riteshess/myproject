	<queryString>
		<![CDATA[SELECT A.ORG_ID,  A.WH_ID,  J.WH_NM,  A.ISSU_NO,  A.ISSU_DT,  B.ITM_ID,  C.ITM_DESC,  G.LOT_ID,
      D.UOM_DESC, G.ISSU_QTY, E.RQMT_AREA_NM,  I.ATT_NM ISSUED_TO,
      F.ORG_DESC,  H.LND_PRICE,  (G.ISSU_QTY * H.LND_PRICE) VAL_ISSUD_QTY, B.DOC_ID_SRC,
      DECODE(A.ISSU_TYPE,323,'Requirement Area' ,324, 'Customer' ,325, 'Warehouse') SRC_TYPE
      ,DECODE(B.ISSU_DOC_TYPE, 326, (SELECT X.MRS_NO FROM MM.MM$MRS X WHERE B.CLD_ID  = X.CLD_ID
                                                                      AND   B.SLOC_ID = X.SLOC_ID
                                                                      AND   B.ORG_ID  = X.ORG_ID
                                                                      AND   B.DOC_ID_SRC  = X.MRS_TXN_ID)
  ,329,(SELECT SUBSTR(Y.SHIPMNT_ID,3) FROM SLS.SLS$SHIPMNT Y
                                                                    WHERE B.CLD_ID = Y.CLD_ID
                                                                    AND B.SLOC_ID = Y.SLOC_ID
                                                                    AND B.ORG_ID = Y.ORG_ID
                                                                    AND B.DOC_ID_SRC = Y.DOC_ID
                                                                    AND ROWNUM = 1))SRC_DOC_NO
      ,DECODE(B.ISSU_DOC_TYPE, 326, (SELECT X.MRS_DT FROM MM.MM$MRS X WHERE B.CLD_ID  = X.CLD_ID
                                                                      AND   B.SLOC_ID = X.SLOC_ID
                                                                      AND   B.ORG_ID  = X.ORG_ID
                                                                      AND   B.DOC_ID_SRC  = X.MRS_TXN_ID)
 ,329,(SELECT Y.DOC_DT FROM SLS.SLS$SHIPMNT Y
                                                                    WHERE B.CLD_ID = Y.CLD_ID
                                                                    AND B.SLOC_ID = Y.SLOC_ID
                                                                    AND B.ORG_ID = Y.ORG_ID
                                                                    AND B.DOC_ID_SRC = Y.DOC_ID
                                                                    AND ROWNUM = 1))SRC_DOC_DT
FROM MM$MTL$ISSU A,MM$MTL$ISSU$ITM B,APP.APP$ITM$PRF C,
APP.APP$UOM$CONV$STD D,APP.APP$RQMT$AREA E,APP.ORG F,
MM$MTL$ISSU$LOT G,MM$STK$SUMM$LOT H,APP.APP$DS$ATT I,APP.APP$WH$ORG J
WHERE A.CLD_ID=B.CLD_ID
AND A.SLOC_ID =B.SLOC_ID
AND A.ORG_ID =B.ORG_ID
AND A.DOC_ID =B.DOC_ID
AND A.CLD_ID = E.CLD_ID(+)
AND A.SLOC_ID = E.SLOC_ID(+)
AND A.ORG_ID = E.ORG_ID(+)
AND A.RQMT_AREA_ID_SRC = E.RQMT_AREA_ID(+)
AND A.ORG_ID =F.ORG_ID
AND a.CLD_ID = F.ORG_CLD_ID
AND A.ORG_ID =F. ORG_ID
AND A.SLOC_ID= F.ORG_CREATE_REF_SLOC_ID
AND A.ISSU_TYPE = I.ATT_ID
AND A.CLD_ID =J.CLD_ID
AND A.SLOC_ID =J.SLOC_ID
AND A.ORG_ID =J.ORG_ID
AND A.WH_ID =J.WH_ID
AND B.CLD_ID =C.CLD_ID
AND B.SLOC_ID=C.SLOC_ID
AND MM.MM_GET_HO_ORG(B.CLD_ID, B.SLOC_ID, B.ORG_ID) = C.HO_ORG_ID
AND B.ITM_ID=C.ITM_ID
AND B.CLD_ID =D.CLD_ID
AND B.SLOC_ID=D.SLOC_ID
AND B.ITM_UOM=D.UOM_ID
AND B.CLD_ID =G.CLD_ID
AND B.SLOC_ID =G.SLOC_ID
AND B.ORG_ID =G.ORG_ID
AND B.WH_ID =G.WH_ID
AND B.DOC_ID =G.DOC_ID
AND B.ITM_ID =G.ITM_ID
and b.DOC_ID_SRC = g.DOC_ID_SRC
AND B.ITM_UOM = G.ITM_UOM
AND G.cld_id =H.cld_id
AND G.sloc_id =H.sloc_id
AND G.org_id =H.org_id
AND G.WH_ID =H.WH_ID
AND G.LOT_ID=H.LOT_ID
AND G.ITM_ID =H.ITM_ID
And A.Cld_Id= nvl($P{CldId},A.Cld_Id)
AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
and a.org_id = nvl($P{OrgId},a.org_id)
and a.wh_id = nvl($P{WhId},a.wh_id)
and a.fy_id = nvl($P{FyId},a.fy_id)
and a.doc_id = nvl($P{IssuDocId},a.doc_id)
and TRUNC(a.ISSU_DT) >= nvl(to_date($P{FromDate},'yyyy-mm-dd'),TRUNC(a.ISSU_DT))
and TRUNC(a.ISSU_DT) <= nvl(to_date($P{ToDate},'yyyy-mm-dd'),TRUNC(a.ISSU_DT))
and b.itm_id = nvl($P{ItmId},b.itm_id)
and a.issu_type = nvl($P{IssuTyp},a.issu_type)
order by A.WH_ID,   A.ISSU_NO desc, SRC_DOC_NO, B.ITM_ID]]>
	</queryString>
