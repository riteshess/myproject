	<queryString>
		<![CDATA[SELECT A.ORG_ID, O.ORG_DESC, A.DOC_ID, A.IDF_APPL_NO, A.IDF_APPL_DT, A.IDF_NO, A.IDF_DT, A.DOC_ID_PO
,A.IMPORTER_NM, A.IMPORTER_DEGN, A.CNTRY_ID_SRC, S.CNTRY_DESC, A.PORT_ID_SRC, U.PORT_NM PORT_SRC_NM, A.PORT_ID_DEST, V.PORT_NM PORT_DEST_NM
, A.PORT_ID_CSTM, W.PORT_NM PORT_CSTM_NM,A.DISCH_DT, A.DLV_DAYS, A.TPT_MODE, T.ATT_NM TPT_MODE_DESC, A.CURR_ID_SP, R.CURR_DESC
,A.IDF_FEE, A.IDF_FEE_ADV, A.IDF_RCPT_NO, A.IDF_RCPT_DT, A.FOB_AMT,EO.EO_NM,

APP.FN_GET_ADDS(A.SLOC_ID, A.CLD_ID, NVL(OT.ORG_ADD_ID1,OT.ORG_ADD_ID2) , 'M')ORG_ADDS, NVL(OT.ORG_EMAIL1,OT.ORG_EMAIL2) EMAIL_ID,
NVL(OT.ORG_PHONE1,OT.ORG_PHONE2) PHONE_NO, NVL(OT.ORG_FAX1, OT.ORG_FAX2) FAX,
APP.FN_GET_ADDS(EO.EO_SLOC_ID, EO.EO_CLD_ID, AD.ADDS_ID , 'M') SUPP_ADDS

, A.INSUR_NO, A.INSUR_WITH, A.INSUR_AMT, A.FREIGHT_AMT, A.OC_TOT_AMT, A.REMARKS
, A.REF_NO, DECODE(A.APPORVED,'Y','Yes','N','No')APPROVED, A.DOC_STAT, X.ATT_NM DOC_STAT_DESC
,B.ITM_ID, P.ITM_DESC,P.CHPTR_ID, B.ITM_UOM, Q.UOM_NM, B.ORD_QTY, B.ITM_PRICE_SP, B.ITM_PRICE_BS, B.ITM_FOB_VAL_SP, B.ITM_AMT_SP, B.ITM_AMT_BS
,C.PO_ID,C.AUTH_PO_NO, C.PO_DT

,(SELECT nvl(SUM(D.OC_AMT_SP),0)FROM MM$IPO$IDF$OC D WHERE D.CLD_ID = A.CLD_ID
AND D.SLOC_ID = A.SLOC_ID
AND D.ORG_ID = A.ORG_ID
AND D.DOC_ID = A.DOC_ID)OC_AMT_SP

FROM MM$IPO$IDF A, MM$IPO$IDF$ITM B,MM$DRFT$PO C, app.org o, APP.APP$ITM$PRF P, APP.APP$UOM$CONV$STD Q
,APP.APP$CURR R, APP.APP$CNTRY S, APP.APP$DS$ATT T, APP.APP$GLBL$CNTRY$PORTS U, APP.APP$GLBL$CNTRY$PORTS V
,APP.APP$GLBL$CNTRY$PORTS W, APP.APP$DS$ATT X
, APP.ORG$OTH$DTL OT, APP.APP$EO$ADDS AD, APP.APP$EO EO
WHERE A.CLD_ID = B.CLD_ID
AND A.SLOC_ID = B.SLOC_ID
AND A.ORG_ID = B.ORG_ID
AND A.DOC_ID = B.DOC_ID
AND A.CLD_ID = C.CLD_ID
AND A.SLOC_ID = C.SLOC_ID
AND A.ORG_ID = C.ORG_ID
AND A.DOC_ID_PO = C.DOC_ID
AND A.CLD_ID = O.ORG_CLD_ID
AND A.SLOC_ID = O.ORG_CREATE_REF_SLOC_ID
AND A.ORG_ID = O.ORG_ID
AND B.CLD_ID = P.CLD_ID
AND B.SLOC_ID = P.SLOC_ID
AND MM.MM_GET_HO_ORG(B.CLD_ID, B.SLOC_ID, B.ORG_ID) = P.HO_ORG_ID
AND B.ITM_ID = P.ITM_ID
AND B.CLD_ID = Q.CLD_ID
AND B.SLOC_ID = Q.SLOC_ID
AND B.ITM_UOM = Q.UOM_ID
AND A.CLD_ID = R.CLD_ID(+)
AND A.SLOC_ID = R.CURR_SLOC_ID(+)
AND A.CURR_ID_SP = R.CURR_ID(+)
AND A.CLD_ID = S.CNTRY_CLD_ID
AND A.SLOC_ID = S.CNTRY_SLOC_ID
AND A.CNTRY_ID_SRC = S.CNTRY_ID
AND A.TPT_MODE = T.ATT_ID(+)
AND A.PORT_ID_SRC = U.PORT_ID(+)
AND A.CNTRY_ID_SRC = U.CNTRY_ID(+)
AND A.PORT_ID_DEST = V.PORT_ID(+)
AND A.CNTRY_ID_SRC = V.CNTRY_ID(+)
AND A.PORT_ID_CSTM = W.PORT_ID(+)
AND A.CNTRY_ID_SRC = W.CNTRY_ID(+)
AND A.DOC_STAT = X.ATT_ID

AND A.CLD_ID = OT.ORG_CLD_ID(+)
AND A.ORG_ID = OT.ORG_ID(+)

AND C.SLOC_ID = EO.EO_SLOC_ID(+)
AND C.CLD_ID = EO.EO_CLD_ID(+)
AND C.ORG_ID = EO.EO_ORG_ID(+)
AND MM.MM_GET_HO_ORG(C.CLD_ID, C.SLOC_ID, C.ORG_ID) = EO.EO_HO_ORG_ID(+)
AND C.EO_ID = EO.EO_ID(+)


AND EO.EO_CLD_ID = AD.CLD_ID(+)
AND EO.EO_SLOC_ID = AD.SLOC_ID(+)
AND EO.EO_HO_ORG_ID = AD.HO_ORG_ID(+)
AND EO.EO_ORG_ID = AD.ORG_ID(+)
AND EO.EO_ID = AD.EO_ID(+)


AND A.CLD_ID = NVL($P{CldId}, A.CLD_ID)
AND A.SLOC_ID = NVL($P{SlocId}, A.SLOC_ID)
AND A.ORG_ID = NVL($P{OrgId}, A.ORG_ID)
AND A.DOC_ID = NVL($P{IDFDocId}, A.DOC_ID)
AND A.DOC_ID_PO = NVL($P{DocId},A.DOC_ID_PO)
ORDER BY A.IDF_APPL_NO]]>
	</queryString>
