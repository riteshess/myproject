and awo.adds_id=aav.adds_id(+)
and awo.cld_id=aav.cld_id(+)
and awo.sloc_id=aav.sloc_id(+)
and awo.WH_ONRSHP_TYPE=ada1.att_id
and awo.WH_STRG_TYPE=ada2.att_id
