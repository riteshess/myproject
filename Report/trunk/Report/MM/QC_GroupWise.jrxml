FROM MM$QC$PARAM A, MM$QC$PARAM$GRP B, APP.ORG C, APP.APP$GRP D, APP.APP$DS$ATT E, APP.APP$DS$ATT F
WHERE A.CLD_ID = B.CLD_ID
