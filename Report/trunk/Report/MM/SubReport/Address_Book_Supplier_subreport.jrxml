AND A.EO_CLD_ID =  $P{CldId}
AND A.EO_SLOC_ID =  $P{SlocId}
AND A.EO_HO_ORG_ID =  MM.MM_GET_HO_ORG($P{CldId},$P{SlocId},$P{OrgId})
ORDER BY A.EO_NM]]>
