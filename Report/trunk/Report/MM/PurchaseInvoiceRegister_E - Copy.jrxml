	<queryString>
		<![CDATA[SELECT A.ORG_ID, B.DOC_ID_RCPT,
D.ORG_DESC, A.DOC_ID, (select substr(doc_txn_id_disp,3) from app.app$doc$txn where doc_txn_id = a.gl_vou_id)vou_disp_id,
(select grp_nm from app.app$grp g where i.ho_org_id = g.ho_org_id and i.grp_id = g.grp_id)grp_nm,
  A.INVC_NO,  A.INVC_DT, A.EO_ID_SRC, E.EO_NM, A.CURR_ID_SP, F.CURR_NOTATION_ALIAS CURR_DESC
,A.TOT_AMT_SP TOT_AMT_SP, A.TOT_AMT_BS, A.ORG_ID_SRC, A.WH_ID_SRC, A.INVC_TYPE, G.ATT_NM INVC_TYPE_NM
, B.ITM_ID, I.ITM_DESC, B.ITM_UOM,  J.UOM_NM, B.FINAL_RCPT_QTY
,NVL(B.DISC_AMT_SP,0) DISC_AMT_SP,    B.ITM_AMT_SP ITM_AMT_SP
,NVL(B.TAXABLE_AMT_SP,(B.FINAL_RCPT_QTY * B.ITM_PRICE)) GROSS_PRICE
-------------tot amt
,(select sum(net_amt_sp)
from
(SELECT (SUM(NVL(B.TAXABLE_AMT_SP,(B.FINAL_RCPT_QTY * B.ITM_PRICE))) +  SUM(NVL(C.TAX_AMT,0)) - SUM(NVL(B.DISC_AMT_SP,0))) NET_AMT_SP
,SUM(A.TOT_AMT_SP) TOT_AMT_SP, SUM(A.TOT_AMT_BS)TOT_AMT_BS
FROM MM.MM$INVC A, MM.MM$INVC$ITM B, MM$INVC$TR C
WHERE A.CLD_ID = B.CLD_ID
AND A.SLOC_ID = B.SLOC_ID
AND A.ORG_ID = B.ORG_ID
AND A.DOC_ID = B.DOC_ID
AND B.CLD_ID = C.CLD_ID(+)
AND B.SLOC_ID = C.SLOC_ID(+)
AND B.ORG_ID = C.ORG_ID(+)
AND B.DOC_ID = C.DOC_ID(+)
AND B.DOC_ID_RCPT = C.DOC_ID_RCPT(+)
AND B.DOC_ID_PO = C.DOC_ID_PO(+)
AND B.DLV_SCHDL_NO = C.DLV_SCHDL_NO(+)
AND B.ITM_ID = C.ITM_ID(+)
AND B.ITM_UOM = C.ITM_UOM(+)
AND A.AUTH_STAT = 'Y'
AND A.INVC_TYPE = NVL($P{InvcType},A.INVC_TYPE)
AND A.CLD_ID = NVL($P{CldId},A.CLD_ID)
AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
AND A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
AND A.DOC_ID= NVL($P{DocId},A.DOC_ID)
AND A.EO_ID_SRC = NVL($P{EoIdSrc},A.EO_ID_SRC)
AND TRUNC(A.INVC_DT) >= NVL(TO_DATE($P{FromDate},'yyyy-mm-dd'),TRUNC(A.INVC_DT))
AND TRUNC(A.INVC_DT) <= NVL(TO_DATE($P{ToDate},'yyyy-mm-dd'),TRUNC(A.INVC_DT))))net_amt_TOT
--end of tot amt
--tax amt and name
,tax.net_amt
,NVL(TN.TAX_AMT_SP,0)TAX_AMT_SP
, (CASE WHEN initcap(TN.TAX_NM) LIKE 'Basic' THEN 'Z'||initcap(TN.TAX_NM) ELSE initcap(TN.TAX_NM) END )TAX_NM
,DECODE(A.INVC_TYPE ,739, A.EXT_DOC_NO, R.INVOICE_NO_SRC) TPT_BILL_NO,
DECODE(A.INVC_TYPE , 739, A.EXT_DOC_DT, R.INVOICE_DT_SRC) TPT_BILL_DT, R.RCPT_NO, R.DN_NO_SRC, R.DN_DT_SRC
FROM MM.MM$INVC A, MM.MM$INVC$ITM B,
APP.ORG D, APP.APP$EO E, APP.APP$CURR_VW F, APP.APP$DS$ATT G
, APP.APP$ITM$PRF I, APP.APP$UOM$CONV$STD J
--tax amt and name
,(select  TL.CLD_ID, TL.SLOC_ID, TL.ORG_ID, TL.DOC_ID, TL.DOC_ID_RCPT,TL.ITM_ID, sum(NVL(TL.TAX_AMT_BS,0))tax_amt_sp,
INITCAP(FC.COA_NM)TAX_NM
from mm$invc$tr$lines TL, fin.fin$COA FC
WHERE TL.CLD_ID = FC.COA_CLD_ID
AND TL.SLOC_ID = FC.COA_SLOC_ID
AND APP.FN_GET_PARENT_ORG_L0(TL.CLD_ID, TL.ORG_ID) = FC.COA_HO_ORG_ID
AND TL.TAX_COA_ID = FC.COA_ID
group by TL.CLD_ID, TL.SLOC_ID, TL.ORG_ID, TL.DOC_ID, TL.ITM_ID, INITCAP(FC.COA_NM),TL.DOC_ID_RCPT
having sum(tl.tax_amt_sp)>0
)TN
,(SELECT a.org_id, a.doc_id, b.itm_id, B.DOC_ID_RCPT,
(SUM(NVL(B.TAXABLE_AMT_SP,(B.FINAL_RCPT_QTY * B.ITM_PRICE))) +  SUM(NVL(C.TAX_AMT,0)) - SUM(NVL(B.DISC_AMT_SP,0))) NET_AMT
FROM MM.MM$INVC A, MM.MM$INVC$ITM B, MM$INVC$TR C
WHERE A.CLD_ID = B.CLD_ID
AND A.SLOC_ID = B.SLOC_ID
AND A.ORG_ID = B.ORG_ID
AND A.DOC_ID = B.DOC_ID
AND B.CLD_ID = C.CLD_ID(+)
AND B.SLOC_ID = C.SLOC_ID(+)
AND B.ORG_ID = C.ORG_ID(+)
AND B.DOC_ID = C.DOC_ID(+)
AND B.DOC_ID_RCPT = C.DOC_ID_RCPT(+)
AND B.DOC_ID_PO = C.DOC_ID_PO(+)
AND B.DLV_SCHDL_NO = C.DLV_SCHDL_NO(+)
AND B.ITM_ID = C.ITM_ID(+)
AND B.ITM_UOM = C.ITM_UOM(+)
AND A.AUTH_STAT = 'Y'
AND A.INVC_TYPE = NVL($P{InvcType},A.INVC_TYPE)
AND A.CLD_ID = NVL($P{CldId},A.CLD_ID)
AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
AND A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
AND A.DOC_ID= NVL($P{DocId},A.DOC_ID)
AND A.EO_ID_SRC = NVL($P{EoIdSrc},A.EO_ID_SRC)
AND TRUNC(A.INVC_DT) >= NVL(TO_DATE($P{FromDate},'yyyy-mm-dd'),TRUNC(A.INVC_DT))
AND TRUNC(A.INVC_DT) <= NVL(TO_DATE($P{ToDate},'yyyy-mm-dd'),TRUNC(A.INVC_DT))
group by a.org_id, a.doc_id, b.itm_id, B.DOC_ID_RCPT, B.DOC_ID_PO)tax
,MM$MTL$RCPT R
WHERE A.CLD_ID = B.CLD_ID
AND A.SLOC_ID = B.SLOC_ID
AND A.ORG_ID = B.ORG_ID
AND A.DOC_ID = B.DOC_ID
AND B.ORG_ID  = TAX.ORG_ID(+)
AND B.DOC_ID  = TAX.DOC_ID(+)
AND B.ITM_ID  = TAX.ITM_ID(+)
AND B.DOC_ID_RCPT = TAX.DOC_ID_RCPT(+)
AND B.ORG_ID  = TN.ORG_ID(+)
AND B.DOC_ID  = TN.DOC_ID(+)
AND B.ITM_ID  = TN.ITM_ID(+)
AND B.DOC_ID_RCPT = TN.DOC_ID_RCPT(+)
AND B.CLD_ID    = I.CLD_ID
AND  B.SLOC_ID = I.SLOC_ID
AND   MM.MM_GET_HO_ORG(B.CLD_ID, B.SLOC_ID, B.ORG_ID) = I.HO_ORG_ID
AND   B.ITM_ID = I.ITM_ID
AND   B.ITM_UOM = J.UOM_ID
AND A.CLD_ID = D.ORG_CLD_ID
AND A.SLOC_ID = D.ORG_CREATE_REF_SLOC_ID
AND A.ORG_ID = D.ORG_ID
AND A.CLD_ID = E.EO_CLD_ID
AND A.SLOC_ID = E.EO_SLOC_ID
AND A.EO_ID_SRC = E.EO_ID
AND MM.MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID)= E.EO_HO_ORG_ID
AND A.CURR_ID_SP = F.CURR_ID
AND A.INVC_TYPE = G.ATT_ID
AND A.AUTH_STAT = 'Y'
and a.INVC_TYPE <> 492
AND B.CLD_ID  = R.CLD_ID(+)
AND B.SLOC_ID = R.SLOC_ID(+)
AND B.ORG_ID  = R.ORG_ID(+)
AND B.DOC_ID_RCPT  = R.DOC_ID(+)
AND A.INVC_TYPE = NVL($P{InvcType},A.INVC_TYPE)
AND A.CLD_ID = NVL($P{CldId},A.CLD_ID)
AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
AND A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
AND A.DOC_ID= NVL($P{DocId},A.DOC_ID)
AND A.EO_ID_SRC = NVL($P{EoIdSrc},A.EO_ID_SRC)
AND TRUNC(A.INVC_DT) >= NVL(TO_DATE($P{FromDate},'yyyy-mm-dd'),TRUNC(A.INVC_DT))
AND TRUNC(A.INVC_DT) <= NVL(TO_DATE($P{ToDate},'yyyy-mm-dd'),TRUNC(A.INVC_DT))
ORDER BY A.INVC_NO DESC, A.INVC_DT]]>
	</queryString>
