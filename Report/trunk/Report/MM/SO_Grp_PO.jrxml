	<queryString>
		<![CDATA[SELECT A.ORG_ID, C.ORG_DESC, A.SO_GRP_ID, trunc(A.SO_GRP_DT)so_grp_dt, A.EO_ID, F.EO_NM,  A.ITM_ID, D.ITM_DESC,
  A.ITM_UOM, E.UOM_NM, sum(A.TOT_SO_QTY)tot_so_qty,
    NVL( B.AUTH_PO_NO, B.PO_ID) PO_NO, B.PO_DT, B.PO_STATUS, G.ATT_NM PO_STAT_NM
   FROM MM$SO$GRP A, MM$DRFT$PO B, APP.ORG C, APP.APP$ITM$PRF D,
   APP.APP$UOM$CONV$STD E, APP.APP$EO F, APP.APP$DS$ATT G
   WHERE A.CLD_ID = B.CLD_ID
   AND A.SLOC_ID  = B.SLOC_ID
   AND A.ORG_ID = B.ORG_ID
   AND A.SO_GRP_ID = B.SO_GRP_ID
   AND A.ORG_ID = C.ORG_ID
   AND MM.MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID) = D.HO_ORG_ID
   AND A.ITM_ID = D.ITM_ID
   AND A.ITM_UOM = E.UOM_ID
   AND MM.MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID) = F.EO_HO_ORG_ID
   AND A.EO_ID = F.EO_ID
   AND B.PO_STATUS = G.ATT_ID
AND A.CLD_ID	 = NVL($P{CldId}, A.CLD_ID)
AND A.SLOC_ID = NVL($P{SlocId}, A.SLOC_ID)
   AND A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
AND A.SO_GRP_ID = NVL($P{SoGrpId}, A.SO_GRP_ID)
AND A.EO_ID = NVL($P{EoId}, A.EO_ID)
AND A.ITM_ID = NVL($P{ItmId}, A.ITM_ID)
AND TRUNC(A.SO_GRP_DT) BETWEEN NVL(TO_DATE($P{FromDate},'YYYY-MM-DD'),trunc(A.SO_GRP_DT)) AND NVL(TO_DATE($P{ToDate},'YYYY-MM-DD'),trunc(A.SO_GRP_DT))
group BY A.ORG_ID, C.ORG_DESC, A.SO_GRP_ID, trunc(A.SO_GRP_DT), A.EO_ID, F.EO_NM,  A.ITM_ID, D.ITM_DESC,
  A.ITM_UOM, E.UOM_NM,  NVL( B.AUTH_PO_NO, B.PO_ID) , B.PO_DT, B.PO_STATUS, G.ATT_NM
order by a.org_id, a.so_grp_id desc]]>
