	<queryString>
		<![CDATA[SELECT   A.ORG_ID, ao.org_desc ,  A.ITM_ID,    A.ITM_UOM,    A.ITM_DESC,    sum(A.LOT_QTY) LOT_QTY,
    sum(A.OSDAYS) OSDAYS,    sum(A.OS_30_DAYS) OS_30_DAYS,    sum(A.OS_60_DAYS) OS_60_DAYS,
    sum(A.OS_90_DAYS) OS_90_DAYS,    sum(A.OS_120_DAYS) OS_120_DAYS,    TRUNC(A.DOC_DT_SRC) DOC_DT
,a.grp_id, ag.grp_nm
FROM    SLS.SLS$REP$PRODUCT$AGEING_VW A, app.org ao, app.app$grp ag
WHERE    a.org_id = ao.org_id
and a.cld_id = ao.org_cld_id
and a.sloc_id = ao.ORG_CREATE_REF_SLOC_ID
and a.cld_id = ag.cld_id
and a.sloc_id = ag.sloc_id
 and a.ho_org_id = ag.ho_org_id
 and a.grp_id = ag.grp_id
and a.org_id = nvl($P{OrgId}, a.org_id)
and a.cld_id = nvl($P{CldId},a.cld_id)
and a.sloc_id = nvl($P{SlocId},a.sloc_id)
and a.ho_org_id = nvl($P{HoOrgId},a.ho_org_id)
and a.itm_id = nvl($P{ItmId},a.itm_id)
and a.grp_id = nvl($P{GrpId},a.grp_id)
and trunc(a.doc_dt_src)<=nvl(to_date($P{ToDate},'yyyy-MM-dd'),trunc(a.doc_dt_src))
GROUP BY
    A.ORG_ID,ao.org_desc,
    A.ITM_ID,
    A.ITM_UOM,
    A.ITM_DESC,
    TRUNC(A.DOC_DT_SRC),a.grp_id, ag.grp_nm]]>
