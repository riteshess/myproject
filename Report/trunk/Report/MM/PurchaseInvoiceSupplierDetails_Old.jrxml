	<queryString>
		<![CDATA[SELECT A.ORG_ID, D.ORG_DESC, A.DOC_ID,A.EO_ID_SRC, E.EO_NM, M.TIN_NO, A.INVC_NO, A.INVC_DT,  L.ATT_NM INVC_TYPE, A.CURR_ID_SP, F.CURR_NOTATION_ALIAS CURR_DESC,B.ITM_UOM,  J.UOM_NM
,sum(B.FINAL_RCPT_QTY) FINAL_RCPT_QTY, sum(B.ITM_PRICE) ITM_PRICE
,sum(NVL(B.DISC_AMT_SP,0)) DISC_AMT_SP, sum(B.DISC_AMT_BS) DISC_AMT_BS,   sum(B.ITM_AMT_SP) ITM_AMT_SP
, sum(B.ITM_AMT_BS) ITM_AMT_BS, sum(NVL(B.TAXABLE_AMT_SP,(B.FINAL_RCPT_QTY*B.ITM_PRICE)))GROSS_PRICE
, sum(NVL(C.TAX_AMT,0)) TAX_AMT_SP
, sum(NVL(C.TAX_AMT_BS,0)) TAX_AMT_BS
,sum(NVL(B.TAXABLE_AMT_SP,(B.FINAL_RCPT_QTY * B.ITM_PRICE)) +  (NVL(C.TAX_AMT,0)) -(NVL(B.DISC_AMT_SP,0))) NET_AMT_SP
,sum(NVL(B.TAXABLE_AMT_BS,(B.FINAL_RCPT_QTY * B.ITM_PRICE_BS)) +  (NVL(C.TAX_AMT_BS,0)) - (NVL(B.DISC_AMT_BS,0))) NET_AMT_BS
,sum(A.TOT_AMT_SP) TOT_AMT_SP,
sum(A.TOT_AMT_BS) TOT_AMT_BS
FROM MM.MM$INVC A, MM.MM$INVC$ITM B, MM$INVC$TR C, APP.ORG D, APP.APP$EO E, APP.APP$CURR_VW F
, APP.APP$UOM$CONV$STD J, APP.APP$ITM$PRF K, APP.APP$DS$ATT L, APP.APP$EO$PRF M
WHERE A.CLD_ID = B.CLD_ID
AND A.SLOC_ID = B.SLOC_ID
AND A.ORG_ID = B.ORG_ID
AND A.DOC_ID = B.DOC_ID
AND B.CLD_ID = C.CLD_ID(+)
AND B.SLOC_ID = C.SLOC_ID(+)
AND B.ORG_ID = C.ORG_ID(+)
AND B.DOC_ID = C.DOC_ID(+)
AND B.DOC_ID_RCPT = C.DOC_ID_RCPT(+)
AND B.DOC_ID_PO = C.DOC_ID_PO(+)
AND B.DLV_SCHDL_NO = C.DLV_SCHDL_NO(+)
AND B.ITM_ID = C.ITM_ID(+)
AND B.ITM_UOM = C.ITM_UOM(+)
AND B.ITM_UOM = J.UOM_ID
AND B.CLD_ID  = K.CLD_ID
AND B.SLOC_ID = K.SLOC_ID
AND MM.MM_GET_HO_ORG(B.CLD_ID, B.SLOC_ID, B.ORG_ID) = K.HO_ORG_ID
AND B.ITM_ID  = K.ITM_ID
AND A.CLD_ID = D.ORG_CLD_ID
AND A.SLOC_ID = D.ORG_CREATE_REF_SLOC_ID
AND A.ORG_ID = D.ORG_ID
AND A.CLD_ID = E.EO_CLD_ID(+)
AND A.SLOC_ID = E.EO_SLOC_ID(+)
AND A.EO_ID_SRC = E.EO_ID(+)
AND MM.MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID)= E.EO_HO_ORG_ID(+)
AND A.CLD_ID = M.EO_CLD_ID(+)
AND A.SLOC_ID = M.EO_SLOC_ID(+)
AND A.EO_ID_SRC = M.EO_ID(+)
AND MM.MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID)= M.EO_HO_ORG_ID(+)

AND A.CURR_ID_SP = F.CURR_ID
AND A.INVC_TYPE = L.ATT_ID
AND A.AUTH_STAT = 'Y'
AND A.INVC_TYPE = NVL($P{InvcType},A.INVC_TYPE)
AND A.CLD_ID = NVL($P{CldId},A.CLD_ID)
AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
AND A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
AND A.DOC_ID= NVL($P{DocId},A.DOC_ID)
AND A.EO_ID_SRC = NVL($P{EoIdSrc},A.EO_ID_SRC)
AND TRUNC(A.INVC_DT) >= NVL(TO_DATE($P{FromDate},'yyyy-mm-dd'),TRUNC(A.INVC_DT))
AND TRUNC(A.INVC_DT) <= NVL(TO_DATE($P{ToDate},'yyyy-mm-dd'),TRUNC(A.INVC_DT))
group by A.ORG_ID, D.ORG_DESC, A.DOC_ID,A.EO_ID_SRC, E.EO_NM, A.INVC_NO, A.INVC_DT,  L.ATT_NM , A.CURR_ID_SP, F.CURR_NOTATION_ALIAS,B.ITM_UOM,  J.UOM_NM, M.TIN_NO
ORDER BY  A.INVC_NO DESC]]>
	</queryString>
