                                    ,'P' ,(A.Disc_Val * A.Po_Amt_Sp)/100),A.DOC_ID,A.REF_PO_DOC_ID,a.po_type,A.VALID_FRM_DT,A.VALID_TO_DT,n.dlv_dt
