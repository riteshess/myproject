	<queryString>
		<![CDATA[SELECT A.ORG_ID, C.ORG_DESC, A.ITM_ID, b.chptr_id, B.ITM_DESC, A.OP_STK, e.ecc_no, e.reg_no
, app.fn_get_fy_start_date(a.cld_id, a.org_id, sysdate, 'FY') op_dt,
A.OP_STK,X.RCPT_DT,X.mnf_qty, (a.op_stk + nvl(X.mnf_qty,0))tot_op_mnf
,X.ISSU_DT,X.ISSU_QTY,(NVL(a.OP_STK,0)+nvl(X.mnf_Qty,0) - nvl(X.ISSU_QTY,0))CLOSING_STK

FROM MM$STK$SUMM$ITM A, APP.APP$ITM$PRF B, APP.ORG C, app.org$oth$dtl e
    ,(select x.cld_id, x.sloc_id, x.org_id, x.wh_id, x.fy_id, x.itm_id,
x.rcpt_no, X.RCPT_DT, y.issu_no, Y.ISSU_DT, sum(x.mnf_qty) mnf_qty, sum(Y.issu_qty) issu_qty
from (select r.cld_id,r.sloc_id, r.org_id,  r.wh_id, R.FY_ID, ri.itm_id,ROWNUM RN, r.rcpt_no, R.RCPT_DT, ri.final_rcpt_qty mnf_qty
      from  mm$mtl$rcpt r, mm$mtl$rcpt$itm ri
      where r.cld_id = ri.cld_id
      and r.sloc_id = ri.sloc_id
      and r.org_id = ri.ORG_ID
      and r.WH_ID = ri.WH_ID
      and r.doc_id = ri.doc_id
      and r.rcpt_src_type in (335,334)
      AND R.RCPT_STAT IN (383, 384)
      AND R.CLD_ID = $P{CldId}
      AND R.SLOC_ID = $P{SlocId}
      AND R.ORG_ID = $P{OrgId}
      AND R.WH_ID = NVL($P{WhId}, R.WH_ID)
      AND RI.ITM_ID = NVL($P{ItmId}, RI.ITM_ID)
  AND TRUNC(R.RCPT_DT) BETWEEN NVL(TO_DATE($P{FromDate}, 'YYYY-MM-DD'), TRUNC(R.RCPT_DT))
  AND NVL(TO_DATE($P{ToDate}, 'YYYY-MM-DD'), TRUNC(R.RCPT_DT)) )x,
(select ISS1.CLD_ID, ISS1.SLOC_ID, ISS1.ORG_ID, ISS1.WH_ID, ISS1.FY_ID,  iss.ITM_ID, iss1.issu_no, ISS1.ISSU_DT, ROWNUM RN, ISS.ISSU_QTY ISSU_QTY
  from SLS.SLS$SHIPMNT$ITM SSI  ,  MM.MM$MTL$ISSU$ITM ISS, MM.MM$MTL$ISSU ISS1
  where ISS.CLD_ID = SSI.CLD_ID
  AND ISS.SLOC_ID = SSI.SLOC_ID
  AND ISS.ORG_ID = SSI.ORG_ID
  AND MM.MM_GET_HO_ORG(ISS.CLD_ID, ISS.SLOC_ID, ISS.ORG_ID)= SSI.HO_ORG_ID
  AND ISS.WH_ID = SSI.WH_ID
  AND ISS.DOC_ID_SRC = SSI.DOC_ID
  AND ISS.ITM_ID = SSI.ITM_ID
  AND   ISS.CLD_ID = ISS1.CLD_ID
  AND ISS.SLOC_ID = ISS1.SLOC_ID
  AND ISS.ORG_ID = ISS1.ORG_ID
  AND ISS.WH_ID = ISS1.WH_ID
  AND ISS.DOC_ID = ISS1.DOC_ID
  AND ISS1.ISSU_STAT = 369
  AND ISS1.CLD_ID = $P{CldId}
  AND ISS1.SLOC_ID = $P{SlocId}
  AND ISS1.ORG_ID = $P{OrgId}
  AND ISS.ITM_ID = $P{ItmId}
  AND ISS1.WH_ID = NVL($P{WhId}, ISS1.WH_ID)
  AND TRUNC(ISS1.ISSU_DT) BETWEEN NVL(TO_DATE($P{FromDate}, 'YYYY-MM-DD'), TRUNC(ISS1.ISSU_DT))
  AND NVL(TO_DATE($P{ToDate}, 'YYYY-MM-DD'), TRUNC(ISS1.ISSU_DT))
)y
WHERE X.RN = Y.RN (+)
group by x.cld_id, x.sloc_id, x.org_id, x.wh_id, x.fy_id,
x.itm_id, x.rcpt_no, y.issu_no, X.RCPT_DT, Y.ISSU_DT )X
WHERE A.CLD_ID = B.CLD_ID
AND A.SLOC_ID = B.SLOC_ID
AND MM.MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID) = B.HO_ORG_ID
AND A.ITM_ID = B.ITM_ID
AND A.ORG_ID = C.ORG_ID
and a.org_id = e.org_id
AND A.CLD_ID = X.CLD_ID(+)
AND A.SLOC_ID = X.SLOC_ID(+)
AND A.ORG_ID = X.ORG_ID(+)
AND A.WH_ID = X.WH_ID(+)
AND A.ITM_ID = X.ITM_ID(+)
AND A.CLD_ID = $P{CldId}
AND A.SLOC_ID = $P{SlocId}
AND A.ORG_ID = $P{OrgId}
AND A.WH_ID = NVL($P{WhId},A.WH_ID)
AND A.ITM_ID = NVL($P{ItmId}, A.ITM_ID)
order by a.itm_id]]>
	</queryString>
