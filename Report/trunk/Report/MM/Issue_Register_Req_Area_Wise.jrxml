	<queryString>
		<![CDATA[SELECT A.DOC_ID,A.ISSU_NO, A.ISSU_DT,
  nvl(A.RQMT_AREA_ID_SRC,0)RQMT_AREA_ID_SRC,
  A.ORG_ID,
  B.ITM_ID,
  G.ISSU_QTY,
  C.ITM_DESC,
  D.UOM_NM UOM_DESC,
  CASE WHEN B.ISSU_DOC_TYPE = 327 THEN
                    (SELECT X.EO_NM FROM APP.APP$EO X WHERE X.EO_ID = A.EO_ID_SRC
                                                      AND X.EO_CLD_ID = A.CLD_ID
                                                      AND X.EO_SLOC_ID = A.SLOC_ID
                                                      AND X.EO_HO_ORG_ID = MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID))
           ELSE    (SELECT X.RQMT_AREA_NM FROM APP.APP$RQMT$AREA X
                                          WHERE X.RQMT_AREA_ID = A.RQMT_AREA_ID_SRC
                                          AND X.CLD_ID = A.CLD_ID
                                          AND X.SLOC_ID = A.SLOC_ID
                                          AND X.ORG_ID = A.ORG_ID )
      END RQMT_AREA_NM,
  F.ORG_DESC,  G.LOT_ID,  H.LND_PRICE,
  (G.ISSU_QTY * H.LND_PRICE) val_issud_qty,
  I.ATT_NM ISSUE_TYPE,  J.WH_NM,  A.WH_ID
FROM MM$MTL$ISSU A,  MM$MTL$ISSU$ITM B,  APP.APP$ITM$PRF C,  APP.APP$UOM$CONV$STD D,  APP.ORG F,  MM$MTL$ISSU$LOT G,
  MM$STK$SUMM$LOT H,  APP.APP$DS$ATT I,  APP.APP$WH$ORG J
WHERE A.CLD_ID                                      = B.CLD_ID
AND A.SLOC_ID                                       = B.SLOC_ID
AND A.ORG_ID                                        = B.ORG_ID
AND A.DOC_ID                                        = B.DOC_ID
AND A.WH_ID                                         = B.WH_ID
AND B.CLD_ID                                        = C.CLD_ID
AND B.SLOC_ID                                       = C.SLOC_ID
AND B.ITM_ID                                        = C.ITM_ID
AND B.CLD_ID                                        = D.CLD_ID
AND B.SLOC_ID                                       = D.SLOC_ID
AND B.ITM_UOM                                       = D.UOM_ID
AND A.CLD_ID                                        = F.ORG_CLD_ID
AND A.ORG_ID                                        = F.ORG_ID
AND A.SLOC_ID                                       = F.ORG_CREATE_REF_SLOC_ID
AND B.CLD_ID                                        = G.CLD_ID
AND B.SLOC_ID                                       = G.SLOC_ID
AND B.ORG_ID                                        = G.ORG_ID
AND B.WH_ID                                         = G.WH_ID
AND B.DOC_ID                                        = G.DOC_ID
AND B.ITM_ID                                        = G.ITM_ID
AND B.DOC_ID_SRC                                    = G.DOC_ID_SRC
AND B.ITM_UOM                                       = G.ITM_UOM
AND G.CLD_ID                                        = H.CLD_ID
AND G.SLOC_ID                                       = H.SLOC_ID
AND G.ORG_ID                                        = H.ORG_ID
AND G.WH_ID                                         = H.WH_ID
AND G.LOT_ID                                        = H.LOT_ID
AND G.ITM_ID                                        = H.ITM_ID
AND A.ISSU_TYPE                                     = I.ATT_ID
AND A.CLD_ID                                        = J.CLD_ID
AND A.SLOC_ID                                       = J.SLOC_ID
AND A.ORG_ID                                        = J.ORG_ID
AND A.WH_ID                                         = J.WH_ID
AND DECODE(B.ISSU_DOC_TYPE, 327, A.EO_ID_SRC, A.RQMT_AREA_ID_SRC)        = NVL($P{RqmtAreaId}, DECODE(B.ISSU_DOC_TYPE, 327, A.EO_ID_SRC, A.RQMT_AREA_ID_SRC))
AND A.ORG_ID                                        = NVL($P{OrgId}, A.ORG_ID)
AND B.ITM_ID                                        = NVL($P{ItmId}, B.ITM_ID)
AND A.WH_ID                                         = NVL($P{WhId}, A.WH_ID)
AND MM.MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, B.ORG_ID) = C.HO_ORG_ID
AND A.CLD_ID                                        = NVL($P{CldId}, A.CLD_ID)
AND A.SLOC_ID                                       = NVL($P{SlocId}, A.SLOC_ID)
AND A.FY_ID                                         = NVL($P{FyId}, A.FY_ID)
AND A.DOC_ID                                        = NVL($P{IssuDocId}, A.DOC_ID)
AND TRUNC(A.ISSU_DT)                               >= NVL(to_date($P{FromDate}, 'yyyy-mm-dd'), TRUNC(A.ISSU_DT))
AND TRUNC(A.ISSU_DT)                               <= NVL(to_date($P{ToDate}, 'yyyy-mm-dd'), TRUNC(A.ISSU_DT))
AND A.ISSU_TYPE                                     = NVL($P{IssuTyp}, A.ISSU_TYPE)
AND A.ISSU_STAT                                     = NVL($P{IssuStat}, A.ISSU_STAT)
ORDER BY A.WH_ID, RQMT_AREA_NM,4, 3,  1 DESC]]>
	</queryString>
