	<queryString>
		<![CDATA[SELECT A.CLD_ID,A.SLOC_ID,A.ORG_ID,H.ORG_DESC,
       A.DOC_ID ,A.INVC_TYPE,I.ATT_NM INVC_TYP,
       A.ORG_ID_SRC,J.ORG_DESC ORG_ID_SRC_NM ,A.WH_ID_SRC,K.WH_NM,
       A.INVC_NO,A.INVC_DT,
       A.EO_ID_SRC,L.EO_NM,
       A.COA_ID,M.COA_NM,
       A.CURR_ID_SP,N.CURR_DESC,
       A.CURR_CONV_FCTR,
       DECODE(A.AUTH_STAT,'Y','Yes'
                         ,'N','No') Auth_Stat,
       Decode(A.Invc_Type,455,(Select Rcpt_No From Mm$mtl$rcpt Where Doc_Id = (SELECT DOC_ID_RCPT FROM MM$INVC$SRC WHERE DOC_ID = $P{DocId} ))
                         ,457,(Select Cpo_Id From Mm$cpo Where Doc_Id =(SELECT DOC_ID_PO FROM MM$INVC$SRC WHERE DOC_ID =  $P{DocId}))
                         ,492,(SELECT INVC_NO FROM MM$INVC WHERE DOC_ID = $P{DocId} ))SOURCE_NO,

       Decode(A.Invc_Type,455,(Select Rcpt_Dt From Mm$mtl$rcpt Where Doc_Id =(SELECT DOC_ID_RCPT FROM MM$INVC$SRC WHERE DOC_ID = $P{DocId} ))
                         ,457,(Select Cpo_Dt From Mm$cpo Where Doc_Id = (Select Doc_Id_Po From Mm$invc$src Where Doc_Id =  $P{DocId}))
                         ,492,(SELECT INVC_DT FROM MM$INVC WHERE DOC_ID = $P{DocId} )) SOURCE_DT,
       A.TOT_AMT_SP,A.TOT_AMT_BS,
       D.ITM_ID,O.ITM_DESC,
       D.ITM_UOM,P.UOM_NM,
       D.ITM_PRICE,D.DISC_AMT_SP,FINAL_RCPT_QTY,(D.ITM_PRICE*FINAL_RCPT_QTY) ITM_AMT,
       E.TAX_RULE_ID,sum(nvl(E.TAX_AMT,0)) TAX_AMT,
       Sum(Nvl(F.Oc_Amt_Sp,0))Oc_Amt,a.remarks,
       G.Tds_Rule_Id,Nvl(G.Tds_Amt,0) Tds_Amt,Q.Tin_No,Q.Cst_No,Nvl(R.Org_Phone1,R.Org_Phone2)Phn,
       R.ORG_EMAIL1,S.ADDS ORG_ADDS,T.ADDS EO_ADDS,V.TIN_NO SUPPLIER_tin
       FROM MM$INVC A,MM$INVC$DTL B,MM$MTL$RCPT C,MM$INVC$ITM D,MM$INVC$TR E,
       Mm$invc$oc F,Mm$invc$tds G,App.Org H, App.App$ds$att I,App.Org J,App.App$wh$org K,
       App.App$supp_Vw L,Fin.Fin$coa M,App.App$curr N,App.App$itm$prf O,
       App.App$uom$conv$std P,App.Org$oth$dtl Q,App.Org$oth$dtl R,App.App$adds$vw S,
       APP.APP$ADDS$VW  T,APP.APP$EO$PRF V
WHERE A.CLD_ID = B.CLD_ID
And A.Sloc_Id = B.Sloc_Id
AND A.ORG_ID = B.ORG_ID
AND A.DOC_ID = B.DOC_ID
AND A.CLD_ID = D.CLD_ID
AND A.SLOC_ID =D.SLOC_ID
AND A.ORG_ID = D.ORG_ID
AND A.DOC_ID = D.DOC_ID
AND B.CLD_ID = D.CLD_ID
AND B.SLOC_ID = D.SLOC_ID
AND B.ORG_ID = D.ORG_ID
AND B.DOC_ID = D.DOC_ID
AND B.DOC_ID_RCPT = D.DOC_ID_RCPT
AND D.CLD_ID =E.CLD_ID(+)
AND D.SLOC_ID =E.SLOC_ID(+)
AND D.ORG_ID =E.ORG_ID(+)
AND D.DOC_ID =E.DOC_ID(+)
AND D.ITM_ID =E.ITM_ID(+)
AND A.CLD_ID =F.CLD_ID(+)
AND A.SLOC_ID =F.SLOC_ID(+)
AND A.ORG_ID =F.ORG_ID(+)
AND A.DOC_ID =F.DOC_ID(+)
AND D.CLD_ID =G.CLD_ID(+)
AND D.SLOC_ID =G.SLOC_ID(+)
AND D.ORG_ID =G.ORG_ID(+)
AND D.DOC_ID =G.DOC_ID(+)
AND D.ITM_ID =G.ITM_ID(+)
AND A.CLD_ID = H.ORG_CLD_ID
AND A.ORG_ID = H.ORG_ID
And A.Invc_Type = I.Att_Id
AND A.CLD_ID     = J.ORG_CLD_ID(+)
AND A.ORG_ID_SRC = J.ORG_ID(+)
AND A.CLD_ID = K.CLD_ID(+)
AND A.SLOC_ID = K.SLOC_ID(+)
AND A.ORG_ID = K.ORG_ID(+)
AND A.WH_ID_SRC = K.WH_ID(+)
AND A.CLD_ID = L.CLD_ID(+)
AND A.SLOC_ID =L.SLOC_ID(+)
AND MM.MM_GET_HO_ORG(A.CLD_ID,A.SLOC_ID,A.ORG_ID) = L.HO_ORG_ID(+)
AND A.ORG_ID = L.ORG_ID(+)
AND A.EO_ID_SRC =L.EO_ID(+)
AND A.CLD_ID = M.COA_CLD_ID(+)
AND A.SLOC_ID = M.COA_SLOC_ID(+)
AND MM.MM_GET_HO_ORG(A.CLD_ID,A.SLOC_ID,A.ORG_ID) = M.COA_HO_ORG_ID(+)
AND A.COA_ID = M.COA_ID(+)
AND A.CLD_ID = N.CLD_ID
AND A.SLOC_ID = N.CURR_SLOC_ID
AND A.CURR_ID_SP = N.CURR_ID
And A.CLD_Id = Q.Org_Cld_Id(+)
And A.Org_Id= Q.Org_Id(+)
And A.Cld_Id = R.Org_Cld_Id(+)
And A.Org_Id= R.Org_Id(+)
AND D.CLD_ID = O.CLD_ID
AND D.SLOC_ID =O.SLOC_ID
AND MM.MM_GET_HO_ORG(D.CLD_ID,D.SLOC_ID,D.ORG_ID) = O.HO_ORG_ID
AND D.ITM_ID = O.ITM_ID
AND D.CLD_ID = P.CLD_ID
AND D.SLOC_ID = P.SLOC_ID
And D.Itm_Uom = P.Uom_Id
And R.Org_Cld_Id = S.Cld_Id(+)
And Nvl(R.Org_Add_Id1,R.Org_Add_Id2) = S.Adds_Id(+)
AND A.CLD_ID =T.CLD_ID(+)
And A.Sloc_Id =T.Sloc_Id(+)
AND A.BILL_ADDS_ID =T.ADDS_ID(+)
And A.Cld_Id = V.Eo_Cld_Id(+)
And A.Sloc_Id = V.Eo_Sloc_Id(+)
And Mm.Mm_Get_Ho_Org(A.Cld_Id,A.Sloc_Id,A.Org_Id) = V.Eo_Ho_Org_Id(+)
And A.Org_Id = V.Eo_Org_Id(+)
And A.Eo_Id_src =V.Eo_Id(+)
AND A.INVC_TYPE IN (455,457,492)
AND A.CLD_ID = NVL($P{CldId},A.CLD_ID)
AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
AND A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
AND A.FY_ID = NVL($P{FyId},A.FY_ID)
AND A.DOC_ID = $P{DocId}
and nvl(A.INVC_TYPE,1) = nvl($P{InvcType},nvl(A.INVC_TYPE,1))
and nvl(A.EO_ID_SRC,1) = nvl($P{EoIdSrc},nvl(A.EO_ID_SRC,1))
and nvl(A.AUTH_STAT,1) = nvl($P{AuthStat},nvl(A.AUTH_STAT,1))
GROUP BY A.CLD_ID,A.SLOC_ID,A.ORG_ID,A.DOC_ID ,A.INVC_TYPE,A.INVC_NO,A.INVC_DT,
       A.EO_ID_SRC,A.COA_ID,A.CURR_ID_SP,A.CURR_CONV_FCTR,A.AUTH_STAT,A.TOT_AMT_SP,A.TOT_AMT_BS,
       D.ITM_ID,D.ITM_UOM,D.ITM_PRICE,D.ITM_AMT_SP,D.DISC_AMT_SP,FINAL_RCPT_QTY,
      E.Tax_Rule_Id,G.Tds_Rule_Id,Nvl(G.Tds_Amt,0),H.Org_Desc,I.Att_Nm,J.Org_Desc,A.Wh_Id_Src,
       A.Org_Id_Src,K.Wh_Nm,L.Eo_Nm,M.Coa_Nm,N.Curr_Desc,O.Itm_Desc,P.Uom_Nm,
       Q.Tin_No,Q.Cst_No,Nvl(R.Org_Phone1,R.Org_Phone2)
       ,R.Org_Email1,S.Adds,T.ADDS,V.TIN_NO,a.remarks
order by a.org_id,a.invc_no,d.itm_id]]>
	</queryString>
