	<queryString>
		<![CDATA[SELECT ORG_DESC,ITM_DESC,EO_NM,INVOICE_ID,INVOICE_DOC_DT,CURR_NOTATION_ALIAS,SUM(ITM_AMT_BS) AS TOT_AMT_BS,SUM(ITM_COST) AS ITM_COST,SUM(PROFIT) AS PROFIT,SUM(PROFIT)/decode (SUM(ITM_AMT_BS),0,1,SUM(ITM_AMT_BS))*100 AS PROFIT_PER
FROM (SELECT distinct  a.cld_id,

                a.org_id, a.ho_org_id, a.invoice_doc_id,

                SUBSTR (a.invoice_id, 3) invoice_id, a.invoice_doc_dt,

                a.curr_id_sp, a.curr_rate,

                a.itm_id, b.itm_desc itm_desc,

                a.itm_rate, a.itm_uom, e.uom_nm uom_nm, a.itm_ship_qty,
nvl(A.itm_AMT_Bs,0)-sl.itm_lot_cost Profit,
  (nvl(A.itm_AMT_Bs,0)-sl.itm_lot_cost/nvl(decode(A.itm_AMT_BS,0,1,A.itm_AMT_BS),1))*100 profit_per,

               DECODE(DECODE (a.itm_disc_typ, 'A', a.itm_disc_val,

                             'P', (  (  NVL (a.itm_ship_qty, 0)

                                      * NVL (a.itm_rate, 0)

                                     )

                                   * NVL (a.itm_disc_val, 0)

                                   / 100

                              )

                            ),0,inv.disc_val,
                  DECODE (a.itm_disc_typ,

                             'A', a.itm_disc_val,

                             'P', (  (  NVL (a.itm_ship_qty, 0)

                                      * NVL (a.itm_rate, 0)

                                     )

                                   * NVL (a.itm_disc_val, 0)

                                   / 100

                              )

                            )) discount,inv.disc_val,


               a.itm_amt_sp as itm_amt_gs,
                NVL (n.tax_amt_bs, 0) + a.itm_amt_bs -                DECODE(DECODE (a.itm_disc_typ, 'A', a.itm_disc_val,

                             'P', (  (  NVL (a.itm_ship_qty, 0)

                                      * NVL (a.itm_rate, 0)

                                     )

                                   * NVL (a.itm_disc_val, 0)

                                   / 100

                              )

                            ),0,inv.disc_val,
                  DECODE (a.itm_disc_typ,

                             'A', a.itm_disc_val,

                             'P', (  (  NVL (a.itm_ship_qty, 0)

                                      * NVL (a.itm_rate, 0)

                                     )

                                   * NVL (a.itm_disc_val, 0)

                                   / 100

                              )

                            )) as itm_amt_bs,
	      NVL (n.tax_amt_sp, 0) + a.itm_amt_sp -                DECODE(DECODE (a.itm_disc_typ, 'A', a.itm_disc_val,

                             'P', (  (  NVL (a.itm_ship_qty, 0)

                                      * NVL (a.itm_rate, 0)

                                     )

                                   * NVL (a.itm_disc_val, 0)

                                   / 100

                              )

                            ),0,inv.disc_val,
                  DECODE (a.itm_disc_typ,

                             'A', a.itm_disc_val,

                             'P', (  (  NVL (a.itm_ship_qty, 0)

                                      * NVL (a.itm_rate, 0)

                                     )

                                   * NVL (a.itm_disc_val, 0)

                                   / 100

                              )

                            )) as itm_amt_sp,


               f.org_desc,


                NVL (n.tax_amt_sp, 0) tax_amt_sp,

                NVL (n.tax_amt_bs, 0) tax_amt_bs,

                o.curr_notation_alias,l.eo_nm assigned_to,

                                    a.eo_nm,a.grp_nm,sl.itm_lot_cost itm_cost

             FROM sls$invoice$dtl_vw a,sls$inv inv,sls$inv$ship$itm invi,sls$shipmnt$itm si,sls$shipmnt$itm$lot sl,

                app.app$sls$itm_vw b,

                sls.sls$so c,

                app.app$uom$conv$std e,

                app.org f,

                (SELECT eo_cld_id, x.eo_sloc_id, x.eo_ho_org_id, x.eo_id,

                        x.eo_nm

                   FROM app.app$eo x

                  WHERE x.eo_cld_id = NVL ($P{CldId}, x.eo_cld_id)

                    AND x.eo_sloc_id = NVL ($P{SlocId}, x.eo_sloc_id)

                    AND x.eo_type = '80') l,

                (SELECT y.att_nm, y.att_id

                   FROM app.app$ds$att y

                  WHERE att_type_id = 69) m,

                (SELECT att_id, att_nm

                   FROM app.app$ds$att

                  WHERE att_type_id = 74) z,

                sls.sls$inv$tr n,

                app.app$curr_vw o

          WHERE a.sloc_id = b.sloc_id

            AND a.itm_id = b.itm_id

            AND a.org_id = b.org_id

            AND a.cld_id = b.cld_id

            AND a.ho_org_id = b.ho_org_id

            AND a.sloc_id = c.sloc_id

            AND a.org_id = c.org_id

            AND a.cld_id = c.cld_id

            AND a.ho_org_id = c.ho_org_id

            AND a.so_id = c.so_id

           AND a.ho_org_id = inv.ho_org_id

            AND a.sloc_id = inv.sloc_id

            AND a.org_id = inv.org_id

           AND a.cld_id = inv.cld_id

          AND si.cld_id = sl.cld_id
          and si.sloc_id = sl.sloc_id
          and si.org_id = sl.org_id
          and si.so_id = sl.so_id
          and si.itm_id = sl.itm_id
          and si.doc_id = sl.doc_id
          and si.wh_id = sl.wh_id
          and si.pick_id =sl.pick_id
           and inv.cld_id = invi.cld_id
           and inv.sloc_id = invi.sloc_id
           and inv.org_id = invi.org_id
           and inv.ho_org_id = invi.ho_org_id
         and inv.doc_id = invi.doc_id
           and a.itm_id = invi.itm_id
           and c.doc_id = invi.so_id

           and a.invoice_doc_id = inv.doc_id

            AND a.sloc_id = e.sloc_id

            AND a.cld_id = e.cld_id

            AND a.itm_uom = e.uom_id

            AND a.cld_id = f.org_cld_id

            AND a.org_id = f.org_id

            AND c.cld_id = l.eo_cld_id(+)

            AND c.sloc_id = l.eo_sloc_id(+)

            AND c.ho_org_id = l.eo_ho_org_id(+)

            AND c.assigned_to = l.eo_id(+)

            AND a.inv_mode = m.att_id

            AND a.eo_catg = z.att_id

            AND a.cld_id = n.cld_id(+)

            AND a.sloc_id = n.sloc_id(+)

            AND a.ho_org_id = n.ho_org_id(+)

            AND a.invoice_doc_id = n.doc_id(+)
            AND a.itm_id = n.itm_id(+)
            AND c.doc_id = n.so_id(+)

            and a.cld_id = si.cld_id
            and a.sloc_id = si.sloc_id
            and a.org_id = si.org_id
            and a.ho_org_id = si.ho_org_id
            and invi.ship_id = si.doc_id
            and a.itm_id = si.itm_id
            and c.doc_id = si.so_id

            AND a.curr_id_sp = o.curr_id and

          inv.inv_stat!=220 and
	inv.inv_type!=480 and
  A.CLD_ID = nvl($P{CldId},a.cld_id) AND
  A.SLOC_ID = nvl($P{SlocId},a.sloc_id) AND
  A.ORG_ID = nvl($P{OrgId},a.org_id) AND
  A.HO_ORG_ID = nvl($P{HoOrgId},a.ho_org_id) AND
  A.INVOICE_DOC_ID = nvl($P{DocId},A.INVOICE_DOC_ID) AND
  A.ITM_ID=nvl($P{ItmId},a.itm_id) and
  a.grp_id =nvl($P{GrpId},a.grp_id) and
  NVL (C.order_stat, 217)   = NVL ($P{Status}, NVL (C.order_stat, 217))and
A.EO_CATG=NVL($P{EoCatId},A.EO_CATG) and
  A.CURR_ID_SP=nvl($P{CurrId},a.curr_id_sp) and
 a.eo_id = NVL ($P{EoId}, a.eo_id)  AND
  trunc(a.invoice_doc_dt) >= nvl (to_date ($P{FromDate}, 'yyyy-MM-dd'), trunc(a.invoice_doc_dt)) and
  trunc(a.invoice_doc_dt) <= nvl (to_date ($P{ToDate}, 'yyyy-MM-dd'), trunc(a.invoice_doc_dt)))
GROUP BY ORG_DESC,ITM_DESC,EO_NM,INVOICE_ID,INVOICE_DOC_DT,CURR_NOTATION_ALIAS]]>
