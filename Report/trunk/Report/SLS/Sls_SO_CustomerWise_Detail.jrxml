	<queryString>
		<![CDATA[SELECT   e.org_desc, SUBSTR (a.so_id, 3) so_id,

         TO_CHAR (a.doc_dt, 'dd-mm-yyyy') doc_dt, a.eo_id, i.eo_nm, a.org_id,

         b.itm_id, f.itm_desc, g.uom_desc, b.itm_uom, b.itm_qty, b.itm_rate,

         b.itm_disc_val,

nvl(DECODE (b.itm_disc_typ,

                 'P', (((b.itm_qty * b.itm_rate * b.itm_disc_val) / 100)),

                 'A', NVL (b.itm_disc_val, 0)

                ),0)discount,

         DECODE (b.itm_disc_typ,

                 'P', (  (b.itm_qty * b.itm_rate)

                       - (  (b.itm_qty * b.itm_rate * NVL (b.itm_disc_val, 0)

                            )

                          / 100

                         )

                  ),

                 'A', ((b.itm_qty * b.itm_rate) - NVL (b.itm_disc_val, 0))

                ) amt,

 c.tax_rule_id,

         j.tax_nm, NVL (c.tax_amt_sp, 0) tax_amt_sp,

         NVL (c.tax_amt_bs, 0) tax_amt_bs, NVL (d.oc_desc, 0) oc_desc,

         NVL (d.amt_sp, 0.00),

         (SELECT i.coa_nm

            FROM fin.fin$coa i

           WHERE i.coa_cld_id = a.cld_id

             AND i.coa_sloc_id = a.sloc_id

             AND i.coa_ho_org_id = a.ho_org_id

             AND i.coa_id = d.coa_id) oc,

         k.curr_id, k.curr_desc, k.curr_notation_alias,

         NVL (l.eo_nm, 'Un Assigned') assigned_to, m.att_nm so_status,
    b.itm_amt_gs_sp itm_amt_gs,
NVL (b.itm_amt_bs, 0) + NVL (c.tax_amt_bs, 0)  as  itm_amt_bs,
        NVL (b.itm_amt_sp, 0) + NVL (c.tax_amt_sp, 0) as  itm_amt_sp,
    a.tot_amt_sp,a.tot_amt_bs,

         n.grp_nm,
   NVL(DECODE (a.disc_type,

                 'P', (((a.tot_itm_amt_gs_sp * a.disc_val) / 100)),

                 'A', NVL (a.disc_val, 0)

                ),0) ordDisc,

app.fn_get_uom_desc(a.cld_id,a.sloc_id,it.uom_basic)uom_basic,
app.fn_get_uom_desc(a.cld_id,a.sloc_id,it.uom_sls)uom_sls

    FROM sls$so a,

         sls$so$itm b,

         sls$so$tr c,

         sls$so$oc d,

         app.org e,

         app.app$itm$prf f,

         app.app$uom$conv$std g,

         app.app$eo i,

         app.app$tax j,

 APP.APP$EO n,
 app.app$eo$mst m,
app.app$itm$prf it,

         app.app$curr_vw_all k,

         (SELECT eo_cld_id, x.eo_sloc_id, x.eo_ho_org_id, x.eo_id, x.eo_nm

            FROM app.app$eo x

           WHERE x.eo_cld_id = $P{CldId}

             AND x.eo_sloc_id = $P{SlocId}

             AND x.eo_type = '80') l,

         (SELECT y.att_nm, y.att_id

            FROM app.app$ds$att y

           WHERE att_type_id = 51) m,

         app.app$grp n

   WHERE a.cld_id = b.cld_id

     AND a.sloc_id = b.sloc_id

     AND a.org_id = b.org_id

     AND a.doc_id = b.doc_id

     AND b.cld_id = c.cld_id(+)

     AND b.sloc_id = c.sloc_id(+)

     AND b.org_id = c.org_id(+)

     AND b.doc_id = c.doc_id(+)

     and b.itm_id = c.itm_id(+)
  and a.ho_org_id		=c.ho_org_id(+)
  and b.itm_id	        = c.itm_id(+)

     AND a.cld_id = d.cld_id(+)

     AND a.sloc_id = d.sloc_id(+)

     AND a.org_id = d.org_id(+)

     AND a.doc_id = d.doc_id(+)

     AND a.org_id = e.org_id

     AND b.cld_id = f.cld_id

     AND b.sloc_id = f.sloc_id

     AND b.itm_id = f.itm_id

     AND B.HO_ORG_ID = F.HO_ORG_ID

     AND b.cld_id = g.cld_id

     AND b.sloc_id = g.sloc_id

     AND b.itm_uom = g.uom_id

     AND a.cld_id = i.eo_cld_id(+)

     AND a.sloc_id = i.eo_sloc_id(+)

     AND a.ho_org_id = i.eo_ho_org_id(+)

     AND a.eo_id = i.eo_id(+)

     AND a.cld_id = l.eo_cld_id(+)

     AND a.sloc_id = l.eo_sloc_id(+)

     AND a.ho_org_id = l.eo_ho_org_id(+)

     AND a.assigned_to = l.eo_id(+)

     AND c.cld_id = j.cld_id(+)

     AND c.sloc_id = j.tax_sloc_id(+)

     AND c.ho_org_id = j.ho_org_id(+)

     AND c.tax_rule_id = j.tax_id(+)

     AND a.curr_id_sp = k.curr_id

     AND a.order_stat = m.att_id

     AND f.cld_id = n.cld_id

     AND f.sloc_id = n.sloc_id

     AND f.ho_org_id = n.ho_org_id

     AND f.grp_id = n.grp_id


     AND a.so_mode = 314


AND A.CLD_ID = N.EO_CLD_ID
AND A.SLOC_ID = n.EO_SLOC_ID
AND A.HO_ORG_ID = N.EO_HO_ORG_ID
AND A.EO_ID = n.EO_ID
AND N.EO_CLD_ID = M.EO_CLD_ID
AND N.EO_SLOC_ID = M.EO_MST_SLOC_ID
AND n.EO_HO_ORG_ID = M.EO_HO_ORG_ID
AND N.EO_MST_ID = M.EO_MST_ID
AND M.EO_MST_ID = NVL($P{Entity_Group},m.EO_MST_ID)

and b.cld_id = it.cld_id
and b.sloc_id = it.sloc_id
and b.ho_org_id = it.ho_org_id
and b.itm_id = it.itm_id


  AND a.cld_id               = NVL ($P{CldId}, a.cld_id)
  AND a.sloc_id              = NVL ($P{SlocId}, a.sloc_id)
  AND a.org_id               = NVL ($P{OrgId}, a.org_id)
  AND a.ho_org_id            = NVL ($P{HoOrgId}, a.ho_org_id)
  AND a.doc_id               = NVL ($P{DocId}, a.doc_id )
  AND a.eo_id                = NVL ($P{EoId}, a.eo_id)
and a.curr_id_sp = NVL($P{CurrId},a.curr_id_sp)
  AND NVL (i.catg_id, 340)   = NVL ($P{EoCatId}, NVL (i.catg_id, 340))
  AND NVL (a.assigned_to, 1) = NVL ($P{AssignTo}, NVL (a.assigned_to,1))

  AND b.itm_id               = NVL ($P{ItmId}, b.itm_id)
  AND f.grp_id               = NVL ($P{GrpId}, f.grp_id)
and NVL (a.ORDER_STAT, 217)   = NVL ($P{Status} , NVL (A.ORDER_STAT, 217))
  AND trunc(A.DOC_DT) >=NVL(TO_DATE($P{FromDate},'yyyy-MM-DD'),trunc(A.DOC_DT))
  AND trunc(A.DOC_DT) <=NVL(TO_DATE($P{ToDate},'yyyy-MM-DD'),trunc(A.DOC_DT))]]>
	</queryString>
