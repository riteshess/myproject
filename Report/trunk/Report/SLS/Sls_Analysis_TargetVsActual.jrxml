	<queryString>
		<![CDATA[SELECT DISTINCT
  A.ORG_ID,
  E.ORG_DESC,
  A.HO_ORG_ID,
  A.BUDGET_METHOD,
  B.ITM_GRP_ID,
  F.GRP_NM,
  B.ITM_ID,
  C.ITM_DESC,
  B.ITM_UOM,
  D.UOM_NM,
  nvl(SUM(B.ITM_QTY),0)TOT_QTY,
nvl(sum(i.itm_ship_qty),0)tot_sls_qty
FROM
  BDG.BDG$EO$BUDGET A,
  BDG.BDG$EO$BUDGET$PROD$PRD$DTL B,
  APP.APP$ITM$PRF C,
  APP.APP$UOM$CONV$STD D,
  APP.ORG E,
  APP.APP$GRP F,
  sls$inv$ship$itm i
WHERE
  A.CLD_ID          = B.CLD_ID
AND A.SLOC_ID       = B.SLOC_ID
AND A.ORG_ID        = B.ORG_ID
AND A.HO_ORG_ID     = B.HO_ORG_ID
AND A.DOC_ID        = B.DOC_ID
AND A.EO_ID         = B.EO_ID
and b.cld_id = i.cld_id(+)
and b.sloc_id = i.sloc_id(+)
and b.org_id = i.org_id(+)
and b.ho_org_id = i.ho_org_id(+)
and b.itm_id = i.itm_id(+)
AND B.CLD_ID        = C.CLD_ID
AND B.SLOC_ID       = C.SLOC_ID
AND B.HO_ORG_ID     = C.HO_ORG_ID
AND B.ITM_ID        = C.ITM_ID
AND B.ITM_UOM       = D.UOM_ID
AND A.ORG_ID        = E.ORG_ID
AND B.CLD_ID        = F.CLD_ID
AND B.SLOC_ID       = F.SLOC_ID
AND B.ORG_ID        = F.ORG_ID
AND B.HO_ORG_ID     = F.HO_ORG_ID
AND B.ITM_GRP_ID    = F.GRP_ID
AND A.BUDGET_METHOD = 2
AND A.CLD_ID        = NVL($P{CldId}, A.CLD_ID)
AND A.SLOC_ID       = NVL($P{SlocId} , A.SLOC_ID)
AND A.ORG_ID        = NVL($P{OrgId}, A.ORG_ID)
AND A.HO_ORG_ID     = NVL($P{HoOrgId}, A.HO_ORG_ID)
AND B.ITM_GRP_ID    = NVL($P{GrpId}, B.ITM_GRP_ID)
AND B.ITM_ID        = NVL($P{ItmId}, B.ITM_ID)
AND TRUNC(A.PRD_START_DT) BETWEEN NVL(TO_DATE($P{FromDate},'YYYY-MM-DD'),TRUNC(
  A.PRD_START_DT)) AND NVL(TO_DATE($P{ToDate},'YYYY-MM-DD'),TRUNC(
  A.PRD_START_DT))
GROUP BY
  A.ORG_ID,
  E.ORG_DESC,
  A.HO_ORG_ID,
  A.PRD_START_DT,
  A.PRD_END_DT,
  A.BUDGET_METHOD,
  B.ITM_GRP_ID,
  F.GRP_NM,
  B.ITM_ID,
  C.ITM_DESC,
  B.ITM_UOM,
  D.UOM_NM,
  B.PRD_DTL_START_DT,
  B.PRD_DTL_END_DT,
  B.PRD_DTL_END_DT
ORDER BY
  B.ITM_ID]]>
	</queryString>
