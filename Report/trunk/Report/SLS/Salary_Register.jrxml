	<queryString>
		<![CDATA[select DISTINCT a.EMP_NM,a.doc_id,
(select count(param_ID) from hcm$setup where
    CLD_ID = L.cld_id
and SLOC_ID = L.sloc_id
and HO_ORG_ID = L.ho_org_id
and param_type = 21
--and param_id = L.leave_id
and param_desc like '%CASUAL%') cl,
(select count(param_desc) from hcm$setup where
    CLD_ID = L.cld_id
and SLOC_ID = L.sloc_id
and HO_ORG_ID = L.ho_org_id
and param_type = 21
--and param_id = L.leave_id
and param_desc like '%MEDICAL%') Ml,
(select count(param_desc) from hcm$setup where
    CLD_ID = L.cld_id
and SLOC_ID = L.sloc_id
and HO_ORG_ID = L.ho_org_id
and param_type = 21
--and param_id = L.leave_id
and param_desc like '%EARNED%') El,
to_char(K.PROC_FRM_DT,'yyyy-mm-dd')PROC_FRM_DT,
to_char(K.PROC_TO_DT,'yyyy-mm-dd')PROC_TO_DT,
 i.org_desc,
(K.PROC_TO_DT-K.PROC_FRM_DT)+1 TTL_DAYS,
    (((K.PROC_TO_DT-K.PROC_FRM_DT)+1)- (SELECT SUM(LV) FROM
        (
            SELECT COUNT(*) LV
            FROM HCM$TIME$MOVE$EDIT$DTL X
            WHERE X.CLD_ID = A.CLD_ID AND X.SLOC_ID = a.SLOC_ID AND X.HO_ORG_ID = a.HO_ORG_ID AND X.ORG_ID = a.ORG_ID
            AND X.ATTEN_DT> = K.PROC_FRM_DT AND X.ATTEN_DT<=K.PROC_TO_DT
            AND X.EMP_DOC_ID=a.doc_id
            AND X.DED_CH='F'
            UNION ALL
            SELECT COUNT(*)/2 LV
            FROM HCM$TIME$MOVE$EDIT$DTL Y
            WHERE Y.CLD_ID = A.CLD_ID AND Y.SLOC_ID = a.SLOC_ID AND Y.HO_ORG_ID = a.HO_ORG_ID AND Y.ORG_ID = a.ORG_ID
            AND Y.ATTEN_DT> = K.PROC_FRM_DT AND Y.ATTEN_DT<=K.PROC_TO_DT
            AND Y.EMP_DOC_ID=a.doc_id
            AND DED_CH='H'
        )
    ) ) PAID_DAYS,
(SELECT SUM(LV) FROM
        (
            SELECT COUNT(*) LV
            FROM HCM$TIME$MOVE$EDIT$DTL X
            WHERE X.CLD_ID = A.CLD_ID AND X.SLOC_ID = a.SLOC_ID AND X.HO_ORG_ID = a.HO_ORG_ID AND X.ORG_ID = a.ORG_ID
            AND X.ATTEN_DT> = K.PROC_FRM_DT AND X.ATTEN_DT<=K.PROC_TO_DT
            AND X.EMP_DOC_ID=a.doc_id
            AND X.DED_CH='F'
            UNION ALL
            SELECT COUNT(*)/2 LV
            FROM HCM$TIME$MOVE$EDIT$DTL Y
            WHERE Y.CLD_ID = A.CLD_ID AND Y.SLOC_ID = a.SLOC_ID AND Y.HO_ORG_ID = a.HO_ORG_ID AND Y.ORG_ID = a.ORG_ID
            AND Y.ATTEN_DT> = K.PROC_FRM_DT AND Y.ATTEN_DT<=K.PROC_TO_DT
            AND Y.EMP_DOC_ID=a.doc_id
            AND DED_CH='H'
        )
    )lwp,
(SELECT count(c.hol_day)
FROM ORG$HCM$HOL$CAL$LOC B,HCM$HOL$CAL$VW C
WHERE A.CLD_ID = B.CLD_ID
    AND A.SLOC_ID = B.SLOC_ID
    AND A.HO_ORG_ID = B.HO_ORG_ID
    AND A.ORG_ID = B.ORG_ID
    AND A.EMP_LOC_ID  = B.LOC_ID
    AND A.DOC_ID = NVL($P{empDocId}, A.DOC_ID)
    AND B.CLD_ID = C.CLD_ID
    AND B.SLOC_ID = C.SLOC_ID
    AND B.HO_ORG_ID = C.HO_ORG_ID
    AND B.HOL_ID = C.HOL_ID
    and  trunc(C.HOL_DAY) >= nvl (to_date ( $P{FromDate}, 'yyyy-MM-dd'),trunc(C.HOL_DAY))
    and  trunc(C.HOL_DAY) <= nvl (to_date ( $P{ToDate} , 'yyyy-MM-dd'),trunc(C.HOL_DAY)))hd,
((K.PROC_TO_DT-K.PROC_FRM_DT)+1
-nvl((SELECT count(c.hol_day)
FROM ORG$HCM$HOL$CAL$LOC B,HCM$HOL$CAL$VW C
WHERE A.CLD_ID = B.CLD_ID
    AND A.SLOC_ID = B.SLOC_ID
    AND A.HO_ORG_ID = B.HO_ORG_ID
    AND A.ORG_ID = B.ORG_ID
    AND A.EMP_LOC_ID  = B.LOC_ID
    AND A.DOC_ID = NVL($P{empDocId}, A.DOC_ID)
    AND B.CLD_ID = C.CLD_ID
    AND B.SLOC_ID = C.SLOC_ID
    AND B.HO_ORG_ID = C.HO_ORG_ID
    AND B.HOL_ID = C.HOL_ID
    and  trunc(C.HOL_DAY) >= nvl (to_date ( $P{FromDate}, 'yyyy-MM-dd'),trunc(C.HOL_DAY))
    and  trunc(C.HOL_DAY) <= nvl (to_date ( $P{ToDate} , 'yyyy-MM-dd'),trunc(C.HOL_DAY))),0)
-nvl((select count(param_ID) from hcm$setup where
    CLD_ID = L.cld_id
and SLOC_ID = L.sloc_id
and HO_ORG_ID = L.ho_org_id
and param_type = 21
--and param_id = L.leave_id
and param_desc like '%CASUAL%'),0)
-nvl((select count(param_desc) from hcm$setup where
    CLD_ID = L.cld_id
and SLOC_ID = L.sloc_id
and HO_ORG_ID = L.ho_org_id
and param_type = 21
--and param_id = L.leave_id
and param_desc like '%MEDICAL%'),0)
-nvl((select count(param_desc) from hcm$setup where
    CLD_ID = L.cld_id
and SLOC_ID = L.sloc_id
and HO_ORG_ID = L.ho_org_id
and param_type = 21
--and param_id = L.leave_id
and param_desc like '%EARNED%'),0)
-nvl((SELECT SUM(LV) FROM
        (
            SELECT COUNT(*) LV
            FROM HCM$TIME$MOVE$EDIT$DTL X
            WHERE X.CLD_ID = A.CLD_ID AND X.SLOC_ID = a.SLOC_ID AND X.HO_ORG_ID = a.HO_ORG_ID AND X.ORG_ID = a.ORG_ID
            AND X.ATTEN_DT> = K.PROC_FRM_DT AND X.ATTEN_DT<=K.PROC_TO_DT
            AND X.EMP_DOC_ID=a.doc_id
            AND X.DED_CH='F'
            UNION ALL
            SELECT COUNT(*)/2 LV
            FROM HCM$TIME$MOVE$EDIT$DTL Y
            WHERE Y.CLD_ID = A.CLD_ID AND Y.SLOC_ID = a.SLOC_ID AND Y.HO_ORG_ID = a.HO_ORG_ID AND Y.ORG_ID = a.ORG_ID
            AND Y.ATTEN_DT> = K.PROC_FRM_DT AND Y.ATTEN_DT<=K.PROC_TO_DT
            AND Y.EMP_DOC_ID=a.doc_id
            AND DED_CH='H'
        )
    ),0))wd
From ORG$HCM$EMP$PRF a, app.org i, HCM$SAL$PROC K, HCM$EMP$LEAVE L
where a.cld_id = i.org_cld_id
and a.org_id = i.org_id
AND K.CLD_ID = a.CLD_ID
AND K.SLOC_ID = a.SLOC_ID
AND K.HO_ORG_ID = a.HO_ORG_ID
AND K.ORG_ID = a.ORG_ID
AND K.EMP_DOC_ID = a.DOC_ID
AND A.CLD_ID=L.CLD_ID(+)
AND A.SLOC_ID=L.SLOC_ID(+)
AND A.ORG_ID=L.ORG_ID(+)
AND A.HO_ORG_ID=L.HO_ORG_ID(+)
AND A.DOC_ID=L.DOC_ID(+)
AND A.EMP_CODE=L.EMP_CODE(+)

AND (K.PROC_FRM_DT>= to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
AND K.PROC_FRM_DT <=to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy'))
AND (K.PROC_TO_DT>= to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
AND K.PROC_TO_DT <=to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy'))
AND a.CLD_ID = $P{cldId}
AND a.SLOC_ID = $P{slocId}
AND a.HO_ORG_ID = $P{hoOrgId}
AND a.ORG_ID = $P{orgId}
AND (A.EMP_DEPT_ID = $P{crtValue} OR A.EMP_LOC_ID =  $P{crtValue}
OR A.EMP_GRP_ID = $P{crtValue} OR A.DOC_ID =$P{crtValue})
AND A.DOC_ID = NVL($P{empDocId}, A.DOC_ID)]]>
	</queryString>
