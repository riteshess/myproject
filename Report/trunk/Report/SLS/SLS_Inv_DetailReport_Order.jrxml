	<queryString>
		<![CDATA[SELECT (
  CASE
    WHEN SUBSTR(adds, instr(adds, 'S'), 3) LIKE 'SOL'
    THEN SUBSTR(adds, INSTR(adds, '-') + 2, 4)
    ELSE ''
  END) BRANCH,
  INV_SUB_TYPE_DESC,
  CLD_ID,
  SLOC_ID,
  ORG_ID,
  HO_ORG_ID,
  listagg(invoice_id,'/') within GROUP(ORDER BY invoice_id)AS invoice_id,
  listagg(DOC_DT,'/') within group(order by doc_dt) as doc_dt,
  org_tin,
  NVL(EO_PART_NO, itm_desc) partitm,
  cust_tin,
  PAN_NO,
  ORG_PHONE1,
  ORG_EMAIL1,
  ORG_FAX1,
  ORG_WEBSITE1,
  ServiceTaxNo,
  CURR_ID_SP,
  EO_NM,
  ITM_ID,
  itm_desc,
  EO_ID,
  SUM(ITM_RATE) ITM_RATE,
  EXT_DOC_DT,
  EXT_DOC_NO,
  uom_nm,
  SUM(discount)DISCOUNT,
  upper(adds)adds,
  Registration_No,
  SUM(ITM_AMT_SP) itm_amt_sp,
  SUM(ITM_AMT_BS) itm_amt_bs,
listagg(ship_detail,',') within group (order by ship_detail) as ship_detail,
remarks,tnc,
  SUM(ITM_SHIP_QTY) itm_ship_qty,
  org_adds,
  ORG_DESC,
  SUM(oth_charg)oth_charg,
  CIN_NO,
  STAX_NO,
  upper(ship_adds)ship_adds,
  reg_adds,
  MR_NO,
  PO_NO,
  OrgTin,
  leg_code,
  CustTin,
  SUM(tax_amt)tax_amt
FROM
  (SELECT inv.CLD_ID,
    k.DOC_SUB_TYP_NM INV_SUB_TYPE_DESC,
    inv.SLOC_ID,
    inv.ORG_ID,
    inv.HO_ORG_ID,
    inv.doc_id,
    inv.DOC_DT doc_dt,
    inv.INV_TYPE,
    inv.CURR_ID_SP,
    c.EO_ID,
    (SELECT eo_nm
    FROM app.app$eo
    WHERE eo_cld_id  = c.cld_id
    AND eo_sloc_id   = c.sloc_id
    AND eo_ho_org_id = c.ho_org_id
    AND eo_id        = c.eo_id
    ) EO_NM,
    ii.ITM_ID,
    b.ITM_DESC itm_desc,
    ii.ITM_RATE itm_rate,
    ii.ITM_UOM,
    e.UOM_NM uom_nm,
    ii.ITM_SHIP_QTY itm_ship_qty,
    NVL(DECODE(NVL(DECODE(ii.ITM_DISC_TYP, 'A', ii.ITM_DISC_VAL, 'P', ((NVL(ii.ITM_SHIP_QTY, 0) * NVL(ii.ITM_RATE, 0)) * NVL(ii.ITM_DISC_VAL, 0) / 100)), 0), 0, (NVL(inv.DISC_VAL, 0))), 0.00) discount,
    NVL(ii.ITM_SHIP_QTY, 0)                                                                     * NVL(ii.ITM_RATE, 0)itm_amt_gs,
    ii.ITM_AMT_BS ITM_AMT_BS,
    ii.ITM_AMT_SP ITM_AMT_SP,
    inv.TOT_AMT_SP TOT_AMT_SP,
    inv.TOT_AMT_BS,
    -- ii.ITM_COST,
    m.ATT_NM inv_wf_stat_desc,
    f.ORG_DESC,
    l.EO_NM assigned_to,
    m.ATT_NM inv_mode,
      (SELECT fn_get_shp_dtl(inv.CLD_ID, inv.SLOC_ID, inv.HO_ORG_ID, inv.ORG_ID, inv.DOC_ID, 'S')
  FROM DUAL
  ) ship_detail,
    (SELECT fn_get_shp_dtl(inv.CLD_ID, inv.SLOC_ID, inv.HO_ORG_ID, inv.ORG_ID, inv.DOC_ID, 'R')
  FROM DUAL
  ) remarks,
  (SELECT fn_get_shp_dtl(inv.CLD_ID, inv.SLOC_ID, inv.HO_ORG_ID, inv.ORG_ID, inv.DOC_ID, 'C')
  FROM DUAL
  ) tnc,
    (SELECT app.fn_get_adds_1(NVL($P{SlocId}, inv.SLOC_ID), NVL($P{CldId}, inv.CLD_ID), inv.BILL_ADD, 'M')
    FROM dual
    ) adds,
    (SELECT app.app$adds$vw.ADDS
    FROM app.app$adds$vw
    WHERE app.app$adds$vw.ADDS_ID = p.ORG_ADD_ID1
    ) org_adds,
    (SELECT app.fn_get_adds_1($P{SlocId}, $P{CldId}, p.ORG_ADD_ID2, 'S')
    FROM DUAL
    ) reg_adds,
    o.CURR_NOTATION_ALIAS,
    c.EXT_DOC_DT,
    c.EXT_DOC_NO,
    SUBSTR(inv.EXT_DOC_NO, 0, INSTR(inv.EXT_DOC_NO, ' ')) MR_NO,
    SUBSTR(inv.EXT_DOC_NO, instr(inv.EXT_DOC_NO, ' ') + 1) PO_NO,
    p.TIN_NO org_tin,
    r.TIN_NO cust_tin,
    NVL(oc.AMT_SP, 0.00) oth_charg,
    p.PAN_NO,
    p.ORG_PHONE1,
    p.ORG_EMAIL1,
    p.ORG_FAX1,
    p.ORG_WEBSITE1,
    p.REG_NO ServiceTaxNo,
    p.REG_NO Registration_No,
    p.CIN_NO,
    p.STAX_NO,
    INV.DISP_DOC_ID as invoice_id,
    (SELECT app.fn_get_adds_1($P{SlocId}, $P{CldId}, c.SHIP_ADDs, 'M') FROM DUAL
    ) ship_adds,
    p.TIN_NO OrgTin,
    r.TIN_NO CustTin,
    c.tax_rule_flg,
    ci.eo_part_no ,
    (SELECT eo_leg_code
    FROM app.app$eo
    WHERE eo_id      = c.eo_id
    AND eo_cld_id    = c.cld_id
    AND eo_sloc_id   = c.sloc_id
    AND eo_ho_org_id = c.ho_org_id
    AND rownum       =1
    )leg_code,
    (SELECT SUM(tr.tax_amt_sp)
    FROM sls$inv$tr$lines tr
    WHERE tr.cld_id  = inv.cld_id
    AND tr.sloc_id   = inv.sloc_id
    AND tr.org_id    = inv.org_id
    AND tr.ho_org_id = inv.ho_org_id
    AND tr.doc_id    = inv.doc_id
    AND tr.itm_id    = ii.itm_id
    AND tr.so_id     = c.doc_id
    AND sr_no       <>1
    )tax_amt,
    intm.doc_dt intm_dt
  FROM sls$inv$ship$itm ii,
    sls$inv inv,
    sls$inv$oc oc,
    app.app$doc$sub$type k,
    app.app$sls$itm_vw b,
    sls.sls$so c,
  --  sls.sls$so D,
    sls$so$itm ci,
    app.app$uom$conv$std e,
    app.org f,
    app.org$oth$dtl p,
    app.org$fy fy,
    (SELECT x.EO_CLD_ID,
      x.EO_SLOC_ID,
      x.EO_HO_ORG_ID,
      x.EO_ID,
      x.EO_NM
    FROM app.app$eo x
    WHERE x.EO_CLD_ID = NVL($P{CldId}, x.EO_CLD_ID)
    AND x.EO_SLOC_ID  = NVL($P{SlocId}, x.EO_SLOC_ID)
    AND x.EO_TYPE     = '80'
    ) l,
    (SELECT y.ATT_NM, y.ATT_ID FROM app.app$ds$att y WHERE y.ATT_TYPE_ID = 69
    ) m,
    sls.sls$inv$tr n,
    app.app$curr_vw o,
    sls$eo$price ep,
    app.app$eo$prf r,
    sls$intm$slip intm
  WHERE inv.cld_id           = ii.cld_id
  AND inv.sloc_id            = ii.sloc_id
  AND inv.org_id             = ii.org_id
  AND inv.ho_org_id          = ii.ho_org_id
  AND inv.doc_id             = ii.doc_id
  AND ii.SLOC_ID             = b.SLOC_ID
  AND ii.ITM_ID              = b.ITM_ID
  AND ii.ORG_ID              = b.ORG_ID
  AND ii.CLD_ID              = b.CLD_ID
  AND ii.HO_ORG_ID           = b.HO_ORG_ID
  AND inv.SLOC_ID            = c.SLOC_ID
  AND inv.ORG_ID             = c.ORG_ID
  AND inv.CLD_ID             = c.CLD_ID
  AND inv.HO_ORG_ID          = c.HO_ORG_ID
  AND ii.SO_ID               = C.DOC_ID
  AND c.FY_ID                = fy.ORG_FY_ID
  AND c.ORG_ID               = fy.ORG_ID
  AND inv.SLOC_ID            = e.SLOC_ID
  AND inv.CLD_ID             = e.CLD_ID
  AND ii.ITM_UOM             = e.UOM_ID
  AND inv.CLD_ID             = f.ORG_CLD_ID
  AND inv.ORG_ID             = f.ORG_ID
  AND inv.ORG_ID             = p.ORG_ID(+)
  AND c.CLD_ID               = l.EO_CLD_ID(+)
  AND c.SLOC_ID              = l.EO_SLOC_ID(+)
  AND c.HO_ORG_ID            = l.EO_HO_ORG_ID(+)
  AND c.ASSIGNED_TO          = l.EO_ID(+)
  AND inv.inv_type           = k.DOC_TYP_ID
  AND inv.inv_sub_type       = k.DOC_sub_TYP_ID
  AND k.doc_id               = 21504
  AND inv.cld_id             = intm.cld_id(+)
  AND inv.sloc_id            = intm.sloc_id(+)
  AND inv.org_id             = intm.org_id(+)
  AND inv.ho_org_id          = intm.ho_org_id(+)
  AND inv.intim_slip_id      = intm.doc_id(+)
  AND inv.INV_MODE           = m.ATT_ID
  AND inv.CLD_ID             = r.EO_CLD_ID
  AND inv.SLOC_ID            = r.EO_SLOC_ID
  AND inv.HO_ORG_ID          = r.EO_HO_ORG_ID
  AND inv.EO_ID              = r.EO_ID
  AND inv.CLD_ID             = n.CLD_ID(+)
  AND inv.SLOC_ID            = n.SLOC_ID(+)
  AND inv.ORG_ID             = N.ORG_ID(+)
  AND inv.HO_ORG_ID          = n.HO_ORG_ID(+)
  AND inv.DOC_ID             = n.DOC_ID(+)
  AND ii.ITM_ID              = n.ITM_ID(+)
  AND c.doc_id               = n.so_id(+)
  AND inv.CURR_ID_SP         = o.CURR_ID
  AND inv.CLD_ID             = oc.CLD_ID(+)
  AND inv.SLOC_ID            = oc.SLOC_ID(+)
  AND inv.ORG_ID             = oc.ORG_ID(+)
  AND inv.HO_ORG_ID          = oc.HO_ORG_ID(+)
  AND inv.DOC_ID             = oc.DOC_ID(+)
  AND inv.CLD_ID             = ep.CLD_ID(+)
  AND inv.SLOC_ID            = ep.SLOC_ID(+)
  AND inv.ORG_ID             = ep.ORG_ID(+)
  AND inv.HO_ORG_ID          = ep.HO_ORG_ID(+)
  AND ii.ITM_ID              = ep.ITM_ID(+)
  AND inv.EO_ID              = ep.EO_ID(+)
  AND c.cld_id               = ci.cld_id
  AND c.sloc_id              = ci.sloc_id
  AND c.org_id               = ci.org_id
  AND c.ho_org_id            = ci.ho_org_id
  AND c.doc_id               = ci.doc_id
  AND ii.itm_id              = ci.itm_id
  AND c.CLD_ID               = NVL($P{CldId}, c.CLD_ID)
  AND c.SLOC_ID              = NVL($P{SlocId}, c.SLOC_ID)
  AND c.ORG_ID               = NVL($P{OrgId}, c.ORG_ID)
  AND c.HO_ORG_ID            = NVL($P{HoOrgId}, c.HO_ORG_ID)
 -- AND c.REF_DOC_SPLIT_SRC    = d.doc_id
  AND c.doc_id               =NVL($P{DocId}, c.doc_id)
  AND NVL(c.ORDER_STAT, 217) = NVL($P{Status}, NVL(c.ORDER_STAT, 217))
  AND TRUNC(c.DOC_DT)       >= NVL(TO_DATE($P{FromDate}, 'yyyy-MM-dd'), TRUNC(c.DOC_DT))
  AND TRUNC(c.DOC_DT)       <= NVL(TO_DATE($P{ToDate}, 'yyyy-MM-dd'), TRUNC(c.DOC_DT))
  AND ii.itm_rate           != 0.00
  )
GROUP BY INV_SUB_TYPE_DESC,
  CLD_ID,
  SLOC_ID,
  ORG_ID,
  HO_ORG_ID,
  org_tin,
  NVL(EO_PART_NO, itm_desc),
  cust_tin,
  PAN_NO,
  ORG_PHONE1,
  ORG_EMAIL1,
  ORG_FAX1,
  ORG_WEBSITE1,
  ServiceTaxNo,
  CURR_ID_SP,
  EO_NM,
  ITM_ID,
  itm_desc,
  EO_ID,
  EXT_DOC_DT,
  EXT_DOC_NO,
  uom_nm,
  adds,
  Registration_No,
  EO_PART_NO,
  ORG_DESC,
  tnc,remarks,
  CIN_NO,
  STAX_NO,
  reg_adds,
  MR_NO,
  PO_NO,
  leg_code,
  OrgTin,
  EO_PART_NO,
  adds,
  org_adds,
  ship_adds,
  CustTin]]>
	</queryString>
