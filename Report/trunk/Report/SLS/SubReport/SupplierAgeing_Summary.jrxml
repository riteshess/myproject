<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="SupplierAgeing_Summary" language="groovy" pageWidth="907" pageHeight="595" orientation="Landscape" columnWidth="891" leftMargin="8" rightMargin="8" topMargin="20" bottomMargin="20" isFloatColumnFooter="true">
