	<queryString>
		<![CDATA[select to_char(s.DOC_DT,'dd-Mon-yyyy') ss_dt,substr(s.SS_ID,3) ss_id,si.ITM_PRICE,si.ITM_QTY,si.ITM_AMT_SP,si.itm_id,
(select org_desc from app.org
                 where org_id=s.org_id
                 and org_create_ref_sloc_id=s.sloc_id
                 and org_cld_id=s.cld_id)org_desc,
(select eo_nm from app.app$eo
              where eo_id=s.EO_ID
              and eo_cld_id=s.cld_id
              and eo_sloc_id=s.sloc_id
              and eo_org_id=s.org_id
              and eo_ho_org_id=s.ho_org_id)custtname,
(select substr(shipmnt_id,3) from sls$shipmnt
                        where doc_id=s.SHIP_ID
                        and cld_id=s.cld_id
                        and sloc_id=s.sloc_id
                        and org_id=s.org_id
                        and ho_org_id=s.ho_org_id) ship_id,
(select itm_desc from app.app$itm$prf
                 where itm_id=si.ITM_ID
                 and cld_id=si.cld_id
                 and sloc_id=si.sloc_id
                 and ho_org_id=si.ho_org_id)itm_desc
from sls$ss s,sls$ss$itm si
where s.cld_id=si.cld_id
and s.sloc_id=si.sloc_id
and s.org_id=si.org_id
and s.ho_org_id=si.org_id
and s.doc_id=si.doc_id
 and  s.CLD_ID = nvl($P{CldId},s.cld_id) AND
  s.SLOC_ID = nvl($P{SlocId},s.sloc_id) AND
  s.ORG_ID = nvl($P{OrgId},s.org_id) AND
  s.HO_ORG_ID = nvl($P{HoOrgId},s.ho_org_id) AND
  s.DOC_ID = nvl($P{DocId},s.DOC_ID) AND
  si.ITM_ID=nvl($P{ItmId},si.itm_id) and
 s.EO_ID=nvl($P{EoId},s.eo_id) and
  trunc(s.doc_dt) >= nvl (to_date ($P{FromDate}, 'yyyy-MM-dd'),s.doc_dt) AND
  trunc(s.doc_dt) <= nvl (to_date ( $P{ToDate}, 'yyyy-MM-dd'),s.doc_dt)
order by custtname,s.ss_id]]>
	</queryString>
