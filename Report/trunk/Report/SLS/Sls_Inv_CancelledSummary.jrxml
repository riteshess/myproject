	<queryString>
		<![CDATA[select disp_doc_id,doc_dt,tot_amt_sp,
(select eo_nm from app.app$eo
where eo_id = a.eo_id
and eo_cld_id = a.cld_id
and eo_sloc_id = a.sloc_id
and eo_ho_org_id = a.ho_org_id)eo_nm,
(
  SELECT
    o.org_desc
  FROM
    app.org o
  WHERE
    a.cld_id    = o.org_cld_id
  AND a.sloc_id = o.org_create_ref_sloc_id
  AND a.org_id  = o.org_id
)
org_desc
from
sls$inv a
where a.INV_STAT=220
and
  A.CLD_ID = nvl($P{CldId},a.cld_id) AND
  A.SLOC_ID = nvl($P{SlocId},a.sloc_id) AND
  A.ORG_ID = nvl($P{OrgId},a.org_id) AND
  A.HO_ORG_ID = nvl($P{HoOrgId},a.ho_org_id)
AND  substr(a.disp_doc_id,3) >=nvl($P{DocId},substr(a.disp_doc_id,3)) and
substr(disp_doc_id,3) <= nvl($P{ToDocId},substr(disp_doc_id,3)) and
  A.CURR_ID_SP=nvl($P{CurrId},a.curr_id_sp) and
  trunc(a.doc_dt) >= nvl (to_date ($P{FromDate}, 'yyyy-MM-dd'), trunc(a.doc_dt)) and
 trunc(a.doc_dt) <= nvl (to_date ($P{ToDate}, 'yyyy-MM-dd'), trunc(a.doc_dt))]]>
	</queryString>
