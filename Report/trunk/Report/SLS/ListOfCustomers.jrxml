  p.ONHOLD_TO_DT,
  o.ORG_DESC
FROM app.app$eo e,
  app.app$eo$prf p,
  app.org o
WHERE e.EO_CLD_ID  = p.EO_CLD_ID
AND e.EO_SLOC_ID   = p.EO_SLOC_ID
AND e.EO_HO_ORG_ID = p.EO_HO_ORG_ID
AND e.EO_ID        = p.EO_ID
AND e.EO_TYPE      = p.EO_TYPE
and e.eo_cld_id    = o.org_cld_id
and e.eo_sloc_id   = o.org_create_ref_sloc_id
and e.eo_ho_org_id = o.org_id
AND p.EO_TYPE      = 68
and e.eo_cld_id = nvl($P{CldId},e.eo_cld_id)
and e.eo_sloc_id = nvl($P{SlocId},e.eo_sloc_id)
and e.eo_ho_org_id = nvl($P{HoOrgId},e.eo_ho_org_id)
AND TRUNC(e.usr_id_create_dt)>=NVL(TO_DATE($P{FromDate},'yyyy-MM-dd'),TRUNC(e.usr_id_create_dt))
AND TRUNC(e.usr_id_create_dt)<=NVL(TO_DATE($P{ToDate},'yyyy-MM-dd'),TRUNC(e.usr_id_create_dt))
