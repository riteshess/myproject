		<![CDATA[SELECT b.org_id,B.ORG_DESC ORG_DESC,C.EO_NM SUPPLIER_NAME,A.TOT_SCORE,D.USR_NAME EVALUATED_BY,
PKG_MM_EVAL.GET_POSITION( NVL($P{slocid},'1'),NVL($P{cldid},'0000'),NVL($P{OrgId},'01'),'S',
NULL,NVL($P{suppid},A.EO_ID) ) POSITION
FROM MM$EO$PERF$RSLT A,APP.ORG B,APP.APP$SUPP_VW C,APP.APP$SEC$USR D
WHERE A.CLD_ID =B.ORG_CLD_ID
AND A.ORG_ID =B.ORG_ID
and A.CLD_ID =C.CLD_ID
AND A. SLOC_ID =c.SLOC_ID
AND A.ORG_ID =C.ORG_ID
AND MM.MM_GET_HO_ORG(A.CLD_ID,A.SLOC_ID,A.ORG_ID) =c.HO_ORG_ID
AND A.EO_ID =C.EO_ID
AND A.SLOC_ID =D.SLOC_ID
and a.usr_id_create=D.usr_id
AND A.CLD_ID = NVL($P{cldid},A.CLD_ID)
AND A.SLOC_ID = NVL($P{slocid},A.SLOC_ID)
AND a.org_id=  NVL($P{OrgId},A.ORG_ID)
AND A.EO_ID = NVL($P{suppid},A.EO_ID)
AND A.USR_ID_CREATE = NVL($P{EvaluatedBy},A.USR_ID_CREATE)
AND A.EVAL_DT >= NVL(TO_DATE($P{FromDate},'yyyy-mm-dd'),A.EVAL_DT)
AND A.EVAL_DT <= NVL(TO_DATE($P{ToDate},'yyyy-mm-dd'),A.EVAL_DT)
AND mm.mm_get_ho_org(a.cld_id,a.sloc_id,a.org_id) = NVL($P{HoOrgId},mm.mm_get_ho_org(a.cld_id,a.sloc_id,a.org_id))
ORDER BY b.org_id,position]]>
