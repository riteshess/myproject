	<queryString>
		<![CDATA[select
       a.org_id,
       a.ho_org_id,
       b.org_desc,
       a.coa_id,
       c.coa_nm,
       a.curr_id_sp,
       a.ar_cc,
       e.glbl_curr_notation_alias curr_nm,
       a.vou_id,
       a.vou_dt,
       a.ar_vou_due_dt,
       a.ar_type_id,
       substr(doc_txn_id_disp,3) doc_id,
       decode(a.ar_type_id,'7',substr(a.ext_doc_no,3),a.ext_doc_no) doc_no,
       a.ext_doc_dt doc_dt,
       a.ext_doc_due_dt due_date,
       a.sls_inv_doc_id,
       a.sls_inv_id,
       a.sls_inv_dt,
       a.amt_sp,
       a.amt_sp * a.ar_cc amt_sp_signed,
       a.amt_adj,
       a.amt_adj_signed,
       a.amt_os,
       a.amt_os_signed,
       a.os_days,
       a.os_days1,
       decode(sign(30 - os_days)* sign(os_days ), -1,0,decode('O','A',a.amt_sp_signed,'O',amt_os_signed)) os_30_days,
       decode(sign(60 - os_days) * sign(os_days - 31), -1,0,decode('O','A',a.amt_sp_signed,'O',amt_os_signed)) os_60_days,
       decode(sign(90 - os_days) * sign(os_days - 61), -1,0,decode('O','A',a.amt_sp_signed,'O',amt_os_signed)) os_90_days,
       decode(sign(120 - os_days) * sign(os_days - 91), -1,0,decode('O','A',a.amt_sp_signed,'O',amt_os_signed)) os_120_days,
       decode(sign(150 - os_days) * sign(os_days - 121), -1,0,decode('O','A',a.amt_sp_signed,'O',amt_os_signed)) os_150_days,
       decode(sign(180 - os_days) * sign(os_days - 151), -1,0,decode('O','A',a.amt_sp_signed,'O',amt_os_signed)) os_180_days,
       decode(sign(365 - os_days) * sign(os_days - 181), -1,0,decode('O','A',a.amt_sp_signed,'O',amt_os_signed)) os_1_yr,
       decode(sign(os_days - 366), -1,0,decode('O','A',a.amt_sp_signed,'O',amt_os_signed)) os_greater_1_yr,
       i.avg_cr_days avg_cr_days,
       i.cr_limit cr_limit,
       a.amt_typ,
       (select eo_nm from app.app$eo
        where eo_type = 80
        and eo_id = $P{Eo_Id}
        and eo_cld_id = $P{Cld_Id}
        and eo_sloc_id = nvl($P{Sloc_Id},eo_sloc_id)) rep_nm,
       (select eo_mst_nm from app.app$eo$mst
        where eo_cld_id = $P{Cld_Id}
        and eo_mst_sloc_id = $P{Sloc_Id}
        and eo_ho_org_id = $P{Ho_Org_Id}
        and eo_mst_id = $P{Entity_Group}) entity_grp
from ( select a.gl_cld_id cld_id,  a.ar_sloc_id sloc_id,  a.gl_ho_org_id ho_org_id, a.ar_org_id org_id,
              a.ar_coa_id coa_id,  a.ar_cog_id cog_id,  a.ar_curr_id_sp curr_id_sp,
              a.ar_type_id,  a.ar_cc,  a.ar_vou_due_dt, a.ar_vou_id vou_id,  a.ar_vou_dt vou_dt,
              a.ar_gl_sl_no, a.ar_ext_doc_typ ext_doc_typ,  a.ar_ext_doc_no ext_doc_no,
              a.ar_ext_doc_dt ext_doc_dt,  a.ar_vou_due_dt ext_doc_due_dt,  a.ar_doc_id_ref1 sls_inv_doc_id,
              a.ar_txn_id_ref1 sls_inv_id, a.ar_txn_dt_ref1 sls_inv_dt,  a.ar_amt_sp amt_sp,
              a.ar_amt_typ amt_typ,
              decode(a.ar_amt_typ,'Dr',nvl(a.ar_amt_sp,0),'Cr',-nvl(a.ar_amt_sp,0),nvl(a.ar_amt_sp,0)) amt_sp_signed,
              abs(nvl(a1.adj_amt_dtl*ar_cc,0)) amt_adj,--dm 2014-02-24
              nvl(a1.adj_amt_dtl*ar_cc,0) amt_adj_signed, --dm 2014-02-24
              nvl(a.ar_amt_sp*a.ar_cc,0) - nvl(abs(a1.adj_amt_dtl)*a.ar_cc,0) amt_os,
              case a.ar_amt_typ when 'Dr' then nvl(a.ar_amt_sp*a.ar_cc,0) + nvl(a1.adj_amt_dtl*a.ar_cc,0)
                                when 'Cr' then -nvl(a.ar_amt_sp*a.ar_cc,0) + nvl(a1.adj_amt_dtl*a.ar_cc,0) end  amt_os_signed, --dm 2014-02-24
              DECODE('D', 'D',
                              (DECODE('I', 'V',TRUNC(to_Date($P{To_Date},'YYYY-MM-DD') - a.AR_VOU_DT),
                                           'I',TRUNC(to_Date($P{To_Date},'YYYY-MM-DD') - NVL(a.AR_EXT_DOC_DT,a.AR_VOU_DT)),
                                           'D',TRUNC(to_Date($P{To_Date},'YYYY-MM-DD') - NVL(a.AR_VOU_DUE_DT,a.AR_VOU_DT)),
                                               TRUNC(to_Date($P{To_Date},'YYYY-MM-DD') - a.AR_VOU_DT) ) + 1),
                          'M',
                              (DECODE('I', 'V', 30*months_between(last_day(to_Date($P{To_Date},'YYYY-MM-DD')),last_day(to_date(a.AR_VOU_DT)))+1,
                                           'I',30*months_between(last_day(to_Date($P{To_Date},'YYYY-MM-DD')),last_day(NVL(a.AR_EXT_DOC_DT,a.AR_VOU_DT)))+1,
                                           'D',30*months_between(last_day(to_Date($P{To_Date},'YYYY-MM-DD')),last_day(NVL(a.AR_VOU_DUE_DT,a.AR_VOU_DT)))+1,
                                               30*months_between(last_day(to_Date($P{To_Date},'YYYY-MM-DD')),last_day(a.AR_VOU_DT))+1))
                     ) OS_DAYS,
             (DECODE('I', 'V', TRUNC(to_Date($P{To_Date},'YYYY-MM-DD') - a.AR_VOU_DT),
                          'I', TRUNC(to_Date($P{To_Date},'YYYY-MM-DD') - NVL(a.AR_EXT_DOC_DT,a.AR_VOU_DT)),
                          'D', TRUNC(to_Date($P{To_Date},'YYYY-MM-DD') - NVL(a.AR_VOU_DUE_DT,a.AR_VOU_DT)),
                               TRUNC(to_Date($P{To_Date},'YYYY-MM-DD') - a.AR_VOU_DT) )+1
                     ) OS_DAYS1
    FROM ar a,  (select a.adj_cld_id,  a.adj_sloc_id,  a.adj_ho_org_id,  a.adj_org_id,  a.adj_appl_inst_id,
                        a.adj_vou_id, trunc(b.gl_vou_dt) adj_vou_dt,  a.adj_arap_sl_no, --dm 2014-05-09
                        a.adj_sl_no,  a.adj_coa_id,  sum(decode(a.adj_amt_typ,'Dr',a.radj_amt_adj,'Cr',-a.radj_amt_adj)) adj_amt_dtl
                 From gl_adj a, gl_lines b, gl g
                 WHERE a.ADJ_CLD_ID =$P{Cld_Id}
                    and a.ADJ_ORG_ID = nvl($P{Org_Id},a.ADJ_ORG_ID)
                    and TRUNC(a.ADJ_MST_VOU_DT) <= to_date(NVL($P{To_Date}, SYSDATE), 'YYYY-MM-DD')
                    and TRUNC(a.RADJ_VOU_DT) <= to_date(NVL($P{To_Date}, SYSDATE), 'YYYY-MM-DD') ---PR 21-05-2014, 10-10-2014
                    and a.ADJ_COA_ID = nvl($P{Coa_Id},a.ADJ_COA_ID)
                    and b.GL_CLD_ID = a.ADJ_CLD_ID
                    and a.ADJ_ORG_ID = b.gl_ORG_ID
                    and a.ADJ_VOU_ID = b.gl_vou_id
                    and a.ADJ_SL_NO = b.gl_SL_NO
                    and a.ADJ_COA_ID = b.gl_coa_id
                    and a.adj_mst_vou_id=g.gl_vou_id
                    and a.adj_cld_id = g.gl_cld_id
                    and a.adj_sloc_id = g.gl_sloc_id
                    and a.adj_ho_org_id = g.gl_ho_org_id
                    and g.gl_rev_flg not in( 'RVS','RVD')
                    group by a.adj_cld_id, a.adj_sloc_id, a.adj_ho_org_id, a.adj_org_id, a.adj_appl_inst_id,
                             a.adj_vou_id, trunc(b.gl_vou_dt), a.adj_arap_sl_no, --dm 2014-05-09
                             a.adj_sl_no, a.adj_coa_id
                  ) a1
       WHERE a.gl_cld_id = $P{Cld_Id}
       and a.ar_org_id = nvl($P{Org_Id},a.ar_org_id)
       and DECODE('V','V',trunc(a.ar_vou_dt),
                      'I',trunc(NVL(a.ar_ext_doc_dt,a.ar_vou_dt)),
                      'D',trunc(NVL(a.ar_vou_due_dt,a.ar_vou_dt)),
                          trunc(a.ar_vou_dt) ) <= to_date(NVL($P{To_Date},SYSDATE),'YYYY-MM-DD')
       and a.ar_cog_id = nvl($P{Cog_Id},a.ar_cog_id)
       and a.ar_coa_id = nvl($P{Coa_Id},a.ar_coa_id)--and a.AR_TXN_TYP_ADV = 'N'  AND a.AR_TYPE_ID  = 7
       and a.ar_curr_id_sp = nvl($P{Curr_Id},a.ar_curr_id_sp)
       and a.gl_cld_id = a1.adj_cld_id(+)
       and a.ar_sloc_id = a1.adj_sloc_id(+)
       and a.gl_ho_org_id = a1.adj_ho_org_id(+)
       and a.ar_org_id = a1.adj_org_id(+)
       and a.ar_vou_id = a1.adj_vou_id(+)
       and a.ar_coa_id = a1.adj_coa_id(+)
       and a.ar_gl_sl_no = a1.adj_sl_no(+)
       AND a.ar_sl_no = a1.adj_arap_sl_no(+) --HERE
       --and NVL(a.AR_AMT_SP*a.AR_CC,0) - NVL(a1.ADJ_AMT_DTL*a.AR_CC,0) > 0 --dm 2014-02-24
       and 0 < case a.ar_amt_typ when 'Dr' then nvl(a.ar_amt_sp*a.ar_cc,0) + nvl(a1.adj_amt_dtl*a.ar_cc,0)
                                 when 'Cr' then abs(  -nvl(a.ar_amt_sp*a.ar_cc,0) + nvl(a1.adj_amt_dtl*a.ar_cc,0)) end --21-05-2014 PR
   ) a,
   app.org b,   fin$coa c,   app.app$doc$txn d,
   app.app$glbl$curr e,   fin.fin$acc$na x1,   app.app$eo x2,
   app.app$eo$prf i, app.APP$EO$ADDS j, --app.APP$ADDS$BK$DTL k,
   gl g
where a.os_days >= 0 -- changed from > to >= --dm 2013-11-16
and a.amt_os <> 0
and a.cld_id = b.org_cld_id --dm 2014-05-09
and a.org_id = b.org_id
and a.cld_id = c.coa_cld_id
and a.sloc_id = c.coa_sloc_id
and a.ho_org_id = c.coa_ho_org_id
and a.cog_id = c.coa_cog_id
and a.coa_id = c.coa_id
--and c.coa_ho_org_id = app.fn_get_parent_org_l0(a.cld_id,a.org_id)
and a.cld_id = d.doc_cld_id
and a.sloc_id = d.doc_sloc_id
and a.org_id = d.doc_org_id
and a.vou_id = d.doc_txn_id
and a.cld_id = g.gl_cld_id
and a.sloc_id = g.gl_sloc_id
and a.org_id = g.gl_org_id
and a.vou_id = g.gl_vou_id
and g.gl_rev_flg not in( 'RVS','RVD')
and a.curr_id_sp = e.glbl_curr_id
and c.coa_cld_id = x1.acc_cld_id
and c.coa_sloc_id = x1.acc_sloc_id
and c.coa_ho_org_id = x1.ho_org_id
and c.coa_acc_id = x1.acc_id
and x1.acc_cld_id = x2.eo_cld_id
and x1.acc_sloc_id = x2.eo_sloc_id
and x1.ho_org_id = x2.eo_ho_org_id
and x1.acc_type_id = x2.eo_id
and x1.acc_type = x2.eo_type

and x2.eo_cld_id = i.eo_cld_id(+)
and x2.eo_sloc_id = i.eo_sloc_id(+)
and x2.eo_ho_org_id = i.eo_ho_org_id(+)
and x2.eo_type=i.eo_type(+)
and x2.eo_id = i.eo_id(+)

and i.eo_id = j.eo_id(+)
and i.eo_sloc_id = j.sloc_id(+)
and i.eo_cld_id = j.cld_id(+)
and i.eo_ho_org_id = j.ho_org_id(+)
and i.EO_ORG_ID=j.org_id(+)

and i.EO_TYPE = 68
and j.BILL_ADDS_DFLT(+) = 'Y'

and x2.eo_mst_id = nvl($P{Entity_Group},x2.eo_mst_id)
and nvl(i.eo_sman_id,'0') = nvl($P{Eo_Id},nvl(i.eo_sman_id,'0'))

and decode('V','V',trunc(a.vou_dt),
               'I',trunc(nvl(a.ext_doc_dt,a.vou_dt)),
               'D',trunc(nvl(a.ar_vou_due_dt,a.vou_dt)),
                   trunc(a.vou_dt) ) <= to_date(nvl($P{To_Date},SYSDATE),'YYYY-MM-DD')---nvl(&DateSelect,sysdate)]]>
	</queryString>
