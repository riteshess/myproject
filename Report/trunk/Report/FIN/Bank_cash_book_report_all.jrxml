and g.doc_cld_id= doc.org_doc_cld_id
and g.doc_sloc_id = doc.org_doc_sloc_id
and g.doc_org_id = doc.org_id
and g.doc_typ_id = doc.ORG_DOC_TYPE_ID
and g.doc_id=doc.org_doc_id


