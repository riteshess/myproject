        a.tvou_type_id voucher_type,
        doc.ORG_DOC_NUM_PREFIX_OTH voucher_notation,
        a.CC_ID
from 	  tvou_lines a,
        tvou gl,
        fin$coa c,
        fin$acc$na b,
        app.org d,
        fin.fin$cog e,
        app.app$doc$txn f,
