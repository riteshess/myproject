    AND a.gl_tax_rule_priority <> 0
    AND a.gl_tax_rule_slno     <> 1 --new
    AND a.gl_cld_id             = b.coa_cld_id
    AND a.gl_sloc_id            = b.coa_sloc_id
    AND a.gl_ho_org_id          = b.coa_ho_org_id
    AND a.gl_tax_rule_coa_id    = b.coa_id
    AND a.gl_cld_id             = d.doc_cld_id
    and a.GL_SLOC_ID            = D.DOC_SLOC_ID
    AND a.gl_org_id             = d.doc_org_id
    AND d.doc_id                = 56
    AND a.gl_vou_id             = d.doc_txn_id
    AND a.gl_cld_id             = org_cld_id --new
    AND a.gl_org_id             = c.org_id
    AND a.gl_cld_id             = ar.gl_cld_id
    AND a.gl_sloc_id            = ar.ar_sloc_id
    AND a.gl_ho_org_id          = ar.gl_ho_org_id
    AND a.gl_org_id             = ar.ar_org_id
    AND a.gl_vou_id             = ar.ar_vou_id
    AND a.gl_cld_id             = tb.tax_cld_id
    AND a.gl_sloc_id            = tb.tax_rule_sloc_id
    and a.GL_HO_ORG_ID          = TB.TAX_HO_ORG_ID
        and inv.SLOC_ID      = AR.AR_SLOC_ID
        AND inv.ORG_ID       = ar.ar_ORG_ID
        and inv.SALES_VOU_ID = AR.AR_VOU_ID
      --and a.gl_tax_rule_id = decode(a.gl_tax_app_flg, 'M', 0 , tb.tax_rule_id
      -- )
      --and a.gl_tax_rule_slno = decode(a.gl_tax_app_flg, 'M', 0 ,
      -- tb.tax_rule_slno )
    AND a.gl_tax_rule_id = DECODE(a.gl_tax_app_flg, 'M', a.gl_tax_rule_id ,TB.TAX_RULE_ID)
    and a.GL_TAX_RULE_SLNO = DECODE(a.GL_TAX_APP_FLG, 'M', a.GL_TAX_RULE_SLNO ,tb.tax_rule_slno )

    AND a.gl_tax_id         = tb.tax_id
    AND tt.tax_cld_id       = tb.tax_cld_id
    AND tt.tax_rule_sloc_id = tb.tax_rule_sloc_id
    AND tt.tax_ho_org_id    = tb.tax_ho_org_id
    AND tt.tax_rule_id      = tb.tax_rule_id
    AND tt.tax_type_tds     = 'N'
