<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="TaxRegister_Coawise" language="groovy" pageWidth="1100" pageHeight="842" columnWidth="1060" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="Org_Id" class="java.lang.String"/>
	<parameter name="From_Date" class="java.lang.String"/>
	<parameter name="To_Date" class="java.lang.String"/>
	<parameter name="Cld_Id" class="java.lang.String"/>
	<parameter name="Coa_Id" class="java.math.BigDecimal"/>
	<parameter name="Path" class="java.lang.String"/>
	<parameter name="Org_Logo" class="java.lang.String"/>
	<parameter name="Report_Type" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
  TYPE_APAR,
  ORG_DESC,
  TAX_NAME,
  TAX_ID,
  PERCENTAGE,
  ACCOUNT_NAME,
  ACCOUNT_ID,
  BASEAMOUNT BASE_AMOUNT,
  VOUCHERID VOUCHER_ID,
  VOUCHERDATE VOUCHER_DATE,
  TDSTYPE TAX_TYPE,
  --TDSAMOUNT TAX_AMOUNT,
  --TDS_AMT Taxable_Amt,
  DECODE(TDSTYPE,'Dr',TDSAMOUNT,-TDSAMOUNT) TAX_AMOUNT,
  DECODE(TDSTYPE,'Dr',TDS_AMT,  -TDS_AMT) Taxable_Amt,
  RULE_SLNO,
  TIN_NO,
  BILL_NO,
  BILL_DT,
  ITM_DESC,
 itm_amt_bs
FROM
  (


    SELECT
      'AP' TYPE_APAR,
      c.org_desc,
      b.coa_nm tax_name,
      b.coa_id tax_id,
      tax.TAX_PER Percentage,
      AP.coa_nm account_name,
      ap.AP_COA_ID Account_id,
      --nvl(ap.AP_AMT_SP,0)*nvl(ap.AP_CC,1) BaseAmount,
      a.GL_TDS_RULE_OP_AMT_LINE BaseAmount,
      SUBSTR(d.DOC_TXN_ID_DISP,3) VoucherId,
      a.GL_VOU_DT VoucherDate,
      a.GL_TDS_RULE_BAL_TYPE TdsType,
      a.GL_TDS_RULE_TDS_AMT_SP TdsAmount,
      a.GL_TDS_RULE_OP_AMT_LINE TDS_AMT,
      a.gl_tds_rule_slno rule_slno,
      ap.tin_no tin_no,
      AP.AP_EXT_DOC_NO BILL_NO,
      AP.AP_EXT_DOC_DT BILL_DT,
      ''      ITM_DESC,
      0
      itm_amt_bs
    FROM
      (
        select distinct
          a.gl_cld_id gl_cld_id,
          a.ap_sloc_id,
          a.gl_ho_org_id,
          a.ap_org_id,
          a.ap_coa_id,
          a.ap_vou_id,
          a.ap_ext_doc_no,
          a.ap_ext_doc_dt,
          e.coa_nm,
          f.tin_no
        FROM
          ap a,
          fin$coa e,
          app.app$eo$prf f
        WHERE
          a.GL_CLD_ID          = $P{Cld_Id}
        AND a.AP_ORG_ID        = NVL( $P{Org_Id}, A.AP_ORG_ID)
        AND TRUNC(a.AP_VOU_DT) > = to_date(NVL($P{From_Date},'1000-01-01'),
          'YYYY-MM-DD')
        AND TRUNC(a.AP_VOU_DT) < = to_date(NVL($P{To_Date},SYSDATE),
          'YYYY-MM-DD')
        AND a.gl_cld_id    = e.coa_cld_id
        AND a.ap_sloc_id   = e.coa_sloc_id
        AND a.gl_ho_org_id = e.coa_ho_org_id
        AND a.ap_coa_id    = e.coa_id
        AND a.gl_cld_id    = f.eo_cld_id(+)
        AND a.ap_sloc_id   = f.eo_sloc_id(+)
        AND a.gl_ho_org_id = f.eo_ho_org_id(+)
        AND a.ap_eo_id     = f.eo_id(+)
        AND a.ap_amt_typ   ='Cr'
      )
      ap,
      gl_tds_rule_line a,
      fin$coa b ,
      app.org c,
      app.app$doc$txn d,
      app.app$tax$rule$line tb,
      app.app$tax$rule tt,
      app.app$tax tax
    WHERE
      a.GL_CLD_ID          = $P{Cld_Id}
    AND a.GL_ORG_ID        = NVL( $P{Org_Id}, a.GL_ORG_ID)
    AND TRUNC(a.GL_VOU_DT) > = to_date(NVL($P{From_Date},'1000-01-01'),
      'YYYY-MM-DD')
    AND TRUNC(a.GL_VOU_DT)      < = to_date(NVL($P{To_Date},SYSDATE),'YYYY-MM-DD')
    AND a.gl_tds_rule_priority <> 0
    AND a.gl_tds_rule_slno     <> 1 --new
    AND a.gl_cld_id             = b.coa_cld_id
    AND a.gl_sloc_id            = b.coa_sloc_id
    AND a.gl_ho_org_id          = b.coa_ho_org_id
    AND a.gl_tds_rule_coa_id    = b.coa_id
    AND a.gl_cld_id             = d.doc_cld_id
    AND a.gl_sloc_id            = d.doc_sloc_id
    AND a.gl_org_id             = d.doc_org_id
    AND d.doc_id                = 56
    AND a.gl_vou_id             = d.doc_txn_id
    AND a.gl_cld_id             = org_cld_id --new
    AND a.gl_org_id             = c.org_id
    AND a.gl_cld_id             = ap.gl_cld_id
    AND a.gl_sloc_id            = ap.ap_sloc_id
    AND a.gl_ho_org_id          = ap.gl_ho_org_id
    AND a.gl_org_id             = ap.ap_org_id
    AND a.gl_vou_id             = ap.ap_vou_id
    AND tb.tax_cld_id           = a.gl_cld_id
    AND tb.tax_rule_sloc_id     = a.gl_sloc_id
    AND tb.tax_ho_org_id        = a.gl_ho_org_id
    AND tb.tax_rule_id          = DECODE(a.GL_TDS_APP_FLG, 'M', tb.tax_rule_id,a.gl_tds_rule_id)
    AND tb.tax_rule_slno        = DECODE(a.GL_TDS_APP_FLG, 'M', tb.tax_rule_slno,a.gl_tds_rule_slno)
    AND tb.tax_id           = a.gl_tds_id
    AND tt.tax_cld_id       = tb.tax_cld_id
    AND tt.tax_rule_sloc_id = tb.tax_rule_sloc_id
    AND tt.tax_ho_org_id    = tb.tax_ho_org_id
    AND tt.tax_rule_id      = tb.tax_rule_id
    AND tt.tax_type_tds     = 'Y'
    AND tax.cld_id          = tb.tax_cld_id
    AND tax.tax_sloc_id     = tb.tax_rule_sloc_id
    AND tax.ho_org_id       = tb.tax_ho_org_id
    AND tax.tax_id          = tb.tax_id
    AND tax.TAX_REG_TYPE    = 'T'
    AND b.coa_id        = NVL( $P{Coa_Id}, b.coa_id)

    UNION ALL

    SELECT
      'AR' TYPE_APAR,
      c.org_desc,
      b.coa_nm tax_name,
      b.coa_id tax_id,
      tax.TAX_PER Percentage,
      AR.coa_nm account_name,
      aR.AR_COA_ID Account_id,
      --nvl(ap.AP_AMT_SP,0)*nvl(ap.AP_CC,1) BaseAmount,
      a.GL_TDS_RULE_OP_AMT_LINE BaseAmount,
      SUBSTR(d.DOC_TXN_ID_DISP,3) VoucherId,
      a.GL_VOU_DT VoucherDate,
      a.GL_TDS_RULE_BAL_TYPE TdsType,
      a.GL_TDS_RULE_TDS_AMT_SP TdsAmount,
      a.GL_TDS_RULE_OP_AMT_LINE TDS_AMT,
      a.GL_TDS_RULE_SLNO RULE_SLNO,
      AR.TIN_NO TIN_NO,
      AR.AR_EXT_DOC_NO BILL_NO,
      AR.AR_EXT_DOC_DT BILL_DT,
inv.itm_desc,
inv.itm_amt_bs
    FROM
      (
        SELECT DISTINCT
          a.gl_cld_id gl_cld_id,
          a.ar_sloc_id,
          a.gl_ho_org_id,
          a.AR_ORG_ID,
          a.ar_coa_id,
          a.ar_ext_doc_no,
          a.ar_ext_doc_dt,
          a.ar_vou_id,
          e.coa_nm,
          f.tin_no
        FROM
          ar a,
          FIN$COA E,
          app.app$eo$prf f
        WHERE
          a.GL_CLD_ID          = $P{Cld_Id}
        AND a.AR_ORG_ID        = NVL( $P{Org_Id}, A.AR_ORG_ID)
        AND TRUNC(a.AR_VOU_DT) > = to_date(NVL($P{From_Date},'1000-01-01'),
          'YYYY-MM-DD')
        AND TRUNC(a.AR_VOU_DT) < = to_date(NVL($P{To_Date},SYSDATE),
          'YYYY-MM-DD')
        AND a.gl_cld_id    = e.coa_cld_id
        AND a.ar_sloc_id   = e.coa_sloc_id
        AND a.gl_ho_org_id = e.coa_ho_org_id
        AND a.ar_coa_id    = e.coa_id
        AND a.gl_cld_id    = f.eo_cld_id(+)
        AND a.ar_sloc_id   = f.eo_sloc_id(+)
        AND a.gl_ho_org_id = f.eo_ho_org_id(+)
        AND a.ar_eo_id     = f.eo_id(+)
      )
      ar,
       (
        select
          y.itm_amt_bs,z.itm_desc,x.sales_vou_id,x.cld_id,x.sloc_id,x.org_id,x.ho_org_id
        FROM
          SLS.SLS$INV X,
          SLS.SLS$INV$SHIP$ITM Y,
          APP.APP$ITM$PRF Z
        WHERE
          x.CLD_ID         = y.CLD_ID
        AND x.SLOC_ID      = y.SLOC_ID
        AND x.HO_ORG_ID    = y.HO_ORG_ID
        AND x.ORG_ID       = y.ORG_ID
        AND x.DOC_ID       = y.DOC_ID
        AND y.CLD_ID       = z.CLD_ID
        AND y.SLOC_ID      = z.SLOC_ID
        and Y.HO_ORG_ID    = Z.HO_ORG_ID
        AND y.ITM_ID       = z.ITM_ID
      )inv,
      gl_tds_rule_line a,
      fin$coa b ,
      app.org c,
      app.app$doc$txn d,
      app.app$tax$rule$line tb,
      app.app$tax$rule tt,
      app.app$tax tax
    WHERE
      a.GL_CLD_ID          = $P{Cld_Id}
    AND a.GL_ORG_ID        = NVL( $P{Org_Id}, a.GL_ORG_ID)
    AND TRUNC(a.GL_VOU_DT) > = to_date(NVL($P{From_Date},'1000-01-01'),
      'YYYY-MM-DD')
    AND TRUNC(a.GL_VOU_DT)      < = to_date(NVL($P{To_Date},SYSDATE),'YYYY-MM-DD')
    AND a.gl_tds_rule_priority <> 0
    AND a.gl_tds_rule_slno     <> 1 --new
    AND a.gl_cld_id             = b.coa_cld_id
    AND a.gl_sloc_id            = b.coa_sloc_id
    AND a.gl_ho_org_id          = b.coa_ho_org_id
    AND a.gl_tds_rule_coa_id    = b.coa_id
    AND a.gl_cld_id             = d.doc_cld_id
    AND a.gl_sloc_id            = d.doc_sloc_id
    AND a.gl_org_id             = d.doc_org_id
    AND d.doc_id                = 56
    AND a.gl_vou_id             = d.doc_txn_id
    AND a.gl_cld_id             = org_cld_id --new
    AND a.gl_org_id             = c.org_id
    AND a.gl_cld_id             = ar.gl_cld_id
    AND a.gl_sloc_id            = ar.ar_sloc_id
    AND a.gl_ho_org_id          = ar.gl_ho_org_id
    AND a.gl_org_id             = ar.ar_org_id
    AND a.gl_vou_id             = ar.ar_vou_id
    AND tb.tax_cld_id           = a.gl_cld_id
    AND tb.tax_rule_sloc_id     = a.gl_sloc_id
    AND tb.tax_ho_org_id        = a.gl_ho_org_id
    AND tb.tax_rule_id          = a.gl_tds_rule_id

    AND tb.tax_id               = a.gl_tds_id
    AND tt.tax_cld_id           = tb.tax_cld_id
    AND tt.tax_rule_sloc_id     = tb.tax_rule_sloc_id
    AND tt.tax_ho_org_id        = tb.tax_ho_org_id
    --AND tt.tax_rule_id          = tb.tax_rule_id
    --AND tb.tax_rule_slno        = a.gl_tds_rule_slno

    AND tt.tax_rule_id          = DECODE(a.GL_TDS_APP_FLG, 'M', tt.tax_rule_id,a.gl_tds_rule_id)
    AND tb.tax_rule_slno        = DECODE(a.GL_TDS_APP_FLG, 'M', tb.tax_rule_slno,a.gl_tds_rule_slno)

    AND tt.tax_type_tds         = 'Y'
    and TAX.CLD_ID              = TB.TAX_CLD_ID
    AND tax.tax_sloc_id         = tb.tax_rule_sloc_id
    AND tax.ho_org_id           = tb.tax_ho_org_id
    AND tax.tax_id              = tb.tax_id
    AND tax.TAX_REG_TYPE        = 'T'
    and inv.SLOC_ID      = AR.AR_SLOC_ID
    AND inv.ORG_ID       = ar.ar_ORG_ID
    and inv.SALES_VOU_ID = AR.AR_VOU_ID
    AND b.coa_id           = NVL( $P{Coa_Id}, b.coa_id)

    UNION ALL

    SELECT
      'AP' TYPE_APAR,
      c.org_desc,
      b.coa_nm tax_name,
      b.coa_id tax_id,
      tax.TAX_PER Percentage,
      AP.coa_nm account_name,
      ap.AP_COA_ID Account_id,
      --nvl(ap.AP_AMT_SP,0)*nvl(ap.AP_CC,1) BaseAmount,
      a.GL_TAX_RULE_OP_AMT_LINE BaseAmount,
      SUBSTR(d.DOC_TXN_ID_DISP,3) VoucherId,
      a.GL_VOU_DT VOUCHERDATE,
      a.GL_TAX_RULE_BAL_TYPE TdsType,
      a.GL_TAX_RULE_TAXABLE_AMT_SP TdsAmount,
      a.GL_TAX_RULE_OP_AMT_LINE TDS_AMT,
      a.GL_TAX_RULE_SLNO RULE_SLNO,
      AP.TIN_NO TIN_NO,
      AP.AP_EXT_DOC_NO BILL_NO,
      AP.AP_EXT_DOC_DT BILL_DT,
      '' ITM_DESC,
      0   itm_amt_bs
    FROM
      (
        SELECT DISTINCT
          a.gl_cld_id gl_cld_id,
          a.ap_sloc_id,
          a.gl_ho_org_id,
          a.ap_org_id,
          a.ap_coa_id,
          a.ap_vou_id,
          a.ap_ext_doc_no,
          a.ap_ext_doc_dt,
          e.coa_nm,
          F.TIN_NO
        FROM
          ap a,
          fin$coa e,
          app.app$eo$prf f
        WHERE
          a.GL_CLD_ID          = $P{Cld_Id}
        AND a.AP_ORG_ID        = NVL( $P{Org_Id}, A.AP_ORG_ID)
        AND TRUNC(a.AP_VOU_DT) > = to_date(NVL($P{From_Date},'1000-01-01'),
          'YYYY-MM-DD')
        AND TRUNC(a.AP_VOU_DT) < = to_date(NVL($P{To_Date},SYSDATE),
          'YYYY-MM-DD')
        AND a.gl_cld_id    = e.coa_cld_id
        AND a.ap_sloc_id   = e.coa_sloc_id
        AND a.gl_ho_org_id = e.coa_ho_org_id
        AND a.ap_coa_id    = e.coa_id
        AND a.gl_cld_id    = f.eo_cld_id(+)
        AND a.ap_sloc_id   = f.eo_sloc_id(+)
        AND a.gl_ho_org_id = f.eo_ho_org_id(+)
        AND a.ap_eo_id     = f.eo_id(+)
        AND a.ap_amt_typ   ='Cr'
      )
      ap,
      gl_tax_rule_line a,
      fin$coa b ,
      app.org c,
      app.app$doc$txn d,
      app.app$tax$rule$line tb,
      app.app$tax$rule tt,
      app.app$tax tax
    WHERE
      a.GL_CLD_ID          = $P{Cld_Id}
    AND a.GL_ORG_ID        = NVL( $P{Org_Id}, a.GL_ORG_ID)
    AND TRUNC(a.GL_VOU_DT) > = to_date(NVL($P{From_Date},'1000-01-01'),
      'YYYY-MM-DD')
    AND TRUNC(a.GL_VOU_DT)      < = to_date(NVL($P{To_Date},SYSDATE),'YYYY-MM-DD')
    AND a.gl_tax_rule_priority <> 0
    AND a.gl_tax_rule_slno     <> 1 --new
    AND a.gl_cld_id             = b.coa_cld_id
    AND a.gl_sloc_id            = b.coa_sloc_id
    AND a.gl_ho_org_id          = b.coa_ho_org_id
    AND a.gl_tax_rule_coa_id    = b.coa_id
    AND a.gl_cld_id             = d.doc_cld_id
    AND a.gl_sloc_id            = d.doc_sloc_id
    AND a.gl_org_id             = d.doc_org_id
    AND d.doc_id                = 56
    AND a.gl_vou_id             = d.doc_txn_id
    AND a.gl_cld_id             = org_cld_id --new
    AND a.gl_org_id             = c.org_id
    AND a.gl_cld_id             = ap.gl_cld_id
    AND a.gl_sloc_id            = ap.ap_sloc_id
    AND a.gl_ho_org_id          = ap.gl_ho_org_id
    AND a.gl_org_id             = ap.ap_org_id
    AND a.gl_vou_id             = ap.ap_vou_id
    AND tb.tax_cld_id           = a.gl_cld_id
    AND tb.tax_rule_sloc_id     = a.gl_sloc_id
    AND tb.tax_ho_org_id        = a.gl_ho_org_id
--    AND tb.tax_rule_id          = a.gl_tax_rule_id
--    AND tb.tax_rule_slno        = a.gl_tax_rule_slno

    AND tb.tax_rule_id          = DECODE(a.GL_TAX_APP_FLG, 'M', tb.tax_rule_id,a.gl_tax_rule_id)
    AND tb.tax_rule_slno        = DECODE(a.GL_TAX_APP_FLG, 'M', tb.tax_rule_slno,a.gl_tax_rule_slno)

    AND tb.tax_id               = a.gl_tax_id
    and TT.TAX_CLD_ID           = TB.TAX_CLD_ID
    AND tt.tax_rule_sloc_id     = tb.tax_rule_sloc_id
    AND tt.tax_ho_org_id        = tb.tax_ho_org_id
    AND tt.tax_rule_id          = tb.tax_rule_id
    AND tt.tax_type_tds         = 'N'
    AND tax.cld_id              = tb.tax_cld_id
    AND tax.tax_sloc_id         = tb.tax_rule_sloc_id
    AND tax.ho_org_id           = tb.tax_ho_org_id
    AND tax.tax_id              = tb.tax_id
    AND tax.TAX_REG_TYPE        = 'T'
    AND b.coa_id        = NVL( $P{Coa_Id}, b.coa_id)

    UNION ALL

     SELECT
      'AR' TYPE_APAR,
      c.org_desc,
      b.coa_nm tax_name,
      b.coa_id tax_id,
      TAX.TAX_PER PERCENTAGE,
      AR.coa_nm account_name,
      aR.AR_COA_ID Account_id,
      --nvl(ap.AP_AMT_SP,0)*nvl(ap.AP_CC,1) BaseAmount,
      a.GL_TAX_RULE_OP_AMT_LINE BaseAmount,
      SUBSTR(d.DOC_TXN_ID_DISP,3) VoucherId,
      a.GL_VOU_DT VoucherDate,
      a.GL_TAX_RULE_BAL_TYPE TdsType,
      a.GL_TAX_RULE_TAXABLE_AMT_SP TdsAmount,
      a.GL_TAX_RULE_OP_AMT_LINE TDS_AMT,
      a.GL_TAX_RULE_SLNO RULE_SLNO,
      AR.TIN_NO TIN_NO,
      AR.AR_EXT_DOC_NO BILL_NO,
      AR.AR_EXT_DOC_DT BILL_DT,
      inv.itm_desc,
      inv.itm_amt_bs
    FROM
      (
        SELECT DISTINCT
          a.gl_cld_id gl_cld_id,
          a.ar_sloc_id,
          a.gl_ho_org_id,
          a.ar_org_id,
          a.ar_coa_id,
          a.ar_vou_id,
          a.ar_ext_doc_no,
          a.ar_ext_doc_dt,
          e.coa_nm,
          f.tin_no
        FROM
          ar a,
          fin$coa e,
          app.app$eo$prf f
        WHERE
          a.GL_CLD_ID          = $P{Cld_Id}
        AND a.AR_ORG_ID        = NVL( $P{Org_Id}, A.AR_ORG_ID)
        AND TRUNC(a.AR_VOU_DT) > = to_date(NVL($P{From_Date},'1000-01-01'),
          'YYYY-MM-DD')
        AND TRUNC(a.AR_VOU_DT) < = to_date(NVL($P{To_Date},SYSDATE),
          'YYYY-MM-DD')
        AND a.gl_cld_id    = e.coa_cld_id
        AND a.ar_sloc_id   = e.coa_sloc_id
        AND a.gl_ho_org_id = e.coa_ho_org_id
        AND a.ar_coa_id    = e.coa_id
        AND a.gl_cld_id    = f.eo_cld_id(+)
        AND a.ar_sloc_id   = f.eo_sloc_id(+)
        AND a.gl_ho_org_id = f.eo_ho_org_id(+)
        AND a.ar_eo_id     = f.eo_id(+)
      )
      ar,
            (
        select
          y.itm_amt_bs,z.itm_desc,x.sales_vou_id,x.cld_id,x.sloc_id,x.org_id,x.ho_org_id
        FROM
          SLS.SLS$INV X,
          SLS.SLS$INV$SHIP$ITM Y,
          APP.APP$ITM$PRF Z
        WHERE
          x.CLD_ID         = y.CLD_ID
        AND x.SLOC_ID      = y.SLOC_ID
        AND x.HO_ORG_ID    = y.HO_ORG_ID
        AND x.ORG_ID       = y.ORG_ID
        AND x.DOC_ID       = y.DOC_ID
        AND y.CLD_ID       = z.CLD_ID
        AND y.SLOC_ID      = z.SLOC_ID
        AND y.HO_ORG_ID    = z.HO_ORG_ID
        AND y.ITM_ID       = z.ITM_ID
      )inv,
      gl_tax_rule_line a,
      fin$coa b ,
      app.org c,
      app.app$doc$txn d,
      app.app$tax$rule$line tb,
      app.app$tax$rule tt,
      app.app$tax tax
    WHERE
      a.GL_CLD_ID          = $P{Cld_Id}
    AND a.GL_ORG_ID        = NVL( $P{Org_Id}, a.GL_ORG_ID)
    AND TRUNC(a.GL_VOU_DT) > = to_date(NVL($P{From_Date},'1000-01-01'),
      'YYYY-MM-DD')
