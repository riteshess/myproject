<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="TrialBalance_GroupWise" language="groovy" pageWidth="842" pageHeight="595" orientation="Landscape" columnWidth="828" leftMargin="7" rightMargin="7" topMargin="10" bottomMargin="10">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="Cld_Id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Cog_Id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Org_Id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Cog_Indent" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Coa_Id" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="From_Date" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Na_Id" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="PostFlag" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="To_Date" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Path" class="java.lang.String"/>
	<parameter name="Report_Type" class="java.lang.String"/>
	<parameter name="Ho_Org_Id" class="java.lang.String"/>
	<parameter name="Include_Grp" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Org_Logo" class="java.lang.String"/>
	<parameter name="GroupAccount" class="java.lang.String"/>
	<parameter name="Entity_Group" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Lbl_Val" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select   DISTINCT cld_id,
    org_id,
    org_desc,
    org_fy_id,
    cog_id,
    substr(cog_id,1,1) cog_id_grp,
    DECODE($P{Cog_Indent},'N',COG_NM,'P',COG_NM_TREE,'I',COG_NM_INDENT,COG_NM_TREE) cog_ind,
    COG_NM,
    COG_SUBLDGR,
    COA_ID,
    COA_NM,
    'Opening Balance as on '|| $P{From_Date} OP_BAL_STR,
    OP_BAL,
    OP_BAL_TYPE,
    DR_AMT,
    CR_AMT,
    CL_BAL,
    CL_BAL_TYPE,
    DISP_SR_NO,
    decode(CL_BAL_TYPE,'Dr',CL_BAL,'Cr',-CL_BAL,CL_BAL) CL_BAL_SIGNED,
    decode(OP_BAL_TYPE,'Dr',OP_BAL,'Cr',-OP_BAL,OP_BAL) OP_BAL_SIGNED,
    COL_VALUE
FROM    (SELECT c.ORG_COA_CLD_ID CLD_ID,
        c.ORG_ID ORG_ID,
        a.ORG_DESC ORG_DESC,
        c.ORG_FY_ID ORG_FY_ID,
        c.ORG_COA_ID COA_ID,
        b.COA_NM COA_NM,
        d.COG_ID,
        d.COG_NM COG_NM,
        d.COG_NM_TREE,
        d.COG_NM_INDENT,
        d.COG_SUBLDGR,
        d.DISP_SR_NO,
        C.ORG_COA_OP_BAL OP_BAL,
        C.ORG_COA_OP_BAL_TYP OP_BAL_type,
        DECODE($P{PostFlag},'A',NVL(e2.gl_dr_amt, 0) + NVL(f2.tmp_dr_amt, 0),'P',NVL(e2.gl_dr_amt, 0)) dr_amt,
        DECODE($P{PostFlag},'A',NVL(e2.gl_cr_amt, 0) + NVL(f2.tmp_cr_amt, 0),'P',NVL(e2.gl_cr_amt, 0)) cr_amt,
        DECODE($P{PostFlag},
        'A',DECODE(LTRIM(RTRIM(UPPER(NVL(c.ORG_COA_OP_BAL_TYP,'Dr')))),
        'DR',ABS(NVL(c.ORG_COA_OP_BAL, 0)
         +NVL(e2.gl_dr_amt, 0) -
        NVL(e2.gl_cr_amt, 0) +NVL(f2.tmp_dr_amt, 0) -NVL(f2.tmp_cr_amt, 0)),
        'CR',ABS(NVL(c.ORG_COA_OP_BAL, 0)
        -NVL(e2.gl_dr_amt, 0) +
        NVL(e2.gl_cr_amt, 0) -NVL(f2.tmp_dr_amt, 0) +NVL(f2.tmp_cr_amt, 0))),
        'P',DECODE(LTRIM(RTRIM(UPPER(NVL(c.ORG_COA_OP_BAL_TYP,'Dr')))),
        'DR',ABS(NVL(c.ORG_COA_OP_BAL, 0)  +
        NVL(e2.gl_dr_amt, 0) -NVL(e2.gl_cr_amt, 0)),
        'CR',ABS(NVL(c.ORG_COA_OP_BAL, 0)  -
        NVL(e2.gl_dr_amt, 0) +NVL(e2.gl_cr_amt, 0)))) cl_BAL,
        DECODE($P{PostFlag},
        'A',DECODE(LTRIM(RTRIM(UPPER(NVL(c.ORG_COA_OP_BAL_TYP,'Dr')))),
        'DR',DECODE(SIGN(NVL(c.ORG_COA_OP_BAL, 0)  +NVL(e2.gl_dr_amt, 0) -
        NVL(e2.gl_cr_amt, 0) +NVL(f2.tmp_dr_amt, 0) -NVL(f2.tmp_cr_amt, 0)),0,'Dr',1,'Dr',-1,'Cr'),
        'CR',DECODE(SIGN(NVL(c.ORG_COA_OP_BAL, 0)  -NVL(e2.gl_dr_amt, 0) +NVL(e2.gl_cr_amt, 0) -
        NVL(f2.tmp_dr_amt, 0) +NVL(f2.tmp_cr_amt, 0)),0,'Dr',1,'Cr',-1,'Dr')),
        'P',DECODE(LTRIM(RTRIM(UPPER(NVL(c.ORG_COA_OP_BAL_TYP,'Dr')))),
        'DR',DECODE(SIGN(NVL(c.ORG_COA_OP_BAL, 0)  +
        NVL(e2.gl_dr_amt, 0) -NVL(e2.gl_cr_amt, 0)),0,'Dr',1,'Dr',-1,'Cr'),
        'CR',DECODE(SIGN(NVL(c.ORG_COA_OP_BAL, 0)  -
        NVL(e2.gl_dr_amt, 0) +NVL(e2.gl_cr_amt, 0)),0,'Dr',1,'Cr',-1,'Dr'))) cl_BAL_type,
        e1.COL_VALUE
        FROM    app.org a,
        (SELECT DISTINCT A.ORG_CLD_ID ORG_CLD_ID,
            A.ORG_ID ORG_ID,
            B.COA_SLOC_ID,
            B.COA_HO_ORG_ID HO_ORG_ID,
            b.COA_ID,
            b.COA_NM,
            b.COA_COG_ID,
            b.COA_ACC_ID
        FROM    app.org A,
            fin$coa B
        WHERE A.ORG_CLD_ID = NVL($P{Cld_Id}, A.ORG_CLD_ID)
        AND   A.ORG_ID = NVL($P{Org_Id}, A.ORG_ID)
        AND   B.COA_CLD_ID = A.ORG_CLD_ID

        AND    B.COA_HO_ORG_ID = NVL(A.ORG_ID_PARENT_L0,A.ORG_ID)
        AND     B.COA_ID LIKE NVL($P{Coa_Id}, COA_ID)
        AND     B.COA_COG_ID LIKE decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, COA_COG_ID)||'%', NVL($P{Cog_Id}, COA_COG_ID))
        )b,
        (SELECT    a.ORG_COA_CLD_ID,
            a.ORG_ID,
            a.ORG_SLOC_ID,
            a.ORG_COA_HO_ORG_ID,
            a.ORG_FY_ID,
            a.ORG_COA_COG_ID,
            a.ORG_COA_ID,
            ABS(NVL(SUM(DECODE(C.CC_AMT_TYPE,'Dr',C.CC_AMT_BS,-C.CC_AMT_BS)),0)) ORG_COA_OP_BAL,
            DECODE(SIGN(SUM(DECODE(C.CC_AMT_TYPE,'Dr',C.CC_AMT_BS,-C.CC_AMT_BS))),1,'Dr',0,'Dr','Cr') ORG_COA_OP_BAL_TYP
        FROM    ORG$COA$FY a,
        ORG$COA$FY$COST$CENTER C,
                (select app.fn_get_fy_id($P{Cld_Id}, $P{Org_Id},TO_DATE($P{From_Date}, 'YYYY-MM-DD'),'FY') fy
                from dual) b
        WHERE    a.ORG_COA_CLD_ID = NVL($P{Cld_Id}, a.ORG_COA_CLD_ID)
        AND      a.ORG_ID = NVL($P{Org_Id}, a.ORG_ID)

        AND A.ORG_COA_ID = C.CC_TXN_ID (+)
        AND A.CC_ID = C.CC_ID (+)

        AND    a.ORG_FY_ID = b.fy
        AND     a.ORG_COA_ID LIKE NVL($P{Coa_Id}, a.ORG_COA_ID)
        AND     a.ORG_COA_COG_ID LIKE decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, a.ORG_COA_COG_ID)||'%', NVL($P{Cog_Id}, a.ORG_COA_COG_ID))
        AND     a.ORG_COA_NA_ID LIKE NVL($P{Na_Id}, a.ORG_COA_NA_ID)
        GROUP BY     a.ORG_COA_CLD_ID,
            a.ORG_ID,
            a.ORG_SLOC_ID,
            a.ORG_COA_HO_ORG_ID,
            a.ORG_FY_ID,
            a.ORG_COA_COG_ID,
            a.ORG_COA_ID

        )  c,
        (
        SELECT DISTINCT COG_CLD_ID COG_CLD_ID,
            A.ho_org_id COG_ORG_ID,
            COG_ID,
            COG_NM,
            SUBSTR(SYS_CONNECT_BY_PATH(COG_NM, '->'),3) COG_NM_TREE,
            LPAD(' ',2*(LENGTH(COG_ID)-1))||COG_NM COG_NM_INDENT,
            COG_ID_PARENT,
            COG_SUBLDGR,
            COG_BS_ITEM,
            COG_PNL_ITEM,
            COG_CF_ITEM,
            COG_TRF_BAL,
            COG_ACTV,
            DISP_SR_NO,
            COG_SLOC_ID
        FROM (select app.FN_GET_PARENT_ORG_L0 ($P{Cld_Id}, $P{Org_Id}) ho_org_id from dual) a,
              (select cog_cld_id,cog_sloc_id,cog_org_id,
                cog_id,cog_id_parent,
                cog_nm,
                cog_subldgr,cog_bs_item,cog_pnl_item,cog_cf_item,cog_trf_bal,
                cog_actv,DISP_SR_NO
                from fin$cog
                where     cog_cld_id = $P{Cld_Id}
                and    cog_org_id = nvl($P{Ho_Org_Id},cog_org_id)
                  ) B
        WHERE   B.COG_ORG_ID = a.ho_org_id
        START WITH nvl(COG_ID_PARENT,'!') in ('!','00')
        CONNECT BY NOCYCLE PRIOR COG_ID = COG_ID_PARENT
        ) d,
        (SELECT    A.GL_CLD_ID,
            B.GL_SLOC_ID,
            A.GL_ORG_ID,
            B.GL_HO_ORG_ID,
            A.GL_COA_ID,
            E.COL_VALUE,
            NVL(SUM(DECODE(GL_AMT_TYP, 'Dr', CC_AMT_BS, 0)),0) gl_dr_amt_op,
            NVL(SUM(DECODE(GL_AMT_TYP, 'Cr', CC_AMT_BS, 0)),0) gl_cr_amt_op
        FROM      GL_LINES A,
                    GL B,
               (SELECT app.fn_get_fy_start_date($P{Cld_Id}, $P{Org_Id}, TO_DATE($P{From_Date},'YYYY-mm-dd'),'FY') FY_START_dATE
               FROM DUAL) C,
               GL$COST$CENTER D,
               --APP.APP$CC E
               (select distinct col_id, cld_id, sloc_id, ho_org_id, org_id, col_value from app.app$cc) e
        WHERE   B.GL_CLD_ID = NVL($P{Cld_Id},B.GL_CLD_ID)
        AND     A.GL_CLD_ID = E.CLD_ID
        AND     A.GL_SLOC_ID = E.SLOC_ID
        AND     A.GL_HO_ORG_ID = E.HO_ORG_ID
        AND     A.GL_ORG_ID =  E.ORG_ID
        AND     D.CC_CCID_LVL1 = E.COL_ID
        AND     B.GL_STAT = DECODE($P{PostFlag}, 'A', B.GL_STAT, 'P', 'FLC')
        AND     A.GL_CLD_ID = B.GL_CLD_ID
        AND     A.GL_SLOC_ID = B.GL_SLOC_ID
        AND     A.GL_VOU_ID = B.GL_VOU_ID
        AND     A.GL_ORG_ID = B.GL_ORG_ID
        AND     A.GL_CLD_ID = D.CC_CLD_ID
        AND     A.GL_SLOC_ID = D.CC_SLOC_ID
        AND     A.GL_HO_ORG_ID = D.CC_HO_ORG_ID
        AND     A.GL_ORG_ID = D.CC_ORG_ID
        AND     A.GL_VOU_ID = D.CC_TXN_ID
        AND     A.CC_ID = D.CC_ID

        AND    a.GL_ORG_ID = NVL($P{Org_Id}, a.GL_ORG_ID)
        AND     a.GL_COA_ID LIKE NVL($P{Coa_Id}, a.GL_COA_ID)
        AND     a.GL_COG_ID LIKE decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, a.GL_COG_ID)||'%', NVL($P{Cog_Id}, a.GL_COG_ID))
        AND     a.GL_NA_ID LIKE NVL($P{Na_Id}, a.GL_NA_ID)
        AND     TRUNC(a.GL_VOU_DT) >= C.FY_START_dATE
        AND     TRUNC(a.GL_VOU_DT) < TO_DATE($P{From_Date}, 'YYYY-mm-dd')
        AND     A.GL_TXN_TYP_MIG not in  ('O','MO')
        AND     D.CC_CCID_LVL1=NVL($P{Lbl_Val},D.CC_CCID_LVL1)
        GROUP BY     A.GL_CLD_ID,
            B.GL_SLOC_ID,
            A.GL_ORG_ID,
            B.GL_HO_ORG_ID,
            E.COL_VALUE,
            A.GL_COA_ID
        ) e1,
        (
        SELECT    A.GL_CLD_ID,
            A.GL_ORG_ID,
            A.GL_COA_ID,
            D.COL_VALUE,
            NVL(SUM(DECODE(GL_AMT_TYP, 'Dr', CC_AMT_BS, 0)),0) gl_dr_amt,
            NVL(SUM(DECODE(GL_AMT_TYP, 'Cr', CC_AMT_BS, 0)),0) gl_cr_amt
        FROM      GL_LINES A,
            GL B,
            GL$COST$CENTER C,
            --APP.APP$CC D
            (select distinct col_id, cld_id, sloc_id, ho_org_id, org_id, col_value from app.app$cc) d
        WHERE     B.GL_CLD_ID = NVL($P{Cld_Id},A.GL_CLD_ID)
        AND     A.GL_CLD_ID = D.CLD_ID
        AND     A.GL_SLOC_ID = D.SLOC_ID
        AND     A.GL_HO_ORG_ID = D.HO_ORG_ID
        AND     A.GL_ORG_ID = D.ORG_ID
        AND     C.CC_CCID_LVL1 = D.COL_ID

        AND     A.GL_CLD_ID = B.GL_CLD_ID
        AND     A.GL_SLOC_ID = B.GL_SLOC_ID
        AND     A.GL_ORG_ID = B.GL_ORG_ID
        AND     A.GL_VOU_ID = B.GL_VOU_ID
        AND     A.GL_CLD_ID = C.CC_CLD_ID
        AND     A.GL_SLOC_ID = C.CC_SLOC_ID
        AND     A.GL_HO_ORG_ID = C.CC_HO_ORG_ID
        AND     A.GL_ORG_ID = C.CC_ORG_ID
        AND     A.GL_VOU_ID = C.CC_TXN_ID
        AND     A.CC_ID = C.CC_ID
        AND     A.GL_ORG_ID = NVL($P{Org_Id}, B.GL_ORG_ID)
        AND     a.GL_COA_ID LIKE NVL($P{Coa_Id}, a.GL_COA_ID)
        AND     a.GL_COG_ID LIKE decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, a.GL_COG_ID)||'%', NVL($P{Cog_Id}, a.GL_COG_ID))
        AND     a.GL_NA_ID LIKE NVL($P{Na_Id}, a.GL_NA_ID)
        AND     B.GL_STAT = DECODE($P{PostFlag}, 'A', B.GL_STAT, 'P', 'FLC')
        AND     TRUNC(a.GL_VOU_DT) >= TO_DATE($P{From_Date}, 'YYYY-mm-dd')
        AND     TRUNC(a.GL_VOU_DT) <= TO_DATE($P{To_Date}, 'YYYY-mm-dd')
        AND     a.GL_EOMST_ID=NVL($P{Entity_Group},GL_EOMST_ID)
        AND     A.GL_TXN_TYP_MIG not in  ('O','MO')
        GROUP BY     A.GL_CLD_ID,
            A.GL_ORG_ID,
            D.COL_VALUE,
            A.GL_COA_ID
        ) e2,
        (
        SELECT    A.TVOU_CLD_ID,
            A.TVOU_SLOC_ID,
            A.TVOU_ORG_ID,
            A.TVOU_HO_ORG_ID,
            A.TVOU_COA_ID,
            E.COL_VALUE,
            NVL(SUM(DECODE(A.TVOU_AMT_TYP,'Dr',D.CC_AMT_BS,0)),0) tmp_dr_amt_op,
            NVL(SUM(DECODE(A.TVOU_AMT_TYP,'Cr',D.CC_AMT_BS,0)),0) tmp_cr_amt_op
        FROM         TVOU_LINES A,
                      (SELECT app.fn_get_fy_start_date($P{Cld_Id}, $P{Org_Id}, TO_DATE($P{From_Date},'YYYY-mm-dd'),'FY') FY_START_dATE
                      FROM DUAL) C,
                      TVOU$COST$CENTER D,
                      --APP.APP$CC E
                      (select distinct col_id, cld_id, sloc_id, ho_org_id, org_id, col_value from app.app$cc) e
        WHERE   A.TVOU_CLD_ID = NVL($P{Cld_Id},A.TVOU_CLD_ID)
        AND     A.TVOU_CLD_ID = E.CLD_ID
        AND     A.TVOU_SLOC_ID = E.SLOC_ID
        AND     A.TVOU_HO_ORG_ID = E.HO_ORG_ID
        AND     A.TVOU_ORG_ID = E.ORG_ID
        AND     D.CC_CCID_LVL1 = E.COL_ID
        AND     A.TVOU_CLD_ID = D.CC_CLD_ID
        AND     A.TVOU_SLOC_ID = D.CC_SLOC_ID
        AND     A.TVOU_HO_ORG_ID = D.CC_HO_ORG_ID
        AND     A.TVOU_ORG_ID = D.CC_ORG_ID
        AND     A.TVOU_ID = D.CC_TXN_ID
        AND     A.CC_ID = D.CC_ID

        AND    A.tvou_ORG_ID = NVL($P{Org_Id}, A.tvou_ORG_ID)
        AND     A.tvou_COA_ID LIKE NVL($P{Coa_Id}, A.tvou_COA_ID)
        AND     A.tvou_COG_ID LIKE decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, A.tvou_COG_ID)||'%', NVL($P{Cog_Id}, A.tvou_COG_ID))
        AND     A.tvou_NA_ID LIKE NVL($P{Na_Id}, A.tvou_NA_ID)
        AND     TRUNC(A.tVOU_DT) >= C.FY_START_dATE
        AND     TRUNC(A.tVOU_DT) < TO_DATE($P{From_Date}, 'YYYY-mm-dd')
        GROUP BY     A.TVOU_CLD_ID,
            A.TVOU_SLOC_ID,
            A.TVOU_ORG_ID,
            E.COL_VALUE,
            A.TVOU_HO_ORG_ID,
            A.TVOU_COA_ID
        ) f1,
        (
        SELECT  a.TVOU_CLD_ID,
                a.TVOU_ORG_ID,
                a.TVOU_COA_ID,
                C.COL_VALUE,
                NVL(SUM(DECODE(a.TVOU_AMT_TYP,'Dr',B.CC_AMT_BS,0)),0) tmp_dr_amt,
                NVL(SUM(DECODE(a.TVOU_AMT_TYP,'Cr',B.CC_AMT_BS,0)),0) tmp_cr_amt
        FROM    TVOU_LINES a,
        TVOU$COST$CENTER B,
        --APP.APP$CC C
        (select distinct col_id, cld_id, sloc_id, ho_org_id, org_id, col_value from app.app$cc) c
        WHERE     a.TVOU_CLD_ID = NVL($P{Cld_Id},TVOU_CLD_ID)
        AND     A.TVOU_CLD_ID = B.CC_CLD_ID
        AND     A.TVOU_SLOC_ID = B.CC_SLOC_ID
        AND     A.TVOU_HO_ORG_ID = B.CC_HO_ORG_ID
        AND     A.TVOU_ORG_ID = B.CC_ORG_ID
        AND     A.TVOU_ID = B.CC_TXN_ID
        AND     A.CC_ID = B.CC_ID
        AND     A.TVOU_CLD_ID = C.CLD_ID
        AND     A.TVOU_SLOC_ID = C.SLOC_ID
        AND     A.TVOU_HO_ORG_ID = C.HO_ORG_ID
        AND     A.TVOU_ORG_ID = C.ORG_ID
        AND     B.CC_CCID_LVL1 = C.COL_ID

        AND    a.tvou_ORG_ID = NVL($P{Org_Id}, tvou_ORG_ID)
        AND     a.tvou_COA_ID LIKE NVL($P{Coa_Id}, tvou_COA_ID)
        AND     a.tvou_COG_ID LIKE decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, tvou_COG_ID)||'%', NVL($P{Cog_Id}, tvou_COG_ID))
        AND     a.tvou_NA_ID LIKE NVL($P{Na_Id}, tvou_NA_ID)
        AND     TRUNC(a.tVOU_DT) >= TO_DATE($P{From_Date}, 'YYYY-mm-dd')
        AND     TRUNC(a.tVOU_DT) <= TO_DATE($P{To_Date}, 'YYYY-mm-dd')
        GROUP BY  a.TVOU_CLD_ID,
                  a.TVOU_ORG_ID,
                  C.COL_VALUE,
                  a.TVOU_COA_ID
        ) f2
    WHERE    A.ORG_CLD_ID = NVL($P{Cld_Id}, A.ORG_CLD_ID)
    AND      A.ORG_ID = NVL($P{Org_Id}, A.ORG_ID)
    AND    B.ORG_CLD_ID = A.ORG_CLD_ID
    AND    B.ORG_ID = A.ORG_ID

    AND         B.ORG_CLD_ID = c.ORG_COA_CLD_ID
    AND         B.ORG_ID = c.ORG_ID
    AND         B.COA_SLOC_ID = c.ORG_SLOC_ID
    AND         B.HO_ORG_ID = c.ORG_COA_HO_ORG_ID
    AND         B.COA_ID = c.ORG_COA_ID

    AND         B.ORG_CLD_ID = d.COG_CLD_ID
    AND         B.COA_SLOC_ID = d.COG_SLOC_ID
    AND         B.HO_ORG_ID = d.COG_ORG_ID
    AND         B.COA_COG_ID = d.COG_ID

    AND         c.ORG_COA_CLD_ID = e1.GL_CLD_ID(+)
    AND         c.ORG_SLOC_ID = e1.GL_SLOC_ID(+)
    AND         c.ORG_ID = e1.GL_ORG_ID(+)
    AND         c.ORG_COA_HO_ORG_ID = e1.GL_HO_ORG_ID(+)
    AND         c.ORG_COA_ID = e1.GL_COA_ID(+)

    AND         c.ORG_COA_CLD_ID = e2.GL_CLD_ID(+)
