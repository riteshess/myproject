	<queryString>
		<![CDATA[--MAIN QUERY


SELECT  organisation,
  coa_nm,
  curr_nm,
vou_id,
  invoicedocno,
  vou_dt,
  --purchase_invoice_no,
  --purchase_invoice_date,
  invoiceno,
  invoicedate,
  invoiceduedate,
  adjvou,
  radj_vou_id,
  adjvoudt,
  amount,
  amt_type,
  paidamount,
  adj_amt_type,
  pendingamount,
  INSTRUMENT_NO,
  INSTRUMENT_DATE,
  ADV_INV_FLG,
  entity_grp

FROM
  (SELECT c.ORG_DESC organisation,
    d.COA_NM coa_nm,
    g.GLBL_CURR_DESC curr_nm,
    a.AP_VOU_ID vou_id,
    SUBSTR(e.DOC_TXN_ID_DISP, 3) invoicedocno,
    a.AP_VOU_DT vou_dt,
   -- a.AP_TXN_ID_REF1 purchase_invoice_no,
  --  a.AP_TXN_DT_REF1 purchase_invoice_date,
    a.AP_EXT_DOC_NO invoiceno,
    a.AP_EXT_DOC_DT invoicedate,
    a.AP_VOU_DUE_DT invoiceduedate,
    SUBSTR(f.DOC_TXN_ID_DISP, 3) adjvou,
    b.RADJ_VOU_ID radj_vou_id,
    b.RADJ_VOU_DT adjvoudt,
    DECODE(b.ADV_INV_FLG, 'A', 0, 'N',
    (SELECT AP.AP_AMT_BS_ORIG amount
    FROM AP
    WHERE AP.GL_CLD_ID  = $P{Cld_Id}
    AND AP.AP_SLOC_ID   = $P{Sloc_Id}
    AND AP.AP_ORG_ID    = $P{Org_Id}
    AND AP.GL_HO_ORG_ID = $P{Ho_Org_Id}
    AND AP.AP_VOU_ID    = b.RADJ_VOU_ID
    AND AP.AP_AMT_TYP   = 'Cr'
    )) amount,
    a.AP_AMT_TYP amt_type,
    NVL(b.AMT_ADJ, 0) paidamount,
    b.AMT_TYP adj_amt_type,
    a.AP_AMT_BS_ORIG - NVL(b.AMT_ADJ, 0) pendingamount,
    x.GL_INSTRMNT_NO INSTRUMENT_NO,
    x.GL_INSTRMNT_DT INSTRUMENT_DATE,
    b.ADV_INV_FLG ADV_INV_FLG,
    (SELECT app.app$eo$mst.EO_MST_NM
    FROM app.app$eo$mst
    WHERE app.app$eo$mst.EO_CLD_ID    = $P{Cld_Id}
    AND app.app$eo$mst.EO_MST_SLOC_ID = $P{Sloc_Id}
    AND app.app$eo$mst.EO_HO_ORG_ID   = $P{Ho_Org_Id}
    AND app.app$eo$mst.EO_MST_ID      = $P{Entity_Group}
    ) entity_grp
  FROM ap a,
    (SELECT GL_ADJ.ADJ_CLD_ID CLD_ID,
      GL_ADJ.ADJ_SLOC_ID SLOC_ID,
      GL_ADJ.ADJ_HO_ORG_ID HO_ORG_ID,
      GL_ADJ.ADJ_ORG_ID ORG_ID,
      GL_ADJ.ADJ_APPL_INST_ID APPL_INST_ID,
      56 DOC_ID,
      GL_ADJ.ADJ_COA_ID COA_ID,
      GL_ADJ.ADJ_MST_VOU_ID MST_VOU_ID,
      GL_ADJ.ADJ_MST_VOU_DT MST_VOU_DT,
      GL_ADJ.ADJ_MST_SL_NO MST_SL_NO,
      GL_ADJ.ADJ_TAB,
      GL_ADJ.ADJ_VOU_ID,
      GL_ADJ.ADJ_VOU_DT,
      GL_ADJ.ADJ_ARAP_SL_NO,
      GL_ADJ.ADJ_SL_NO,
      GL_ADJ.ADJ_AMT_SP,
      GL_ADJ.RADJ_TAB RADJ_TAB,
      GL_ADJ.RADJ_VOU_ID,
      GL_ADJ.RADJ_VOU_DT,
      GL_ADJ.RADJ_ARAP_SL_NO,
      GL_ADJ.RADJ_SL_NO,
      GL_ADJ.RADJ_AMT_ADJ * GL_ADJ.CURR_CC_TRX AMT_ADJ,
      GL_ADJ.ADJ_AMT_TYP AMT_TYP,
      GL_ADJ.ADV_INV_FLG
    FROM GL_ADJ
    WHERE GL_ADJ.ADJ_CLD_ID = NVL($P{Cld_Id}, GL_ADJ.ADJ_CLD_ID)
    AND GL_ADJ.ADJ_COA_ID   = NVL($P{Coa_Id}, GL_ADJ.ADJ_COA_ID)
    ) b,
    app.org c,
    fin$coa d,
    app.app$doc$txn e,
    app.app$doc$txn f,
    APP.APP$GLBL$CURR g,
    GL_LINE_INSTRUMNT x
  WHERE a.GL_CLD_ID       = b.CLD_ID
  AND a.AP_SLOC_ID        = b.SLOC_ID
  AND a.GL_HO_ORG_ID      = b.HO_ORG_ID
  AND a.AP_ORG_ID         = b.ORG_ID
  AND a.AP_VOU_ID         = b.ADJ_VOU_ID
  AND a.AP_COA_ID         = b.COA_ID
  AND a.AP_GL_SL_NO       = b.ADJ_SL_NO
  AND a.AP_SL_NO          = b.ADJ_ARAP_SL_NO
  AND c.ORG_CLD_ID        = a.GL_CLD_ID
  AND c.ORG_ID            = a.AP_ORG_ID
  AND e.DOC_CLD_ID        = a.GL_CLD_ID
  AND e.DOC_SLOC_ID       = a.AP_SLOC_ID
  AND e.DOC_ORG_ID        = a.AP_ORG_ID
  AND e.DOC_TXN_ID        = a.AP_VOU_ID
  AND b.CLD_ID            = f.DOC_CLD_ID
  AND b.SLOC_ID           = f.DOC_SLOC_ID
  AND b.ORG_ID            = f.DOC_ORG_ID
  AND b.DOC_ID            = f.DOC_ID
  AND b.RADJ_VOU_ID       = f.DOC_TXN_ID
  AND a.GL_CLD_ID         = e.DOC_CLD_ID
  AND a.AP_SLOC_ID        = e.DOC_SLOC_ID
  AND a.AP_ORG_ID         = e.DOC_ORG_ID
  AND a.AP_VOU_ID         = e.DOC_TXN_ID
  AND d.COA_CLD_ID        = a.GL_CLD_ID
  AND d.COA_SLOC_ID       = a.AP_SLOC_ID
  AND d.COA_ID            = a.AP_COA_ID
  AND g.GLBL_CURR_ID      = a.AP_CURR_ID_SP
  AND a.AP_SLOC_ID        = x.GL_SLOC_ID(+)
  AND a.AP_ORG_ID         = x.GL_ORG_ID(+)
  AND a.AP_VOU_ID         = x.GL_VOU_ID(+)
  AND a.AP_COA_ID         = x.GL_COA_ID(+)
  AND a.GL_HO_ORG_ID      = x.GL_HO_ORG_ID(+)
  AND a.GL_CLD_ID         = x.GL_CLD_ID(+)
  AND a.AP_AMT_TYP        = 'Dr'
  AND a.GL_CLD_ID         = $P{Cld_Id}
  AND a.AP_ORG_ID         = NVL($P{Org_Id}, a.AP_ORG_ID)
  AND a.AP_EOMST_ID       = NVL($P{Entity_Group}, a.AP_EOMST_ID)
  AND TRUNC(a.AP_VOU_DT) >= to_date(NVL($P{From_Date}, '1000-01-01'), 'YYYY-MM-DD')
  AND TRUNC(a.AP_VOU_DT) <= to_date(NVL($P{To_Date}, SysDate), 'YYYY-MM-DD')
  AND a.AP_AMT_BS_ORIG   >= NVL($P{AmountX1}, 0)
  AND a.AP_AMT_BS_ORIG   <= NVL($P{AmountX2}, 999999999999999999)
  AND a.AP_CURR_ID_SP     = NVL($P{Curr_Id}, a.AP_CURR_ID_SP)
  AND a.AP_COA_ID         = NVL($P{Coa_Id}, a.AP_COA_ID)
  AND e.DOC_ID            = 56
  AND 56                  = e.DOC_ID
  AND d.COA_HO_ORG_ID     = NVL($P{Ho_Org_Id}, d.COA_HO_ORG_ID)


  UNION

  SELECT c.ORG_DESC organisation,
    d.COA_NM coa_nm,
    g.GLBL_CURR_DESC curr_nm,
    a.TVOU_ID vou_id,
    SUBSTR(e.DOC_TXN_ID_DISP, 3) || '(P)' invoicedocno,
    a.TVOU_DT vou_dt,
   -- '' purchase_invoice_no,
    --'' purchase_invoice_date,
    a.TVOU_BILL_no invoiceno,
    a.TVOU_BILL_DATE invoicedate,
    a.TVOU_due_dt invoiceduedate,
    SUBSTR(f.DOC_TXN_ID_DISP, 3) adjvou,
    b.RADJ_VOU_ID radj_vou_id,
    b.RADJ_VOU_DT adjvoudt,
    DECODE(b.ADV_INV_FLG, 'A', 0, 'N',
    (SELECT TVOU_LINES.TVOU_AMT_BS amount
    FROM TVOU_LINES
    WHERE TVOU_LINES.TVOU_CLD_ID  = $P{Cld_Id}
    AND TVOU_LINES.TVOU_SLOC_ID   = $P{Sloc_Id}
    AND TVOU_LINES.TVOU_ORG_ID    = $P{Org_Id}
    AND TVOU_LINES.TVOU_HO_ORG_ID = $P{Ho_Org_Id}
    AND TVOU_ID               = b.RADJ_VOU_ID
    AND TVOU_LINES.TVOU_AMT_TYP   = 'Cr'
    )) amount,
    a.TVOU_AMT_TYP amt_type,
    NVL(b.AMT_ADJ, 0) paidamount,
    b.AMT_TYP adj_amt_type,
    a.TVOU_AMT_BS - NVL(b.AMT_ADJ, 0) pendingamount,
    x.TVOU_INSTRMNT_NO INSTRUMENT_NO,
    x.TVOU_INSTRMNT_DT INSTRUMENT_DATE,
    b.ADV_INV_FLG ADV_INV_FLG,
    (SELECT app.app$eo$mst.EO_MST_NM
    FROM app.app$eo$mst
    WHERE app.app$eo$mst.EO_CLD_ID    = $P{Cld_Id}
    AND app.app$eo$mst.EO_MST_SLOC_ID = $P{Sloc_Id}
    AND app.app$eo$mst.EO_HO_ORG_ID   = $P{Ho_Org_Id}
    AND app.app$eo$mst.EO_MST_ID      = $P{Entity_Group}
    ) entity_grp
  FROM TVOU_LINES a,
    (SELECT tvou_CLD_ID CLD_ID,
      tvou_ADJ_SLOC_ID SLOC_ID,
      tvou_HO_ORG_ID HO_ORG_ID,
      tvou_ADJ_ORG_ID ORG_ID,
      tvou_ADJ_APPL_INST_ID APPL_INST_ID,
      55 DOC_ID,
      tvou_ADJ_COA_ID COA_ID,
      TVOU_ADJ.TVOU_MST_VOU_ID MST_VOU_ID,
      TVOU_ADJ.TVOU_MST_VOU_DT MST_VOU_DT,
      TVOU_ADJ.TVOU_MST_SL_NO MST_SL_NO,
      TVOU_ADJ.TVOU_ADJ_TAB ADJ_TAB,
      TVOU_ADJ.TVOU_ADJ_VOU_ID ADJ_VOU_ID,
      TVOU_ADJ.TVOU_ADJ_VOU_DT ADJ_VOU_DT,
      TVOU_ADJ.TVOU_ADJ_ARAP_SL_NO ADJ_ARAP_SL_NO,
      TVOU_ADJ.TVOU_ADJ_SL_NO ADJ_SL_NO,
      TVOU_ADJ.TVOU_ADJ_AMT_SP ADJ_AMT_SP,
      TVOU_ADJ.TVOU_RADJ_TAB RADJ_TAB,
      TVOU_ADJ.TVOU_RADJ_VOU_ID RADJ_VOU_ID,
      TVOU_ADJ.TVOU_RADJ_VOU_DT RADJ_VOU_DT,
      TVOU_ADJ.TVOU_RADJ_ARAP_SL_NO RADJ_ARAP_SL_NO,
      TVOU_ADJ.TVOU_RADJ_SL_NO RADJ_SL_NO,
      TVOU_ADJ.TVOU_ADJ_AMT_ADJ * TVOU_ADJ.TVOU_CURR_CC_TRX AMT_ADJ,
      TVOU_ADJ.TVOU_AMT_TYP AMT_TYP,
      TVOU_ADJ.TVOU_ADV_INV_FLG ADV_INV_FLG
    FROM TVOU_ADJ
    WHERE TVOU_ADJ.TVOU_CLD_ID   = NVL($P{Cld_Id}, TVOU_ADJ.TVOU_CLD_ID)
    AND TVOU_ADJ.TVOU_ADJ_COA_ID = NVL($P{Coa_Id}, TVOU_ADJ.TVOU_ADJ_COA_ID)
    ) b,
    app.org c,
    fin$coa d,
    app.app$doc$txn e,
    app.app$doc$txn f,
    APP.APP$GLBL$CURR g,
    FIN.TVOU_LINE_CHQ x
   , TVOU TV
    WHERE a.TVOU_CLD_ID     = b.CLD_ID
  AND a.TVOU_SLOC_ID      = b.SLOC_ID
  AND a.TVOU_HO_ORG_ID    = b.HO_ORG_ID
  AND a.TVOU_ORG_ID       = b.ORG_ID
  AND a.TVOU_ID           = b.ADJ_VOU_ID
  AND a.TVOU_COA_ID       = b.COA_ID
  AND a.TVOU_SL_NO        = b.ADJ_SL_NO


  AND c.ORG_CLD_ID        = a.TVOU_CLD_ID
  AND c.ORG_ID            = a.TVOU_ORG_ID
  AND e.DOC_CLD_ID        = a.TVOU_CLD_ID
  AND e.DOC_SLOC_ID       = a.TVOU_SLOC_ID
  AND e.DOC_ORG_ID        = a.TVOU_ORG_ID
  AND e.DOC_TXN_ID        = a.TVOU_ID
  AND b.CLD_ID            = f.DOC_CLD_ID
  AND b.SLOC_ID           = f.DOC_SLOC_ID
  AND b.ORG_ID            = f.DOC_ORG_ID
 -- AND b.DOC_ID            = f.DOC_ID
  AND b.RADJ_VOU_ID       = f.DOC_TXN_ID
  AND a.TVOU_CLD_ID       = e.DOC_CLD_ID
  AND a.TVOU_SLOC_ID      = e.DOC_SLOC_ID
  AND a.TVOU_ORG_ID       = e.DOC_ORG_ID
  AND a.TVOU_ID           = e.DOC_TXN_ID
  AND d.COA_CLD_ID        = a.TVOU_CLD_ID
  AND d.COA_SLOC_ID       = a.TVOU_SLOC_ID
  AND d.COA_ID            = a.TVOU_COA_ID
  AND g.GLBL_CURR_ID      = a.TVOU_CURR_ID_SP
  AND a.TVOU_SLOC_ID      = x.TVOU_SLOC_ID(+)
  AND a.TVOU_ORG_ID       = x.TVOU_ORG_ID(+)
  AND a.TVOU_ID       = x.TVOU_ID(+)
  AND a.TVOU_COA_ID       = x.TVOU_COA_ID(+)
  AND a.TVOU_HO_ORG_ID    = x.TVOU_HO_ORG_ID(+)
  AND a.TVOU_CLD_ID       = x.TVOU_CLD_ID(+)
  AND a.TVOU_AMT_TYP      = 'Dr'
  AND a.TVOU_CLD_ID       = $P{Cld_Id}
  AND a.TVOU_ORG_ID       = NVL($P{Org_Id}, a.TVOU_ORG_ID)
  AND a.TVOU_EOMST_ID     = NVL($P{Entity_Group}, a.TVOU_EOMST_ID)
  AND TRUNC(a.tVOU_DT) >= to_date(NVL($P{From_Date}, '1000-01-01'), 'YYYY-MM-DD')
  AND TRUNC(a.tVOU_DT) <= to_date(NVL($P{To_Date}, SysDate), 'YYYY-MM-DD')
  AND a.TVOU_AMT_BS      >= NVL($P{AmountX1}, 0)
  AND a.TVOU_AMT_BS      <= NVL($P{AmountX2}, 999999999999999999)
  AND  A.TVOU_ID = DECODE($P{PostFlag},'P','1','A',A.TVOU_ID,A.TVOU_ID)
  AND a.TVOU_CURR_ID_SP   = NVL($P{Curr_Id}, a.TVOU_CURR_ID_SP)
  AND a.TVOU_COA_ID       = NVL($P{Coa_Id}, a.TVOU_COA_ID)
  AND e.DOC_ID            = 55
  AND 55                  = e.DOC_ID
  AND d.COA_HO_ORG_ID     = NVL($P{Ho_Org_Id}, d.COA_HO_ORG_ID)

   AND A.TVOU_CLD_ID = tv.TVOU_CLD_ID
                AND A.TVOU_SLOC_ID = tv.TVOU_SLOC_ID
                AND A.TVOU_HO_ORG_ID = tv.TVOU_HO_ORG_ID
                AND A.TVOU_ID = tv.TVOU_ID

  )]]>
	</queryString>
