<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="PaymentReg_Inst_SubReport" language="groovy" pageWidth="150" pageHeight="25" whenNoDataType="NoDataSection" columnWidth="150" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="ho_org_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="coa_id" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="cld_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="vou_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select nvl(a.gl_instrmnt_no,'CASH') cheque_number,
       a.gl_instrmnt_dt cheque_date,
       a.gl_amt_sp chq_amt,
       --a.gl_coa_bnk_id,
       (select att_nm from app.app$ds$att where att_type_id = 25 and att_id = a.gl_instrmnt_typ) type,
       (select f.coa_nm from fin$coa f where
                                                      f.coa_sloc_id=a.gl_sloc_id
                                                      and f.coa_cld_id=a.gl_cld_id
                                                      and f.coa_ho_org_id=a.gl_ho_org_id
                                                      and f.coa_id=a.gl_coa_bnk_id ) bank_name
from   gl_line_instrumnt a
where
a.gl_cld_id = $P{cld_id}
and a.gl_ho_org_id = $P{ho_org_id}
and a.gl_vou_id = $P{vou_id}
and (a.gl_coa_id = $P{coa_id} OR a.gl_coa_bnk_id=$P{coa_id})

union ALl

select nvl(a.TVOU_INSTRMNT_NO,'CASH') cheque_number,
       a.TVOU_INSTRMNT_DT cheque_date,
       a.TVOU_AMT_SP chq_amt,
       --a.tvou_coa_bnk_id,
       (select att_nm from app.app$ds$att where att_type_id = 25 and att_id = a.TVOU_INSTRMNT_TYP) type,
       (select f.coa_nm from fin$coa f where
                                                              f.coa_sloc_id=a.tvou_sloc_id
                                                             and  f.coa_cld_id=a.tvou_cld_id
                                                             and f.coa_ho_org_id=a.tvou_org_id
                                                             and f.coa_id=a.tvou_coa_bnk_id
                                                       ) bank_name
from   tvou_line_chq a
where
a.TVOU_CLD_ID = $P{cld_id}
and a.TVOU_HO_ORG_ID = $P{ho_org_id}
and a.TVOU_ID = $P{vou_id}
and (a.TVOU_COA_ID = $P{coa_id} OR a.tvou_coa_bnk_id=$P{coa_id})]]>
	</queryString>
	<field name="CHEQUE_NUMBER" class="java.lang.String"/>
	<field name="CHEQUE_DATE" class="java.sql.Timestamp"/>
	<field name="CHQ_AMT" class="java.math.BigDecimal"/>
	<field name="TYPE" class="java.lang.String"/>
	<field name="BANK_NAME" class="java.lang.String"/>
	<sortField name="CHEQUE_DATE"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<detail>
		<band height="25" splitType="Stretch">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement stretchType="RelativeToTallestObject" x="84" y="10" width="28" height="15"/>
				<textElement textAlignment="Left" verticalAlignment="Middle">
					<font fontName="Arial" size="7" isPdfEmbedded="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{CHEQUE_NUMBER}]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="dd-MM-yyyy" isBlankWhenNull="true">
				<reportElement stretchType="RelativeToTallestObject" x="112" y="10" width="38" height="15"/>
				<textElement verticalAlignment="Middle">
					<font fontName="Arial" size="7"/>
				</textElement>
				<textFieldExpression class="java.sql.Timestamp"><![CDATA[$F{CHEQUE_DATE}]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="#,##0.00;-#,##0.00" isBlankWhenNull="true">
				<reportElement stretchType="RelativeToTallestObject" x="30" y="10" width="54" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="Arial" size="7"/>
				</textElement>
				<textFieldExpression class="java.math.BigDecimal"><![CDATA[$F{CHQ_AMT}]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement stretchType="RelativeToTallestObject" x="0" y="10" width="30" height="15"/>
				<textElement verticalAlignment="Middle">
					<font fontName="Arial" size="7"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{TYPE}]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement mode="Transparent" x="0" y="0" width="150" height="25" forecolor="#0000CC" backcolor="#000000"/>
				<textElement>
					<font fontName="Arial" size="7" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{BANK_NAME}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
