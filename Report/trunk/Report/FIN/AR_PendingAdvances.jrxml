	<queryString>
		<![CDATA[select org_desc,
       curr_nm,
       coa_nm,
       ar_vou_id,
       DOC_TXN_ID_DISP,
       Ar_VOU_DT,
       Ar_AMT_SP,
       Ar_AMT_ADJ,
       Ar_AMT_OS,
       NARR,
       ENTITY_GRP
from
(

select	b.org_desc org_desc,
		g.glbl_curr_notation_alias curr_nm,
		c.coa_nm coa_nm,
		a.ar_vou_id ar_vou_id,
		substr(d.doc_txn_id_disp,3) doc_txn_id_disp,
		a.ar_vou_dt ar_VOU_DT,
		a.ar_amt_sp ar_amt_sp,
		a.ar_amt_adj ar_amt_adj,
		nvl(a.ar_amt_sp,0) - nvl(a.ar_amt_adj,0) ar_amt_os,
    gl.GL_NARR narr,
(select eo_mst_nm from app.app$eo$mst
		where eo_cld_id = $P{Cld_Id}
		and eo_mst_sloc_id = $P{Sloc_Id}
		and eo_ho_org_id = $P{Ho_Org_Id}
		 and eo_mst_id = $P{Entity_Group}) entity_grp
from 	ar a,
		  app.org b,
		fin$coa c,
		app.app$doc$txn d,
		app.app$glbl$curr g,
   gl_lines gl
where	a.gl_cld_id = nvl($P{Cld_Id},a.gl_cld_id)
and		a.ar_org_id = nvl($P{Org_Id},a.ar_org_id)
and a.AR_COA_ID= nvl($P{Coa_Id},a.AR_COA_ID)
and a.ar_eomst_id = nvl($P{Entity_Group},a.ar_eomst_id)
and	c.coa_ho_org_id = nvl($P{Ho_Org_Id},c.coa_ho_org_id)
and c.coa_cog_id = nvl($P{Cog_Id},c.coa_cog_id)
and trunc(a.ar_vou_dt) >= to_date(NVL($P{From_Date},'1000-01-01'),'YYYY-MM-DD')
and trunc(a.ar_vou_dt) <= to_date(NVL($P{To_Date},SYSDATE),'YYYY-MM-DD')
and a.ar_amt_sp >= nvl($P{AmountX1},0)
AND a.ar_amt_sp <= nvl($P{AmountX2},999999999999999999)
and a.ar_curr_id_sp = nvl($P{Curr_Id},a.ar_curr_id_sp)
and	nvl(a.ar_amt_sp,0) - nvl(a.ar_amt_adj,0) > 0

and   a.gl_cld_id=gl.GL_CLD_ID
and   a.ar_org_id=gl.GL_ORG_ID
and   a.ar_SLOC_ID=gl.GL_SLOC_ID
and   a.GL_HO_ORG_ID=gl.GL_HO_ORG_ID
and   a.ar_VOU_ID=gl.GL_VOU_ID
and   a.ar_TYPE_ID=gl.GL_TYPE_ID
and   a.ar_GL_SL_NO=gl.GL_SL_NO
and   a.ar_COA_ID=gl.GL_COA_ID

--and	a.ar_txn_typ_adv = 'Y'
and a.ar_amt_typ = 'Cr'

and	b.org_cld_id = a.gl_cld_id
and	b.org_id = a.ar_org_id

and	c.coa_cld_id = a.gl_cld_id
and	c.coa_sloc_id = a.ar_sloc_id

and	c.coa_id = a.ar_coa_id
and	d.doc_cld_id = a.gl_cld_id
and	d.doc_sloc_id = a.ar_sloc_id
and	d.doc_org_id = a.ar_org_id
and	d.doc_txn_id = a.ar_vou_id

and	g.glbl_curr_id = a.ar_curr_id_sp


union
SELECT
b.org_desc org_desc,
g.glbl_curr_notation_alias curr_nm,
    c.coa_nm coa_nm,

  a.tvou_id ar_vou_id,
  -- P for Private- in flag= A
   substr(d.doc_txn_id_disp,3)|| '(P)' DOC_TXN_ID_DISP,

   a.TVOU_DT Ar_VOU_DT,
   a.TVOU_amt_sp   ar_amt_sp,
   0 ar_amt_adj,
   a.TVOU_amt_sp ar_amt_os,
    A.NARR NARR,

   (select eo_mst_nm from app.app$eo$mst
    where eo_cld_id = $P{Cld_Id}
    and eo_mst_sloc_id = $P{Sloc_Id}
    and eo_ho_org_id = $P{Ho_Org_Id}
    and eo_mst_id = $P{Entity_Group}) entity_grp

                FROM     TVOU_LINES A,
                         TVOU tv,
                        app.org b,
                        fin$coa c,
                        app.app$glbl$curr g, app.app$doc$txn d

                WHERE    A.TVOU_ID = DECODE($P{PostFlag},'P','1','A',A.TVOU_ID,A.TVOU_ID)
                AND a.TVOU_CLD_ID = NVL($P{Cld_Id},A.TVOU_CLD_ID)
                and A.TVOU_ORG_ID = NVL($P{Org_Id}, A.TVOU_ORG_ID)
                AND a.TVOU_COA_ID = NVL($P{Coa_Id}, a.TVOU_COA_ID)



             AND a.TVOU_COG_ID=  NVL($P{Cog_Id}, a.TVOU_COG_ID)
                AND a.TVOU_NA_ID = NVL($P{Na_Id}, a.TVOU_NA_ID)
                AND TRUNC(a.TVOU_DT) >= TO_DATE($P{From_Date},'YYYY-mm-dd')
                AND TRUNC(a.TVOU_DT) <= TO_DATE($P{To_Date}, 'YYYY-mm-dd')

                AND A.TVOU_CLD_ID = tv.TVOU_CLD_ID
                AND A.TVOU_SLOC_ID = tv.TVOU_SLOC_ID
                AND A.TVOU_HO_ORG_ID = tv.TVOU_HO_ORG_ID
                AND A.TVOU_ID = tv.TVOU_ID
                AND A.TVOU_AMT_TYP= 'Cr'

                and	b.org_cld_id = a.tvou_cld_id
                and	b.org_id     = a.tvou_org_id

                and	c.coa_cld_id    =   a.tvou_cld_id
                and	c.coa_sloc_id   =   a.tvou_sloc_id
                and C.COA_HO_ORG_ID =   A.TVOU_HO_ORG_ID
                and	c.coa_id        =   a.tvou_coa_id
                and	g.glbl_curr_id  =   a.tvou_curr_id_sp

                and	d.doc_cld_id    =   a.tvou_cld_id

                and	d.doc_sloc_id   =   a.tvou_sloc_id
                and	d.doc_org_id    =   a.tvou_org_id
                and	d.doc_txn_id    =   a.tvou_id
              --  and a.tvou_type_id IN (2,4)
                )]]>
	</queryString>
