	<queryString>
		<![CDATA[select cld_id,
        orgid,
        slocid,
        organisation,
        type_name,
        voucher_id,
        voucher_no,
        voucher_date,
        coaid,
        coaname,
        cogid,
        cogname,
        amount_type,
        amount,
        amount_base,
        debit_amount,
        credit_amount,
        narration,
        usr_name,
        posted_by,
        cc_id from
(select	a.gl_cld_id cld_id,
        a.gl_org_id orgid,
        a.gl_sloc_id slocid,
        d.org_desc organisation,
        g.doc_typ type_name,
        substr(f.doc_txn_id_disp,3) voucher_id,
        a.gl_vou_id voucher_no,
        a.gl_vou_dt voucher_date,
        a.gl_coa_id coaid,
        c.coa_nm coaname,
        a.gl_cog_id cogid,
        e.cog_nm cogname,
        a.gl_amt_typ amount_type,
        a.gl_amt_sp amount,
        a.gl_amt_bs  amount_base,
        decode(  a.gl_amt_typ,'Dr', a.gl_amt_sp,0) debit_amount,
        decode(  a.gl_amt_typ,'Cr', a.gl_amt_sp,0) credit_amount,
        substr(a.gl_narr,1) narration,
        h.usr_name usr_name,
        i.usr_name posted_by,
        a.cc_id
from 	  gl_lines a,
        gl gl,
        fin$coa c,
        fin$acc$na b,
        app.org d,
        fin.fin$cog e,
        app.app$doc$txn f,
        app.app$doc$typ_vw g,
        app.app$sec$usr h,
        app.app$sec$usr i
where a.gl_txn_typ_mig not in  ('O','MO')
and a.gl_cld_id = nvl($P{Cld_Id},a.gl_cld_id)
and a.gl_org_id= nvl($P{Org_Id},a.gl_org_id)
and a.gl_vou_dt >= nvl(to_date($P{From_Date},'YYYY-MM-DD'),to_date('2000-01-01','YYYY-MM-DD'))
and a.gl_vou_dt  <= nvl(to_date($P{To_Date},'YYYY-MM-DD'),SYSDATE)
and case
	     when ($P{Book_Type} = 74 and a.gl_type_id = 11) then 1
	     when ($P{Book_Type} = 75 and a.gl_type_id = 12) then 1
	     when ($P{Book_Type} = 76 and a.gl_type_id = 9) then 1
	     when ($P{Book_Type} = 77 and a.gl_type_id = 10) then 1
	     when ($P{Book_Type} = 78 and a.gl_type_id = 8) then 1
	     when ($P{Book_Type} = 79 and a.gl_type_id = 7) then 1
	     when ($P{Book_Type} = 80 and a.gl_type_id in (2,3,6,1)) then 1
	     when ($P{Book_Type} = 81 and a.gl_type_id in (4,5,6,1,13)) then 1
	     when ($P{Book_Type} = 82 and a.gl_type_id = 13) then 1
	     when ($P{Book_Type} = 83 and a.gl_type_id = 1 and b.acc_type <> 78) then 1
	     when ($P{Book_Type} = 86 and a.gl_type_id = 14) then 1
       when ($P{Book_Type} = 87 and a.gl_type_id = 16) then 1
       when ($P{Book_Type} = 88 and a.gl_type_id = 17) then 1
	end = 1

and a.gl_cld_id=gl.gl_cld_id
and a.gl_sloc_id = gl.gl_sloc_id
and a.gl_ho_org_id = gl.gl_ho_org_id
and a.gl_vou_id=gl.gl_vou_id

and a.gl_cld_id = c.coa_cld_id
and a.gl_sloc_id=c.coa_sloc_id
and a.gl_ho_org_id = c.coa_ho_org_id
and a.gl_coa_id=c.coa_id

and b.acc_cld_id = c.coa_cld_id
and b.acc_sloc_id=c.coa_sloc_id
and b.ho_org_id = c.coa_ho_org_id
and b.acc_id=c.coa_acc_id


and a.gl_cld_id = d.org_cld_id
and a.gl_org_id=d.org_id

and a.gl_cld_id = e.cog_cld_id
and a.gl_sloc_id = e.cog_sloc_id
and a.gl_ho_org_id=e.cog_org_id
and a.gl_cog_id=e.cog_id

and a.gl_cld_id=f.doc_cld_id
and a.gl_sloc_id=f.doc_sloc_id
and a.gl_org_id=f.doc_org_id
and a.gl_vou_id=f.doc_txn_id

and a.gl_cld_id= g.doc_cld_id
and a.gl_sloc_id = g.doc_sloc_id
and a.gl_org_id = g.doc_org_id
and a.gl_type_id = g.doc_typ_id
and g.DOC_TYP_RESET_DEF = 'Y'
and g.doc_id = 56

and a.gl_sloc_id  = h.sloc_id
and a.usr_id_create = h.usr_id

and gl.gl_sloc_id  = i.sloc_id(+)
and gl.usr_id_post=i.usr_id(+)

union

select	a.tvou_cld_id cld_id,
        a.tvou_org_id orgid,
        a.tvou_sloc_id slocid,
        d.org_desc organisation,
        g.doc_typ type_name,
		substr(f.doc_txn_id_disp,3)|| '(P)' voucher_id,
        a.tvou_id voucher_no,
        a.tvou_dt voucher_date,
        a.tvou_coa_id coaid,
        c.coa_nm coaname,
        a.tvou_cog_id cogid,
        e.cog_nm cogname,
        a.tvou_amt_typ amount_type,
        a.tvou_amt_sp amount,
        a.tvou_amt_bs  amount_base,
        decode(  a.tvou_amt_typ,'Dr', a.tvou_amt_sp,0) debit_amount,
        decode(  a.tvou_amt_typ,'Cr', a.tvou_amt_sp,0) credit_amount,
        substr(a.narr,1) narration,
        h.usr_name usr_name,
        null posted_by,
        a.cc_id
from 	  tvou_lines a,
        tvou gl,
        fin$coa c,
        fin$acc$na b,
        app.org d,
        fin.fin$cog e,
        app.app$doc$txn f,
        app.app$doc$typ_vw g,
        app.app$sec$usr h
where a.tvou_id = decode($P{PostFlag},'P','1','A',a.tvou_id,a.tvou_id)
--and a.tvou_txn_typ_mig not in  ('O','MO')
and a.tvou_cld_id = nvl($P{Cld_Id},a.tvou_cld_id)
and a.tvou_org_id= nvl($P{Org_Id},a.tvou_org_id)
and a.tvou_dt >= nvl(to_date($P{From_Date},'YYYY-MM-DD'),to_date('2000-01-01','YYYY-MM-DD'))
and a.tvou_dt  <= nvl(to_date($P{To_Date},'YYYY-MM-DD'),SYSDATE)
and case
	     when ($P{Book_Type} = 74 and a.tvou_type_id = 11) then 1
	     when ($P{Book_Type} = 75 and a.tvou_type_id = 12) then 1
	     when ($P{Book_Type} = 76 and a.tvou_type_id = 9) then 1
	     when ($P{Book_Type} = 77 and a.tvou_type_id = 10) then 1
	     when ($P{Book_Type} = 78 and a.tvou_type_id = 8) then 1
	     when ($P{Book_Type} = 79 and a.tvou_type_id = 7) then 1
	     when ($P{Book_Type} = 80 and a.tvou_type_id in (2,3,6,1)) then 1
	     when ($P{Book_Type} = 81 and a.tvou_type_id in (4,5,6,1,13)) then 1
	     when ($P{Book_Type} = 82 and a.tvou_type_id = 13) then 1
	     when ($P{Book_Type} = 83 and a.tvou_type_id = 1 and b.acc_type <> 78) then 1
	     when ($P{Book_Type} = 86 and a.tvou_type_id = 14) then 1
       when ($P{Book_Type} = 87 and a.tvou_type_id = 16) then 1
       when ($P{Book_Type} = 88 and a.tvou_type_id = 17) then 1
	end = 1

and a.tvou_cld_id=gl.tvou_cld_id
and a.tvou_sloc_id = gl.tvou_sloc_id
and a.tvou_ho_org_id = gl.tvou_ho_org_id
and a.tvou_id=gl.tvou_id

and a.tvou_cld_id = c.coa_cld_id
and a.tvou_sloc_id=c.coa_sloc_id
and a.tvou_ho_org_id = c.coa_ho_org_id
and a.tvou_coa_id=c.coa_id

and b.acc_cld_id = c.coa_cld_id
and b.acc_sloc_id=c.coa_sloc_id
and b.ho_org_id = c.coa_ho_org_id
and b.acc_id=c.coa_acc_id


and a.tvou_cld_id = d.org_cld_id
and a.tvou_org_id=d.org_id

and a.tvou_cld_id = e.cog_cld_id
and a.tvou_sloc_id = e.cog_sloc_id
and a.tvou_ho_org_id=e.cog_org_id
and a.tvou_cog_id=e.cog_id

and a.tvou_cld_id=f.doc_cld_id
and a.tvou_sloc_id=f.doc_sloc_id
and a.tvou_org_id=f.doc_org_id
and a.tvou_id=f.doc_txn_id

and a.tvou_cld_id= g.doc_cld_id
and a.tvou_sloc_id = g.doc_sloc_id
and a.tvou_org_id = g.doc_org_id
and a.tvou_type_id = g.doc_typ_id
and g.DOC_TYP_RESET_DEF = 'Y'
and g.doc_id = 56

and a.tvou_sloc_id  = h.sloc_id
and a.usr_id_create = h.usr_id

)]]>
