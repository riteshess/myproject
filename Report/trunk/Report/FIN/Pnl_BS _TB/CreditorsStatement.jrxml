	<queryString>
		<![CDATA[select a.org_desc,e.type_id,
       b.coa_id,
       b.coa_nm,


       null          eo_phone, --h.eo_phone,
       null          eo_email_id, --h.eo_email_id,
       null          eo_fax_no, --h.eo_fax_no,
       i.avg_cr_days avg_cr_days, --i.avg_cr_days,
       i.cr_limit    cr_limit, --i.cr_limit,

       --op.org_coa_op_bal open_bal,

       nvl(op.org_coa_op_bal, 0) + nvl(op1.gl_amt, 0) open_bal,

       --op.org_coa_op_bal_typ org_coa_op_bal_typ,

       decode(sign(nvl(op.org_coa_op_bal, 0) + nvl(op1.gl_amt, 0)),
              1,
              'Dr',
              0,
              'Dr',
              -1,
              'Cr') org_coa_op_bal_typ,

       --comment this to get opening balance

       e.voucher_date,
       e.docType,
       e.docTypeNm,
       e.vou_id,
       e.doc_txn_id_disp,
       e.sales_invoice_doc_id,
   (decode(e.voucher_date,to_date('31-03-2014','dd-mm-rrrr'),e.sales_invoice_id,e.debtors_invoice)) sales_invoice_id,
       --e.sales_invoice_id,
	   decode(e.voucher_date,to_date('31-03-2014','dd-mm-rrrr'),e.sales_invoice_date,e.debtors_invoice_date) sales_invoice_date,
       --e.sales_invoice_date,
     NVL(decode(e.voucher_date,to_date('31-03-2014','dd-mm-rrrr'),e.sales_invoice_date,e.debtors_invoice_date), e.voucher_date)  Bill_Vou_Date,
  NVL(to_char(decode(e.voucher_date,to_date('31-03-2014','dd-mm-rrrr'),e.sales_invoice_date,e.debtors_invoice_date), 'MON-YY'), e.month) month,
   (decode(e.voucher_date,to_date('31-03-2014','dd-mm-rrrr'),e.debtors_invoice,e.sales_invoice_id)) debtors_invoice,
       --e.debtors_invoice,
	   decode(e.voucher_date,to_date('31-03-2014','dd-mm-rrrr'),e.debtors_invoice_date,e.sales_invoice_date) debtors_invoice_date,
       --e.debtors_invoice_date,
       e.due_date,
       e.over_due_days,
       e.curr_nm,
       nvl(e.ap_amt_sp, 0) ap_amt_sp,
       e.ap_amt_typ BalanceType,
       e.rate,
       nvl(e.damt, 0) damt,
       nvl(e.camt, 0) camt,
       nvl(e.amt, 0) amt,
       b.coa_ho_org_id,
       (select eo_mst_nm from app.app$eo$mst
		where eo_cld_id = $P{Cld_Id}
		and eo_mst_sloc_id = $P{Sloc_Id}
		and eo_ho_org_id = $P{Ho_Org_Id}
		 and eo_mst_id = $P{Entity_Group}) entity_grp
  from app.org    a,
       fin$coa    b,
       org$coa    b1,
       fin$acc$na c,
       app.app$eo d,
       --app.app$eo$pers$comm h,
       app.app$eo$prf i,
       (select a.org_coa_cld_id,
               a.org_id,
               a.org_fy_id,
               a.org_coa_id,
               sum(decode(a.org_coa_op_bal_typ,
                          'Dr',
                          a.org_coa_op_bal,
                          -a.org_coa_op_bal)) org_coa_op_bal,
               decode(sign(sum(decode(a.org_coa_op_bal_typ,
                                      'Dr',
                                      a.org_coa_op_bal,
                                      -a.org_coa_op_bal))),
                      1,
                      'Dr',
                      0,
                      'Dr',
                      'Cr') org_coa_op_bal_typ
          from org$coa$fy a,
               (select app.fn_get_fy_id($P{Cld_Id},
                                        $P{Org_Id},
                                        to_date($P{From_Date}, 'yyyy-MM-dd'),
                                        'FY') fy_id
                  from dual) b
         where a.org_coa_cld_id = nvl($P{Cld_Id}, a.org_coa_cld_id)
           and a.org_id = nvl($P{Org_Id}, a.org_id)
           and a.org_fy_id = b.fy_id
           and a.org_coa_id = nvl($P{Coa_Id}, a.org_coa_id)
         group by org_coa_cld_id, org_id, org_fy_id, org_coa_id) op,
       (select a.gl_cld_id,
               a.gl_org_id,
               a.gl_coa_id,
               nvl(sum(decode(gl_amt_typ, 'Dr', gl_amt_bs, 'Cr', -gl_amt_bs)),
                   0) gl_amt
          from gl_lines a,
               gl b,
               (select app.fn_get_fy_start_date($P{Cld_Id},
                                                $P{Org_Id},
                                                TO_DATE($P{From_Date},
                                                        'YYYY-mm-dd'),
                                                'FY') fy_start_date
                  from dual) c,
               app.app$eo d
         where b.gl_cld_id = nvl($P{Cld_Id}, b.gl_cld_id)
           and a.gl_org_id = nvl($P{Org_Id}, a.gl_org_id)
           and a.gl_coa_id = nvl($P{Coa_Id}, a.gl_coa_id)
           and b.gl_stat = 'FLC'
           and a.gl_cld_id = b.gl_cld_id
           and a.gl_sloc_id = b.gl_sloc_id
           and a.gl_vou_id = b.gl_vou_id
           and trunc(a.gl_vou_dt) >= c.fy_start_date
           and trunc(a.gl_vou_dt) < to_date($P{From_Date}, 'YYYY-mm-dd')
           and a.gl_txn_typ_mig not in ('O', 'MO')

           and d.eo_cld_id = a.gl_cld_id
           and d.eo_sloc_id = a.gl_sloc_id
           and d.eo_ho_org_id = a.gl_ho_org_id
           and d.eo_id = a.gl_eo_id
           and d.eo_bhav = 'C'

         group by a.gl_cld_id, a.gl_org_id, a.gl_coa_id) op1,
       (select a.gl_cld_id cld_id, a.ap_org_id org_id, a.ap_coa_id coa_id, COUNT(*) cnt FROM ap a WHERE a.gl_cld_id = $P{Cld_Id} AND a.ap_org_id = NVL($P{Org_Id}, a.ap_org_id) AND TRUNC(a.ap_vou_dt) >= to_date(NVL($P{From_Date}, '1000-01-01'), 'YYYY-MM-DD') AND TRUNC(a.ap_vou_dt) <= to_date(NVL($P{To_Date}, SYSDATE), 'YYYY-MM-DD') AND NVL(ap_amt_sp, 0) * NVL(ap_cc, 1) > 0 AND ap_curr_id_sp = NVL($P{Curr_Id}, a.ap_curr_id_sp) AND a.ap_coa_id = NVL($P{Coa_Id}, a.ap_coa_id) group by a.gl_cld_id, a.ap_org_id, a.ap_coa_id) x1,
-- add this---
(
SELECT a.tvou_cld_id cld_id,
  a.tvou_org_id org_id,
  a.tvou_coa_id coa_id,
  COUNT(*) cnt
FROM tvou_lines a,
  app.app$eo f
WHERE a.tvou_cld_id   = $P{Cld_Id}
AND a.tvou_org_id     = NVL($P{Org_Id}, a.tvou_org_id)
AND a.tvou_id         = DECODE($P{PostFlag}, 'P', '1', 'A', a.tvou_id, a.tvou_id)
AND TRUNC(a.tvou_dt) >= to_date(NVL($P{From_Date}, '1000-01-01'), 'YYYY-MM-DD')
AND TRUNC(a.tvou_dt) <= to_date(NVL($P{To_Date}, SYSDATE), 'YYYY-MM-DD')
AND a.tvou_amt_bs     > 0
AND a.tvou_curr_id_sp = NVL($P{Curr_Id}, a.tvou_curr_id_sp)
AND a.tvou_coa_id     = NVL($P{Coa_Id}, a.tvou_coa_id)
AND f.eo_cld_id       = a.tvou_cld_id
AND f.eo_sloc_id      = a.tvou_sloc_id
AND f.eo_ho_org_id    = a.tvou_ho_org_id
AND f.eo_id           = a.tvou_eo_id
AND f.eo_bhav         = 'C'
GROUP BY a.tvou_cld_id,
  a.tvou_org_id,
  a.tvou_coa_id
) x2,
--comment this to get opening balance
(
SELECT a.ap_type_id type_id,
  a.gl_cld_id cld_id,
  a.ap_sloc_id sloc_id, -- add this here
  a.ap_org_id org_id,
  a.ap_coa_id coa_id,
  f.glbl_curr_notation_alias curr_nm,
  a.ap_vou_id vou_id,
  SUBSTR(b.doc_txn_id_disp, 3) doc_txn_id_disp,
  a.ap_vou_dt voucher_date,
  a.ap_doc_id_ref1 sales_invoice_doc_id,
  a.ap_txn_id_ref1 sales_invoice_id,
  a.ap_txn_dt_ref1 sales_invoice_date,
  TO_CHAR(NVL(a.ap_txn_dt_ref1, a.ap_vou_dt), 'MON-YY') MONTH,
  DECODE(a.ap_amt_typ, 'Dr', SUBSTR(a.ap_ext_doc_no, 3), a.ap_ext_doc_no) debtors_invoice,
  a.ap_ext_doc_dt debtors_invoice_date,
  a.ap_vou_due_dt due_date,
  ((to_date(NVL($P{To_Date}, sysdate), 'YYYY-MM-DD')) - a.ap_vou_due_dt) over_due_days,
  a.ap_amt_sp,
  a.ap_amt_typ,
  NVL(a.ap_cc, 1) rate,
  DECODE(ltrim(rtrim(upper(a.ap_amt_typ))), 'DR', NVL(a.ap_amt_sp, 0) * NVL(ap_cc, 1), 0) damt,
  DECODE(ltrim(rtrim(upper(a.ap_amt_typ))), 'CR', NVL(a.ap_amt_sp, 0) * NVL(ap_cc, 1), 0) camt,
  DECODE(ltrim(rtrim(upper(a.ap_amt_typ))), 'DR', NVL(a.ap_amt_sp, 0) * NVL(ap_cc, 1), 'CR', -NVL(a.ap_amt_sp, 0) * NVL(ap_cc, 1)) amt,
  vt.DOC_TYP_ID docType,
  vt.doc_typ docTypeNm
FROM ap a,
  app.app$doc$txn b,
 app.app$doc$typ_vw vt,
  app.app$glbl$curr f
WHERE a.gl_cld_id                     = $P{Cld_Id}
AND a.ap_org_id                       = NVL($P{Org_Id}, a.ap_org_id)
AND TRUNC(a.ap_vou_dt)               >= to_date(NVL($P{From_Date}, '1000-01-01'), 'YYYY-MM-DD')
AND TRUNC(a.ap_vou_dt)               <= to_date(NVL($P{To_Date}, SYSDATE), 'YYYY-MM-DD')
AND NVL(ap_amt_sp, 0) * NVL(ap_cc, 1) > 0 -- dm 2014-05-09
AND a.ap_curr_id_sp                   = NVL($P{Curr_Id}, a.ap_curr_id_sp)
AND a.ap_coa_id                       = NVL($P{Coa_Id}, a.ap_coa_id)
AND b.doc_cld_id                      = a.gl_cld_id
AND b.doc_sloc_id                     = a.ap_sloc_id
AND b.doc_org_id                      = a.ap_org_id
AND b.doc_txn_id                      = a.ap_vou_id
AND f.glbl_curr_id                    = a.ap_curr_id_sp
AND a.ap_eomst_id                     = NVL($P{Entity_Group},a.ap_eomst_id)
and a.gl_cld_id= vt.doc_cld_id
and a.ap_sloc_id = vt.doc_sloc_id
and a.ap_org_id = vt.doc_org_id
and a.ap_type_id = vt.doc_typ_id
and vt.doc_id = 55
and vt.DOC_TYP_RESET_DEF='Y'

UNION ALL
SELECT a.tvou_type_id type_id,
