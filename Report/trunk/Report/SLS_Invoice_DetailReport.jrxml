	<queryString>
		<![CDATA[SELECT   cld_id, sloc_id, org_id, ho_org_id, invoice_id,
invoice_doc_dt,USR_ID_CREATE_DT ,
          curr_id_sp,curr_rate, bill_add, eo_nm, itm_id, itm_desc,
          itm_rate, ext_doc_dt, ext_doc_no, base_price, uom_nm,
discount, adds, org_tin_no, org_reg_no, org_cst_no, org_ecc_no,
org_range,
          cust_org_id, cust_reg_no, cust_vat_no, cust_ecc_no,
          cust_leg_code cust_leg_code, SUM (itm_amt_sp) itm_amt_sp,
          SUM (itm_amt_bs) itm_amt_bs, SUM (itm_ship_qty) itm_ship_qty,
          invoice_doc_id, org_adds, org_desc,
          (SELECT fn_get_shp_dtl (cld_id,
                                  sloc_id,
                                  ho_org_id,
                                  org_id,
                                  invoice_doc_id,
                                  'S'
                                 )
             FROM DUAL) ship_detail,
          (SELECT fn_get_shp_dtl (cld_id,
                                  sloc_id,
                                  ho_org_id,
                                  org_id,
                                  invoice_doc_id,
                                  'T'
                                 )
             FROM DUAL) trans_detail,
          (SELECT fn_get_shp_dtl (cld_id,
                                  sloc_id,
                                  ho_org_id,
                                  org_id,
                                  invoice_doc_id,
                                  'M'
                                 )
             FROM DUAL) mode_detail,
             (SELECT fn_grp_cnt (cld_id,
                                  sloc_id,
                                  ho_org_id,
                                  org_id,
                                  invoice_doc_id

                                 )
                                 from dual
                                 )grp_cnt,
                                 grp_nm

     FROM (SELECT DISTINCT SUBSTR (a.shipmnt_id, 3) shipmnt_id,
a.cld_id,K.USR_ID_CREATE_DT,
                           a.eo_catg, a.sloc_id, a.org_id, a.ho_org_id,
                           a.invoice_doc_id,
                           SUBSTR (a.invoice_id, 3) invoice_id,
                           a.invoice_doc_dt, a.inv_type, a.inv_typ_nm,
                           a.curr_id_sp, a.curr_rate, a.eo_id, a.bill_add,
                           a.eo_nm, a.shipmnt_dt, a.so_id, c.doc_dt so_dt,
                           a.itm_id, b.itm_desc itm_desc, a.ship_amt_sp,
                           a.ship_amt_bs, a.itm_rate, a.itm_uom,
                           e.uom_nm uom_nm, a.itm_ship_qty,
                           NVL (DECODE (a.itm_disc_typ,
                                        'A', a.itm_disc_val,
                                        'P', (  (  NVL (a.itm_ship_qty, 0)
                                                 * NVL (itm_rate, 0)
                                                )
                                              * NVL (a.itm_disc_val, 0)
                                              / 100
                                         )
                                       ),
                                0
                               ) discount,
                             NVL (a.itm_ship_qty, 0)
                           * NVL (itm_rate, 0) itm_amt_gs,
                           a.itm_amt_bs, a.itm_amt_sp, a.tot_amt_sp,
                           a.tot_amt_bs, a.itm_cost, m.att_nm
inv_wf_stat_desc,
                           f.org_desc, l.eo_nm assigned_to, m.att_nm
inv_mode,
                           (SELECT adds
                              FROM app.app$adds$vw
                             WHERE app.app$adds$vw.adds_id = a.bill_add)
adds,
                           (SELECT adds
                              FROM app.app$adds$vw
                             WHERE app.app$adds$vw.adds_id =
p.org_add_id1)
org_adds,
                           NVL (n.tax_amt_sp, 0) tax_amt_sp,
                           NVL (n.tax_amt_bs, 0) tax_amt_bs, a.grp_nm,
                           o.curr_notation_alias,
                           ROUND (a.itm_amt_sp / a.itm_ship_qty, 2)
base_price,
                           a.ext_doc_dt, a.ext_doc_no,v.tin_no org_tin_no,
                           v.reg_no org_reg_no, v.cst_no org_cst_no,
                           v.ecc_no org_ecc_no, v.RANGE org_range,
                           k.eo_org_id cust_org_id,
                           k.regn_certi_no cust_reg_no, k.vat_no
cust_vat_no,
                           k.ecc_no cust_ecc_no, u.eo_leg_code cust_leg_code
                      FROM sls$invoice$dtl_vw a,
                           app.app$sls$itm_vw b,
                           sls.sls$so c,
                           app.app$uom$conv$std e,
                           app.org f,
                           app.org$oth$dtl p,
                           (SELECT eo_cld_id, x.eo_sloc_id, x.eo_ho_org_id,
                                   x.eo_id, x.eo_nm
                              FROM app.app$eo x
                             WHERE x.eo_cld_id =NVL('0000',x.eo_cld_id)
                               AND x.eo_sloc_id =NVL(1,x.eo_sloc_id)
                               AND x.eo_type = '80') l,
                           (SELECT y.att_nm, y.att_id
                              FROM app.app$ds$att y
                             WHERE att_type_id = 69) m,
                           (SELECT att_id, att_nm
                              FROM app.app$ds$att
                             WHERE att_type_id = 74) z,
                           sls.sls$inv$tr n,
                           app.app$curr_vw o,
                           app.org$oth$dtl v,
                           app.app$eo u,
                           app.app$eo$prf k
                     WHERE a.sloc_id = b.sloc_id
                       AND a.itm_id = b.itm_id
                       AND a.org_id = b.org_id
                       AND a.cld_id = b.cld_id
                       AND a.ho_org_id = b.ho_org_id
                       AND a.sloc_id = c.sloc_id
                       AND a.org_id = c.org_id
                       AND a.cld_id = c.cld_id
                       AND a.ho_org_id = c.ho_org_id
                       AND a.so_id = c.so_id
                       AND a.sloc_id = e.sloc_id
                       AND a.cld_id = e.cld_id
                       AND a.itm_uom = e.uom_id
                       AND a.cld_id = f.org_cld_id
                       AND a.org_id = f.org_id
                       AND a.org_id = p.org_id(+)
                       AND c.cld_id = l.eo_cld_id(+)
                       AND c.sloc_id = l.eo_sloc_id(+)
                       AND c.ho_org_id = l.eo_ho_org_id(+)
                       AND c.assigned_to = l.eo_id(+)
                       AND a.inv_mode = m.att_id
                       AND a.eo_catg = z.att_id
                       AND a.cld_id = n.cld_id(+)
                       AND a.sloc_id = n.sloc_id(+)
                       AND a.ho_org_id = n.ho_org_id(+)
                       AND a.invoice_doc_id = n.doc_id(+)
                       AND a.itm_id = n.itm_id(+)
                       AND a.curr_id_sp = o.curr_id
                       AND v.org_id = a.org_id
                       AND u.eo_cld_id = k.eo_cld_id
                       AND u.eo_sloc_id = k.eo_sloc_id
                       AND u.eo_ho_org_id = k.eo_ho_org_id
                       AND u.eo_id = k.eo_id
                       AND u.eo_cld_id = a.cld_id
                       AND u.eo_sloc_id = a.sloc_id
                       AND u.eo_ho_org_id = a.ho_org_id
                       AND u.eo_id = a.eo_id
                    AND a.cld_id =NVL($P{CldId},a.cld_id)


                    AND a.sloc_id =NVL($P{SlocId},a.sloc_id)


                    AND a.org_id =NVL($P{OrgId},a.org_id)


                    AND a.ho_org_id =NVL($P{HoOrgId},a.ho_org_id)


                    AND a.invoice_doc_id =NVL($P{DocId},a.invoice_doc_id)


                    AND a.itm_id =NVL($P{ItmId},a.itm_id)


                    AND a.grp_id =NVL($P{GrpId},a.grp_id)


                    AND NVL(c.order_stat,217)=


                    NVL($P{Status},NVL(c.order_stat,217))


                    AND a.eo_catg =NVL($P{EoCatId},a.eo_catg)


                    AND a.curr_id_sp =NVL($P{CurrId},a.curr_id_sp)


                    AND a.inv_type =NVL($P{InvType},a.inv_type)


                    AND TRUNC(a.invoice_doc_dt)>=


                    NVL(TO_DATE($P{FromDate},'yyyy-MM-dd'),


                    TRUNC(a.invoice_doc_dt)


                    )


                    AND TRUNC(a.invoice_doc_dt)<=


                    NVL(TO_DATE($P{ToDate},'yyyy-MM-dd'),


                    TRUNC(a.invoice_doc_dt)


                    ))


GROUP BY cld_id,
         sloc_id,
         org_id,
         ho_org_id,
         invoice_id,
         invoice_doc_dt,
         curr_id_sp,
         curr_rate,
         bill_add,
         eo_nm,
         itm_id,
         itm_desc,
         itm_rate,
         ext_doc_dt,
         ext_doc_no,
         base_price,
         uom_nm,
         discount,
         adds,
         invoice_doc_id,
         org_adds,
         org_desc,
         org_tin_no,
         org_reg_no,
         org_cst_no,
         org_ecc_no,
         org_range,
         cust_org_id,
         cust_reg_no,
         cust_vat_no,
         cust_ecc_no,
         cust_leg_code,
         grp_nm,
         USR_ID_CREATE_DT]]>
	</queryString>
