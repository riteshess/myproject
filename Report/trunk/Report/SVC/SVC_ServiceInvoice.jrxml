	<queryString>
		<![CDATA[SELECT   A.DOC_ID,  A.HO_ORG_ID,  A.EO_ID, a.eo_nm
  , A.ORG_ID   ,(SELECT ORG_DESC
                FROM APP.ORG
                WHERE ORG_CLD_ID = A.CLD_ID
                AND ORG_ID = A.ORG_ID) ORG_DESC,
                APP.FN_GET_ADDS ( $P{Sloc_Id}, $P{Cld_Id}, nvl(E.org_add_id1, E.org_add_id2),'M') ORG_ADDS,
  nvl(e.ORG_PHONE1, e.ORG_PHONE2) org_phone, nvl(e.org_email1, e.org_email2)org_email
,e.TIN_NO, e.CST_NO
    ,A.INVC_NO,  TO_CHAR(A.INVC_DT,NVL($P{DateFormat},'dd-MM-yyyy'))INVC_DT,A.INVC_TYPE
    ,(select att_nm    from svc$ds$att    where att_id = a.invc_type)Invc_type_nm
    ,A.ADDS_ID_BILL ,((select adds from app.app$adds$vw where adds_id = a.adds_id_bill)) eo_adds,
    A.COA_ID
  ,B.ITM_ID ,(select itm_desc  from app.app$itm$prf
              where CLD_ID = B.CLD_ID
              AND SLOC_ID = B.SLOC_ID
              AND ho_org_id = b.ho_org_id
              and itm_id = b.itm_id)itm_desc
  ,B.ITM_UOM, (select uom_nm from app.app$uom$conv$std where uom_id = b.itm_uom)uom_nm
  ,B.ITM_QTY, nvl(b.ITM_PRICE,0)Itm_price, DECODE(B.FOC_ITM,'Y','Yes','N','No' )FOC_ITM,   b.SVC_ITM_FLG
  ,D.COA_NM,   nvl((b.itm_qty * b.itm_price),0) amt_sp
  ,b.disc_amt_sp,  nvl(b.tax_amt_sp,0)tax_amt_sp, a.tot_amt_sp,  D.SR_NO,   D.TRAN_TYPE,    D.TRAN_CAT  ,nvl(OC.OC_AMT_SP,0)OC_AMT_SP
, a.curr_id_sp,(select curr_desc from app.app$curr where curr_id = a.curr_id_sp)curr_desc
  ,Decode(A.INVC_TYPE, 95,(SELECT SC_NO FROM SVC$SC
        WHERE C.CLD_ID = CLD_ID
        AND C.SLOC_ID = SLOC_ID
        AND C.ORG_ID = ORG_ID
        AND C.HO_ORG_ID = HO_ORG_ID
        AND C.DOC_ID_SRC = DOC_ID),
        96,(SELECT DOC_NO FROM SVC$CM$WO
        WHERE C.CLD_ID = CLD_ID
        AND C.SLOC_ID = SLOC_ID
        AND C.ORG_ID = ORG_ID
        AND C.HO_ORG_ID = HO_ORG_ID
        AND C.DOC_ID_SRC = DOC_ID))CONT_NO
,Decode(A.INVC_TYPE, 95,(SELECT to_char(SC_DT,NVL($P{DateFormat},'dd-MM-yyyy')) FROM SVC$SC
        WHERE C.CLD_ID = CLD_ID
        AND C.SLOC_ID = SLOC_ID
        AND C.ORG_ID = ORG_ID
        AND C.HO_ORG_ID = HO_ORG_ID
        AND C.DOC_ID_SRC = DOC_ID),
        96,(SELECT to_char (TRUNC(DOC_DT), NVL($P{DateFormat},'dd-MM-yyyy')) FROM SVC$CM$WO
        WHERE C.CLD_ID = CLD_ID
        AND C.SLOC_ID = SLOC_ID
        AND C.ORG_ID = ORG_ID
        AND C.HO_ORG_ID = HO_ORG_ID
        AND C.DOC_ID_SRC = DOC_ID))CONT_DT, f.bill_cycle,(select att_nm from svc$ds$att where att_id = f.bill_cycle)bill_cycle_nm, nvl(b.disc_amt_sp,0)disc_amt_sp

From SVC$INVC  A, SVC$INVC$ITM B, SVC$INVC$SRC C,   SVC$INVC$CALC D ,app.org$oth$dtl e,SVC$INVC$OC OC, SVC$SC f
Where  A.CLD_ID = B.CLD_ID
AND A.SLOC_ID = B.SLOC_ID
AND A.ORG_ID = B.ORG_ID
AND A.HO_ORG_ID = B.HO_ORG_ID
AND A.DOC_ID = B.DOC_ID
and A.CLD_ID = B.CLD_ID
AND A.SLOC_ID = c.SLOC_ID
AND A.ORG_ID = c.ORG_ID
AND A.HO_ORG_ID = c.HO_ORG_ID
AND A.DOC_ID = C.DOC_ID
        AND C.CLD_ID = F.CLD_ID(+)
        AND C.SLOC_ID = F.SLOC_ID(+)
        AND C.ORG_ID = f.ORG_ID(+)
        AND C.HO_ORG_ID = f.HO_ORG_ID(+)
        AND C.DOC_ID_SRC = f.DOC_ID(+)
and A.CLD_ID = d.CLD_ID(+)
AND A.SLOC_ID = d.SLOC_ID(+)
AND A.ORG_ID = d.ORG_ID(+)
AND A.HO_ORG_ID = d.HO_ORG_ID(+)
AND A.DOC_ID = D.DOC_ID(+)
AND A.COA_ID = D.COA_ID(+)

and A.CLD_ID = OC.CLD_ID(+)
AND A.SLOC_ID = OC.SLOC_ID(+)
AND A.ORG_ID = OC.ORG_ID(+)
AND A.HO_ORG_ID = OC.HO_ORG_ID(+)
AND A.DOC_ID = OC.DOC_ID(+)
AND A.AUTH_STAT = 'Y'
AND A.ORG_ID = E.ORG_ID(+)
AND A.CLD_ID = NVL($P{Cld_Id},A.CLD_ID)
AND A.SLOC_ID = NVL($P{Sloc_Id},A.SLOC_ID)
and A.HO_ORG_ID = NVL($P{Ho_Org_Id}, A.HO_ORG_ID)
and A.ORG_ID = NVL($P{Org_Id}, A.ORG_ID)
AND A.doc_id = NVL($P{INV_Doc_Id}, A.DOC_ID)
and A.EO_ID = NVL($P{Eo_Id}, A.EO_ID)
and trunc(A.INVC_DT) >= nvl(TO_DATE($P{From_Date}, 'YYYY-mm-dd') ,trunc(A.INVC_DT))
AND trunc(A.INVC_DT) <= nvl(TO_DATE($P{To_Date}, 'YYYY-mm-dd'),trunc(A.INVC_DT))
order by A.INVC_NO desc]]>
