<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="Salary_Register" language="groovy" pageWidth="2040" pageHeight="1400" orientation="Landscape" whenNoDataType="AllSectionsNoDetail" columnWidth="2000" leftMargin="20" rightMargin="20" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="0.75"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="Crosstab Data Text" hAlign="Center"/>
	<parameter name="cldId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="slocId" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="hoOrgId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="orgId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="empDocId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Path" class="java.lang.String"/>
	<parameter name="FromDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ToDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="crtValue" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ReportType" class="java.lang.String"/>
	<parameter name="Org_Logo" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT --A.CLD_ID,A.HO_ORG_ID,A.ORG_ID,A.DOC_ID,A.SAL_ID,
  --(A.SEQ_ORD || ' ' || A.SAL_DESC ) SAL_DES,
  NVL(DECODE((A.SEQ_ORD
  || ' '
  || A.SAL_DESC ),'BS','1BS'),(A.SEQ_ORD
  || ' '
  || A.SAL_DESC )) SAL_DESC ,
  SUM(NVL(A.SAL_AMT,0))SAL_AMT,
  NVL(
  (SELECT s.param_desc
  FROM hcm$setup s
  WHERE param_id =$P{crtValue}
  AND param_type =3
  AND s.cld_id   =$P{cldId}
  AND s.sloc_id  =$P{slocId}
  AND s.ho_org_id=$P{hoOrgId}
  ),'') locationc,
  NVL(
  (SELECT s.param_desc
  FROM hcm$setup s
  WHERE param_id =$P{crtValue}
  AND param_type =1
  AND s.cld_id   =$P{cldId}
  AND s.sloc_id  =$P{slocId}
  AND s.ho_org_id=$P{hoOrgId}
  ),'') groupc,
  NVL(
  (SELECT g.dept_nm
  FROM app.app$dept g
  WHERE dept_id  =$P{crtValue}
  AND g.cld_id   =$P{cldId}
  AND g.sloc_id  =$P{slocId}
  AND g.ho_org_id=$P{hoOrgId}
  ),'') Dept,
  A.EMP_ID,
  a.seq_ord,
  A.EMP_NM,
  B.ORG_DESC,
  DECODE(A.SAL_STATUS,'P','Pending','A','Approved') SAL_STATUS,
   a.sal_status_nm,
  WRK_DYS

FROM
  (SELECT PRF.CLD_ID,
    PRF.SLOC_ID,
    PRF.HO_ORG_ID,
    PRF.ORG_ID,
    PRF.DOC_ID,
    SL.SAL_ID,
    C.SAL_STATUS, ( select att.att_nm from  hcm$ds$att  att
    where pr.sal_status= att.att_id) sal_status_nm,
    pr.wrk_dys wrk_dys,
    (SELECT ST.SAL_NOTATION
    FROM ORG$HCM$SALARY ST
    WHERE PRF.CLD_ID  = ST.CLD_ID
    AND PRF.SLOC_ID   = ST.SLOC_ID
    AND PRF.HO_ORG_ID = ST.HO_ORG_ID
    AND SL.SAL_ID     = ST.SAL_ID
    AND rownum        =1
    ) SAL_DESC,
    NVL(SL.SAL_AMT,0) SAL_AMT,
    SL.PROC_FRM_DT,
    SL.PROC_TO_DT,
    PRF.EMP_ID,
    PRF.EMP_NM,
    0 SEQ_ORD
  FROM ORG$HCM$EMP$PRF PRF,
    HCM$SAL$PROC$SAL SL,
    HCM$SAL$PROC PR,
    HCM$SAL$PROC$TXN C
  WHERE SL.CLD_ID      = C.CLD_ID(+)
  AND SL.SLOC_ID       = C.SLOC_ID(+)
  AND SL.ORG_ID        = C.ORG_ID(+)
  AND SL.HO_ORG_ID     = C.HO_ORG_ID(+)
  AND SL.DOC_ID        = C.DOC_ID(+)
      AND Pr.CLD_ID    =PRF.CLD_ID
    AND Pr.SLOC_ID   =PRF.SLOC_ID
    AND Pr.ORG_ID    =PRF.ORG_ID
    AND Pr.HO_ORG_ID =PRF.HO_ORG_ID
    AND Pr.EMP_DOC_ID  =PRF.DOC_ID
     and   Pr.CLD_ID  = C.CLD_ID(+)
    AND Pr.SLOC_ID   = C.SLOC_ID(+)
    AND Pr.ORG_ID    = C.ORG_ID(+)
    AND Pr.HO_ORG_ID = C.HO_ORG_ID(+)
    AND Pr.DOC_ID    = C.DOC_ID(+)
   and C.SAL_STATUS is not null

  AND PRF.CLD_ID       =SL.CLD_ID
  AND PRF.SLOC_ID      =SL.SLOC_ID
  AND PRF.HO_ORG_ID    =SL.HO_ORG_ID
  AND PRF.ORG_ID       =SL.ORG_ID
  AND PRF.DOC_ID       =SL.EMP_DOC_ID
  AND SL.SAL_BEHAV     =11
  AND SL.SAL_ID        ='PAR0000017'
  AND SL.PROC_FRM_DT   =to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND SL.PROC_TO_DT    = to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND PRF.CLD_ID       = $P{cldId}
  AND PRF.SLOC_ID      = $P{slocId}
  AND PRF.HO_ORG_ID    = $P{hoOrgId}
  AND PRF.ORG_ID       = $P{orgId}
  AND (PRF.EMP_DEPT_ID = $P{crtValue}
  OR PRF.EMP_LOC_ID    = $P{crtValue}
  OR PRF.EMP_GRP_ID    = $P{crtValue}
  OR PRF.DOC_ID        = $P{crtValue})
  AND PRF.DOC_ID       = NVL($P{empDocId}, PRF.DOC_ID)

  UNION
  SELECT PRF.CLD_ID,
    PRF.SLOC_ID,
    PRF.HO_ORG_ID,
    PRF.ORG_ID,
    PRF.DOC_ID,
    SL.SAL_ID,
    C.SAL_STATUS, ( select att.att_nm from  hcm$ds$att  att
    where pr.sal_status= att.att_id) sal_status_nm,
    pr.wrk_dys wrk_dys,
    (SELECT ST.SAL_NOTATION
    FROM ORG$HCM$SALARY ST
    WHERE PRF.CLD_ID  = ST.CLD_ID
    AND PRF.SLOC_ID   = ST.SLOC_ID
    AND PRF.HO_ORG_ID = ST.HO_ORG_ID
    AND SL.SAL_ID     = ST.SAL_ID
    AND rownum        =1
    ) SAL_DESC,
    NVL(SL.SAL_AMT,0) SAL_AMT,
    SL.PROC_FRM_DT,
    SL.PROC_TO_DT,
    PRF.EMP_ID,
    PRF.EMP_NM,
    1 SEQ_ORD
  FROM ORG$HCM$EMP$PRF PRF,
    HCM$SAL$PROC$SAL SL,
    hcm$saL$PROC PR,
    HCM$SAL$PROC$TXN C
  WHERE SL.CLD_ID      = C.CLD_ID(+)
  AND SL.SLOC_ID       = C.SLOC_ID(+)
  AND SL.ORG_ID        = C.ORG_ID(+)
  AND SL.HO_ORG_ID     = C.HO_ORG_ID(+)
  AND SL.DOC_ID        = C.DOC_ID(+)
      AND Pr.CLD_ID    =PRF.CLD_ID
    AND Pr.SLOC_ID   =PRF.SLOC_ID
    AND Pr.ORG_ID    =PRF.ORG_ID
    AND Pr.HO_ORG_ID =PRF.HO_ORG_ID
    AND Pr.EMP_DOC_ID  =PRF.DOC_ID
     and   Pr.CLD_ID  = C.CLD_ID(+)
    AND Pr.SLOC_ID   = C.SLOC_ID(+)
    AND Pr.ORG_ID    = C.ORG_ID(+)
    AND Pr.HO_ORG_ID = C.HO_ORG_ID(+)
    AND Pr.DOC_ID    = C.DOC_ID(+)
   and C.SAL_STATUS is not null

  AND PRF.CLD_ID       =SL.CLD_ID
  AND PRF.SLOC_ID      =SL.SLOC_ID
  AND PRF.HO_ORG_ID    =SL.HO_ORG_ID
  AND PRF.ORG_ID       =SL.ORG_ID
  AND PRF.DOC_ID       =SL.EMP_DOC_ID
  AND SL.SAL_BEHAV     =11
  AND SL.SAL_ID!       ='PAR0000017'
  AND SL.PROC_FRM_DT   =to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND SL.PROC_TO_DT    = to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND PRF.CLD_ID       = $P{cldId}
  AND PRF.SLOC_ID      = $P{slocId}
  AND PRF.HO_ORG_ID    = $P{hoOrgId}
  AND PRF.ORG_ID       = $P{orgId}
  AND (PRF.EMP_DEPT_ID = $P{crtValue}
  OR PRF.EMP_LOC_ID    = $P{crtValue}
  OR PRF.EMP_GRP_ID    = $P{crtValue}
  OR PRF.DOC_ID        = $P{crtValue})
  AND PRF.DOC_ID       = NVL($P{empDocId}, PRF.DOC_ID)
  UNION
  SELECT PRF.CLD_ID,
    PRF.SLOC_ID,
    PRF.HO_ORG_ID,
    PRF.ORG_ID,
    PRF.DOC_ID,
    SL.SAL_ID,
    C.SAL_STATUS, ( select att.att_nm from  hcm$ds$att  att
    where pr.sal_status= att.att_id) sal_status_nm,

    pr.wrk_dys wrk_dys,
    (SELECT ST.param_desc
    FROM hcm$setup ST
    WHERE PRF.CLD_ID  = ST.CLD_ID
    AND PRF.SLOC_ID   = ST.SLOC_ID
    AND PRF.HO_ORG_ID = ST.HO_ORG_ID
    AND SL.SAL_ID     = ST.PARAM_ID

    AND rownum        =1

    ) SAL_DESC,
    NVL(SL.SAL_AMT,0) SAL_AMT,
    SL.PROC_FRM_DT,
    SL.PROC_TO_DT,
    PRF.EMP_ID,
    PRF.EMP_NM,
    2 SEQ_ORD
  FROM ORG$HCM$EMP$PRF PRF,
    HCM$SAL$PROC$SAL SL,
    HCM$SAL$PROC$TXN C,
    hcm$sal$proc pr
  WHERE SL.CLD_ID      = C.CLD_ID(+)
  AND SL.SLOC_ID       = C.SLOC_ID(+)
  AND SL.ORG_ID        = C.ORG_ID(+)
  AND SL.HO_ORG_ID     = C.HO_ORG_ID(+)
  AND SL.DOC_ID        = C.DOC_ID(+)
      AND Pr.CLD_ID    =PRF.CLD_ID
    AND Pr.SLOC_ID   =PRF.SLOC_ID
    AND Pr.ORG_ID    =PRF.ORG_ID
    AND Pr.HO_ORG_ID =PRF.HO_ORG_ID
    AND Pr.EMP_DOC_ID  =PRF.DOC_ID
     and   Pr.CLD_ID  = C.CLD_ID(+)
    AND Pr.SLOC_ID   = C.SLOC_ID(+)
    AND Pr.ORG_ID    = C.ORG_ID(+)
    AND Pr.HO_ORG_ID = C.HO_ORG_ID(+)
    AND Pr.DOC_ID    = C.DOC_ID(+)
   and C.SAL_STATUS is not null
  AND PRF.CLD_ID       =SL.CLD_ID
  AND PRF.SLOC_ID      =SL.SLOC_ID
  AND PRF.HO_ORG_ID    =SL.HO_ORG_ID
  AND PRF.ORG_ID       =SL.ORG_ID
  AND PRF.DOC_ID       =SL.EMP_DOC_ID
  AND SL.SAL_BEHAV     =12
  AND SL.PROC_FRM_DT   =to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND SL.PROC_TO_DT    = to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND PRF.CLD_ID       = $P{cldId}
  AND PRF.SLOC_ID      = $P{slocId}
  AND PRF.HO_ORG_ID    = $P{hoOrgId}
  AND PRF.ORG_ID       = $P{orgId}
  AND (PRF.EMP_DEPT_ID = $P{crtValue}
  OR PRF.EMP_LOC_ID    = $P{crtValue}
  OR PRF.EMP_GRP_ID    = $P{crtValue}
  OR PRF.DOC_ID        = $P{crtValue})
  AND PRF.DOC_ID       = NVL($P{empDocId}, PRF.DOC_ID)

  UNION
  SELECT PRF.CLD_ID,
    PRF.SLOC_ID,
    PRF.HO_ORG_ID,
    PRF.ORG_ID,
    PRF.DOC_ID,
    SL.SAL_ID,
    C.SAL_STATUS, ( select att.att_nm from  hcm$ds$att  att
    where pr.sal_status= att.att_id) sal_status_nm,
    pr.wrk_dys wrk_dys,
    (SELECT ST.SAL_NOTATION
    FROM ORG$HCM$SALARY ST
    WHERE PRF.CLD_ID  = ST.CLD_ID
    AND PRF.SLOC_ID   = ST.SLOC_ID
    AND PRF.HO_ORG_ID = ST.HO_ORG_ID
    AND SL.SAL_ID     = ST.SAL_ID
    AND rownum        =1
    ) SAL_DESC,
    NVL(SL.SAL_AMT,0) SAL_AMT,
    SL.PROC_FRM_DT,
    SL.PROC_TO_DT,
    PRF.EMP_ID,
    PRF.EMP_NM,
    3 SEQ_ORD
  FROM ORG$HCM$EMP$PRF PRF,
    HCM$SAL$PROC$SAL SL,
    HCM$SAL$PROC$TXN C,
    hcm$sal$proc pr
  WHERE SL.CLD_ID      = C.CLD_ID(+)
  AND SL.SLOC_ID       = C.SLOC_ID(+)
  AND SL.ORG_ID        = C.ORG_ID(+)
  AND SL.HO_ORG_ID     = C.HO_ORG_ID(+)
  AND SL.DOC_ID        = C.DOC_ID(+)
      AND Pr.CLD_ID    =PRF.CLD_ID
    AND Pr.SLOC_ID   =PRF.SLOC_ID
    AND Pr.ORG_ID    =PRF.ORG_ID
    AND Pr.HO_ORG_ID =PRF.HO_ORG_ID
    AND Pr.EMP_DOC_ID  =PRF.DOC_ID
     and   Pr.CLD_ID  = C.CLD_ID(+)
    AND Pr.SLOC_ID   = C.SLOC_ID(+)
    AND Pr.ORG_ID    = C.ORG_ID(+)
    AND Pr.HO_ORG_ID = C.HO_ORG_ID(+)
    AND Pr.DOC_ID    = C.DOC_ID(+)
   and C.SAL_STATUS is not null

  AND PRF.CLD_ID       =SL.CLD_ID
  AND PRF.SLOC_ID      =SL.SLOC_ID
  AND PRF.HO_ORG_ID    =SL.HO_ORG_ID
  AND PRF.ORG_ID       =SL.ORG_ID
  AND PRF.DOC_ID       =SL.EMP_DOC_ID
  AND SL.SAL_BEHAV     =13
  AND SL.PROC_FRM_DT   =to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND SL.PROC_TO_DT    = to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND PRF.CLD_ID       = $P{cldId}
  AND PRF.SLOC_ID      = $P{slocId}
  AND PRF.HO_ORG_ID    = $P{hoOrgId}
  AND PRF.ORG_ID       = $P{orgId}
  AND (PRF.EMP_DEPT_ID = $P{crtValue}
  OR PRF.EMP_LOC_ID    = $P{crtValue}
  OR PRF.EMP_GRP_ID    = $P{crtValue}
  OR PRF.DOC_ID        = $P{crtValue})
  AND PRF.DOC_ID       = NVL($P{empDocId}, PRF.DOC_ID)
  UNION
  --SEQ5



  SELECT PRF.CLD_ID,
    PRF.SLOC_ID,
    PRF.HO_ORG_ID,
    PRF.ORG_ID,
    PRF.DOC_ID,
    SL.SAL_ID,
    C.SAL_STATUS, ( select att.att_nm from  hcm$ds$att  att
    where pr.sal_status= att.att_id) sal_status_nm,
    pr.wrk_dys wrk_dys,
    (SELECT ST.SAL_NOTATION
    FROM ORG$HCM$SALARY ST
    WHERE PRF.CLD_ID  = ST.CLD_ID
    AND PRF.SLOC_ID   = ST.SLOC_ID
    AND PRF.HO_ORG_ID = ST.HO_ORG_ID
    AND g.SAL_ID     = ST.SAL_ID
    AND rownum        =1
    ) SAL_DESC,
    NVL(G.SAL_AMT,0) SAL_AMT,
    SL.PROC_FRM_DT,
    SL.PROC_TO_DT,
    PRF.EMP_ID,
    PRF.EMP_NM,
    5 SEQ_ORD
  FROM ORG$HCM$EMP$PRF PRF,
    HCM$SAL$PROC$SAL SL,
    hcm$sal$proc pr,
    HCM$SAL$PROC$TXN C,
     HCM$EMP$MON$SAL G
  WHERE SL.CLD_ID      = C.CLD_ID(+)
  AND SL.SLOC_ID       = C.SLOC_ID(+)
  AND SL.ORG_ID        = C.ORG_ID(+)
  AND SL.HO_ORG_ID     = C.HO_ORG_ID(+)
  AND SL.DOC_ID        = C.DOC_ID(+)
  AND SL.CLD_ID      = G.CLD_ID
  AND SL.SLOC_ID       = G.SLOC_ID
  AND SL.ORG_ID        = G.ORG_ID
  AND SL.HO_ORG_ID     = G.HO_ORG_ID
  AND SL.EMP_DOC_ID   = G.EMP_DOC_ID
      AND Pr.CLD_ID    =PRF.CLD_ID
    AND Pr.SLOC_ID   =PRF.SLOC_ID
    AND Pr.ORG_ID    =PRF.ORG_ID
    AND Pr.HO_ORG_ID =PRF.HO_ORG_ID
    AND Pr.EMP_DOC_ID  =PRF.DOC_ID
     and   Pr.CLD_ID  = C.CLD_ID(+)
    AND Pr.SLOC_ID   = C.SLOC_ID(+)
    AND Pr.ORG_ID    = C.ORG_ID(+)
    AND Pr.HO_ORG_ID = C.HO_ORG_ID(+)
    AND Pr.DOC_ID    = C.DOC_ID(+)
   and C.SAL_STATUS is not null

  AND PRF.CLD_ID       =SL.CLD_ID
  AND PRF.SLOC_ID      =SL.SLOC_ID
  AND PRF.HO_ORG_ID    =SL.HO_ORG_ID
  AND PRF.ORG_ID       =SL.ORG_ID
  AND PRF.DOC_ID       =SL.EMP_DOC_ID
  AND SL.SAL_BEHAV     =10
  AND SL.PROC_FRM_DT   =to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND SL.PROC_TO_DT    = to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy')
   AND G.PROC_FRM_DT   =to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND G.PROC_TO_DT    = to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy')
  AND PRF.CLD_ID       = $P{cldId}
  AND PRF.SLOC_ID      = $P{slocId}
  AND PRF.HO_ORG_ID    = $P{hoOrgId}
  AND PRF.ORG_ID       = $P{orgId}
  AND (PRF.EMP_DEPT_ID = $P{crtValue}
  OR PRF.EMP_LOC_ID    = $P{crtValue}
  OR PRF.EMP_GRP_ID    = $P{crtValue}
  OR PRF.DOC_ID        = $P{crtValue})
  AND PRF.DOC_ID       = NVL($P{empDocId}, PRF.DOC_ID)


  --SEQ-5

  UNION
    (SELECT PF.CLD_ID,
      PF.SLOC_ID,
      PF.HO_ORG_ID,
      PF.ORG_ID,
      PF.EMP_DOC_ID,
      PF.DED_DOC_ID SAL_ID,
      C.SAL_STATUS, ( select att.att_nm from  hcm$ds$att  att
    where pr.sal_status= att.att_id) sal_status_nm,
      pr.wrk_dys wrk_dys,
      (SELECT DED_DESC
      FROM HCM$DED D
      WHERE D.DOC_ID  =PF.DED_DOC_ID
      AND PF.CLD_ID   =D.CLD_ID
      AND PF.SLOC_ID  =D.SLOC_ID
      AND PF.HO_ORG_ID=D.HO_ORG_ID
      AND PF.ORG_ID   =D.ORG_ID
      AND rownum      =1
      ) SAL_DESC,
      NVL(PF.EMP_DED_AMT,0) SAL_AMT,
      PF.PROC_FRM_DT PROC_FRM_DT,
      PF.PROC_TO_DT PROC_TO_DT,
      (SELECT EMP_ID
      FROM ORG$HCM$EMP$PRF PRF
      WHERE PRF.CLD_ID  = PF.CLD_ID
      AND PRF.SLOC_ID   = PF.SLOC_ID
      AND PRF.DOC_ID    = PF.EMP_DOC_ID
      AND PRF.ORG_ID    = PF.ORG_ID
      AND PRF.HO_ORG_ID = PF.HO_ORG_ID
      ) EMP_ID,
      (SELECT EMP_NM
      FROM ORG$HCM$EMP$PRF PRF
      WHERE PRF.CLD_ID  = PF.CLD_ID
      AND PRF.SLOC_ID   = PF.SLOC_ID
      AND PRF.DOC_ID    = PF.EMP_DOC_ID
      AND PRF.ORG_ID    = PF.ORG_ID
      AND PRF.HO_ORG_ID = PF.HO_ORG_ID
      ) EMP_NM,
      6 SEQ_ORD
    FROM HCM$MON$PF$DED PF,
      HCM$SAL$PROC$TXN C,
      hcm$sal$proc pr,
      ORG$HCM$EMP$PRF PRF
    WHERE PF.CLD_ID    = C.CLD_ID(+)
    AND PF.SLOC_ID     = C.SLOC_ID(+)
    AND PF.ORG_ID      = C.ORG_ID(+)
    AND PF.HO_ORG_ID   = C.HO_ORG_ID(+)
    AND PF.DOC_ID      = C.DOC_ID(+)
    AND PF.CLD_ID      =PRF.CLD_ID
    AND PF.SLOC_ID     =PRF.SLOC_ID
    AND PF.ORG_ID      =PRF.ORG_ID
    AND PF.HO_ORG_ID   =PRF.HO_ORG_ID
    AND PF.EMP_CODE    =PRF.EMP_CODE
    AND PF.EMP_DOC_ID  =PRF.DOC_ID
    AND Pr.CLD_ID    =PRF.CLD_ID
    AND Pr.SLOC_ID   =PRF.SLOC_ID
    AND Pr.ORG_ID    =PRF.ORG_ID
    AND Pr.HO_ORG_ID =PRF.HO_ORG_ID
    AND Pr.EMP_DOC_ID  =PRF.DOC_ID
       and   Pr.CLD_ID  = C.CLD_ID(+)
    AND Pr.SLOC_ID   = C.SLOC_ID(+)
    AND Pr.ORG_ID    = C.ORG_ID(+)
    AND Pr.HO_ORG_ID = C.HO_ORG_ID(+)
    AND Pr.DOC_ID    = C.DOC_ID(+)
   and C.SAL_STATUS is not null

    AND PF.CLD_ID      = $P{cldId}
    AND PF.SLOC_ID     =$P{slocId}
    AND PF.HO_ORG_ID   =$P{hoOrgId}
    AND PF.ORG_ID      =$P{orgId}
    AND PF.PROC_FRM_DT = to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
    AND PF.PROC_TO_DT  = to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy')
    AND PF.EMP_DOC_ID IN
