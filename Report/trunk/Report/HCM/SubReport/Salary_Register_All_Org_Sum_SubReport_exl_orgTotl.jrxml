      FROM ORG$HCM$EMP$PRF PRF
      WHERE
       $X{IN,PRF.EMP_LOC_ID ,multiLoc}
     	     )

)

   -- NSSF END
    UNION


   ( SELECT PF.CLD_ID,
      PF.SLOC_ID,
      PF.HO_ORG_ID,
      PF.ORG_ID,
      PF.EMP_DOC_ID,
      PF.DOC_ID SAL_ID,
      C.SAL_STATUS,
      pr.wrk_dys WRK_DYS,
        f.ded_DESC sal_desc,
      NVL(PF.EMP_DED_AMT,0) SAL_AMT,
      PF.PROC_FRM_DT PROC_FRM_DT,
      PF.PROC_TO_DT PROC_TO_DT,
      (SELECT EMP_ID
      FROM ORG$HCM$EMP$PRF PRF
      WHERE PRF.CLD_ID  = PF.CLD_ID
      AND PRF.SLOC_ID   = PF.SLOC_ID
      AND PRF.DOC_ID    = PF.EMP_DOC_ID
      AND PRF.ORG_ID    = PF.ORG_ID
      AND PRF.HO_ORG_ID = PF.HO_ORG_ID
      ) EMP_ID,
      (SELECT EMP_NM
      FROM ORG$HCM$EMP$PRF PRF
      WHERE PRF.CLD_ID  = PF.CLD_ID
      AND PRF.SLOC_ID   = PF.SLOC_ID
      AND PRF.DOC_ID    = PF.EMP_DOC_ID
      AND PRF.ORG_ID    = PF.ORG_ID
      AND PRF.HO_ORG_ID = PF.HO_ORG_ID
      ) EMP_NM ,
	  (  SELECT sp.param_desc
      FROM hcm$setup sp
      WHERE  prf.cld_id =sp.cld_id
      and prf.ho_org_id =sp.ho_org_id
      and prf.sloc_id   =sp.sloc_id
      and prf.emp_loc_id=SP.PARAM_ID
	  AND ROWNUM=1
      and  PRF.CLD_ID   = PF.CLD_ID
      AND PRF.SLOC_ID   = PF.SLOC_ID
      AND PRF.DOC_ID    = PF.EMP_DOC_ID
      AND PRF.ORG_ID    = PF.ORG_ID
      AND PRF.HO_ORG_ID = PF.HO_ORG_ID
    ) LOC,
      10.3 SEQ_ORD
    FROM HCM$MON$SUPER$DED PF,
      ORG$HCM$EMP$PRF PRF,
      hcm$sal$proc pr,
      HCM$SAL$PROC$TXN C,
       HCM$EMP$DED E ,
        HCM$DED F

    WHERE PF.CLD_ID  = C.CLD_ID(+)
    AND PF.SLOC_ID   = C.SLOC_ID(+)
    AND PF.ORG_ID    = C.ORG_ID(+)
    AND PF.HO_ORG_ID = C.HO_ORG_ID(+)
    AND PF.DOC_ID    = C.DOC_ID(+)
    AND Pr.CLD_ID    =PRF.CLD_ID
    AND Pr.SLOC_ID   =PRF.SLOC_ID
    AND Pr.ORG_ID    =PRF.ORG_ID
    AND Pr.HO_ORG_ID =PRF.HO_ORG_ID

    AND Pr.EMP_DOC_ID  =PRF.DOC_ID

    AND PF.CLD_ID    =PRF.CLD_ID
    AND PF.SLOC_ID   =PRF.SLOC_ID
    AND PF.ORG_ID    =PRF.ORG_ID
    AND PF.HO_ORG_ID =PRF.HO_ORG_ID
      --AND PF.EMP_CODE=PRF.EMP_CODE
    AND PF.EMP_DOC_ID    = PRF.DOC_ID
    AND E.ORG_ID         = PRF.ORG_ID
    AND E.HO_ORG_ID      = PRF.HO_ORG_ID
    AND E.CLD_ID         = PRF.CLD_ID
    AND E.SLOC_ID        = PRF.SLOC_ID
    AND E.EMP_DOC_ID     =PRF.DOC_ID
    AND E.cld_id       = PF.cld_id

AND E.sloc_id      = PF.sloc_id
AND E.ho_org_id    = PF.ho_org_id
AND E.ORG_ID       = PF.ORG_ID
AND E.EMP_DOC_ID   = PF.EMP_DOC_ID
AND E.DED_DOC_ID   = PF.DED_DOC_ID

AND E.ORG_ID       = F.ORG_ID
AND E.HO_ORG_ID    = F.HO_ORG_ID
AND E.CLD_ID       = F.CLD_ID
AND E.ORG_ID       = F.ORG_ID
AND E.DED_DOC_ID   = F.DOC_ID
  AND f.DED_TYPE     = 94


    AND PF.CLD_ID      = $P{cldId}
    AND PF.SLOC_ID     =$P{slocId}
    AND PF.HO_ORG_ID   =$P{hoOrgId}
    AND PF.ORG_ID      =$P{orgId}

    AND PF.PROC_FRM_DT = to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
    AND PF.PROC_TO_DT  = to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy')

AND PF.EMP_DOC_ID IN
      (SELECT PRF.DOC_ID
      FROM ORG$HCM$EMP$PRF PRF
      WHERE $X{IN,PRF.EMP_LOC_ID ,multiLoc}

     	     )

      )

  --SUPERANNUATION END


  UNION

    (SELECT PF.CLD_ID,
      PF.SLOC_ID,
      PF.HO_ORG_ID,
      PF.ORG_ID,
      PF.EMP_DOC_ID,
      PF.DOC_ID SAL_ID,
      C.SAL_STATUS,
      pf.wrk_dys WRK_DYS,

      ('NET PAY') SAL_DESC,
      NVL(PF.TOT_SAL_AMT,0) SAL_AMT,
      PF.PROC_FRM_DT PROC_FRM_DT,
      PF.PROC_TO_DT PROC_TO_DT,
      (SELECT EMP_ID
      FROM ORG$HCM$EMP$PRF PRF
      WHERE PRF.CLD_ID  = PF.CLD_ID
      AND PRF.SLOC_ID   = PF.SLOC_ID
      AND PRF.DOC_ID    = PF.EMP_DOC_ID
      AND PRF.ORG_ID    = PF.ORG_ID
      AND PRF.HO_ORG_ID = PF.HO_ORG_ID
      ) EMP_ID,
      (SELECT EMP_NM
      FROM ORG$HCM$EMP$PRF PRF
      WHERE PRF.CLD_ID  = PF.CLD_ID
      AND PRF.SLOC_ID   = PF.SLOC_ID
      AND PRF.DOC_ID    = PF.EMP_DOC_ID
      AND PRF.ORG_ID    = PF.ORG_ID
      AND PRF.HO_ORG_ID = PF.HO_ORG_ID
      ) EMP_NM ,
	  (  SELECT sp.param_desc
      FROM hcm$setup sp
      WHERE  prf.cld_id =sp.cld_id
      and prf.ho_org_id =sp.ho_org_id
      and prf.sloc_id   =sp.sloc_id
      and prf.emp_loc_id=SP.PARAM_ID
	  AND ROWNUM=1
      and  PRF.CLD_ID   = PF.CLD_ID
      AND PRF.SLOC_ID   = PF.SLOC_ID
      AND PRF.DOC_ID    = PF.EMP_DOC_ID
      AND PRF.ORG_ID    = PF.ORG_ID
      AND PRF.HO_ORG_ID = PF.HO_ORG_ID
    ) LOC,
      13 SEQ_ORD
    FROM HCM$SAL$PROC PF,
      HCM$SAL$PROC$TXN C,
      ORG$HCM$EMP$PRF PRF
    WHERE PF.CLD_ID  = C.CLD_ID(+)
    AND PF.SLOC_ID   = C.SLOC_ID(+)
    AND PF.ORG_ID    = C.ORG_ID(+)
    AND PF.HO_ORG_ID = C.HO_ORG_ID(+)
    AND PF.DOC_ID    = C.DOC_ID(+)
    AND PF.CLD_ID    =PRF.CLD_ID
    AND PF.SLOC_ID   =PRF.SLOC_ID
    AND PF.ORG_ID    =PRF.ORG_ID
    AND PF.HO_ORG_ID =PRF.HO_ORG_ID
      --AND PF.EMP_CODE=PRF.EMP_CODE
    AND PF.EMP_DOC_ID  =PRF.DOC_ID
    AND PF.CLD_ID      = $P{cldId}
    AND PF.SLOC_ID     =$P{slocId}
    AND PF.HO_ORG_ID   =$P{hoOrgId}
    AND PF.ORG_ID      =$P{orgId}
    AND PF.PROC_FRM_DT = to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
    AND PF.PROC_TO_DT  = to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy')
     AND PF.EMP_DOC_ID IN
      (SELECT PRF.DOC_ID
      FROM ORG$HCM$EMP$PRF PRF
      WHERE
	  $X{IN,PRF.EMP_LOC_ID ,multiLoc}

     	     )


    )
) A,
  APP.ORG B
WHERE A.CLD_ID = B.ORG_CLD_ID(+)
AND A.ORG_ID   = B.ORG_ID(+)
GROUP BY A.SEQ_ORD,
  A.SAL_DESC,
  A.EMP_ID,
  A.EMP_NM,
  A.LOC,
  B.ORG_DESC,
  A.SAL_STATUS,

  wrk_dys
order by a.emp_id]]>
