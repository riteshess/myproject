	</parameter>
	<parameter name="Eo_Id" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="BucketSelect" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="PostFlag" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT A.ORG_ID, B.ORG_DESC,  A.COA_ID,  C.COA_NM,   A.CURR_ID_SP,    a.AR_CC,    E.GLBL_CURR_NOTATION_ALIAS CURR_NM,  A.VOU_ID,   A.VOU_DT,   		a.AR_VOU_DUE_DT,  VAT_NO,  EO_PHONE,  EO_EMAIL_ID,
        EO_FAX_NO, SUBSTR(a.DOC_TXN_ID_DISP,3) DOC_ID,   A.EXT_DOC_NO DOC_NO,   A.EXT_DOC_DT DOC_DT,  A.EXT_DOC_DUE_DT DUE_DATE, A.SLS_INV_DOC_ID, A.SLS_INV_ID,  A.SLS_INV_DT,  A.AMT_SP,
		A.AMT_SP * A.AR_CC AMT_SP_SIGNED,
		A.AMT_ADJ,  A.AMT_ADJ_SIGNED,  A.AMT_OS,  A.AMT_OS_SIGNED,  A.OS_DAYS,
        DECODE(SIGN(30 - OS_DAYS)* SIGN(OS_DAYS ), -1,0,decode($P{AmtSel},'A',A.AMT_SP_SIGNED,'O',AMT_OS_SIGNED)) OS_30_DAYS,
		DECODE(SIGN(60 - OS_DAYS) * SIGN(OS_DAYS - 31), -1,0,decode($P{AmtSel},'A',A.AMT_SP_SIGNED,'O',AMT_OS_SIGNED)) OS_60_DAYS,
		DECODE(SIGN(90 - OS_DAYS) * SIGN(OS_DAYS - 61),      -1,0,decode($P{AmtSel},'A',A.AMT_SP_SIGNED,'O',AMT_OS_SIGNED)) OS_90_DAYS,
		DECODE(SIGN(120 - OS_DAYS) * SIGN(OS_DAYS - 91), -1,0,decode($P{AmtSel},'A',A.AMT_SP_SIGNED,'O',AMT_OS_SIGNED)) OS_120_DAYS,
		DECODE(SIGN(180 - OS_DAYS) * SIGN(OS_DAYS - 121), -1,0,decode($P{AmtSel},'A',A.AMT_SP_SIGNED,'O',AMT_OS_SIGNED)) OS_180_DAYS,
		DECODE(SIGN(365 - OS_DAYS) * SIGN(OS_DAYS - 181), -1,0,decode($P{AmtSel},'A',A.AMT_SP_SIGNED,'O',AMT_OS_SIGNED)) OS_1_YR,
		DECODE(SIGN(OS_DAYS - 366), -1,0,decode($P{AmtSel},'A',A.AMT_SP_SIGNED,'O',AMT_OS_SIGNED)) OS_GREATER_1_YR,
		x3.avg_cr_days avg_cr_days,
		x3.cr_limit cr_limit
FROM ( 	SELECT a.CLD_ID,
		a.SLOC_ID,
		a.HO_ORG_ID,
		a.ORG_ID,
		a.COA_ID,
		a.CURR_ID_SP,
		a.AR_TYPE_ID,
		a.AR_CC,
		a.AR_VOU_DUE_DT,
		a.VOU_ID,
		a.VOU_DT,
		a.AR_GL_SL_NO,
		a.EXT_DOC_TYP,
		a.EXT_DOC_NO,
		a.EXT_DOC_DT,
		a.EXT_DOC_DUE_DT,
		a.SLS_INV_DOC_ID,
		a.SLS_INV_ID,
		a.SLS_INV_DT,
		a.AMT_SP,
		a.AMT_TYP,
		a.ar_sl_no,
		AMT_SP_SIGNED,
		abs(NVL(a1.ADJ_AMT_DTL*AR_CC,0)) AMT_ADJ,--dm 2014-02-24
                NVL(a1.ADJ_AMT_DTL*AR_CC,0) AMT_ADJ_SIGNED, --dm 2014-02-24
		NVL(a.AMT_SP*a.AR_CC,0) - NVL(abs(a1.ADJ_AMT_DTL)*a.AR_CC,0) AMT_OS,
		CASE a.AMT_TYP WHEN 'Dr' THEN NVL(a.AMT_SP*a.AR_CC,0) + NVL(a1.ADJ_AMT_DTL*a.AR_CC,0)
		WHEN 'Cr' THEN -NVL(a.AMT_SP*a.AR_CC,0) + NVL(a1.ADJ_AMT_DTL*a.AR_CC,0) END  AMT_OS_SIGNED, --dm 2014-02-24

		a.OS_DAYS,
		a.doc_txn_id_disp
FROM

	 (SELECT a.GL_CLD_ID CLD_ID,
			a.AR_SLOC_ID SLOC_ID,
			a.GL_HO_ORG_ID HO_ORG_ID,
			a.AR_ORG_ID ORG_ID,
			a.AR_COA_ID COA_ID,
			a.AR_CURR_ID_SP CURR_ID_SP,
			a.AR_TYPE_ID,
			a.AR_CC,
			a.AR_VOU_DUE_DT,
			a.AR_VOU_ID VOU_ID,
			a.AR_VOU_DT VOU_DT,
			a.AR_GL_SL_NO,
			a.AR_EXT_DOC_TYP EXT_DOC_TYP,
			a.AR_EXT_DOC_NO EXT_DOC_NO,
			a.AR_EXT_DOC_DT EXT_DOC_DT,
			a.AR_VOU_DUE_DT EXT_DOC_DUE_DT,
			a.AR_DOC_ID_REF1 SLS_INV_DOC_ID,
			a.AR_TXN_ID_REF1 SLS_INV_ID,
			a.AR_TXN_DT_REF1 SLS_INV_DT,
			a.AR_AMT_SP AMT_SP,
			a.AR_AMT_TYP AMT_TYP,
			a.AR_SL_NO,
			DECODE(a.AR_AMT_TYP,'Dr',NVL(a.AR_AMT_SP,0),'Cr',-NVL(a.AR_AMT_SP,0),NVL(a.AR_AMT_SP,0)) AMT_SP_SIGNED,
			DECODE($P{BucketSelect},
			'D',
			(DECODE($P{DateSelect},
			'V',TRUNC(to_Date($P{AsOnDate},'YYYY-MM-DD') - a.AR_VOU_DT),
			'I',TRUNC(to_Date($P{AsOnDate},'YYYY-MM-DD') - NVL(a.AR_EXT_DOC_DT,a.AR_VOU_DT)),
			'D',TRUNC(to_Date($P{AsOnDate},'YYYY-MM-DD') - NVL(a.AR_VOU_DUE_DT,a.AR_VOU_DT)),
			TRUNC(to_Date($P{AsOnDate},'YYYY-MM-DD') - a.AR_VOU_DT) )+1),
			'M',
			(DECODE($P{DateSelect},
			'V',30*months_between(last_day(to_Date($P{AsOnDate},'YYYY-MM-DD')),last_day(to_date(a.AR_VOU_DT)))+1,
			'I',30*months_between(last_day(to_Date($P{AsOnDate},'YYYY-MM-DD')),last_day(NVL(a.AR_EXT_DOC_DT,a.AR_VOU_DT)))+1,
			'D',30*months_between(last_day(to_Date($P{AsOnDate},'YYYY-MM-DD')),last_day(NVL(a.AR_VOU_DUE_DT,a.AR_VOU_DT)))+1,
				30*months_between(last_day(to_Date($P{AsOnDate},'YYYY-MM-DD')),last_day(a.AR_VOU_DT))+1))) OS_DAYS,
			d.doc_txn_id_disp
	  from ar a,
			app.app$doc$txn d
		WHERE a.GL_CLD_ID = $P{cld_id}
			and a.GL_HO_ORG_ID = nvl($P{Org_ID},a.GL_HO_ORG_ID)
			  and DECODE($P{DateSelect},'V',TRUNC(a.AR_VOU_DT),
						'I',TRUNC(NVL(a.AR_EXT_DOC_DT,a.AR_VOU_DT)),
						'D',TRUNC(NVL(a.AR_VOU_DUE_DT,a.AR_VOU_DT)),
						TRUNC(a.AR_VOU_DT) ) <= to_date(NVL($P{AsOnDate},SYSDATE),'YYYY-MM-DD')
			  and AR_COA_ID = nvl($P{Coa_ID},AR_COA_ID)

			and d.doc_cld_id = a.gl_cld_id
			and d.doc_org_id = a.ar_org_id
			and d.doc_sloc_id = a.ar_sloc_id
			and d.doc_txn_id = a.ar_vou_id

	 union

	 SELECT a.TVOU_CLD_ID CLD_ID,
			a.TVOU_SLOC_ID SLOC_ID,
			a.TVOU_HO_ORG_ID HO_ORG_ID,
			a.TVOU_ORG_ID ORG_ID,
			a.TVOU_COA_ID COA_ID,
			a.TVOU_CURR_ID_SP CURR_ID_SP,
			a.TVOU_TYPE_ID,
			a.TVOU_CC,
			a.TVOU_DUE_DT,
			a.TVOU_ID VOU_ID,
			a.TVOU_DT VOU_DT,
			a.TVOU_SL_NO,
			null EXT_DOC_TYP,
			a.TVOU_BILL_NO EXT_DOC_NO,
			a.TVOU_BILL_DATE EXT_DOC_DT,
			a.TVOU_DUE_DT EXT_DOC_DUE_DT,
			null SLS_INV_DOC_ID,
			null SLS_INV_ID,
			null SLS_INV_DT,
			a.TVOU_AMT_SP AMT_SP,
			a.TVOU_AMT_TYP AMT_TYP,
			a.TVOU_SL_NO,
			DECODE(a.TVOU_AMT_TYP,'Dr',NVL(a.TVOU_AMT_SP,0),'Cr',-NVL(a.TVOU_AMT_SP,0),NVL(a.TVOU_AMT_SP,0)) AMT_SP_SIGNED,
			DECODE($P{BucketSelect},
			'D',
			(DECODE($P{DateSelect},
			'V',TRUNC(to_Date($P{AsOnDate},'YYYY-MM-DD') - a.TVOU_DT),
			'I',TRUNC(to_Date($P{AsOnDate},'YYYY-MM-DD') - NVL(a.TVOU_BILL_DATE,a.TVOU_DT)),
			'D',TRUNC(to_Date($P{AsOnDate},'YYYY-MM-DD') - NVL(a.TVOU_DUE_DT,a.TVOU_DT)),
			TRUNC(to_Date($P{AsOnDate},'YYYY-MM-DD') - a.TVOU_DT) )+1),
			'M',
			(DECODE($P{DateSelect},
			'V',30*months_between(last_day(to_Date($P{AsOnDate},'YYYY-MM-DD')),last_day(to_date(a.TVOU_DT)))+1,
			'I',30*months_between(last_day(to_Date($P{AsOnDate},'YYYY-MM-DD')),last_day(NVL(a.TVOU_BILL_DATE,a.TVOU_DT)))+1,
			'D',30*months_between(last_day(to_Date($P{AsOnDate},'YYYY-MM-DD')),last_day(NVL(a.TVOU_DUE_DT,a.TVOU_DT)))+1,
				30*months_between(last_day(to_Date($P{AsOnDate},'YYYY-MM-DD')),last_day(a.TVOU_DT))+1))) OS_DAYS,
			d.doc_txn_id_disp
	  from tvou_lines a,
               app.app$eo b,app.app$doc$txn d
		WHERE a.TVOU_CLD_ID = $P{cld_id}
				and a.TVOU_HO_ORG_ID = nvl($P{Org_ID},a.TVOU_HO_ORG_ID)
				and DECODE($P{DateSelect},'V',TRUNC(a.TVOU_DT),
						'I',TRUNC(NVL(a.TVOU_BILL_DATE,a.TVOU_DT)),
						'D',TRUNC(NVL(a.TVOU_DUE_DT,a.TVOU_DT)),
						TRUNC(a.TVOU_DT) ) <= to_date(NVL($P{AsOnDate},SYSDATE),'YYYY-MM-DD')
				and TVOU_COA_ID = nvl($P{Coa_ID},TVOU_COA_ID)

				and a.tvou_cld_id = b.eo_cld_id
				and a.tvou_sloc_id = b.eo_sloc_id
				and a.tvou_ho_org_id = b.eo_ho_org_id
				and a.tvou_eo_id = b.eo_id

				and d.doc_cld_id = a.tvou_cld_id
				and d.doc_org_id = a.tvou_org_id
				and d.doc_sloc_id = a.tvou_sloc_id
				and d.doc_txn_id = a.tvou_id

				and b.eo_bhav = 'D'

				and a.tvou_id = decode($P{PostFlag},'P','1','A',a.tvou_id,a.tvou_id)

	) a,

	( select adj_cld_id,
			adj_org_id,
			adj_appl_inst_id,
			adj_vou_id,
			adj_vou_dt,
			adj_arap_sl_no,
			adj_sl_no,
			adj_coa_id,
			sum(DECODE(adj_amt_typ,'Dr',radj_amt_adj,'Cr',-radj_amt_adj)) adj_amt_dtl
		from
		(select  a.adj_cld_id,
			a.adj_org_id,
			a.adj_appl_inst_id,
			a.adj_vou_id,
			trunc(b.gl_vou_dt) adj_vou_dt,
			a.adj_arap_sl_no, --dm 2014-05-09
			a.adj_sl_no,
			a.adj_coa_id,
			a.adj_amt_typ,
			a.radj_amt_adj
      From gl_adj a,
		   gl_lines b
      WHERE a.ADJ_CLD_ID = $P{cld_id}
			and   a.ADJ_HO_ORG_ID =  nvl($P{Org_ID},a.ADJ_HO_ORG_ID)
           and TRUNC(a.ADJ_MST_VOU_DT) <= to_date(NVL($P{AsOnDate},SYSDATE),'YYYY-MM-DD')
	   and TRUNC(a.RADJ_VOU_DT) <= to_date(NVL($P{AsOnDate},SYSDATE),'YYYY-MM-DD')
           and a.ADJ_COA_ID = nvl($P{Coa_ID},a.ADJ_COA_ID)
           and b.GL_CLD_ID = a.ADJ_CLD_ID
           and a.ADJ_ORG_ID = b.gl_ORG_ID
           and a.ADJ_VOU_ID = b.gl_vou_id
           and a.ADJ_SL_NO = b.gl_SL_NO
           and a.ADJ_COA_ID = b.gl_coa_id

	--union ar+tvou_adj+tvou_lines (for line_amount and tvou_id - radj_vou_id)
		union all

		select b.tvou_cld_id,
			   b.tvou_adj_org_id,
			   b.tvou_adj_appl_inst_id,
			   b.tvou_adj_vou_id,
			   trunc(c.tvou_dt) adj_vou_dt,
			   b.tvou_adj_arap_sl_no,
			   b.tvou_radj_sl_no,
			   b.tvou_adj_coa_id,
			   b.tvou_amt_typ,
			   b.tvou_radj_amt_adj
		from ar a,
			tvou_adj b,
			tvou_lines c
		where  b.tvou_cld_id = $P{cld_id}
		   and   b.tvou_ho_org_id =  nvl($P{Org_ID},b.tvou_ho_org_id)
           and TRUNC(b.tvou_mst_vou_dt) <= to_date(NVL($P{AsOnDate},SYSDATE),'YYYY-MM-DD')
		   and TRUNC(b.tvou_radj_vou_dt) <= to_date(NVL($P{AsOnDate},SYSDATE),'YYYY-MM-DD')
           and b.tvou_adj_coa_id = nvl($P{Coa_ID},b.tvou_adj_coa_id)

		   and a.gl_cld_id = b.tvou_cld_id
		   and a.ar_sloc_id = b.tvou_adj_sloc_id
		   and a.ar_org_id = b.tvou_adj_org_id
		   and a.ar_coa_id = b.tvou_adj_coa_id

           and b.tvou_cld_id = c.tvou_cld_id
		   and b.tvou_adj_sloc_id = c.TVOU_SLOC_ID
           and b.tvou_adj_org_id = c.tvou_org_id
           and b.tvou_radj_vou_id = c.tvou_id
           and b.tvou_radj_sl_no = c.tvou_sl_no
           and b.tvou_adj_coa_id = c.tvou_coa_id

			and a.gl_cld_id = c.tvou_cld_id
			and a.ar_sloc_id = c.tvou_sloc_id
			and a.ar_org_id = c.tvou_org_id
			and a.gl_ho_org_id = c.tvou_ho_org_id
			and a.ar_coa_id = c.tvou_coa_id
			and a.ar_eo_id = c.tvou_eo_id

			and c.tvou_id = decode($P{PostFlag},'P','1','A',c.tvou_id,c.tvou_id)

	--union ar+tvou_lines+tvou_adj_dtl(for line_amount and tvou_id - adj_vou_id)
		union all

		select b.tvou_cld_id,
				b.tvou_org_id,
				b.tvou_appl_inst_id,
				b.tvou_id,
				trunc(b.tvou_dt) adj_vou_dt,
				c.tvou_adj_arap_sl_no,
				c.tvou_adj_sl_no,
				c.tvou_adj_coa_id,
				c.tvou_amt_typ,
				c.tvou_radj_amt_adj
		from ar a,
			tvou_lines b,
			tvou_adj_dtl c
		where  a.gl_cld_id = $P{cld_id}
			and a.gl_ho_org_id = nvl($P{Org_ID},a.gl_ho_org_id)
			and a.ar_coa_id = nvl($P{Coa_ID},a.ar_coa_id)

			and a.gl_cld_id = b.tvou_cld_id
			and a.ar_sloc_id = b.tvou_sloc_id
			and a.ar_org_id = b.tvou_org_id
			and a.gl_ho_org_id = b.tvou_ho_org_id
			and a.ar_coa_id = b.tvou_coa_id
			and a.ar_eo_id = b.tvou_eo_id

			and b.tvou_cld_id = c.tvou_cld_id
			and b.tvou_sloc_id = c.tvou_adj_sloc_id
			and b.tvou_org_id = c.tvou_adj_org_id
			and b.tvou_ho_org_id = c.tvou_ho_org_id
			and b.tvou_coa_id = c.tvou_adj_coa_id
			and b.tvou_id = c.tvou_adj_vou_id
			and b.tvou_sl_no = c.tvou_adj_sl_no

			and b.tvou_id = decode($P{PostFlag},'P','1','A',b.tvou_id,b.tvou_id)

	--union ar+tvou_lines+tvou_adj_dtl(for line_amount and tvou_id - radj_vou_id)

	union all

		select b.tvou_cld_id,
				b.tvou_org_id,
				b.tvou_appl_inst_id,
				b.tvou_id,
				trunc(b.tvou_dt) adj_vou_dt,
				c.tvou_adj_arap_sl_no,
				c.tvou_radj_sl_no,
				c.tvou_adj_coa_id,
				c.tvou_amt_typ,
				c.tvou_radj_amt_adj
		from ar a,
			tvou_lines b,
			tvou_adj_dtl c
		where a.gl_cld_id = $P{cld_id}
			and a.gl_ho_org_id = nvl($P{Org_ID},a.gl_ho_org_id)
			and a.ar_coa_id = nvl($P{Coa_ID},a.ar_coa_id)

			and a.gl_cld_id = b.tvou_cld_id
			and a.ar_sloc_id = b.tvou_sloc_id
			and a.ar_org_id = b.tvou_org_id
			and a.gl_ho_org_id = b.tvou_ho_org_id
			and a.ar_coa_id = b.tvou_coa_id
			and a.ar_eo_id = b.tvou_eo_id

			and b.tvou_cld_id = c.tvou_cld_id
			and b.tvou_sloc_id = c.tvou_adj_sloc_id
			and b.tvou_org_id = c.tvou_adj_org_id
			and b.tvou_ho_org_id = c.tvou_ho_org_id
			and b.tvou_coa_id = c.tvou_adj_coa_id
			and b.tvou_id = c.tvou_radj_vou_id
			and b.tvou_sl_no = c.tvou_radj_sl_no

			and b.tvou_id = decode($P{PostFlag},'P','1','A',b.tvou_id,b.tvou_id)
		) group by  adj_cld_id,
					adj_org_id,
					adj_appl_inst_id,
					adj_vou_id,
					adj_vou_dt,
					adj_arap_sl_no,
					adj_sl_no,
					adj_coa_id
	) a1

	WHERE a.CLD_ID = $P{cld_id}
	and a.HO_ORG_ID = nvl($P{Org_ID},a.HO_ORG_ID)
	and DECODE($P{DateSelect},'V',TRUNC(a.VOU_DT),
		'I',TRUNC(NVL(a.EXT_DOC_DT,a.VOU_DT)),
		'D',TRUNC(NVL(a.AR_VOU_DUE_DT,a.VOU_DT)),
	TRUNC(a.VOU_DT) ) <= to_date(NVL($P{AsOnDate},SYSDATE),'YYYY-MM-DD')

	and a.coa_id = nvl($P{Coa_ID},a.coa_id)
	and a.cld_id = a1.adj_cld_id(+)
	and a.org_id = a1.adj_org_id(+)
	and a.coa_id = a1.adj_coa_id(+)
	and a.vou_id = a1.adj_vou_id(+)
	and a.ar_gl_sl_no = a1.adj_sl_no(+)
	and a.ar_sl_no = a1.adj_arap_sl_no(+)

        and 0 < CASE a.AMT_TYP WHEN 'Dr' THEN NVL(a.AMT_SP*a.AR_CC,0) + NVL(a1.ADJ_AMT_DTL*a.AR_CC,0)
            WHEN 'Cr' THEN ABS(  -NVL(a.AMT_SP*a.AR_CC,0) + NVL(a1.ADJ_AMT_DTL*a.AR_CC,0)) END

    ) A,
            APP.ORG B,
            FIN$COA C,
            --APP.APP$DOC$TXN D,
            APP.APP$GLBL$CURR E,
            FIN.FIN$ACC$NA X1,
			APP.APP$EO X2 ,
			APP.APP$EO$PRF X3 ,
			APP.APP$EO$PERS$COMM X4
