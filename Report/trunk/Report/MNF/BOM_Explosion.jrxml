	<queryString>
		<![CDATA[SELECT DISTINCT  A.HO_ORG_ID, P.ORG_DESC  , A.BOM_ID,  A.BOM_DESC,  A.BOM_RMRK,
  A.DOC_ID,  A.BOM_REV_NO,  DECODE(A.BOM_ACTV,'Y','Yes', 'N','No - On ') BOM_ACTV,  A.BOM_INACTTV_DT, A.BOM_INACTV_REASON,    A.BOM_LEG_CD,   A.OUTPT_ITM_LEAD_TM LD_TM,
  A.BOM_REV_NO,   A.BOM_REV_DT,   A.VLD_FRM_DT, A.VLD_END_DT,
  C.ATT_NM BOM_TYPE_DESC,  D.ATT_NM BOM_STAT_DESC,  A.BOM_MODE,  E.ATT_NM BOM_MODE_DESC,
  A.USR_ID_CREATE_DT,  H.USR_NAME USR_ID_CREATE_NM,  I.DOC_ID RT_DOC_ID,  I.RT_ID DISP_RT_ID,   I.RT_DESC,  I.RT_REV_NO,
  J.ATT_NM BOM_ATT_DESC,   A.OUTPT_ITM_ID,  A.OUTPT_ITM_QTY,  L.ITM_DESC OUTPT_ITM_DESC,   O.UOM_NM OUTPT_ITM_UOM,B.ITM_ID,
  B.ITM_TYPE ITM_TYPE_ID,  M.ITM_DESC INPT_ITM_DESC,  initcap(F.ATT_NM) ITM_TYPE_DESC,  ITM_QTY,  ITM_UOM,  K.UOM_NM, B.ITM_PRICE, B.ITM_TOT_AMT,
  OP.OP_DESC, OP.OP_ID , B.OP_SR_NO
FROM MNF$BOM A,  MNF$BOM$OP$ITM B, MNF$OP OP,  MNF$DS$ATT C,  MNF$DS$ATT D,  MNF$DS$ATT E,  MNF$DS$ATT F,
  APP.APP$SEC$USR H,  MNF$RT I,  MNF$DS$ATT J,  APP.APP$UOM$CONV$STD K,  APP.APP$ITM$PRF L,  APP.APP$ITM$PRF M,
  ORG$MNF$BOM N,  APP.APP$UOM$CONV$STD O,  APP.ORG P
WHERE A.CLD_ID      = B.CLD_ID
AND A.SLOC_ID       = B.SLOC_ID
AND A.HO_ORG_ID     = B.HO_ORG_ID
AND A.DOC_ID        = B.DOC_ID
AND A.BOM_TYPE      = C.ATT_ID
AND A.BOM_STAT      = D.ATT_ID
AND A.BOM_MODE      = E.ATT_ID
AND A.SLOC_ID       = H.SLOC_ID
AND A.USR_ID_CREATE = H.USR_ID
AND A.CLD_ID        = I.CLD_ID
AND A.SLOC_ID       = I.SLOC_ID
AND A.HO_ORG_ID     = I.HO_ORG_ID
AND A.RT_ID         = I.DOC_ID
AND A.BOM_ATT       = J.ATT_ID
AND B.ITM_TYPE      = F.ATT_ID
AND B.CLD_ID        = K.CLD_ID
AND B.SLOC_ID       = K.SLOC_ID
AND B.ITM_UOM       = K.UOM_ID
AND A.CLD_ID        = L.CLD_ID
AND A.SLOC_ID       = L.SLOC_ID
AND A.HO_ORG_ID     = L.HO_ORG_ID
AND A.OUTPT_ITM_ID  = L.ITM_ID
AND B.CLD_ID        = M.CLD_ID
AND B.SLOC_ID       = M.SLOC_ID
AND B.HO_ORG_ID     = M.HO_ORG_ID
AND B.ITM_ID        = M.ITM_ID
AND L.CLD_ID        = O.CLD_ID
AND L.SLOC_ID       = O.SLOC_ID
AND L.UOM_SLS       = O.UOM_ID
--AND B.ITM_TYPE     NOT  IN( 63, 67)
--AND A.DOC_ID        = N.DOC_ID
AND A.CLD_ID        = N.CLD_ID
AND A.SLOC_ID       = N.SLOC_ID
AND A.HO_ORG_ID     = N.HO_ORG_ID
AND n.CLD_ID        = P.ORG_CLD_ID
AND n.SLOC_ID        = P.ORG_CREATE_REF_SLOC_ID
AND n.ORG_ID        = P.ORG_ID
AND B.CLD_ID = OP.CLD_ID
AND B.SLOC_ID= OP.SLOC_ID
AND B.HO_ORG_ID = OP.HO_ORG_ID
AND B.OP_ID = OP.DOC_ID
AND a.cld_id    = NVL ($P{CldId}, a.cld_id)
AND a.sloc_id   = NVL ($P{SlocId}, a.sloc_id)
AND a.ho_org_id = NVL ($P{HoOrgId}, a.ho_org_id)
AND n.org_id    = NVL ($P{OrgId}, n.org_id)
AND a.doc_id    = NVL ($P{BomDocId}, a.doc_id)
AND B.OP_ID = NVL($P{OpDocId}, B.OP_ID)
ORDER BY A.BOM_ID, B.OP_SR_NO, B.ITM_TYPE DESC]]>
