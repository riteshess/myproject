		<![CDATA[SELECT  A.HO_ORG_ID,  A.ORG_ID,   O.ORG_DESC,   A.DOC_ID, A.DOC_DT, A.RC_ID,  A.MRS_ID
        ,B.MRS_NO,    B.MRS_DT, C.ITM_ID,   P.ITM_DESC,   C.ITM_UOM,   Q.UOM_NM,  C.REQ_QTY,   C.AUTH_QTY
        ,C.STK_RESV_QTY,    C.ISSU_QTY,   C.PEND_QTY , (SELECT ATT_NM FROM APP.APP$DS$ATT WHERE ATT_ID = B.Mrs_Stat)STAT
FROM MNF$RC A,   MM.MM$MRS B,  MM.MM$MRS$ITM C,   APP.ORG O,    APP.APP$ITM$PRF P,  APP.APP$UOM$CONV$STD Q
WHERE A.CLD_ID    = B.CLD_ID
AND   A.SLOC_ID   = B.SLOC_ID
AND   A.ORG_ID    = B.ORG_ID
AND   A.MRS_ID    = B.MRS_TXN_ID
AND   B.CLD_ID    = C.CLD_ID
AND   B.SLOC_ID   = C.SLOC_ID
AND   B.ORG_ID    = C.ORG_ID
AND   B.MRS_TXN_ID= C.MRS_TXN_ID
AND   A.CLD_ID    = O.ORG_CLD_ID
AND   A.SLOC_ID   = O.ORG_CREATE_REF_SLOC_ID
AND   A.ORG_ID    = O.ORG_ID
AND   C.CLD_ID    = P.CLD_ID
AND   C.SLOC_ID   = P.SLOC_ID
AND   MM.MM_GET_HO_ORG(C.CLD_ID, C.SLOC_ID,C.ORG_ID)    = P.HO_ORG_ID
AND   C.ITM_ID     = P.ITM_ID
AND   C.CLD_ID     = Q.CLD_ID
AND   C.SLOC_ID    = Q.SLOC_ID
AND   C.ITM_UOM    = Q.UOM_ID
AND   A.CLD_ID    = $P{CldId}
AND   A.SLOC_ID   = $P{SlocId}
AND   A.ORG_ID    = $P{OrgId}
AND   A.HO_ORG_ID = $P{HoOrgId}
AND   C.ITM_ID  = NVL($P{ItmId}, c.ITM_ID)
AND   A.DOC_ID    = NVL($P{RcDocId}, A.DOC_ID)
AND   TRUNC(A.DOC_DT) >= NVL(TO_DATE($P{FromDate}, 'yyyy-mm-dd'),TRUNC(A.DOC_DT))
AND   TRUNC(A.DOC_DT) <= NVL(TO_DATE($P{ToDate}, 'yyyy-mm-dd'),TRUNC(A.DOC_DT))
ORDER BY A.RC_ID, A.DOC_DT DESC]]>
	</queryString>
