	<queryString>
		<![CDATA[SELECT DISTINCT A.HO_ORG_ID,  A.ORG_ID, D.ORG_DESC, A.DOC_ID,  A.DOC_DT,  A.JC_ID,  A.JC_STAT, A.OP_SR_NO,
      B.ITM_TYPE,  B.ITM_ID, C.ITM_DESC,  B.ITM_UOM, E.UOM_NM, B.ITM_QTY,  B.ITM_PRICE,  B.ITM_TOT_AMT,  B.ITM_SCRAP_PER
FROM MNF$JC A, MNF$JC$OP$ITM B, APP.APP$ITM$PRF C, APP.ORG D, APP.APP$UOM$CONV$STD E
WHERE A.CLD_ID  = B.CLD_ID
AND   A.SLOC_ID = B.SLOC_ID
AND   A.ORG_ID  = B.ORG_ID
AND   A.HO_ORG_ID = B.HO_ORG_ID
AND   A.DOC_ID  = B.DOC_ID
AND   B.CLD_ID  = C.CLD_ID
AND   B.SLOC_ID = C.SLOC_ID
AND   B.HO_ORG_ID = C.HO_ORG_ID
AND   B.ITM_ID    = C.ITM_ID
AND   A.CLD_ID    = D.ORG_CLD_ID
AND   A.SLOC_ID   = D.ORG_CREATE_REF_SLOC_ID
AND   A.ORG_ID    = D.ORG_ID
AND   A.JC_STAT   = 74
AND   B.CLD_ID    = E.CLD_ID
AND   B.SLOC_ID   = E.SLOC_ID
AND   B.ITM_UOM   = E.UOM_ID
AND   B.ITM_TYPE  = 62
   AND a.cld_id = NVL ($P{CldId}, a.cld_id)
   AND a.sloc_id = NVL ($P{SlocId}, a.sloc_id)
   AND a.ho_org_id = NVL ($P{HoOrgId}, a.ho_org_id)
   AND a.org_id = NVL ($P{OrgId}, a.org_id)
   AND a.doc_id = NVL ($P{JcDocId} , a.doc_id)
   AND A.OUTPT_ITM_ID = NVL($P{ItmId}, A.OUTPT_ITM_ID)
 	AND B.OP_ID = NVL($P{OpDocId}, B.OP_ID)
   and trunc(a.doc_dt) >= NVL(TO_DATE($P{FromDate},'yyyy-MM-DD'),trunc(a.doc_dt))
    and trunc(a.doc_dt) <= NVL(TO_DATE($P{ToDate},'yyyy-MM-DD'),trunc(a.doc_dt))
ORDER BY A.jc_id DESC]]>
	</queryString>
