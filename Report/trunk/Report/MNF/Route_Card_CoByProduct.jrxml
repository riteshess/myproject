	<queryString>
		<![CDATA[SELECT a.cld_id,
  a.sloc_id,
  a.ho_org_id,
  a.org_id,
  a.doc_id,
  a.doc_dt,
  a.rc_id disp_doc_id,
  a.outpt_itm_id,
  a.outpt_itm_qty,
  rc_stat doc_stat_id,
  rc_mode doc_mode_id,
  b.att_nm doc_stat,
  c.att_nm doc_mode,
  d.itm_desc,
  e.itm_id,
  e.itm_qty,
  f.itm_desc,
  h.org_desc,
  i.uom_nm  ,
  CASE
    WHEN itm_type=64
    THEN 'Co-Prduct'
    WHEN itm_type=65
    THEN 'By-Product'
  END itm_type_desc
FROM mnf$rc a,
  mnf$ds$att b,
  mnf$ds$att c,
  app.app$itm$prf d,
  mnf$rc$op$itm e,
  app.app$itm$prf f,
  (SELECT x.cld_id,
    x.sloc_id,
    x.ho_org_id,
    x.doc_id,
    x.itm_id outpt_itm_id,
    x.itm_qty outpt_itm_qty
  FROM mnf$rc$op$itm x
  WHERE x.op_sr_no =
    (SELECT MAX (op_sr_no) FROM mnf$rc$op$itm y
    )
  AND itm_type = 67
  ) g,
  app.org h,
  app.app$uom$conv$std i
WHERE g.cld_id     = d.cld_id
AND g.sloc_id      = d.sloc_id
AND g.ho_org_id    = d.ho_org_id
AND g.outpt_itm_id = d.itm_id
AND a.rc_stat      = b.att_id
AND a.rc_mode      = c.att_id
AND a.doc_id       =e.doc_id
AND e.cld_id       = f.cld_id
AND e.sloc_id      = f.sloc_id
AND e.ho_org_id    = f.ho_org_id
AND e.itm_id       = f.itm_id
AND e.itm_type    IN (64,65)
AND a.doc_id       = g.doc_id
AND a.cld_id       = h.org_cld_id
AND a.org_id       = h.org_id
AND a.cld_id       = i.cld_id
AND a.sloc_id      = i.sloc_id
AND e.itm_uom    =   i.uom_id
AND a.cld_id       = NVL ($P{CldId}, a.cld_id)
AND a.sloc_id      = NVL ($P{SlocId}, a.sloc_id)
AND a.ho_org_id    = NVL ($P{HoOrgId}, a.ho_org_id)
AND a.org_id       = NVL ($P{OrgId}, a.org_id)
AND a.doc_id       = NVL ($P{RcDocId} , a.doc_id)
AND TRUNC(A.DOC_DT) >= NVL(TO_DATE($P{FromDate},'yyyy-MM-dd'), TRUNC(A.DOC_DT))
AND TRUNC(A.DOC_DT) <= NVL(TO_DATE($P{ToDate},'yyyy-MM-dd'), TRUNC(A.DOC_DT))]]>
	</queryString>
