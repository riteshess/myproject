	<queryString>
		<![CDATA[SELECT A.HO_ORG_ID,        A.ORG_ID,              F.ORG_DESC,   A.DOC_ID,  A.DOC_DT MOVE_DT,   A.JE_ID MSN,    A.OUTPT_ITM_ID,    E.ITM_DESC
      ,A.OUTPT_ITM_QTY,    A.ACT_OUTPT_ITM_QTY
      ,   D.RC_ID,    D.DOC_DT
FROM  MNF$JE A,    MNF$JE$SRC C, MNF$RC D, APP.APP$ITM$PRF E,    APP.ORG F
WHERE A.CLD_ID    = C.CLD_ID
AND   A.SLOC_ID   = C.SLOC_ID
AND   A.ORG_ID    = C.ORG_ID
AND   A.HO_ORG_ID  = C.HO_ORG_ID
AND   A.DOC_ID    = C.DOC_ID
AND   C.CLD_ID    = D.CLD_ID
AND   C.SLOC_ID   = D.SLOC_ID
AND   C.ORG_ID    = D.ORG_ID
AND   C.HO_ORG_ID = D.HO_ORG_ID
AND   C.DOC_ID_SRC = D.DOC_ID
AND   A.CLD_ID    = E.CLD_ID
AND   A.SLOC_ID   = E.SLOC_ID
AND   A.HO_ORG_ID = E.HO_ORG_ID
AND   A.OUTPT_ITM_ID = E.ITM_ID
AND   A.CLD_ID    = F.ORG_CLD_ID
AND   A.SLOC_ID   = F.ORG_CREATE_REF_SLOC_ID
AND   A.ORG_ID    = F.ORG_ID
AND   A.JE_STAT= 74
AND   A.CLD_ID    = $P{CldId}
AND   A.SLOC_ID   = $P{SlocId}
AND   A.ORG_ID    = $P{OrgId}
AND   A.HO_ORG_ID = $P{HoOrgId}
AND   A.OUTPT_ITM_ID  = NVL($P{ItmId}, A.OUTPT_ITM_ID)
and   d.doc_id = nvl($P{RcDocId},d.doc_id)
AND   A.DOC_ID    = NVL($P{JeDocId}, A.DOC_ID)
AND   TRUNC(A.DOC_DT) >= NVL(TO_DATE($P{FromDate}, 'yyyy-mm-dd'),TRUNC(A.DOC_DT))
AND   TRUNC(A.DOC_DT) <= NVL(TO_DATE($P{ToDate}, 'yyyy-mm-dd'),TRUNC(A.DOC_DT))
ORDER BY d.Rc_ID ,A.JE_ID DESC]]>
	</queryString>
