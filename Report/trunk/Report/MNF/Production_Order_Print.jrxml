	<queryString>
		<![CDATA[SELECT  A.HO_ORG_ID, A.ORG_ID,  Q.ORG_DESC,A.PDO_ID,   A.DOC_DT, A.PDO_RMRK,  A.DOC_ID, A.PDO_REV_NO, A.PDO_REV_DT,a.pln_strt_tm, a.pln_end_tm,
B.OP_ID OP_DOC_ID,
          F.OP_TYPE, B.WC_ID, B.OP_LOC_TYPE, F.OP_DESC, G.WC_DESC,  A.PDO_STAT,D.ATT_NM PDO_STAT_DESC,
          A.PDO_MODE, E.ATT_NM PDO_MODE_DESC, A.USR_ID_CREATE_DT, H.USR_NAME USR_ID_CREATE_NM, I.DOC_ID RT_DOC_ID, I.RT_ID DISP_RT_ID,
          I.RT_REV_NO, I.RT_DESC, A.OUTPT_ITM_ID, A.OUTPT_ITM_QTY, A.ITM_BATCH_NO,  O.ITM_ID INPT_ITM_ID, O.ITM_TYPE ITM_TYPE_ID,
          R.ATT_NM ITM_TYPE_DESC, O.ITM_QTY, O.ITM_UOM, K.UOM_NM, O.ITM_PRICE,  O.ITM_TOT_AMT, O.ITM_SCRAP_PER, O.ITM_RTRN_QTY,
          L.ITM_DESC OUTPT_ITM_DESC, M.ITM_DESC INPT_ITM_DESC,  F.OP_ID, B.OP_SR_NO   ,P.ATT_NM ISSU_TYP_NM
          ,S.BOM_ID,  S.BOM_DESC
     FROM MNF$PDO A,          MNF$PDO$OP B,          MNF$DS$ATT D,          MNF$DS$ATT E,          MNF$OP F,
          MNF$WC G,          APP.APP$SEC$USR H,          MNF$RT I,          APP.APP$UOM$CONV$STD K,          APP.APP$ITM$PRF L,
          APP.APP$ITM$PRF M,          MNF$PDO$OP$ITM O,          MNF$DS$ATT R,          APP.ORG Q,          MNF$DS$ATT P, MNF$BOM S
    WHERE A.CLD_ID = B.CLD_ID
      AND A.SLOC_ID = B.SLOC_ID
      AND A.HO_ORG_ID = B.HO_ORG_ID
      AND A.DOC_ID = B.DOC_ID
      AND A.PDO_STAT = D.ATT_ID
      AND A.PDO_MODE = E.ATT_ID
      AND B.CLD_ID = F.CLD_ID
      AND B.SLOC_ID = F.SLOC_ID
      AND B.HO_ORG_ID = F.HO_ORG_ID
      AND B.OP_ID = F.DOC_ID
      AND B.CLD_ID = G.CLD_ID
      AND B.SLOC_ID = G.SLOC_ID
      AND B.HO_ORG_ID = G.HO_ORG_ID
      AND B.WC_ID = G.WC_ID
      AND A.SLOC_ID = H.SLOC_ID
      AND A.USR_ID_CREATE = H.USR_ID
      AND A.CLD_ID = I.CLD_ID
      AND A.SLOC_ID = I.SLOC_ID
      AND A.HO_ORG_ID = I.HO_ORG_ID
      AND A.RT_ID = I.DOC_ID
      AND O.ITM_TYPE = R.ATT_ID
    AND O.CLD_ID = K.CLD_ID
    AND O.SLOC_ID = K.SLOC_ID
    AND O.ITM_UOM = K.UOM_ID
    AND A.CLD_ID = L.CLD_ID
    AND A.SLOC_ID = L.SLOC_ID
    AND A.HO_ORG_ID = L.HO_ORG_ID
    AND A.OUTPT_ITM_ID = L.ITM_ID
    AND O.CLD_ID = M.CLD_ID
    AND O.SLOC_ID = M.SLOC_ID
    AND O.HO_ORG_ID = M.HO_ORG_ID
    AND A.CLD_ID  = Q.ORG_CLD_ID
    AND A.SLOC_ID  = Q.ORG_CREATE_REF_SLOC_ID
    AND A.ORG_ID = Q.ORG_ID
    AND O.ITM_ID = M.ITM_ID
    AND B.DOC_ID=O.DOC_ID
    AND B.OP_ID=O.OP_ID
    AND B.OP_SR_NO=O.OP_SR_NO
    AND O.ISSU_TYPE = P.ATT_ID
    AND A.CLD_ID  = S.CLD_ID
    AND A.SLOC_ID = S.SLOC_ID
    AND A.HO_ORG_ID = S.HO_ORG_ID
    AND   A.BOM_ID  = S.DOC_ID
    and a.cld_id=nvl($P{CldId},a.cld_id)
            and a.sloc_id=nvl($P{SlocId},a.sloc_id)
            and a.ho_org_id=nvl($P{HoOrgId} ,a.ho_org_id)
            and a.org_id=nvl($P{OrgId} ,a.org_id)
            and a.doc_id=nvl($P{PdoDocId},a.doc_id)
           and trunc(a.doc_dt) >= NVL(TO_DATE($P{FromDate},'yyyy-MM-DD'),trunc(a.doc_dt))
	  and trunc(a.doc_dt) <= NVL(TO_DATE($P{ToDate},'yyyy-MM-DD'),trunc(a.doc_dt))

order by 4,  b.op_sr_no,O.ITM_TYPE]]>
