	<queryString>
		<![CDATA[SELECT a.cld_id,
    a.sloc_id,
    a.org_id,
    a.wh_id,
    a.fy_id,
    a.itm_id,
    b.itm_desc,
    a.rqmt_area_id,
    a.itm_uom_bs,
    a.rcpt_stk,
    a.consm_stk,
    a.ret_stk,
    a.usr_id_mod,
    a.usr_id_mod_dt,
    a.avl_stk,
    a.req_stk,
    c.uom_desc,
    d.org_desc
  FROM mm.mm$stk$summ$itm$rqmt a,
    app.app$itm$prf b,
      app.app$uom$conv$std c,
    app.org d
  WHERE a.cld_id                                 = b.cld_id
  AND a.sloc_id                                  = b.sloc_id
  AND a.itm_id                                   = b.itm_id
  AND app.FN_GET_PARENT_ORG_L0(a.cld_id,a.org_id)=b.ho_org_id
  AND a.cld_id                                   = c.cld_id
  AND a.sloc_id                                  = c.sloc_id
  AND a.itm_uom_bs                               = c.uom_id
  AND a.cld_id                                   = d.org_cld_id
  AND a.sloc_id                                  = d.org_create_ref_sloc_id
  AND a.org_id                                   = d.org_id
  AND a.cld_id                                   = NVL ($P{CldId}, a.cld_id)
  AND a.sloc_id                                  = NVL ($P{SlocId}, a.sloc_id)
  AND a.org_id                                   = NVL ($P{OrgId}, a.org_id)
  AND a.itm_id                                   = NVL ($P{ItmId}, a.itm_id)]]>
	</queryString>
