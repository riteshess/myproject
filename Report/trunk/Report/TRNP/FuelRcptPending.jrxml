	<queryString>
		<![CDATA[SELECT A.ORG_ID, C.ORG_DESC, A.HO_ORG_ID, A.DOC_ID, A.EO_ID,  D.EO_NM DRIVER_NM , A.INV_NO, A.INV_DT, A.CURR_ID_SP, G.GLBL_CURR_NOTATION_ALIAS curr
    ,A.CURR_CONV_FCTR, A.FUEL_QTY, A.UOM, E.UOM_NM, A.AMT_SP, A.AMT_BS, A.STATUS,     F.ATT_NM STAT_NM,  A.FUEL_RATE, A.VEHICLE_NO
FROM SLS$TRNP$OUTSTN$FUEL$DTL A,  APP.ORG C, APP.APP$EO D, APP.APP$UOM$CONV$STD E, SLS.SLS$DS$ATT F, APP.APP$GLBL$CURR G
WHERE A.ORG_ID = C.ORG_ID
AND   A.CLD_ID = D.EO_CLD_ID
AND   A.SLOC_ID = D.EO_SLOC_ID
AND   A.HO_ORG_ID = D.EO_HO_ORG_ID
AND   A.DRIVER_EO_ID = D.EO_ID
AND   A.UOM = E.UOM_ID
AND   A.STATUS = F.ATT_ID
AND   A.CURR_ID_SP = G.GLBL_CURR_ID
AND A.STATUS = 19
AND	A.CLD_ID = NVL($P{CldId},A.CLD_ID)
AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
AND	A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
AND A.HO_ORG_ID = NVL($P{HoOrgId},A.HO_ORG_ID)
AND A.DOC_ID =NVL($P{SuppInvcDocId}, A.DOC_ID)
and nvl(a.driver_eo_id,'0')=nvl($P{DriverId}, nvl(a.driver_eo_id,'0'))
and a.vehicle_no = nvl($P{VehicleNo}, a.vehicle_no)
AND TRUNC(A.INV_DT) BETWEEN NVL(TO_DATE($P{FromDate},'YYYY-MM-DD'),TRUNC(A.INV_DT)) AND NVL(TO_DATE($P{ToDate},'YYYY-MM-DD'),TRUNC(A.INV_DT))]]>
