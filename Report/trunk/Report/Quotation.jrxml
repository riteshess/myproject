	<queryString>
		<![CDATA[SELECT
A.CLD_ID,A.SLOC_ID,A.ORG_ID,A.ORG_DESC,A.QUOT_ID,A.QUOT_DT,A.EO_NM,A.QUOT_STAT,
A.ITM_ID,A.ITM_DESC,A.UOM_DESC,A.QUOT_QTY,A.ITM_PRICE,
Decode(A.Disc_Type,'P',((A.Amt*A.Disc_Val)/100)
                  ,'A',A.DISC_VAL) QUOT_DISC,
A.DISCOUNT,
A.AMT,
A.TAX_AMT,
A.Oc_Amt,
A.Oc_Desc,A.Doc_Id,A.Tax_Rule_Id, A.Quot_Amt_Sp,A.Valid_Fm_Dt,A.Valid_To_Dt,A.Vldt_Tlrnc_Days,
A.Adds,A.Rfq_Id,A.Rfq_Dt,A.Curr_Desc
From
(Select A.Cld_Id,A.Sloc_Id,A.Org_Id,E.Org_Desc,A.Quot_Id,A.Quot_Dt,F.Eo_Nm,G.Att_Nm Quot_Stat,
B.ITM_ID,H.ITM_DESC,I.UOM_DESC,B.QUOT_QTY,B.ITM_PRICE,A.DISC_TYPE,
A.DISC_VAL,
DECODE(B.DISC_TYPE,'P',(B.QUOT_QTY *(B.ITM_PRICE * B.DISC_VAL/100)),
                   'A',( B.DISC_VAL)) DISCOUNT,
DECODE(B.DISC_TYPE,'P',((B.QUOT_QTY *B.ITM_PRICE) - ((B.QUOT_QTY *B.ITM_PRICE * B.DISC_VAL)/100)),
                   'A',((B.QUOT_QTY *B.ITM_PRICE) - B.DISC_VAL)) AMT,
sum(nvl(C.TAX_AMT,0)) TAX_AMT,
       SUM(nvl(D.OC_AMT_SP,0))OC_AMT,
                D.Oc_Desc,A.Doc_Id,C.Tax_Rule_Id,  A.Quot_Amt_Sp,A.Valid_Fm_Dt,A.Valid_To_Dt,
                A.VLDT_TLRNC_DAYS,L.ADDS,M.RFQ_ID,to_char(M.DOC_DT,'dd-MM-yyyy') RFQ_DT,n.curr_desc
                 FROM MM$QUOT A,MM$QUOT$ITM B,MM$QUOT$TR C,MM$QUOT$OC D,APP.ORG E,
                 APP.APP$EO F,APP.APP$DS$ATT G,APP.APP$ITM$PRF H,APP.APP$UOM$CONV$STD I,
                 APP.APP$ADDS$VW  L,MM$RFQ M,app.app$curr n
                 WHERE A.CLD_ID =B.CLD_ID
                 AND A.SLOC_ID =B.SLOC_ID
                 AND A.ORG_ID =B.ORG_ID
                 AND A.DOC_ID =B.DOC_ID
                 AND A.CLD_ID =C.CLD_ID(+)
                 AND A.SLOC_ID =C.SLOC_ID(+)
                 AND A.ORG_ID =C.ORG_ID(+)
                 AND A.DOC_ID =C.DOC_ID(+)
                 AND A.CLD_ID =D.CLD_ID(+)
                 AND A.SLOC_ID =D.SLOC_ID(+)
                 AND A.ORG_ID =D.ORG_ID(+)
                 AND A.DOC_ID =D.DOC_ID(+)
                 AND A.ORG_ID =E.ORG_ID
                 AND A.CLD_ID =F.EO_CLD_ID
                 AND A.SLOC_ID =F.EO_SLOC_ID
                 AND MM.MM_GET_HO_ORG(A.CLD_ID,A.SLOC_ID,A.ORG_ID)=F.EO_HO_ORG_ID
                 AND A.EO_ID =F.EO_ID
                 AND A.QUOT_STATUS =G.ATT_ID
                 AND A.CLD_ID =L.CLD_ID(+)
                 AND A.SLOC_ID =L.SLOC_ID(+)
                 AND A.BILL_ADDS_ID =L.ADDS_ID(+)
                 AND A.CLD_ID =M.CLD_ID (+)
                 AND A.SLOC_ID =M.SLOC_ID(+)
                 AND A.ORG_ID = M.ORG_ID(+)
                 AND A.REF_DOC_ID =M.DOC_ID(+)
                 and a.Cld_Id = n.cld_id
                 and a.sloc_id = n.curr_sloc_id
                 and a.curr_id_sp = n.curr_id
                 AND B.CLD_ID =H.CLD_ID
                 AND B.SLOC_ID =H.SLOC_ID
                 AND B.ITM_ID =H.ITM_ID
                 AND B.CLD_ID =I.CLD_ID
                 AND B.SLOC_ID =I.SLOC_ID
                 AND B.ITM_UOM =I.UOM_ID
                 And A.Cld_Id= nvl($P{CldId},A.Cld_Id)
                 AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
                 AND A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
                 AND A.QUOT_ID = NVL($P{QuotId},A.QUOT_ID)
                 AND A.DOC_ID = $P{DocId}
group by
A.CLD_ID,A.SLOC_ID,A.ORG_ID,E.ORG_DESC,A.QUOT_ID,A.QUOT_DT,F.EO_NM,G.ATT_NM ,
B.ITM_ID,H.ITM_DESC,I.UOM_DESC,B.QUOT_QTY,B.ITM_PRICE,
DECODE(B.DISC_TYPE,'P',(B.QUOT_QTY *(B.ITM_PRICE * B.DISC_VAL/100)),
                   'A',( B.DISC_VAL)),
DECODE(B.DISC_TYPE,'P',((B.QUOT_QTY *B.ITM_PRICE) - ((B.QUOT_QTY *B.ITM_PRICE * B.DISC_VAL)/100)),
                   'A',((B.QUOT_QTY *B.ITM_PRICE) - B.DISC_VAL)),
                D.OC_DESC,A.DOC_ID,c.tax_rule_id,  A.QUOT_AMT_SP,
                A.VALID_FM_DT,A.VALID_TO_DT,A.VLDT_TLRNC_DAYS,
L.ADDS,M.RFQ_ID,to_char(M.DOC_DT,'dd-MM-yyyy'),A.DISC_VAL,A.DISC_TYPE,n.curr_desc
Order By A.Quot_Id) A
ORDER BY A.ORG_ID,A.QUOT_ID,A.ITM_ID]]>
