	<queryString>
		<![CDATA[SELECT A.RQMT_AREA_ID,A.RQMT_AREA_NM,B.ADDS,C.WH_NM,
DECODE(A.PRODUCTION_FLG,'Y','Yes'
                       ,'N','No') PROD_FLG,
DECODE(A.COST_CENTRE_FLG,'Y','Yes'
                       ,'N','No') COST_CENTER,D.ORG_DESC
FROM APP.APP$RQMT$AREA A,APP.APP$ADDS$VW B,APP.APP$WH C,APP.ORG D
WHERE A.CLD_ID =B.CLD_ID
AND   A.SLOC_ID =B.SLOC_ID
AND   A.ADDS_ID =B.ADDS_ID
AND   A.CLD_ID =C.CLD_ID
AND   A.SLOC_ID = C.SLOC_ID
AND   A.DFLT_WH =C.WH_ID
AND   A.CLD_ID =D.ORG_CLD_ID
AND   A.ORG_ID = D.ORG_ID
AND   A.CLD_ID = NVL($P{CldId},A.CLD_ID)
AND   A.SLOC_ID =NVL( $P{SlocId},A.SLOC_ID)
AND   A.ORG_ID = $P{OrgId}
AND   A.DFLT_WH = NVL($P{WhId},A.DFLT_WH)
AND   A.PRODUCTION_FLG = NVL($P{ProdFlag},A.PRODUCTION_FLG)
AND   A.COST_CENTRE_FLG = NVL($P{CostCntrFlg},A.COST_CENTRE_FLG)
AND   A.RQMT_AREA_ID = NVL($P{RqmtAreaId},A.RQMT_AREA_ID)]]>
	</queryString>
