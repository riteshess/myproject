	<queryString>
		<![CDATA[SELECT A.CLD_ID,
       A.SLOC_ID,
       A.ORG_ID,
       E.ORG_DESC,
       A.DOC_ID,
       NVL(A.AUTH_PO_NO,A.PO_ID) PO_NO,
       A.PO_DT,
       A.AMD_NO,
       A.AMD_DT,
       A.EO_ID,
       F.EO_NM,
       M.QUOT_ID,M.QUOT_DT,A.PO_AMT_BS,A.PO_AMT_SP
      FROM MM$DRFT$PO A,APP.ORG E,
           APP.APP$EO F,MM$QUOT M
                WHERE  A.ORG_ID =E.ORG_ID
                 AND A.CLD_ID =F.EO_CLD_ID
                 AND A.SLOC_ID =F.EO_SLOC_ID
                AND MM.MM_GET_HO_ORG(A.CLD_ID,A.SLOC_ID,A.ORG_ID) =F.EO_HO_ORG_ID
                 AND A.EO_ID =F.EO_ID
                 AND A.CLD_ID =M.CLD_ID(+)
                 AND A.SLOC_ID =M.SLOC_ID(+)
                 AND A.ORG_ID = M.ORG_ID(+)
                 AND A.QUOT_DOC_ID = M.DOC_ID (+)
                 AND A.CLD_ID= NVL( $P{CldId}  ,A.CLD_ID)
                 AND A.SLOC_ID = NVL($P{SlocId} ,A.SLOC_ID)
                 AND A.ORG_ID = NVL( $P{OrgId} ,A.ORG_ID)
                 AND A.DOC_ID =nvl($P{DocId},a.doc_id)
                 AND A.PO_DT >=NVL(TO_DATE($P{FromDate},'yyyy-mm-dd'),A.PO_DT)
                 AND A.PO_DT <=NVL(TO_DATE($P{ToDate},'yyyy-mm-dd'),A.PO_DT)
                 AND A.PO_STATUS = NVL($P{PoStat},A.PO_STATUS)
                 AND A.EO_ID = NVL($P{EoId},A.EO_ID)
                 ORDER BY A.ORG_ID,NVL(A.AUTH_PO_NO,A.PO_ID),F.EO_NM]]>
	</queryString>
