	<queryString>
		<![CDATA[SELECT A.ISSU_NO,A.ISSU_DT,B.ITM_ID,C.ITM_DESC,G.LOT_ID,A.ORG_ID,
D.UOM_DESC,round(B.ISSU_QTY,nvl($P{roundoff},0)),
