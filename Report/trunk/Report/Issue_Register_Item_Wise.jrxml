F.ORG_DESC
,ROUND(H.LND_PRICE,NVL($P{roundoff},0)),
(round(B.ISSU_QTY,nvl($P{roundoff},0)) * round(H.LND_PRICE,nvl($P{roundoff},0))) val_issud_qty
FROM MM$MTL$ISSU A,MM$MTL$ISSU$ITM B,APP.APP$ITM$PRF C,
APP.APP$UOM$CONV$STD D,APP.APP$RQMT$AREA E,APP.ORG F,MM$MTL$ISSU$LOT G,MM$STK$SUMM$LOT H
WHERE A.CLD_ID =B.CLD_ID
AND A.SLOC_ID =B.SLOC_ID
AND A.ORG_ID =B.ORG_ID
AND A.WH_ID = B.WH_ID
AND A.DOC_ID =B.DOC_ID
AND A.CLD_ID = E.CLD_ID(+)
