	<queryString>
		<![CDATA[select c.coa_nm,sum(B.AMT_SP) amt_sp,sum(B.AMT_bs) amt_bs from sls$so a,sls$so$oc b,fin.fin$coa c

where a.doc_id=b.doc_id

and   A.CLD_ID=C.coa_CLD_ID

and   A.SLOC_ID=C.coa_SLOC_ID

and   A.HO_ORG_ID=C.coa_HO_ORG_ID

and   B.coa_ID =C.coa_ID

and   a.doc_id=nvl($P{DocId},a.doc_id)
group by c.coa_nm]]>
	</queryString>
