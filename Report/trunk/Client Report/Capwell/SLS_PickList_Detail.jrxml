	<queryString>
		<![CDATA[SELECT a.CLD_ID,
  a.SLOC_ID,
  a.ORG_ID,
  a.HO_ORG_ID,
  a.DOC_DT,
  e.EO_NM,
  c.DOC_DT AS so_DT,
  c.SO_ID,
  (select itm_desc from app.app$itm$prf
  where itm_id = b.ITM_ID
  and cld_id = b.cld_id
  and sloc_id = b.sloc_id
  and ho_org_id = b.ho_org_id)itm_desc,
  (select uom_nm from app.app$uom$conv$std
  where uom_id = b.ITM_UOM
  and cld_id = b.cld_id
  and sloc_id = b.sloc_id)uom,
  b.PICK_QTY,
  a.PICK_ID,
  (select O.ORG_DESC
  from APP.ORG O
  where a.CLD_ID = O.ORG_CLD_ID
  and a.SLOC_ID  = O.ORG_CREATE_REF_SLOC_ID
  and a.ORG_ID   = O.ORG_ID
  )ORG_DESC,
  (select usr_name from app.app$sec$usr
  where usr_id = a.usr_id_create
  and sloc_id = a.sloc_id)usr_name
FROM SLS$PICK a,
  SLS$PICK$ITM b,
  app.app$eo e,
  SLS$SO c
WHERE a.CLD_ID  = b.CLD_ID
AND a.SLOC_ID   = b.SLOC_ID
AND a.ORG_ID    = b.ORG_ID
AND a.HO_ORG_ID = b.HO_ORG_ID
AND a.DOC_ID    = b.DOC_ID
AND a.WH_ID     = b.WH_ID
AND a.EO_ID     = e.EO_ID
AND a.CLD_ID    = e.EO_CLD_ID
AND a.HO_ORG_ID = e.EO_HO_ORG_ID
AND b.CLD_ID    = c.CLD_ID
AND b.SLOC_ID   = c.SLOC_ID
AND b.ORG_ID    = c.ORG_ID
AND b.HO_ORG_ID = c.HO_ORG_ID
AND b.SO_ID     = c.DOC_ID
   AND a.doc_id = NVL ( $P{DocId}, a.doc_id)
   AND a.cld_id = NVL ( $P{CldId}, a.cld_id)
   AND a.sloc_id = NVL ( $P{SlocId}, a.sloc_id)
   AND a.org_id = NVL ( $P{OrgId}, a.org_id)
   AND a.ho_org_id = NVL ( $P{HoOrgId}, a.ho_org_id)
   AND b.itm_id =nvl( $P{ItmId},b.itm_id)
   AND trunc(a.doc_dt) >= NVL (TO_DATE ( $P{FromDate}, 'yyyy-MM-dd'), trunc(a.doc_dt))
