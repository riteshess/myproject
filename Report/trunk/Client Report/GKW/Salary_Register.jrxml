	<queryString>
		<![CDATA[--Client Report

SELECT EMP_DOC_ID,EMP_NM,TTL_DYS-(CL+EL+ABS+HD) WD,HD,CL,EL,ML,ABS,TTL_DYS TTL,groupc,Dept,locationc,DOC_ID,ORG_DESC
FROM
(SELECT A.EMP_DOC_ID,P.EMP_NM,P.DOC_ID,B.ORG_DESC,(A.PROC_TO_DT-A.PROC_FRM_DT)+1 TTL_DYS,

nvl((select g.dept_nm from app.app$dept g where dept_id=$P{crtValue} and g.cld_id=$P{cldId}
 and g.sloc_id=$P{slocId} and g.ho_org_id=$P{hoOrgId}),'') Dept,



nvl((select s.param_desc from hcm$setup s where param_id=$P{crtValue}
and param_type=3 and s.cld_id=$P{cldId} and s.sloc_id=$P{slocId}
and s.ho_org_id=$P{hoOrgId}),'') locationc,


nvl((select s.param_desc from hcm$setup s where param_id=$P{crtValue} and param_type=1 and s.cld_id=$P{cldId} and s.sloc_id=$P{slocId}
and s.ho_org_id=$P{hoOrgId}),'') groupc,

            (SELECT COUNT(B.LEAVE_ID)
            FROM HCM$TIME$MOVE$EDIT$DTL B
            WHERE B.ATTEN_DT>=A.PROC_FRM_DT AND B.ATTEN_DT<=A.PROC_TO_DT
                          AND B.LEAVE_ID='PAR0000009'
                          AND A.EMP_DOC_ID = B.EMP_DOC_ID ) CL,
             (SELECT COUNT(B.LEAVE_ID)
            FROM HCM$TIME$MOVE$EDIT$DTL B
            WHERE B.ATTEN_DT>=A.PROC_FRM_DT AND B.ATTEN_DT<=A.PROC_TO_DT
                          AND B.LEAVE_ID='PAR0000010'
                          AND A.EMP_DOC_ID = B.EMP_DOC_ID ) EL,
(SELECT COUNT(B.LEAVE_ID)
            FROM HCM$TIME$MOVE$EDIT$DTL B
            WHERE B.ATTEN_DT>=A.PROC_FRM_DT AND B.ATTEN_DT<=A.PROC_TO_DT
                          AND B.LEAVE_ID='PAR0000011'
                          AND A.EMP_DOC_ID = B.EMP_DOC_ID ) ML,
            (SELECT COUNT(HOL_ID)
            FROM HCM$HOL$CAL$VW V
            WHERE V.HOL_DAY>=A.PROC_FRM_DT AND V.HOL_DAY<=A.PROC_TO_DT) HD,
           ( (SELECT COUNT(B.DED_CH)
            FROM HCM$TIME$MOVE$EDIT$DTL B
            WHERE B.ATTEN_DT>=A.PROC_FRM_DT AND B.ATTEN_DT<=A.PROC_TO_DT
                          AND B.DED_CH='F'
                          AND A.EMP_DOC_ID = B.EMP_DOC_ID ) +
            (SELECT COUNT(B.DED_CH)/2
            FROM HCM$TIME$MOVE$EDIT$DTL B
            WHERE B.ATTEN_DT>=A.PROC_FRM_DT AND B.ATTEN_DT<=A.PROC_TO_DT
                          AND B.DED_CH='H'
                          AND A.EMP_DOC_ID = B.EMP_DOC_ID ) ) ABS
FROM HCM$SAL$PROC A,ORG$HCM$EMP$PRF p,APP.ORG B
WHERE A.SLOC_ID=P.SLOC_ID
AND P.CLD_ID = B.ORG_CLD_ID(+)
AND P.ORG_ID = B.ORG_ID(+)

AND A.ORG_ID=P.ORG_ID
AND A.HO_ORG_ID=P.HO_ORG_ID
AND A.CLD_ID=P.CLD_ID
AND A.EMP_DOC_ID=P.DOC_ID
--AND A.PROC_FRM_DT = '01-MAY-2015'
--AND A.PROC_TO_DT = '31-MAY-2015')
AND
(A.PROC_FRM_DT>= to_date(to_date($P{FromDate},'yyyy-mm-dd'),'dd-mm-yy')
AND A.PROC_FRM_DT <=to_date(to_date($P{ToDate},'yyyy-mm-dd'),'dd-mm-yy'))

AND p.CLD_ID = $P{cldId}
AND p.SLOC_ID = $P{slocId}
AND p.HO_ORG_ID = $P{hoOrgId}
AND p.ORG_ID = $P{orgId}
AND (p.EMP_DEPT_ID = $P{crtValue} OR p.EMP_LOC_ID =  $P{crtValue}
OR p.EMP_GRP_ID = $P{crtValue} OR p.DOC_ID =$P{crtValue})
AND A.EMP_DOC_ID = NVL($P{empDocId}, P.DOC_ID))]]>
	</queryString>
