	<queryString>
		<![CDATA[SELECT distinct cld_id,
  sloc_id,
  org_id,
  ho_org_id,
  invoice_id,
  invoice_doc_dt,
  usr_id_create_dt,
  REPLACE (org_adds1, ',', ',') org_adds1,
  curr_id_sp,
  curr_rate,
  bill_add,
  eo_nm,
  itm_id,
 itm_desc,
 EOPARTNO,
 -- itm_desc ||DECODE(NVL(EOPARTNO,' '),' ',' ','(')||EOPARTNO||DECODE(NVL(EOPARTNO,' '),' ',' ',')') NEW_ITM_desc,
tech_nam ||DECODE(NVL(EOPARTNO,' '),' ',' ','(')||EOPARTNO||DECODE(NVL(EOPARTNO,' '),' ',' ',')') NEW_ITM_desc,
  itm_rate,
  ext_doc_dt,
  ext_doc_no,
  base_price,
  discount,
  org_tin_no,
  org_reg_no,
  org_cst_no,
  org_ecc_no,
  NVL (org_phone, '                    ') || ', E-mail '  || org_email org_phone,
  org_range,
  ORG_PAN,
  cust_org_id,
  cust_reg_no,
  cust_tin_no,
  cust_vat_no,
  cust_ecc_no,
  cust_cr_days ||' Days' cust_cr_days,
  cust_cr_lmt,
  cust_division,
  cust_collect,
  cust_range,
  cust_leg_code cust_leg_code,
  order_no,
  order_date,
  inv_t,inv_type,
  removal_good_time,
  SUM (itm_amt_sp) itm_amt_sp,
  SUM (itm_amt_bs) itm_amt_bs,
  SUM (itm_ship_qty) itm_ship_qty,
  invoice_doc_id,
  org_adds,
  org_desc,
  org_division,
  ship_adds,
 -- ship_add,
  (SELECT fn_get_shp_dtl (cld_id, sloc_id, ho_org_id, org_id, invoice_doc_id, 'S' )  FROM DUAL  ) ship_detail,
  (SELECT fn_get_shp_dtl (cld_id, sloc_id, ho_org_id, org_id, invoice_doc_id, 'N' )  FROM DUAL  ) trans_nm_detail,
  (SELECT fn_get_shp_dtl (cld_id, sloc_id, ho_org_id, org_id, invoice_doc_id, 'V' )  FROM DUAL  ) veh_detail,
  (SELECT fn_get_shp_dtl (cld_id, sloc_id, ho_org_id, org_id, invoice_doc_id, 'T' )  FROM DUAL  ) trans_detail,
  (SELECT fn_get_shp_dtl (cld_id, sloc_id, ho_org_id, org_id, invoice_doc_id, 'M' )  FROM DUAL  ) mode_detail,
  (SELECT fn_get_shp_dtl (cld_id, sloc_id, ho_org_id, org_id, invoice_doc_id, 'C' )  FROM DUAL  ) terms_cond,
  (SELECT fn_get_shp_dtl (cld_id, sloc_id, ho_org_id, org_id, invoice_doc_id, 'R' )  FROM DUAL  ) remarks,
  (SELECT fn_grp_cnt (cld_id, sloc_id, ho_org_id, org_id, invoice_doc_id )  FROM DUAL  ) grp_cnt,  grp_nm,

  nvl((SELECT CHP_desc FROM app.app$glbl$itm$chptr WHERE chp_id = chptr_id ),'-') chptr_nm,



  SO_ID,  comod,sige1,sige2,oth_charg,coa,ASBL_AMT_SP
,chptr_id,vendor,adds,pp_qty,tva_no,tot_no_packets,seq, copy,pack,
challan_no,org_website2,to_number(sum_pack) sum_pack
FROM
  (SELECT  SUBSTR (a.shipmnt_id, 3) shipmnt_id,

    a.cld_id,
    a.usr_id_create_dt,
    a.eo_catg,
    a.sloc_id,
    a.org_id,
    a.ho_org_id,
    a.invoice_doc_id,
    SUBSTR (a.invoice_id, 3) invoice_id,
    a.invoice_doc_dt,
    a.inv_type,
    a.inv_typ_nm,
    a.curr_id_sp,
    a.curr_rate,
    a.eo_id,
    a.bill_add,
    a.eo_nm,
    a.shipmnt_dt,
    a.so_id,
    a.ship_add,
    c.doc_dt so_dt,
    a.itm_id,
    a.ship_amt_sp,
    a.ship_amt_bs,
    a.itm_rate,
    a.itm_uom,
    a.itm_ship_qty,

  nvl(DECODE(  NVL (DECODE (a.itm_disc_typ, 'A', a.itm_disc_val, 'P', ( ( NVL (a.itm_ship_qty, 0) * NVL (a.itm_rate, 0) ) * NVL (a.itm_disc_val, 0) / 100 ) ),0 ) ,0,  (NVL(A.DISC_VAL,0)) ),0.00)      discount,
    NVL (a.itm_ship_qty, 0) * NVL (a.itm_rate, 0) itm_amt_gs,
    a.itm_amt_bs,
    a.itm_amt_sp,
    a.tot_amt_sp,
    a.tot_amt_bs,
    a.itm_cost,
    m.att_nm inv_wf_stat_desc,
    l.eo_nm assigned_to,
    m.att_nm inv_mode,
 --(select SLS.FN_GET_PACK_NO( a.sloc_id,a.cld_id,a.org_id, a.invoice_doc_id,a.itm_id) from dual) pack,
--    (SELECT adds FROM app.app$adds$vw WHERE app.app$adds$vw.adds_id = a.bill_add) adds,
    (SELECT app.fn_get_adds   ($P{SlocId}, $P{CldId}, a.bill_add, 'M' )FROM DUAL) adds,
    (SELECT app.fn_get_adds_1 ($P{SlocId}, $P{CldId}, a.ship_add, 'M' )FROM DUAL ) ship_adds,
    (SELECT app.fn_get_adds_1 ($P{SlocId}, $P{CldId}, p.org_add_id1, 'S' )FROM DUAL ) org_adds,
    (SELECT app.fn_get_adds_1 ($P{SlocId}, $P{CldId}, p.org_add_id2, 'S' )FROM DUAL ) org_adds1,
    (select app.fn_get_commodity($P{SlocId}, $P{CldId},$P{HoOrgId},a.itm_id)FROM DUAL )comod,
    (SELECT  SLS.FN_GET_EOPART_NO($P{SlocId}, $P{CldId},$P{OrgId},a.itm_id,A.EO_ID) FROM DUAL ) EOPARTNO,
    nvl((SELECT  APP.fn_get_att_val($P{SlocId}, $P{CldId},$P{OrgId},a.itm_id,'ATT0002') FROM DUAL ),'  ') sige1,
nvl((SELECT  APP.fn_get_att_val($P{SlocId}, $P{CldId},$P{OrgId},a.itm_id,'ATT0003') FROM DUAL ),'  ') sige2,
    a.grp_nm,
    o.curr_notation_alias,
    ROUND (a.itm_amt_sp / a.itm_ship_qty, 2) base_price,
    a.ext_doc_dt,
    a.ext_doc_no,
    v.tin_no org_tin_no,
    v.reg_no org_reg_no,
    v.cst_no org_cst_no,
    v.ecc_no org_ecc_no,
    v.RANGE org_range,
    v.division org_division,
    v.pan_no   org_PAN,
    k.eo_org_id cust_org_id,
    k.regn_certi_no cust_reg_no,
    k.vat_no cust_vat_no,
    k.tin_no cust_tin_no,
    k.tds_division cust_division,
    k.tds_collectorate cust_collect,
    k.tds_range cust_range,
    k.ecc_no cust_ecc_no,
    k.avg_cr_days cust_cr_days,
    k.cr_limit    cust_cr_lmt,
    u.eo_leg_code cust_leg_code,
    SUBSTR (c.so_id, 3) order_no,
    c.doc_dt order_date,
    a.inv_type inv_t,
    a.shipmnt_dt removal_good_time,
b.chptr_id,p.org_email1 org_email,p.org_phone1 org_phone,f.org_desc,b.tech_nam,b.itm_desc
,nvl(oc.amt_sp,0.00) oth_charg,co.coa_nm coa,a.ASBL_AMT_SP,
(select regn_certi_no from app.app$eo$prf
		  where eo_cld_id=a.cld_id
		  and eo_sloc_id=a.sloc_id
		  and eo_ho_org_id=a.ho_org_id
		  and eo_id=a.eo_id)vendor,
      (select pp.pck_itm_qty from SLS.SLS$pack$pck$mtl pp
                       where pi.cld_id=pp.cld_id
                       and pi.sloc_id=pp.sloc_id
                      and pi.org_id=pp.org_id
                      and pi.ho_org_id=pp.ho_org_id
                      and pi.doc_id=pp.pick_id
		and rownum = 1
                    -- and pai.doc_id = pp.doc_id
                      )pp_qty,p.tva_no,
(select tot_no_of_pckts from sls$pack$itm pa
		    where pa.cld_id = pai.cld_id
		    and pa.sloc_id = pai.sloc_id
		    AND PA.org_id = pai.org_id
		    and pa.ho_org_id = pai.ho_org_id
		    and pa.doc_id = pai.doc_id
        AND PA.ITM_ID = PI.ITM_ID
		    --and rownum=1
        )tot_no_packets,
    copy.seq, copy.copy,sh.pack_no pack,p.challan_no,p.org_website2,
(sh.pack_no,'[A-Za-z]') sum_pack
  FROM sls$invoice$dtl_vw a,sls$inv$oc oc,fin.fin$coa co,sls$shipmnt si,sls$shipmnt$itm sh,
    app.app$sls$itm_vw b,
    sls.sls$so c,sls.sls$so$itm ci,sls.sls$pick$itm pi,sls.sls$pack pai,
  app.app$uom$conv$std e,
    app.org f,
    app.org$oth$dtl p
    ,(SELECT eo_cld_id,
      x.eo_sloc_id,
      x.eo_ho_org_id,
      x.eo_id,
      x.eo_nm
    FROM app.app$eo x
    WHERE x.eo_cld_id = NVL ($P{CldId}, x.eo_cld_id)
    AND x.eo_sloc_id  = NVL ($P{SlocId}, x.eo_sloc_id)
    AND x.eo_type     = '74'
    ) l,
    (select y.att_nm, y.att_id from app.app$ds$att y where att_type_id = 69
    ) m,
    (SELECT att_id, att_nm FROM app.app$ds$att WHERE att_type_id = 74
    ) z,
    app.app$curr_vw o,
    app.org$oth$dtl v,
    app.app$eo u,
    app.app$eo$prf k,
    ((SELECT 1 seq, 'ORIGINAL FOR BUYER'copy FROM DUAL) UNION
(SELECT 2 seq,'DUPLICATE FOR TRANSPORTER (CENVAT COPY)' copy FROM DUAL) UNION
(SELECT 3 seq, 'OFFICE COPY' copy FROM DUAL) UNION
(SELECT 4 seq, 'EXTRA COPY' copy FROM DUAL) UNION
(SELECT 5 seq ,'EXTRA COPY 'copy FROM DUAL)) COPY
  WHERE a.sloc_id             = b.sloc_id
  AND a.itm_id                = b.itm_id
  AND a.org_id                = b.org_id
  AND a.cld_id                = b.cld_id
  AND a.ho_org_id             = b.ho_org_id
  and a.itm_id                = b.itm_id
  AND a.sloc_id               = c.sloc_id
  AND a.org_id                = c.org_id
  AND a.cld_id                = c.cld_id
  AND a.ho_org_id             = c.ho_org_id
  AND a.so_id                 = c.so_id
  AND a.sloc_id               = e.sloc_id
  AND a.cld_id                = e.cld_id
  AND a.itm_uom               = e.uom_id
  AND a.cld_id                = f.org_cld_id
  AND a.org_id                = f.org_id
  and a.sloc_id               = f.org_create_ref_sloc_id
  AND a.org_id                = p.org_id(+)
  and a.cld_id                = p.org_cld_id(+)
  AND c.cld_id                = l.eo_cld_id(+)
  AND c.sloc_id               = l.eo_sloc_id(+)
  AND c.ho_org_id             = l.eo_ho_org_id(+)
  AND c.assigned_to           = l.eo_id(+)
  AND a.inv_mode              = m.att_id
  AND a.eo_catg               = z.att_id
  AND a.curr_id_sp            = o.curr_id
  AND v.org_id                = a.org_id
  and u.eo_cld_id             = a.cld_id
  AND u.eo_sloc_id            = a.sloc_id
  AND u.eo_ho_org_id          = a.ho_org_id
  and u.eo_id                 = a.eo_id
  AND u.eo_cld_id             = k.eo_cld_id
  AND u.eo_sloc_id            = k.eo_sloc_id
  AND u.eo_ho_org_id          = k.eo_ho_org_id
  AND u.eo_id                 = k.eo_id



 and a.cld_id		 = oc.cld_id(+)
  and a.sloc_id		 = oc.sloc_id(+)
  and a.org_id		 = oc.org_id(+)
  and a.ho_org_id 		 = oc.ho_org_id(+)
  and a.invoice_doc_id		 =oc.doc_id(+)
   and oc.coa_id		 =co.coa_id(+)
  and c.cld_id=ci.cld_id
  and c.sloc_id=ci.sloc_id
  and c.org_id=ci.org_id
  and c.ho_org_id=ci.ho_org_id
  and c.doc_id=ci.doc_id
  and a.itm_id=ci.itm_id
  and ci.cld_id=pi.cld_id
  and ci.sloc_id=pi.sloc_id
  and ci.org_id=pi.org_id
  and ci.ho_org_id=pi.ho_org_id
  and ci.doc_id=pi.so_id
  and ci.itm_id=pi.itm_id
  and pi.cld_id = pai.cld_id(+)
  and pi.sloc_id = pai.sloc_id(+)
  and pi.org_id = pai.org_id(+)
  and pi.ho_org_id = pai.ho_org_id(+)
  and pi.doc_id=pai.pick_id(+)
  and pi.cld_id = si.cld_id
  and pi.sloc_id = si.sloc_id
  and pi.org_id = si.org_id
  and pi.ho_org_id = si.ho_org_id
  and pi.doc_id = si.pick_id
  and a.cld_id = si.cld_id
  and a.sloc_id = si.sloc_id
  and a.org_id = si.org_id
  and a.ho_org_id = si.ho_org_id
  and a.shipmnt_id = si.shipmnt_id

  and si.cld_id = sh.cld_id
  and si.sloc_id = sh.sloc_id
  and si.org_id = sh.org_id
  and si.ho_org_id = sh.ho_org_id
  and si.doc_id = sh.doc_id
  and a.itm_id = sh.itm_id
  and pi.doc_id = sh.pick_id
  and c.doc_id = sh.so_id


  AND a.cld_id                =NVL($P{CldId},a.cld_id)
  AND a.sloc_id               =NVL($P{SlocId},a.sloc_id)
  AND a.org_id                =NVL($P{OrgId},a.org_id)
  AND a.ho_org_id             =NVL($P{HoOrgId},a.ho_org_id)
  AND a.invoice_doc_id        =NVL($P{DocId},a.invoice_doc_id)
  AND a.itm_id                =NVL($P{ItmId},a.itm_id)
  AND a.grp_id                =NVL($P{GrpId},a.grp_id)
  AND NVL(c.order_stat,217)   = NVL($P{Status},NVL(c.order_stat,217))
  AND a.eo_catg               =NVL($P{EoCatId},a.eo_catg)
  AND a.curr_id_sp            =NVL($P{CurrId},a.curr_id_sp)
  AND a.inv_type              =NVL($P{InvType},a.inv_type)
  AND TRUNC(a.invoice_doc_dt)>= NVL(TO_DATE($P{FromDate}
  ,'yyyy-MM-dd'), TRUNC(a.invoice_doc_dt) )
  AND TRUNC(a.invoice_doc_dt)<= NVL(TO_DATE($P{ToDate},'yyyy-MM-dd'), TRUNC(a.invoice_doc_dt) )
 -- and a.invoice_doc_id=''
  )
GROUP BY cld_id,
  sloc_id,
  org_id,
  ho_org_id,
  invoice_id,
  invoice_doc_dt,
  curr_id_sp,
  curr_rate,
  bill_add,
  eo_nm,
  itm_id,
  itm_rate,
  ext_doc_dt,
  ext_doc_no,
  base_price,

  discount,
  invoice_doc_id,
  org_adds,
  org_desc,
  org_tin_no,
  org_reg_no,
  org_cst_no,
  org_ecc_no,
  org_range,
  cust_org_id,
  cust_reg_no,
  cust_vat_no,
  cust_ecc_no,
  cust_cr_days,
   cust_cr_lmt,
  cust_leg_code,
  grp_nm,
  cust_tin_no,
  usr_id_create_dt,
  org_division,
  cust_division,
  cust_range,
  cust_collect,
  org_phone,
  org_email,
  order_no,
  order_date,
  inv_t,
  org_adds1,
  removal_good_time,
  SO_ID,
 -- ship_add,
  ship_adds,chptr_id,
  comod
  ,EOPARTNO
  ,org_pan,ASBL_AMT_SP
  ,sige1,sige2--,ASBL_AMT_SP
,oth_charg,coa,vendor,pack,
chptr_id,org_email,org_phone,org_desc,tech_nam,itm_desc,adds,pp_qty,tva_no,tot_no_packets,seq, copy,challan_no,org_website2,inv_type,to_number(sum_pack)
order by seq]]>
	</queryString>
