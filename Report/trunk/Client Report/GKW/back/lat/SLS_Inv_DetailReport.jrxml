	<field name="CUST_CR_LMT" class="java.math.BigDecimal"/>
	<field name="CUST_DIVISION" class="java.lang.String"/>
	<field name="CUST_COLLECT" class="java.lang.String"/>
	<field name="CUST_RANGE" class="java.lang.String"/>
	<field name="CUST_LEG_CODE" class="java.lang.String"/>
	<field name="INV_T" class="java.math.BigDecimal"/>
	<field name="INV_TYPE" class="java.math.BigDecimal"/>
	<field name="REMOVAL_GOOD_TIME" class="java.sql.Timestamp"/>
	<field name="ITM_AMT_SP" class="java.math.BigDecimal"/>
	<field name="ITM_AMT_BS" class="java.math.BigDecimal"/>
	<field name="ITM_SHIP_QTY" class="java.math.BigDecimal"/>
