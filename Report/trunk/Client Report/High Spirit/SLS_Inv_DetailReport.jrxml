	<queryString>
		<![CDATA[SELECT DISTINCT CLD_ID,
  SLOC_ID,
  ORG_ID,
  HO_ORG_ID,
  invoice_id,
  INVOICE_DOC_DT,
  USR_ID_CREATE_DT,
  NVL(REPLACE(org_adds1, ',', ','),'') org_adds1,
  CURR_ID_SP,
  CURR_RATE,
  BILL_ADD,
  EO_NM,
  ITM_ID,
  ITM_DESC,
  EOPARTNO,
  TECH_NAM
  || DECODE(NVL(EOPARTNO, ' '), ' ', ' ', '(')
  || EOPARTNO
  || DECODE(NVL(EOPARTNO, ' '), ' ', ' ', ')') NEW_ITM_desc,
itm_desc,
  ITM_RATE,
  EXT_DOC_DT,
  EXT_DOC_NO,
  base_price,
  discount,
  org_tin_no,
  org_reg_no,
  org_cst_no,
  org_ecc_no,
  NVL(org_phone, '                    ')
  || ', E-mail '
  || org_email org_phone,
  org_range,
  org_PAN,
  cust_org_id,
  cust_reg_no,
  cust_tin_no,
  cust_vat_no,
  cust_ecc_no,
  cust_cr_days
  || ' Days' cust_cr_days,
  cust_cr_lmt,
  cust_division,
  cust_collect,
  cust_range,
  cust_leg_code cust_leg_code,
  inv_t,
  INV_TYPE,
  removal_good_time,
  SUM(ITM_AMT_SP) itm_amt_sp,
  SUM(ITM_AMT_BS) itm_amt_bs,
  SUM(ITM_SHIP_QTY) itm_ship_qty,
  sum(tot_amt_sp) tot_amt_sp,
  INVOICE_DOC_ID,
  org_adds,
  ORG_DESC,org_email,
  org_division,
  ship_adds,
  (SELECT fn_get_shp_dtl(CLD_ID, SLOC_ID, HO_ORG_ID, ORG_ID, INVOICE_DOC_ID, 'S')
  FROM DUAL
  ) ship_detail,
  (SELECT fn_get_shp_dtl(CLD_ID, SLOC_ID, HO_ORG_ID, ORG_ID, INVOICE_DOC_ID, 'N')
  FROM DUAL
  ) trans_nm_detail,
  (SELECT fn_get_shp_dtl(CLD_ID, SLOC_ID, HO_ORG_ID, ORG_ID, INVOICE_DOC_ID, 'V')
  FROM DUAL
  ) veh_detail,
  (SELECT fn_get_shp_dtl(CLD_ID, SLOC_ID, HO_ORG_ID, ORG_ID, INVOICE_DOC_ID, 'T')
  FROM DUAL
  ) trans_detail,
  (SELECT fn_get_shp_dtl(CLD_ID, SLOC_ID, HO_ORG_ID, ORG_ID, INVOICE_DOC_ID, 'M')
  FROM DUAL
  ) mode_detail,
  (SELECT fn_get_shp_dtl(CLD_ID, SLOC_ID, HO_ORG_ID, ORG_ID, INVOICE_DOC_ID, 'C')
  FROM DUAL
  ) terms_cond,
  (SELECT fn_get_shp_dtl(CLD_ID, SLOC_ID, HO_ORG_ID, ORG_ID, INVOICE_DOC_ID, 'R')
  FROM DUAL
  ) remarks,
  (SELECT fn_grp_cnt(CLD_ID, SLOC_ID, HO_ORG_ID, ORG_ID, INVOICE_DOC_ID)
  FROM DUAL
  ) grp_cnt,
  GRP_NM,
  NVL(
  (SELECT app.app$glbl$itm$chptr.CHP_DESC
  FROM app.app$glbl$itm$chptr
  WHERE app.app$glbl$itm$chptr.CHP_ID = CHPTR_ID
  ), '-') chptr_nm,
  comod,
  sige1,
  sige2,
  oth_charg,
  coa,
  SUM(ASBL_AMT_SP) ASBL_AMT_SP,
  CHPTR_ID,
  vendor,
  adds,
  pp_qty,
  TVA_NO,
  tot_no_packets,
  seq,
  copy,
  pack,
  CHALLAN_NO,
  ORG_WEBSITE2,
  sum_pack sum_pack,uom_nm,NVL(remove_hr,'')remove_hr,NVL(remove_mm,'')remove_mm
FROM
  (SELECT SUBSTR(a.SHIPMNT_ID, 3) shipmnt_id,
    a.CLD_ID,
    a.USR_ID_CREATE_DT,
    a.EO_CATG,
    a.SLOC_ID,
    a.ORG_ID,
    a.HO_ORG_ID,
    a.INVOICE_DOC_ID,
    SUBSTR(a.INVOICE_ID, 3) invoice_id,
    a.INVOICE_DOC_DT,
    a.INV_TYPE,
    a.INV_TYP_NM,
    a.CURR_ID_SP,
    a.CURR_RATE,
    a.EO_ID,
    a.BILL_ADD,
    a.EO_NM,
    a.SHIPMNT_DT,
    a.SO_ID,
    a.SHIP_ADD,
    c.DOC_DT so_dt,
    a.ITM_ID,
    a.SHIP_AMT_SP,
    a.SHIP_AMT_BS,
    a.ITM_RATE,
    a.ITM_UOM,
    a.ITM_SHIP_QTY,
    NVL(DECODE(NVL(DECODE(a.ITM_DISC_TYP, 'A', a.ITM_DISC_VAL, 'P', ((NVL(a.ITM_SHIP_QTY, 0) * NVL(a.ITM_RATE, 0)) * NVL(a.ITM_DISC_VAL, 0) / 100)), 0), 0, (NVL(a.DISC_VAL, 0))), 0.00) discount,
    NVL(a.ITM_SHIP_QTY, 0)                                                                   * NVL(a.ITM_RATE, 0) itm_amt_gs,
    a.ITM_AMT_BS,
    a.ITM_AMT_SP,
    a.TOT_AMT_SP,
    a.TOT_AMT_BS,
    a.ITM_COST,
    m.ATT_NM inv_wf_stat_desc,
    l.EO_NM assigned_to,
    m.ATT_NM inv_mode,
    (SELECT app.fn_get_adds($P{SlocId}, $P{CldId}, a.BILL_ADD, 'M') FROM DUAL
    ) adds,
    (SELECT app.fn_get_adds_1($P{SlocId}, $P{CldId}, a.SHIP_ADD, 'M') FROM DUAL
    ) ship_adds,
    (SELECT app.fn_get_adds_1($P{SlocId}, $P{CldId}, p.ORG_ADD_ID1, 'S')
    FROM DUAL
    ) org_adds,
    (SELECT app.fn_get_adds_1($P{SlocId}, $P{CldId}, p.ORG_ADD_ID2, 'S')
    FROM DUAL
    ) org_adds1,
    (SELECT app.fn_get_commodity($P{SlocId}, $P{CldId}, $P{HoOrgId}, a.ITM_ID)
    FROM DUAL
    ) comod,
    NVL(ii.eo_part_no,
    (SELECT SLS.FN_GET_EOPART_NO($P{SlocId}, $P{CldId}, $P{OrgId}, a.ITM_ID, a.EO_ID)
    FROM DUAL
    )) EOPARTNO,
    NVL(
    (SELECT APP.fn_get_att_val($P{SlocId}, $P{CldId}, $P{OrgId}, a.ITM_ID, 'ATT0002')
    FROM DUAL
    ), '  ') sige1,
    NVL(
    (SELECT APP.fn_get_att_val($P{SlocId}, $P{CldId}, $P{OrgId}, a.ITM_ID, 'ATT0003')
    FROM DUAL
    ), '  ') sige2,
    a.GRP_NM,
    o.CURR_NOTATION_ALIAS,
    ROUND(a.ITM_AMT_SP / a.ITM_SHIP_QTY, 2) base_price,
    a.EXT_DOC_DT,
    a.EXT_DOC_NO,
    v.TIN_NO org_tin_no,
    v.REG_NO org_reg_no,
    v.CST_NO org_cst_no,
    v.ECC_NO org_ecc_no,
    v.Range org_range,
    v.DIVISION org_division,
    v.PAN_NO org_PAN,
    k.EO_ORG_ID cust_org_id,
    k.REGN_CERTI_NO cust_reg_no,
    k.VAT_NO cust_vat_no,
    k.TIN_NO cust_tin_no,
    k.TDS_DIVISION cust_division,
    k.TDS_COLLECTORATE cust_collect,
    k.TDS_RANGE cust_range,
    k.ECC_NO cust_ecc_no,
    k.AVG_CR_DAYS cust_cr_days,
    k.CR_LIMIT cust_cr_lmt,
    u.EO_LEG_CODE cust_leg_code,
    SUBSTR(c.SO_ID, 3) order_no,
    c.DOC_DT order_date,
    a.INV_TYPE inv_t,
    a.SHIPMNT_DT removal_good_time,
initcap(TO_CHAR(a.SHIPMNT_DT,'HH24sp')) remove_hr,
initcap(TO_CHAR(a.SHIPMNT_DT,'MIsp')) remove_mm,
    b.CHPTR_ID,
    p.ORG_EMAIL1 org_email,
    p.ORG_PHONE1 org_phone,
    f.ORG_DESC,
    b.TECH_NAM,
    b.ITM_DESC,
    NVL(oc.AMT_SP, 0.00) oth_charg,
    co.COA_NM coa,
    a.ASBL_AMT_SP,
    (SELECT app.app$eo$prf.REGN_CERTI_NO
    FROM app.app$eo$prf
    WHERE app.app$eo$prf.EO_CLD_ID  = a.CLD_ID
    AND app.app$eo$prf.EO_SLOC_ID   = a.SLOC_ID
    AND app.app$eo$prf.EO_HO_ORG_ID = a.HO_ORG_ID
    AND app.app$eo$prf.EO_ID        = a.EO_ID
    ) vendor,
    (SELECT pp.PCK_ITM_QTY
    FROM SLS.SLS$pack$pck$mtl pp
    WHERE pi.CLD_ID  = pp.CLD_ID
    AND pi.SLOC_ID   = pp.SLOC_ID
    AND pi.ORG_ID    = pp.ORG_ID
    AND pi.HO_ORG_ID = pp.HO_ORG_ID
    AND pi.DOC_ID    = pp.PICK_ID
    AND RowNum       = 1
    ) pp_qty,
    p.TVA_NO,
    (SELECT pa.TOT_NO_OF_PCKTS
    FROM sls$pack$itm pa
    WHERE pa.CLD_ID  = pai.CLD_ID
    AND pa.SLOC_ID   = pai.SLOC_ID
    AND pa.ORG_ID    = pai.ORG_ID
    AND pa.HO_ORG_ID = pai.HO_ORG_ID
    AND pa.DOC_ID    = pai.DOC_ID
    AND pa.ITM_ID    = pi.ITM_ID
    ) tot_no_packets,
    COPY.seq,
    COPY.copy,
    sh.PACK_NO pack,
    p.CHALLAN_NO,
    p.ORG_WEBSITE2,
    to_number(regexp_substr(trim(sh.PACK_NO), '[[:digit:]]*')) sum_pack,e.uom_nm
  FROM sls$invoice$dtl_vw a,
    sls$inv$oc oc,
    fin.fin$coa co,
    sls$shipmnt si,
    sls$shipmnt$itm sh,
    app.app$sls$itm_vw b,
    sls$inv$ship$itm ii,
    sls.sls$so c,
    sls.sls$so$itm ci,
    sls.sls$pick$itm pi,
    sls.sls$pack pai,
    app.app$uom$conv$std e,
    app.org f,
    app.org$oth$dtl p,
    (SELECT x.EO_CLD_ID,
      x.EO_SLOC_ID,
      x.EO_HO_ORG_ID,
      x.EO_ID,
      x.EO_NM
    FROM app.app$eo x
    WHERE x.EO_CLD_ID = NVL($P{CldId}, x.EO_CLD_ID)
    AND x.EO_SLOC_ID  = NVL($P{SlocId}, x.EO_SLOC_ID)
    AND x.EO_TYPE     = '74'
    ) l,
    (SELECT y.ATT_NM, y.ATT_ID FROM app.app$ds$att y WHERE y.ATT_TYPE_ID = 69
    ) m,
    (SELECT app.app$ds$att.ATT_ID,
      app.app$ds$att.ATT_NM
    FROM app.app$ds$att
    WHERE app.app$ds$att.ATT_TYPE_ID = 74
    ) z,
    app.app$curr_vw o,
    app.org$oth$dtl v,
    app.app$eo u,
    app.app$eo$prf k,
    (
    (SELECT 1 seq, 'ORIGINAL FOR BUYER' copy FROM DUAL
    )
  UNION
    (SELECT 2 seq, 'DUPLICATE FOR TRANSPORTER (CENVAT COPY)' copy FROM DUAL
    )
  UNION
    (SELECT 3 seq, 'OFFICE COPY' copy FROM DUAL
    )
  UNION
    (SELECT 4 seq, 'EXTRA COPY' copy FROM DUAL
    )
  UNION
    (SELECT 5 seq, 'EXTRA COPY ' copy FROM DUAL
    )) COPY
  WHERE a.SLOC_ID              = b.SLOC_ID
  AND a.ITM_ID                 = b.ITM_ID
  AND a.ORG_ID                 = b.ORG_ID
  AND a.CLD_ID                 = b.CLD_ID
  AND a.HO_ORG_ID              = b.HO_ORG_ID
  AND a.ITM_ID                 = b.ITM_ID
  AND a.SLOC_ID                = c.SLOC_ID
  AND a.ORG_ID                 = c.ORG_ID
  AND a.CLD_ID                 = c.CLD_ID
  AND a.HO_ORG_ID              = c.HO_ORG_ID
  AND a.SO_ID                  = c.SO_ID
  AND a.SLOC_ID                = e.SLOC_ID
  AND a.CLD_ID                 = e.CLD_ID
  AND a.ITM_UOM                = e.UOM_ID
  AND a.CLD_ID                 = f.ORG_CLD_ID
  AND a.ORG_ID                 = f.ORG_ID
  AND a.SLOC_ID                = f.ORG_CREATE_REF_SLOC_ID
  AND a.ORG_ID                 = p.ORG_ID(+)
  AND a.CLD_ID                 = p.ORG_CLD_ID(+)
  AND c.CLD_ID                 = l.EO_CLD_ID(+)
  AND c.SLOC_ID                = l.EO_SLOC_ID(+)
  AND c.HO_ORG_ID              = l.EO_HO_ORG_ID(+)
  AND c.ASSIGNED_TO            = l.EO_ID(+)
  AND a.INV_MODE               = m.ATT_ID
  AND a.EO_CATG                = z.ATT_ID
  AND a.CURR_ID_SP             = o.CURR_ID
  AND v.ORG_ID                 = a.ORG_ID
  AND u.EO_CLD_ID              = a.CLD_ID
  AND u.EO_SLOC_ID             = a.SLOC_ID
  AND u.EO_HO_ORG_ID           = a.HO_ORG_ID
  AND u.EO_ID                  = a.EO_ID
  AND u.EO_CLD_ID              = k.EO_CLD_ID
  AND u.EO_SLOC_ID             = k.EO_SLOC_ID
  AND u.EO_HO_ORG_ID           = k.EO_HO_ORG_ID
  AND u.EO_ID                  = k.EO_ID
  AND a.CLD_ID                 = oc.CLD_ID(+)
  AND a.SLOC_ID                = oc.SLOC_ID(+)
  AND a.ORG_ID                 = oc.ORG_ID(+)
  AND a.HO_ORG_ID              = oc.HO_ORG_ID(+)
  AND a.INVOICE_DOC_ID         = oc.DOC_ID(+)
  AND oc.COA_ID                = co.COA_ID(+)
  AND c.CLD_ID                 = ci.CLD_ID
  AND c.SLOC_ID                = ci.SLOC_ID
  AND c.ORG_ID                 = ci.ORG_ID
  AND c.HO_ORG_ID              = ci.HO_ORG_ID
  AND c.DOC_ID                 = ci.DOC_ID
  AND a.ITM_ID                 = ci.ITM_ID
  AND ci.CLD_ID                = pi.CLD_ID
  AND ci.SLOC_ID               = pi.SLOC_ID
  AND ci.ORG_ID                = pi.ORG_ID
  AND ci.HO_ORG_ID             = pi.HO_ORG_ID
  AND ci.DOC_ID                = pi.SO_ID
  AND ci.ITM_ID                = pi.ITM_ID
  AND pi.CLD_ID                = pai.CLD_ID(+)
  AND pi.SLOC_ID               = pai.SLOC_ID(+)
  AND pi.ORG_ID                = pai.ORG_ID(+)
  AND pi.HO_ORG_ID             = pai.HO_ORG_ID(+)
  AND pi.DOC_ID                = pai.PICK_ID(+)
  AND pi.CLD_ID                = si.CLD_ID
  AND pi.SLOC_ID               = si.SLOC_ID
  AND pi.ORG_ID                = si.ORG_ID
  AND pi.HO_ORG_ID             = si.HO_ORG_ID
  AND pi.DOC_ID                = si.PICK_ID
  AND a.CLD_ID                 = si.CLD_ID
  AND a.SLOC_ID                = si.SLOC_ID
  AND a.ORG_ID                 = si.ORG_ID
  AND a.HO_ORG_ID              = si.HO_ORG_ID
  AND a.SHIPMNT_ID             = si.SHIPMNT_ID
  AND si.CLD_ID                = sh.CLD_ID
  AND si.SLOC_ID               = sh.SLOC_ID
  AND si.ORG_ID                = sh.ORG_ID
  AND si.HO_ORG_ID             = sh.HO_ORG_ID
  AND si.DOC_ID                = sh.DOC_ID
  AND a.ITM_ID                 = sh.ITM_ID
  AND pi.DOC_ID                = sh.PICK_ID
  AND c.DOC_ID                 = sh.SO_ID
  AND a.CLD_ID                 = ii.CLD_ID
  AND a.SLOC_ID                = ii.SLOC_ID
  AND a.ORG_ID                 = ii.ORG_ID
  AND a.HO_ORG_ID              = ii.HO_ORG_ID
  AND a.INVOICE_DOC_ID         = ii.DOC_ID
  AND a.ITM_ID                 = ii.ITM_ID
  AND sh.DOC_ID                = ii.SHIP_ID
  AND c.DOC_ID                 = ii.SO_ID
  AND a.CLD_ID                 = NVL($P{CldId}, a.CLD_ID)
  AND a.EO_CATG                = NVL($P{EoCatId}, a.EO_CATG)
  AND a.SLOC_ID                = NVL($P{SlocId}, a.SLOC_ID)
  AND a.ORG_ID                 = NVL($P{OrgId}, a.ORG_ID)
  AND a.HO_ORG_ID              = NVL($P{HoOrgId}, a.HO_ORG_ID)
  AND a.INVOICE_DOC_ID         = NVL($P{DocId}, a.INVOICE_DOC_ID)
  AND a.INV_TYPE               = NVL($P{InvType}, a.INV_TYPE)
  AND a.CURR_ID_SP             = NVL($P{CurrId}, a.CURR_ID_SP)
  AND a.ITM_ID                 = NVL($P{ItmId}, a.ITM_ID)
  AND a.GRP_ID                 = NVL($P{GrpId}, a.GRP_ID)
  AND NVL(c.ORDER_STAT, 217)   = NVL($P{Status}, NVL(c.ORDER_STAT, 217))
  AND TRUNC(a.INVOICE_DOC_DT) >= NVL(TO_DATE($P{FromDate}, 'yyyy-MM-dd'), TRUNC(a.INVOICE_DOC_DT))
  AND TRUNC(a.INVOICE_DOC_DT) <= NVL(TO_DATE($P{ToDate}, 'yyyy-MM-dd'), TRUNC(a.INVOICE_DOC_DT))
  )
GROUP BY CLD_ID,
  SLOC_ID,
  ORG_ID,
  HO_ORG_ID,
  invoice_id,
  INVOICE_DOC_DT,
  USR_ID_CREATE_DT,
  CURR_ID_SP,
  CURR_RATE,
  BILL_ADD,
  EO_NM,
  ITM_ID,
  ITM_DESC,
  EOPARTNO,
  ITM_RATE,
  EXT_DOC_DT,
  EXT_DOC_NO,
  base_price,
  discount,
  org_tin_no,
  org_reg_no,
  org_cst_no,
  org_ecc_no,
  org_range,
  org_PAN,
  cust_org_id,
  cust_reg_no,
  cust_tin_no,
  cust_vat_no,
  cust_ecc_no,
  cust_cr_lmt,
  cust_division,
  cust_collect,
  cust_range,
  cust_leg_code,
  inv_t,
  INV_TYPE,
  removal_good_time,
  INVOICE_DOC_ID,
  org_adds,
  ORG_DESC,
  org_division,
  ship_adds,
  GRP_NM,
  comod,
  sige1,
  sige2,
  oth_charg,
  coa,
  CHPTR_ID,
  vendor,
  adds,
  pp_qty,
  TVA_NO,
  tot_no_packets,
  seq,
  copy,
  pack,
  CHALLAN_NO,
  ORG_WEBSITE2,
  sum_pack,
  cust_cr_days,
  org_phone,
  org_email,
  org_adds1,
  TECH_NAM,uom_nm,remove_hr,remove_mm
ORDER BY seq]]>
	</queryString>
	<field name="CLD_ID" class="java.lang.String"/>
	<field name="SLOC_ID" class="java.math.BigDecimal"/>
	<field name="ORG_ID" class="java.lang.String"/>
