	<queryString>
		<![CDATA[SELECT DISTINCT inv_sub_typE,(case when substr(adds,instr(adds,'S'),3) like 'SOL' THEN
        substr(adds,INSTR(ADDS,'-')+2,4)
        ELSE '' END)BRANCH,
  cld_id,
  sloc_id,
  org_id,
  ho_org_id,
  invoice_id,
  doc_dt,
  org_tin,
  NVL(eo_part_no,itm_desc) partitm,
  cust_tin,
  tax_amt_sp,
  pan_no,
  org_phone1,
  org_email1,
  org_fax1,
  org_website1,
  ServiceTaxNo,
  curr_id_sp,
  curr_rate,
  doc_seq,
  bill_add,
  eo_nm,
  itm_id,
  itm_desc,
  eo_id,
  itm_rate,
  ext_doc_dt,
  ext_doc_no,
  base_price,
  uom_nm,
  discount,
  adds,
  registration_no,
  copy_type,
  SUM(itm_amt_sp)itm_amt_sp,
  SUM(itm_amt_bs)itm_amt_bs,
  (SELECT fn_get_shp_dtl (cld_id, sloc_id, ho_org_id, org_id, doc_id, 'R' )
  FROM DUAL
  ) remarks,
  (SELECT fn_get_shp_dtl (cld_id, sloc_id, ho_org_id, org_id, doc_id, 'C' )
  FROM DUAL
  ) tnc,
  SUM(itm_ship_qty)itm_ship_qty,
  doc_id,
  org_adds,
  org_desc,
  oth_charg,
  CIN_NO,
  STAX_NO,
  ship_adds,
  reg_adds,
  tax_per,
  MR_NO,
  PO_NO,
  OrgTin,
  CustTin,order_id,order_dt,
  itm_desc
  || DECODE(NVL(eo_part_no, ' '), ' ', ' ', '(')
  || eo_part_no
  || DECODE(NVL(eo_part_no, ' '), ' ', ' ', ')') NEW_ITM_desc,tax_amt
FROM
  (SELECT DISTINCT
    a.cld_id,
    a.sloc_id,
    a.org_id,
    a.ho_org_id,
    a.doc_id,
    inv.doc_dt,
    INV.inv_type,
    a.eo_part_no,
    inv.curr_id_sp,
    INV.curr_rate,
    inv.eo_id,
    INV.bill_add,
  (select e.eo_nm from app.app$eo e
                  where e.eo_cld_id = a.cld_id
                  and e.eo_sloc_id = a.sloc_id
                  and e.eo_ho_org_id = a.ho_org_id
                  and e.eo_id = inv.eo_id)eo_nm,
    a.so_id,
    c.doc_dt so_dt,
    A.itm_id,
    b.itm_desc itm_desc,
    a.itm_rate,
    a.itm_uom,
    e.uom_nm uom_nm,
    a.itm_ship_qty,
    NVL(DECODE( NVL (DECODE (a.itm_disc_typ, 'A', a.itm_disc_val, 'P', ( ( NVL (a.itm_ship_qty, 0) * NVL (a.itm_rate, 0) ) * NVL (a.itm_disc_val, 0) / 100 ) ),0 ) ,0, (NVL(INV.DISC_VAL,0)) ),0.00) discount,
    NVL(a.itm_ship_qty,0)                                                                          * NVL(itm_rate,0)itm_amt_gs,
    a.itm_amt_bs,
    a.itm_amt_sp,
    INV.tot_amt_sp,
    INV.tot_amt_bs,
    m.att_nm inv_wf_stat_desc,
    f.org_desc,
    l.eo_nm assigned_to,
    m.att_nm inv_mode,
    --(SELECT adds FROM app.app$adds$vw WHERE app.app$adds$vw.adds_id =a.bill_add) adds,
    (
    SELECT app.fn_get_adds_1( NVL($P{SlocId},a.sloc_id),NVL($P{CldId},a.cld_id),INV.bill_add ,'M')
    FROM dual
    ) adds,
    (SELECT adds
    FROM app.app$adds$vw
    WHERE app.app$adds$vw.addS_id =p.org_add_id1
    )org_adds,
    (SELECT app.fn_get_adds_1 ($P{SlocId}, $P{CldId}, p.org_add_id2, 'S' )
    FROM DUAL
    ) reg_adds,
    NVL(n.tax_amt_sp,0) tax_amt_sp,
    NVL(n.tax_amt_bs,0) tax_amt_bs,
    o.curr_notation_alias,
    ROUND(a.itm_amt_sp /a.itm_ship_qty,2)base_price,
    INV.ext_doc_dt,
    INV.ext_doc_no,
    q.att_id doc_seq,
    SUBSTR(INV.ext_doc_no,0,INSTR(INV.ext_doc_no,' ')) MR_NO,
    SUBSTR(INV.ext_doc_no,instr(INV.ext_doc_no,' ')+1) PO_NO,
    q.att_nm copy_type,
    P.TIN_NO org_tin,
    r.tin_no cust_tin,
    NVL(oc.amt_sp,0.00) oth_charg,
    p.pan_no,
    p.org_phone1,
    p.org_email1,
    p.org_fax1,
    p.org_website1,
    p.reg_no ServiceTaxNo,
    P.REG_NO Registration_No,
    p.CIN_NO,
    p.STAX_NO,
    --decode(a.INV_SUB_TYPE_ID,2,'15-16/' || F.ORG_ALIAS || '/' || SUBSTR(inv.disp_doc_id,3)
    --      ,1,'15-16/' || F.ORG_ALIAS || '/' || SUBSTR(inv.disp_doc_id,3)
    --      ,'15-16/' || F.ORG_ALIAS || '/' || SUBSTR(inv.disp_doc_id,3))  invoice_id
    ((TO_CHAR(fy.FY_STRT, 'YY')
    || '-'
    || TO_CHAR(fy.FY_END, 'YY')
    || '/'
    || DECODE(F.ORG_ALIAS,'UP','I','ID')
    || '/'
    || SUBSTR(inv.disp_doc_id, 5))) invoice_id,
    (SELECT app.fn_get_adds_1 ($P{SlocId}, $P{CldId}, C.ship_addS, 'M' )FROM DUAL
    ) ship_adds,
    (SELECT tax_per
    FROM app.app$tax
    WHERE tax_id    = n.tax_rule_id
    AND tax_sloc_id = n.sloc_id
    AND cld_id      = n.cld_id
    AND ho_org_id   = n.ho_org_id
    )tax_per,
    p.tin_no OrgTin,
    r.tin_no CustTin,
    k.DOC_SUB_TYP_NM INV_SUB_TYPE,substr(c.so_id,3) order_id,to_char(c.doc_dt,'dd-MM-yyyy') order_dt,
  (select sum(tr.tax_amt_bs) from sls$inv$tr$lines tr
		    where tr.cld_id = inv.cld_id
		    and tr.sloc_id = inv.sloc_id
		    and tr.org_id = inv.org_id
		    and tr.ho_org_id = inv.ho_org_id
 		    and tr.doc_id = inv.doc_id
		    and tr.itm_id = a.itm_id
		    and tr.so_id = c.doc_id
		    and sr_no <>1)tax_amt

  FROM sls$inv$ship$itm a,
    sls$inv inv,
    sls$inv$oc oc,
    app.app$sls$itm_vw b,
    sls.sls$so c,
    app.app$uom$conv$std e,
    app.org f,
    app.org$oth$dtl p,
    app.org$fy fy,
    (SELECT eo_cld_id,
      x.eo_sloc_id,
      x.eo_ho_org_id,
      x.eo_id,
      x.eo_nm
    FROM app.app$eo x
    WHERE x.eo_cld_id =NVL($P{CldId},x.eo_cld_id)
    AND x.eo_sloc_id  =NVL($P{SlocId},x.eo_sloc_id)
    AND x.eo_type     ='80'
    )l,
    (SELECT y.att_nm,y.att_id FROM app.app$ds$att y WHERE att_type_id =69
    )m,
    sls.sls$inv$tr n,
    app.app$curr_vw o,
    sls$eo$price ep,
    (SELECT att_id,
      att_nm
    FROM app.app$ds$att
    WHERE att_type_id =127
    AND att_id        =710
    OR att_id         =711
    )q,
    app.app$eo$prf r,
    app.app$doc$sub$type k
  WHERE a.cld_id = inv.cld_id
  and a.sloc_id = inv.sloc_id
  and a.org_id = inv.org_id
  and a.ho_org_id = inv.ho_org_id
  and a.doc_id = inv.doc_id
  AND INV.inv_type = k.DOC_TYP_ID
          AND INV.inv_sub_type = k.DOC_sub_TYP_ID
          AND k.doc_id = 21504
  and a.sloc_id      =b.sloc_id
  AND a.itm_id         =b.itm_id
  AND a.org_id         =b.org_id
  AND a.cld_id         =b.cld_id
  AND a.ho_org_id      =b.ho_org_id
  AND a.sloc_id        =c.sloc_id
  AND a.org_id         =c.org_id
  AND a.cld_id         =c.cld_id
  AND a.ho_org_id      =c.ho_org_id
  AND a.so_id          =c.DOC_id
  AND c.fy_id          = fy.org_fy_id
  AND c.org_id         = fy.org_id
  AND a.sloc_id        =e.sloc_id
  AND a.cld_id         =e.cld_id
  AND a.itm_uom        =e.uom_id
  AND a.cld_id         =f.org_cld_id
  AND a.org_id         =f.org_id
  AND a.org_id         =p.org_id(+)
  AND c.cld_id         =l.eo_cld_id(+)
  AND c.sloc_id        =l.eo_sloc_id(+)
  AND c.ho_org_id      =l.eo_ho_org_id(+)
  AND c.assigned_to    =l.eo_id(+)
  AND inv.inv_mode       =m.att_id
  AND a.cld_id         =r.eo_cld_id
  AND a.sloc_id        =r.eo_sloc_id
  AND a.ho_org_id      =r.eo_ho_org_id
  AND inv.eo_id          =r.eo_id
  AND a.cld_id         =n.cld_id(+)
  AND a.sloc_id        =n.sloc_id(+)
  AND a.ho_org_id      =n.ho_org_id(+)
  AND a.doc_id =n.doc_id(+)
  AND a.itm_id         =n.itm_id(+)
  AND inv.curr_id_sp    =o.curr_id
  AND a.cld_id         = oc.cld_id(+)
  AND a.sloc_id        = oc.sloc_id(+)
  AND a.org_id         = oc.org_id(+)
  AND a.ho_org_id      = oc.ho_org_id(+)
  AND a.doc_id =oc.doc_id(+)
  AND a.cld_id         = ep.cld_id(+)
  AND a.sloc_id        = ep.sloc_id(+)
  AND a.org_id         = ep.org_id(+)
  AND a.ho_org_id      = ep.ho_org_id(+)
  AND a.itm_id         =ep.itm_id(+)
  AND inv.eo_id          =ep.eo_id(+)
    --and a.eo_catg  =ep.catg_id(+)

  AND a.cld_id                =NVL($P{CldId},a.cld_id)
  AND a.sloc_id               =NVL($P{SlocId},a.sloc_id)
  AND a.org_id                =NVL($P{OrgId},a.org_id)
  AND a.ho_org_id             =NVL($P{HoOrgId},a.ho_org_id)
  AND a.doc_id        =NVL($P{DocId},a.doc_id)
  AND a.itm_id                =NVL($P{ItmId},a.itm_id)
  AND NVL(c.order_stat,217)   = NVL($P{Status},NVL(c.order_stat,217))
  AND inv.curr_id_sp           =NVL($P{CurrId},inv.curr_id_sp)
  AND TRUNC(inv.doc_dt)>= NVL(TO_DATE($P{FromDate},'yyyy-MM-dd'), TRUNC(inv.doc_dt) )
  AND TRUNC(inv.doc_dt)<= NVL(TO_DATE($P{ToDate},'yyyy-MM-dd'), TRUNC(inv.doc_dt) )
  )
GROUP BY cld_id, inv_sub_typE, org_id, sloc_id, invoice_id,
ho_org_id, curr_id_sp, doc_dt, bill_add, eo_nm,
itm_id, itm_desc, itm_rate, ext_doc_dt, ext_doc_no,
base_price, uom_nm, discount, adds, copy_type,
doc_id, org_adds, curr_rate, tax_amt_sp, org_phone1,
org_email1, org_fax1, org_website1, pan_no, doc_seq,
org_tin, cust_tin, oth_charg, ServiceTaxNo, NVL(eo_part_no,itm_desc),
registration_no, eo_id, CIN_NO, STAX_NO, ship_adds,
reg_adds, tax_per, MR_NO, PO_NO, org_desc,
OrgTin, CustTin,order_id,order_dt,eo_part_no,tax_amt]]>
	</queryString>
