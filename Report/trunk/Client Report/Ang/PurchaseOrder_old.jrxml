		<![CDATA[SELECT A.CLD_ID,  A.SLOC_ID,  A.ORG_ID,   E.ORG_DESC,   NVL(A.AUTH_PO_NO,A.PO_ID),
       A.PO_ID,   A.PO_DT,    A.AMD_NO,   A.AMD_DT,     F.EO_NM,
       B.ITM_ID,  H.ITM_DESC,I.UOM_DESC,B.ORD_QTY,B.ITM_PRICE,A.REMARKS,
upper(APP.FN_GET_ADDS ($P{SlocId},$P{CldId}, NVL(O.ORG_ADD_ID1,O.ORG_ADD_ID2),'M')) ORG_ADDS  ,
DECODE(A.DISC_TYPE,'P',(((B.ORD_QTY *B.ITM_PRICE * B.DISC_VAL)/100)),
                   'A',( B.DISC_VAL)) DISCOUNT,
DECODE(A.DISC_TYPE,'P',((B.ORD_QTY *B.ITM_PRICE) - ((B.ORD_QTY *B.ITM_PRICE * B.DISC_VAL)/100)),
                   'A',((B.ORD_QTY *B.ITM_PRICE) - B.DISC_VAL)) AMT,B.TOT_DISC_AMT_SP
                  ,C.TAX_RULE_ID,NVL(C.TAX_AMT,0),SUM(NVL(D.OC_AMT_SP,0)),
                  A.DOC_ID,S.ATT_NM PO_TYPE,
                 upper(L.ADDS)adds,
NVL(M.QUOT_ID,(SELECT FL.FLD_VAL
FROM MM$DRFT$PO$FLX FL, APP.APP$FLX$FLD FLN
WHERE FL.FLD_ID = FLN.FLD_ID
AND UPPER(FLN.FLD_NM) LIKE UPPER('Reference Document No%')
AND A.CLD_ID = FL.CLD_ID
AND A.SLOC_ID = FL.SLOC_ID
AND A.ORG_ID = FL.ORG_ID
AND A.DOC_ID = FL.DOC_ID
AND ROWNUM = 1))QUOT_ID,
M.QUOT_DT QUOT_DT,
(SELECT  TO_CHAR(TO_DATE(FL.FLD_VAL, 'DD-MM-YYYY HH24:MI:SS'), 'DD/MM/YYYY')
FROM MM$DRFT$PO$FLX FL, APP.APP$FLX$FLD FLN
WHERE FL.FLD_ID = FLN.FLD_ID
AND UPPER(FLN.FLD_NM) LIKE UPPER('Reference Document Date')
AND A.CLD_ID = FL.CLD_ID
AND A.SLOC_ID = FL.SLOC_ID
AND A.ORG_ID = FL.ORG_ID
AND A.DOC_ID = FL.DOC_ID
AND ROWNUM = 1)QUOT_DT1,
 O.CST_NO ORG_CST_NO,  O.TIN_NO ORG_TIN_NO,
                 O.PAN_NO ORG_PAN_NO,O.ECC_NO ORG_ECC_NO, O.ESI_NO ORG_ESI_NO, NVL(O.REG_NO,O.CIN_NO)  CIN_NO,O.STAX_NO ,
                 P.TIN_NO SUPPLIER_TIN, NVL(Q.ORG_PHONE1,Q.ORG_PHONE2),
                 Q.ORG_FAX1,Q.ORG_EMAIL2,A.PO_AMT_SP,T.CURR_DESC,T.CURR_ID,
      P.ECC_NO,P.PAN_GIR_NO,P.SERVICE_TAX_NO,V.EO_PHONE , V.EO_EMAIL_ID,V.EO_FAX_NO,
      (SELECT SS.ATT_NM FROM APP.APP$DS$ATT SS, MM$DRFT$PO$DLV$SCHDL U
       WHERE U.CLD_ID = A.CLD_ID
       AND U.SLOC_ID = A.SLOC_ID
       AND U.ORG_ID = A.ORG_ID
       AND U.DOC_ID = A.DOC_ID
       AND U.DLV_MODE = SS.ATT_ID
       AND ROWNUM = 1) DISP_MODE,
       (SELECT SS.ATT_NM FROM APP.APP$DS$ATT SS, MM$DRFT$PO$PMT$SCHDL U
       WHERE U.CLD_ID = A.CLD_ID
       AND U.SLOC_ID = A.SLOC_ID
       AND U.ORG_ID = A.ORG_ID
       AND U.DOC_ID = A.DOC_ID
       AND U.PAY_MODE = SS.ATT_ID
       AND ROWNUM = 1) PAY_MODE
       ,(SELECT upper(ADDS) FROM APP.APP$ADDS$VW AD, MM.MM$DRFT$PO$DLV$SCHDL DL
                    WHERE AD.CLD_ID  = DL.CLD_ID
                    AND   AD.SLOC_ID = DL.SLOC_ID
                    AND   AD.ADDS_ID = DL.DLV_ADDS_ID
                    AND   DL.CLD_ID   = B.CLD_ID
                    AND   DL.SLOC_ID   = B.SLOC_ID
                    AND   DL.ORG_ID    = B.ORG_ID
                    AND   DL.DOC_ID    = B.DOC_ID
                    AND   DL.ITM_ID    = B.ITM_ID
                    AND   B.ITM_UOM   = B.ITM_UOM
                    AND   ROWNUM = 1)DELV_ADDS,
                    (SELECT AV.ATT_VAL--APP.FN_GET_ATT_VAL(ATT.SLOC_ID, ATT.CLD_ID, ATT.HO_ORG_ID, ATT.ITM_ID, ATT.ATT_TYPE_ID)
                    FROM APP.APP$ITM$ATT ATT, APP.APP$ITM$ATT$VAL AV
                    WHERE  ATT.CLD_ID  = AV.CLD_ID
                    AND ATT.SLOC_ID = AV.SLOC_ID
                    AND ATT.HO_ORG_ID = AV.HO_ORG_ID
                    AND ATT.ATT_TYPE_ID = AV.ATT_TYPE_ID
                    and att.att_id = av.att_id
                     and att.att_type_id = 'ATT0007'
                   and  ATT.CLD_ID = H.CLD_ID
                    AND ATT.SLOC_ID = H.SLOC_ID
                    AND ATT.HO_ORG_ID = H.HO_ORG_ID
                    AND ATT.ITM_ID = H.ITM_ID
                    AND ROWNUM  =1)ATT_VAL
,(SELECT U.USR_NAME FROM APP.APP$SEC$USR U , MM$DRFT$PO$WF$TXN T
  WHERE T.CLD_ID  = A.CLD_ID
  AND T.SLOC_ID = A.SLOC_ID
  AND T.ORG_ID  = A.ORG_ID
  AND T.TXN_DOC_ID = A.DOC_ID
  AND T.WF_STATUS_ID = 'A'
  AND T.USR_ID_TO = U.USR_ID
  AND ROWNUM = 1)APPROVED_BY
,(select x.EO_PER_NM   FROM APP.APP$EO$PERS$COMM X WHERE X.EO_HO_ORG_ID = P.EO_HO_ORG_ID AND X.EO_ID = P.EO_ID AND ROWNUM = 1)CONT_NM
FROM MM$DRFT$PO A, MM$DRFT$PO$ITM B, MM$DRFT$PO$TR C, MM$DRFT$PO$OC D, APP.ORG E,
      App.App$eo F, App.App$itm$prf H, App.App$uom$conv$std I,
      APP.APP$ADDS$VW  L, MM$QUOT M, APP.ORG$OTH$DTL O, APP.APP$EO$PRF P, APP.ORG$OTH$DTL Q,
      APP.APP$DS$ATT S, app.app$curr t,APP.APP$EO$PERS$COMM v
                 WHERE A.CLD_ID =B.CLD_ID
                 AND A.SLOC_ID =B.SLOC_ID
                 AND A.ORG_ID =B.ORG_ID
                 And A.Doc_Id =B.Doc_Id
                 And A.CLD_Id = O.Org_Cld_Id(+)
                 And A.Org_Id= O.Org_Id(+)
                 And A.CLD_Id = Q.Org_Cld_Id(+)
                 AND A.ORG_ID= Q.ORG_ID(+)
                 AND A.CLD_ID =D.CLD_ID(+)
                 AND A.SLOC_ID =D.SLOC_ID(+)
                 AND A.ORG_ID =D.ORG_ID(+)
                 AND A.DOC_ID =D.DOC_ID(+)
                 AND A.PO_TYPE = S.ATT_ID
                 AND A.ORG_ID =E.ORG_ID
                 AND A.CLD_ID =F.EO_CLD_ID
                 AND A.SLOC_ID =F.EO_SLOC_ID
                 And Mm.Mm_Get_Ho_Org(A.Cld_Id,A.Sloc_Id,A.Org_Id) = F.Eo_Ho_Org_Id
                 AND A.EO_ID =F.EO_ID
                 AND A.CLD_ID =M.CLD_ID(+)
                 AND A.SLOC_ID =M.SLOC_ID(+)
                 AND A.ORG_ID = M.ORG_ID(+)
                 AND A.QUOT_DOC_ID = M.DOC_ID (+)
                 AND B.CLD_ID =C.CLD_ID(+)
                 AND B.SLOC_ID =C.SLOC_ID(+)
                 AND B.ORG_ID =C.ORG_ID(+)
                 AND B.DOC_ID =C.DOC_ID(+)
                 AND B.ITM_ID =C.ITM_ID(+)
                 and b.itm_uom = c.itm_uom(+)
                 AND B.CLD_ID =H.CLD_ID
                 AND B.SLOC_ID =H.SLOC_ID
                 AND MM.MM_GET_HO_ORG(B.CLD_ID,B.SLOC_ID,B.ORG_ID) = H.HO_ORG_ID
                 AND B.ITM_ID =H.ITM_ID
                 AND B.CLD_ID =I.CLD_ID
                 AND B.SLOC_ID =I.SLOC_ID
                 AND B.ITM_UOM =I.UOM_ID
                 AND A.CLD_ID =L.CLD_ID(+)
                 AND A.SLOC_ID =L.SLOC_ID(+)
                 AND A.BILL_ADDS_ID =L.ADDS_ID(+)
                 And A.Cld_Id = P.Eo_Cld_Id(+)
                 And A.Sloc_Id = P.Eo_Sloc_Id(+)
                 And Mm.Mm_Get_Ho_Org(A.Cld_Id,A.Sloc_Id,A.Org_Id) = P.Eo_Ho_Org_Id(+)
                 And A.Eo_Id =P.Eo_Id(+)
                 and a.Cld_Id = t.cld_id
                 and a.sloc_id = t.curr_sloc_id
                 and a.curr_id_sp = t.curr_id

                 and a.Eo_Id =v.Eo_Id(+)
                 and a.Cld_Id = v.eo_cld_id(+)
                 and a.sloc_id = v.eo_sloc_id(+)
    and a.org_id    = v.eo_org_id(+)
                 and mm.mm_get_ho_org(a.cld_id,  a.sloc_id, a.ORG_ID) =v.eo_ho_org_id(+)

                 AND A.CLD_ID= NVL(  $P{CldId} ,A.CLD_ID)
                 AND A.SLOC_ID = NVL( $P{SlocId} ,A.SLOC_ID)
                 AND A.ORG_ID = NVL($P{OrgId}  ,A.ORG_ID)
           AND A.DOC_ID =$P{DocId}
                   GROUP BY A.CLD_ID,A.SLOC_ID,A.ORG_ID,E.ORG_DESC,NVL(A.AUTH_PO_NO,A.PO_ID),A.PO_ID,
                         A.PO_DT,A.AMD_NO,A.AMD_DT,F.EO_NM,
                         B.ITM_ID,H.ITM_DESC,I.UOM_DESC,B.ORD_QTY,B.ITM_PRICE,
DECODE(A.DISC_TYPE,'P',(((B.ORD_QTY *B.ITM_PRICE * B.DISC_VAL)/100)),
                   'A',( B.DISC_VAL)) ,
DECODE(A.DISC_TYPE,'P',((B.ORD_QTY *B.ITM_PRICE) - ((B.ORD_QTY *B.ITM_PRICE * B.DISC_VAL)/100)),
                   'A',((B.Ord_Qty *B.Itm_Price) - B.Disc_Val)),b.tot_disc_amt_sp
                  ,C.Tax_Rule_Id,A.Doc_Id,L.Adds,M.Quot_Id,M.Quot_Dt,Nvl(C.Tax_Amt,0),O.Cst_No,O.Tin_No,P.tin_NO ,
                  NVL(Q.ORG_PHONE1,Q.ORG_PHONE2), Q.ORG_FAX1,Q.ORG_EMAIL2,A.PO_AMT_SP,S.ATT_NM,t.curr_desc,t.curr_id,a.remarks,
                  A.BILL_ADDS_ID,o.org_add_id1,o.org_add_id2,P.ECC_NO,P.PAN_GIR_NO,v.eo_phone , v.eo_email_id,v.eo_fax_no
                ,B.ITM_UOM ,B.ITM_ID, B.DOC_ID, B.ORG_ID, B.SLOC_ID, B.CLD_ID, O.PAN_NO, O.ECC_NO , O.ESI_NO,O.REG_NO
                ,H.CLD_ID, H.SLOC_ID, H.HO_ORG_ID, H.ITM_ID, O.CIN_NO,O.STAX_NO,P.SERVICE_TAX_NO,P.EO_HO_ORG_ID, P.EO_ID
                  ORDER BY A.PO_ID,B.ITM_ID]]>
