	<queryString>
		<![CDATA[SELECT ORG_DESC,assigned_to,EO_NM,INVOICE_ID,INVOICE_DOC_DT,CURR_NOTATION_ALIAS,SUM(ITM_AMT_BS) AS TOT_AMT_BS,SUM(ITM_COST) AS ITM_COST,SUM(PROFIT) AS PROFIT,SUM(PROFIT)/decode (SUM(ITM_AMT_BS),0,1,SUM(ITM_AMT_BS))*100 AS PROFIT_PER
FROM (SELECT
  DISTINCT(A.SHIPMNT_ID),
  A.CLD_ID,
  A.SLOC_ID,
  A.ORG_ID,
  A.HO_ORG_ID,
  A.INVOICE_DOC_ID,
  SUBSTR(A.INVOICE_ID,3) INVOICE_ID,
  A.INVOICE_DOC_DT,
  A.CURR_ID_SP,
  A.CURR_RATE,
  A.EO_ID,
  A.BILL_ADD,
  A.EO_NM,
  A.SHIPMNT_DT,
  A.SO_ID,
  C.DOC_DT SO_DT,
  A.ITM_ID,
  B.ITM_DESC ITM_DESC,
  A.SHIP_AMT_SP,
  A.SHIP_AMT_BS,
  A.ITM_RATE,
  A.ITM_UOM,
  E.UOM_NM UOM_NM ,
  A.ITM_SHIP_QTY,
  A.ITM_DISC_TYP,
  A.ITM_DISC_VAL,
  A.ITM_AMT_BS,
  A.ITM_AMT_SP,
  A.TOT_AMT_SP,
  A.TOT_AMT_BS,
  A.ITM_COST,
  nvl(A.ITM_AMT_Bs,0)-nvl(A.ITM_COST,0) Profit,
  (nvl(A.ITM_AMT_Bs,0)-nvl(A.ITM_COST,0)
/nvl(decode(A.ITM_AMT_BS,0,1,A.ITM_AMT_BS),1))*100 profit_per,
nvl(l.eo_nm,'Un Assigned') assigned_to,
(DECODE
(A.INV_MODE,314,'APPROVED',315,'AMENDED',316,'HOLD',317,'FORWARDED',313,'INCOMPLETE','INCOMPLETE'))
INV_WF_STAT_DESC,
  F.ORG_DESC,
  (SELECT ADDS FROM APP.APP$ADDS$VW WHERE APP.APP$ADDS$VW.ADDS_ID =
A.BILL_ADD) ADDS,g.CURR_NOTATION_ALIAS
FROM
  SLS$INVOICE$DTL_VW A,
  APP.APP$SLS$ITM_VW B,
  SLS.SLS$SO C,
  APP.APP$UOM$CONV$STD E,
  APP.ORG F,
  app.app$curr_vw g,
(SELECT eo_cld_id,
      x.eo_sloc_id,
      x.eo_ho_org_id,
      x.eo_id,
      x.eo_nm
    FROM app.app$eo x
    WHERE x.eo_cld_id = nvl($P{CldId},x.eo_cld_id)
    AND x.eo_sloc_id  = nvl($P{SlocId},x.eo_sloc_id)
    AND x.eo_type     = '80'
    ) l
WHERE
  A.SLOC_ID = B.SLOC_ID AND
  A.ITM_ID = B.ITM_ID AND
  A.ORG_ID = B.ORG_ID AND
  A.CLD_ID = B.CLD_ID AND
  A.HO_ORG_ID = B.HO_ORG_ID AND
  A.SLOC_ID = C.SLOC_ID AND
  A.ORG_ID = C.ORG_ID AND
  A.CLD_ID = C.CLD_ID AND
  A.HO_ORG_ID = C.HO_ORG_ID AND
  A.SO_ID = C.SO_ID AND
  A.SLOC_ID = E.SLOC_ID AND
  A.CLD_ID = E.CLD_ID AND
  A.ITM_UOM = E.UOM_ID AND
  A.CLD_ID = F.ORG_CLD_ID AND
  A.ORG_ID = F.ORG_ID AND
  A.inv_mode=314 AND
C.CLD_ID=l.eo_cld_id(+) AND
  c.sloc_id=l.eo_sloc_id(+) and
  C.HO_ORG_ID=l.eo_ho_org_id(+) AND
  c.assigned_to=l.eo_id(+) and
  A.curr_id_sp=g.curr_id and
  A.CLD_ID = nvl($P{CldId},a.cld_id) AND
  A.SLOC_ID = nvl($P{SlocId},a.sloc_id) AND
  A.ORG_ID = nvl($P{OrgId},a.org_id) AND
  A.HO_ORG_ID = nvl($P{HoOrgId},a.ho_org_id) AND
  A.INVOICE_DOC_ID = nvl($P{DocID},A.INVOICE_DOC_ID) AND
  A.ITM_ID=nvl($P{ItmId},a.itm_id) and
  a.grp_id =nvl($P{GrpId},a.grp_id) and
a.eo_id = nvl($P{EoId},a.eo_id ) and
A.EO_CATG=NVL( $P{EoCatId},A.EO_CATG)
and NVL (C.order_stat, 217)   = NVL ($P{Status} , NVL (C.order_stat, 217)) and
  A.CURR_ID_SP=nvl($P{CurrId},a.curr_id_sp) and
C.ASSIGNED_TO=NVL($P{AssignTo},C.ASSIGNED_TO) and
  a.inv_type=nvl($P{InvTyp},a.inv_type) and
  a.invoice_doc_dt >= nvl (to_date ( $P{FromDate}, 'yyyy-MM-dd'),
a.invoice_doc_dt) and
  A.invoice_doc_dt <= nvl (to_date ( $P{ToDate} , 'yyyy-MM-dd'),
A.invoice_doc_dt))
GROUP BY ORG_DESC,assigned_to,EO_NM,INVOICE_ID,INVOICE_DOC_DT,CURR_NOTATION_ALIAS]]>
	</queryString>
