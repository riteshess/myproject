	<queryString>
		<![CDATA[SELECT DC_DETAIL.CUST_NAME,DC_DETAIL.CUST_ADDRESS,DC_DETAIL.EO_ADDS,DC_DETAIL.CLD_ID,DC_DETAIL.SLOC_ID,DC_DETAIL.HO_ORG_ID,
       DC_DETAIL.ORG_ID,DC_DETAIL.CONTACT_NAME,DC_DETAIL.REMARKS,DC_DETAIL.SALES,DC_DETAIL.NO_OF_VISIT,
       LEVEL_DET.ADDS_LBL,LEVEL_DET.LBL_ID,LEVEL_DET.ADDS_LOC,MTH,
         (SELECT ORG_DESC FROM APP.ORG
               WHERE
               ORG_CLD_ID=DC_DETAIL.CLD_ID AND
               ORG_ID=DC_DETAIL.ORG_ID
       )ORG_DESC,
( SELECT B.ADDS_LOC
          FROM APP.APP$ADDS$LBL A, APP.APP$ADDS$LBL$VLU B, APP.APP$ADDS$BK$DTL C
         WHERE A.CLD_ID = B.CLD_ID
           AND A.SLOC_ID = B.SLOC_ID
           AND A.LBL_ID = B.LBL_ID
           AND B.CLD_ID = C.CLD_ID
           AND B.SLOC_ID = C.SLOC_ID
           AND B.LBL_ID = C.LBL_ID
           AND B.LOC_ID=C.LOC_ID
	 AND DC_DETAIL.EO_ADDS=C.ADDS_ID
	 AND A.LBL_ID='LBL001'
           ) state,
( SELECT B.ADDS_LOC
          FROM APP.APP$ADDS$LBL A, APP.APP$ADDS$LBL$VLU B, APP.APP$ADDS$BK$DTL C
         WHERE A.CLD_ID = B.CLD_ID
           AND A.SLOC_ID = B.SLOC_ID
           AND A.LBL_ID = B.LBL_ID
           AND B.CLD_ID = C.CLD_ID
           AND B.SLOC_ID = C.SLOC_ID
           AND B.LBL_ID = C.LBL_ID
           AND B.LOC_ID=C.LOC_ID
	 AND DC_DETAIL.EO_ADDS=C.ADDS_ID
	 AND A.LBL_ID='LBL002'
           ) city,
DC_DETAIL.DOC_DT,DC_DETAIL.SALES_EXEC,DC_DETAIL.eXPCTD_AMT,
DC_DETAIL.EXPCTD_CLOSER_ON,DC_DETAIL.DC_DESC,SUBSTR(DC_DETAIL.DC_ID,3) DC_ID,DC_DETAIL.NEXT_ACTION,DC_DETAIL.NEXT_DATE,DC_DETAIL.docdate
FROM

(SELECT CUST_NAME,CUST_ADDRESS,EO_ADDS,CLD_ID,SLOC_ID,HO_ORG_ID,ORG_ID,CONTACT_NAME,MTH,REMARKS,SUM(SALES_VALUE)SALES,COUNT(EO_ID)NO_OF_VISIT
,DOC_DT,SALES_EXEC,eXPCTD_AMT,
EXPCTD_CLOSER_ON,DC_DESC,DC_ID,NEXT_ACTION,NEXT_DATE,docdate
  FROM

   (SELECT
        A.EO_ID,A.EO_ADDS,A.CLD_ID,A.SLOC_ID,A.HO_ORG_ID,A.ORG_ID,
       (SELECT EO_NM
                FROM APP.APP$EO E
                      WHERE
                        E.EO_CLD_ID=A.CLD_ID AND
                        E.EO_SLOC_ID=A.SLOC_ID AND
                        E.EO_HO_ORG_ID=A.HO_ORG_ID AND
                        E.EO_ID=A.EO_ID
                                        )CUST_NAME,
       (SELECT ADDS
                FROM APP.APP$ADDS$VW G
                      WHERE
                        G.SLOC_ID=A.SLOC_ID AND
                        G.CLD_ID=A.CLD_ID AND
                        G.ADDS_ID=A.EO_ADDS
                                        )CUST_ADDRESS,


       A.CONTACT_NM CONTACT_NAME,
       A.DC_DESC REMARKS,
       D.TOT_AMT_SP SALES_VALUE,
       TO_CHAR(A.DOC_DT,'MON')MTH,a.doc_dt docdate
       ,TO_CHAR(A.doc_dt,'dd-mm-yyyy') DOC_DT
,s.eo_nm SALES_EXEC,a.eXPCTD_AMT,
a.EXPCTD_CLOSER_ON,a.DC_DESC,a.DC_ID,a.NEXT_ACTION,a.NEXT_DATE


     FROM
    SLS$DC A,SLS$OPP$DC B,SLS$QUOT C,SLS$SO D,
(SELECT eo_cld_id cld_id, x.eo_sloc_id sloc_id, x.eo_ho_org_id ho_org_id,

                                  x.eo_id, x.eo_nm

                             FROM app.app$eo x

                            WHERE x.eo_cld_id = NVL ($P{CldId}, x.eo_cld_id)

                              AND x.eo_sloc_id = NVL ($P{SlocId},
x.eo_sloc_id)

                              AND x.eo_type = '80') s

    WHERE

    A.CLD_ID=B.CLD_ID(+) AND
    A.SLOC_ID=B.SLOC_ID(+) AND
    A.ORG_ID=B.ORG_ID(+) AND
    A.HO_ORG_ID=B.HO_ORG_ID(+) AND
    A.DOC_ID=B.DC_ID(+) AND

    B.CLD_ID=C.CLD_ID(+) AND
    B.SLOC_ID=C.SLOC_ID(+) AND
    B.ORG_ID=C.ORG_ID(+) AND
    B.HO_ORG_ID=C.HO_ORG_ID(+) AND
    B.DOC_ID=C.ENQ_ID(+) AND

    C.CLD_ID=D.CLD_ID(+) AND
    C.SLOC_ID=D.SLOC_ID(+) AND
    C.ORG_ID=D.ORG_ID(+) AND
    C.HO_ORG_ID=D.HO_ORG_ID(+) AND
    C.DOC_ID=D.QUOT_ID(+)
 and a.cld_id=s.cld_id

   and a.sloc_id = s.sloc_id

   AND a.ho_org_id = s.ho_org_id

   and A.ASSIGNED_TO=s.eo_id and
    trunc(a.doc_dt) >=NVL (TO_DATE ( $P{FromDate},
'yyyy-MM-dd'),trunc(A.DOC_DT))
AND trunc(a.doc_dt) <=NVL (TO_DATE ($P{ToDate} , 'yyyy-MM-dd'),
trunc(A.DOC_DT))
    )

 GROUP BY

 CUST_NAME,CUST_ADDRESS,CONTACT_NAME,REMARKS,EO_ADDS,CLD_ID,SLOC_ID,HO_ORG_ID,ORG_ID,MTH,DOC_DT,SALES_EXEC,eXPCTD_AMT,
EXPCTD_CLOSER_ON,DC_DESC,DC_ID,NEXT_ACTION,NEXT_DATE,docdate)DC_DETAIL,

(SELECT CLD_ID,SLOC_ID,LBL_ID, ADDS_LBL, ADDS_LOC, LOC_ID,ADDS_ID,LEVEL_ID FROM (
        SELECT A.CLD_ID,A.SLOC_ID,A.LBL_ID, A.ADDS_LBL, B.ADDS_LOC, B.LOC_ID,C.ADDS_ID,LEVEL_ID
          FROM APP.APP$ADDS$LBL A, APP.APP$ADDS$LBL$VLU B, APP.APP$ADDS$BK$DTL C
         WHERE A.CLD_ID = B.CLD_ID
           AND A.SLOC_ID = B.SLOC_ID
           AND A.LBL_ID = B.LBL_ID
           AND B.CLD_ID = C.CLD_ID
           AND B.SLOC_ID = C.SLOC_ID
           AND B.LBL_ID = C.LBL_ID
           AND B.LOC_ID=C.LOC_ID

	 )
)LEVEL_DET

WHERE

DC_DETAIL.CLD_ID=LEVEL_DET.CLD_ID AND
DC_DETAIL.SLOC_ID=LEVEL_DET.SLOC_ID AND
DC_DETAIL.EO_ADDS=LEVEL_DET.ADDS_ID
--LEVEL_DET.LBL_ID=nvl(,LEVEL_DET.LBL_ID)

order by mth,doc_dt desc]]>
	</queryString>
