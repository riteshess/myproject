	<queryString>
		<![CDATA[SELECT SUBSTR (a.shipmnt_id, 3) shipmnt_id, a.cld_id, a.eo_catg,
                a.sloc_id, a.org_id, a.ho_org_id, a.invoice_doc_id,w.wh_nm,w.wh_id,
                SUBSTR (a.invoice_id, 3) invoice_id, to_date(a.invoice_doc_dt,'dd-mm-yyyy') invoice_date,
                a.inv_type, a.inv_typ_nm, a.curr_id_sp, a.curr_rate, a.eo_id,
                a.bill_add, a.eo_nm, a.shipmnt_dt, a.so_id, c.doc_dt so_dt,
                a.itm_id, b.itm_desc itm_desc, a.ship_amt_sp, a.ship_amt_bs,
                a.itm_rate, a.itm_uom, e.uom_nm uom_nm, a.itm_ship_qty,
                NVL (DECODE (a.itm_disc_typ,
                             'A', a.itm_disc_val,
                             'P', (  (  NVL (a.itm_ship_qty, 0)* NVL (itm_rate, 0))* NVL (a.itm_disc_val, 0)/ 100)),
                             0) discount,
                NVL (a.itm_ship_qty, 0) * NVL (itm_rate, 0) itm_amt_gs,
                a.itm_amt_bs, a.itm_amt_sp, a.tot_amt_sp, a.tot_amt_bs,
                a.itm_cost, m.att_nm inv_wf_stat_desc, f.org_desc,
                l.eo_nm assigned_to, m.att_nm inv_mode,
                (SELECT adds
                   FROM app.app$adds$vw
                  WHERE app.app$adds$vw.adds_id = a.bill_add) adds,
                avg(NVL (n.tax_amt_sp, 0)) tax_amt_sp, a.grp_nm, a.grp_nm, o.curr_notation_alias
           FROM sls$invoice$dtl_vw a,    app.app$sls$itm_vw b,    sls.sls$so c,  app.app$uom$conv$std e,  app.org f,  APP.APP$WH$ORG w,
                (SELECT eo_cld_id, x.eo_sloc_id, x.eo_ho_org_id, x.eo_id, x.eo_nm
                   FROM app.app$eo x
                  WHERE x.eo_cld_id = NVL ($P{CldId}, x.eo_cld_id)
                    AND x.eo_sloc_id = NVL ($P{SlocId}, x.eo_sloc_id)
                    AND x.eo_type = '80') l,
                (SELECT y.att_nm, y.att_id
                   FROM app.app$ds$att y
                  WHERE att_type_id = 69) m,
                (SELECT att_id, att_nm
                   FROM app.app$ds$att
                  WHERE att_type_id = 74) z,
                sls.sls$inv$tr n,
                app.app$curr_vw o
          WHERE a.sloc_id = b.sloc_id
            AND a.itm_id = b.itm_id
            AND a.org_id = b.org_id
            AND a.cld_id = b.cld_id
            AND a.ho_org_id = b.ho_org_id
            AND a.sloc_id = c.sloc_id
            AND a.org_id = c.org_id
            AND a.cld_id = c.cld_id
            AND a.ho_org_id = c.ho_org_id
            AND a.so_id = c.so_id
            AND a.sloc_id = e.sloc_id
            AND a.cld_id = e.cld_id
            AND a.itm_uom = e.uom_id
            AND a.cld_id = f.org_cld_id
            AND a.org_id = f.org_id
            AND c.cld_id = l.eo_cld_id(+)
            AND c.sloc_id = l.eo_sloc_id(+)
            AND c.ho_org_id = l.eo_ho_org_id(+)
            AND c.assigned_to = l.eo_id(+)
            AND a.inv_mode = m.att_id
            AND a.eo_catg = z.att_id
            AND a.cld_id = n.cld_id(+)
            AND a.sloc_id = n.sloc_id(+)
            AND a.ho_org_id = n.ho_org_id(+)
        	  and a.org_id=n.org_id(+)
            AND a.invoice_doc_id = n.doc_id(+)
            AND a.itm_id = n.itm_id(+)
	          AND a.curr_id_sp = o.curr_id
            AND c.CLD_ID =w.CLD_ID
            AND c.SLOC_ID =w.SLOC_ID
            AND c.ORG_ID =w.ORG_ID
            AND c.WH_ID =w.WH_ID
            and  A.CLD_ID = nvl($P{CldId},a.cld_id)
            AND  A.SLOC_ID = nvl($P{SlocId},a.sloc_id)
            AND  A.ORG_ID = nvl($P{OrgId},a.org_id)
            AND  A.HO_ORG_ID = nvl($P{HoOrgId},a.ho_org_id)
            AND  A.INVOICE_DOC_ID = nvl($P{DocId},A.INVOICE_DOC_ID)
            AND  A.ITM_ID=nvl($P{ItmId},a.itm_id)
            and  a.grp_id =nvl($P{GrpId},a.grp_id)
            and  NVL (C.order_stat, 217)   = NVL ($P{Status}, NVL (C.order_stat, 217))
            and A.EO_CATG=NVL($P{EoCatId},A.EO_CATG)
            and  A.CURR_ID_SP=nvl($P{CurrId},a.curr_id_sp)
            and  a.inv_type=nvl($P{InvType},a.inv_type)
            and  a.invoice_doc_dt >= nvl (to_date ($P{FromDate}, 'yyyy-MM-dd'), a.invoice_doc_dt)
            and  a.invoice_doc_dt <= nvl (to_date ($P{ToDate}, 'yyyy-MM-dd'), a.invoice_doc_dt)
  group by  NVL (DECODE (a.itm_disc_typ,
                             'A', a.itm_disc_val,
                             'P', (  (  NVL (a.itm_ship_qty, 0)* NVL (itm_rate, 0))* NVL (a.itm_disc_val, 0)/ 100)),
                             0),SUBSTR (a.shipmnt_id, 3) , a.cld_id, a.eo_catg,
                a.sloc_id, a.org_id, a.ho_org_id, a.invoice_doc_id,w.wh_nm,w.wh_id,
                SUBSTR (a.invoice_id, 3) , to_date(a.invoice_doc_dt,'dd-mm-yyyy'),
                a.inv_type, a.inv_typ_nm, a.curr_id_sp, a.curr_rate, a.eo_id,
                a.bill_add, a.eo_nm, a.shipmnt_dt, a.so_id, c.doc_dt ,
                a.itm_id, b.itm_desc , a.ship_amt_sp, a.ship_amt_bs,
                a.itm_rate, a.itm_uom, e.uom_nm , a.itm_ship_qty,
                 NVL (a.itm_ship_qty, 0) * NVL (itm_rate, 0) ,
                a.itm_amt_bs, a.itm_amt_sp, a.tot_amt_sp, a.tot_amt_bs,
                a.itm_cost, m.att_nm, f.org_desc,
                l.eo_nm , m.att_nm ,
                a.grp_nm,
                o.curr_notation_alias
order by eo_nm]]>
	</queryString>
