	<queryString>
		<![CDATA[select e.eo_id,e.eo_nm,e.eo_org_id,e.eo_ho_org_id,d.eo_phone,d.eo_email_id,d.eo_fax_no,d.eo_per_nm,
(select att_nm from app.app$ds$att
               where att_id=e.catg_id)eo_type,
(select org_desc from app.org
                 where org_id=e.eo_org_id
                 and org_cld_id=e.eo_cld_id
                 and org_create_ref_sloc_id=e.eo_sloc_id)org_desc,
(select adds from app.app$adds$vw
             where adds_id=(select a.adds_id from app.app$eo$adds a
                                           where a.cld_id=e.eo_cld_id
                                           and   a.sloc_id=e.eo_sloc_id
                                           and   a.org_id=e.eo_org_id
                                           and   a.ho_org_id=e.eo_ho_org_id
                                           and   a.eo_id    =e.eo_id
                                           and ship_adds_dflt='Y'
                                           and rownum = 1)) cust_adds,
(select curr_notation_alias from app.app$curr_vw
		       where curr_id=e.curr_id_sp)cust_curr,
p.avg_dlv_days,p.avg_cr_days,cr_limit
   from app.app$eo e,app.app$eo$pers$comm d,app.app$eo$prf p
   where e.eo_cld_id = d.eo_cld_id(+)
   and e.eo_sloc_id = d.eo_sloc_id(+)
   and e.eo_org_id = d.eo_org_id(+)
   and e.eo_ho_org_id = d.eo_ho_org_id(+)
   and e.eo_id    =   d.eo_id(+)

   and e.eo_cld_id = p.eo_cld_id(+)
   and e.eo_sloc_id = p.eo_sloc_id(+)
   and e.eo_org_id = p.eo_org_id(+)
   and e.eo_ho_org_id = p.eo_ho_org_id(+)
   and e.eo_id    =   p.eo_id(+)
   and e.eo_type = p.eo_type(+)

   and e.eo_type in(68,81)
   and e.eo_cld_id = NVL ($P{CldId}, e.eo_cld_id)
   AND e.eo_sloc_id = NVL ( $P{SlocId}, e.eo_sloc_id)
   AND e.eo_org_id = NVL ( $P{OrgId}, e.eo_org_id)
   AND e.eo_ho_org_id = NVL ( $P{HoOrgId}, e.eo_ho_org_id)
   and e.eo_id=nvl($P{EoId} ,e.eo_id)
   AND trunc(e.usr_id_create_dt) >= NVL (TO_DATE ( $P{FromDate}, 'yyyy-MM-dd'), trunc(e.usr_id_create_dt))
   AND trunc(e.usr_id_create_dt) <= NVL (TO_DATE ( $P{ToDate} , 'yyyy-MM-dd'), trunc(e.usr_id_create_dt))
   order by eo_id]]>
	</queryString>
