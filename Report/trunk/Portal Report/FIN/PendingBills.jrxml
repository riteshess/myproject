	<queryString>
		<![CDATA[select
substr(d.DOC_TXN_ID_DISP,3) DOC_TXN_ID_DISP,
B.ORG_DESC,
C.COA_NM,
a.ARAP_VOU_ID,
a.ARAP_VOU_DT,
a.ARAP_AMT_SP,
a.ARAP_NET_AMT_OT,
a.ARAP_AMT_ADJ,
a.arap_ext_doc_no,
a.arap_ext_doc_dt,
a.ARAP_VOU_DUE_DT,
(a.ARAP_AMT_SP-a.ARAP_AMT_ADJ) as PendingAmt,
(select eo_mst_nm from app.app$eo$mst
		where eo_cld_id = $P{Cld_Id}
		and eo_mst_sloc_id = $P{Sloc_Id}
		and eo_ho_org_id = $P{Ho_Org_Id}
		 and eo_mst_id = $P{Entity_Group}) entity_grp
from arap_os_vw a,app.org b,fin$coa c,app.app$doc$txn d
where
a.ARAP_COA_ID= nvl($P{Coa_Id},a.ARAP_COA_ID)
and a.ARAP_EOMST_ID = nvl($P{Entity_Group},a.ARAP_EOMST_ID)
and C.COA_HO_ORG_ID = nvl($P{Ho_Org_Id},C.COA_HO_ORG_ID)
and A.ARAP_ORG_ID = nvl($P{Org_Id},A.ARAP_ORG_ID)
and a.ARAP_AMT_SP >= nvl($P{AmountX1},0)
AND a.ARAP_AMT_SP <= nvl($P{AmountX2},999999999999999999)
and  a.ARAP_CURR_ID_SP= nvl($P{Curr_Id},a.ARAP_CURR_ID_SP)
and a.ARAP_VOU_DT >= to_date($P{From_Date},'YYYY-MM-DD')
and a.ARAP_VOU_DT <= to_date($P{To_Date},'YYYY-MM-DD')

and a.ARAP_TXN_ADV_FLG='N'
and a.ARAP_FLG ='AP'

and a.ARAP_AMT_TYP='Cr'
--and d.doc_typ_id = 8

and A.ARAP_ORG_ID = B.ORG_ID

and A.ARAP_COA_ID = C.COA_ID

and a.ARAP_VOU_ID = d.DOC_TXN_ID]]>
	</queryString>
