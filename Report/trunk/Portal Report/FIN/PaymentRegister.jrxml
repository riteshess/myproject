	<queryString>
		<![CDATA[select a.org_desc,
       b.coa_cld_id,
       a.org_id,
       b.coa_id,
       b.coa_nm,

       null          eo_phone, --h.eo_phone,
       null          eo_email_id, --h.eo_email_id,
       null          eo_fax_no, --h.eo_fax_no,
       i.avg_cr_days avg_cr_days, --i.avg_cr_days,
       i.cr_limit    cr_limit, --i.cr_limit,

       --op.org_coa_op_bal open_bal,

       nvl(op.org_coa_op_bal, 0) + nvl(op1.gl_amt, 0) open_bal,

       --op.org_coa_op_bal_typ org_coa_op_bal_typ,

       decode(sign(nvl(op.org_coa_op_bal, 0) + nvl(op1.gl_amt, 0)),
              1,
              'Dr',
              0,
              'Dr',
              -1,
              'Cr') org_coa_op_bal_typ,

       --comment this to get opening balance

       e.voucher_date,
       to_char(e.voucher_date, 'MON-YY') month,
       e.vou_id,
       e.doc_txn_id_disp,
       e.sales_invoice_doc_id,
   (decode(e.voucher_date,to_date('31-03-2014','dd-mm-rrrr'),e.sales_invoice_id,e.debtors_invoice)) sales_invoice_id,
       --e.sales_invoice_id,
	   decode(e.voucher_date,to_date('31-03-2014','dd-mm-rrrr'),e.sales_invoice_date,e.debtors_invoice_date) sales_invoice_date,
       --e.sales_invoice_date,
   (decode(e.voucher_date,to_date('31-03-2014','dd-mm-rrrr'),e.debtors_invoice,e.sales_invoice_id)) debtors_invoice,
       --e.debtors_invoice,
	   decode(e.voucher_date,to_date('31-03-2014','dd-mm-rrrr'),e.debtors_invoice_date,e.sales_invoice_date) debtors_invoice_date,
       --e.debtors_invoice_date,
       e.due_date,
       e.over_due_days,
       e.curr_nm,
       nvl(e.ap_amt_sp, 0) ap_amt_sp,
       e.ap_amt_typ BalanceType,
       e.rate,
       nvl(e.damt, 0) damt,
       nvl(e.camt, 0) camt,
       nvl(e.amt, 0) amt,
       b.coa_ho_org_id,
       (select eo_mst_nm from app.app$eo$mst
		where eo_cld_id = $P{Cld_Id}
		and eo_mst_sloc_id = $P{Sloc_Id}
		and eo_ho_org_id = $P{Ho_Org_Id}
		 and eo_mst_id = $P{Entity_Group}) entity_grp
  from app.org    a,
       fin$coa    b,
       org$coa    b1,
       fin$acc$na c,
       app.app$eo d,
       --app.app$eo$pers$comm h,
       app.app$eo$prf i,
       (select a.org_coa_cld_id,
               a.org_id,
               a.org_fy_id,
               a.org_coa_id,
               sum(decode(a.org_coa_op_bal_typ,
                          'Dr',
                          a.org_coa_op_bal,
                          -a.org_coa_op_bal)) org_coa_op_bal,
               decode(sign(sum(decode(a.org_coa_op_bal_typ,
                                      'Dr',
                                      a.org_coa_op_bal,
                                      -a.org_coa_op_bal))),
                      1,
                      'Dr',
                      0,
                      'Dr',
                      'Cr') org_coa_op_bal_typ
          from org$coa$fy a,
               (select app.fn_get_fy_id($P{Cld_Id},
                                        $P{Org_Id},
                                        to_date($P{From_Date}, 'yyyy-MM-dd'),
                                        'FY') fy_id
                  from dual) b
         where a.org_coa_cld_id = nvl($P{Cld_Id}, a.org_coa_cld_id)
           and a.org_id = nvl($P{Org_Id}, a.org_id)
           and a.org_fy_id = b.fy_id
           and a.org_coa_id = nvl($P{Coa_Id}, a.org_coa_id)
         group by org_coa_cld_id, org_id, org_fy_id, org_coa_id) op,
       (select a.gl_cld_id,
               a.gl_org_id,
               a.gl_coa_id,
               nvl(sum(decode(gl_amt_typ, 'Dr', gl_amt_bs, 'Cr', -gl_amt_bs)),
                   0) gl_amt
          from gl_lines a,
               gl b,
               (select app.fn_get_fy_start_date($P{Cld_Id},
                                                $P{Org_Id},
                                                TO_DATE($P{From_Date},
                                                        'YYYY-mm-dd'),
                                                'FY') fy_start_date
                  from dual) c,
               app.app$eo d
         where b.gl_cld_id = nvl($P{Cld_Id}, b.gl_cld_id)
           and a.gl_org_id = nvl($P{Org_Id}, a.gl_org_id)
           and a.gl_coa_id = nvl($P{Coa_Id}, a.gl_coa_id)
           and b.gl_stat = 'FLC'
           and a.gl_cld_id = b.gl_cld_id
           and a.gl_sloc_id = b.gl_sloc_id
           and a.gl_vou_id = b.gl_vou_id
           and trunc(a.gl_vou_dt) >= c.fy_start_date
           and trunc(a.gl_vou_dt) < to_date($P{From_Date}, 'YYYY-mm-dd')
           and a.gl_txn_typ_mig not in ('O', 'MO')

           and d.eo_cld_id = a.gl_cld_id
           and d.eo_sloc_id = a.gl_sloc_id
           and d.eo_ho_org_id = a.gl_ho_org_id
           and d.eo_id = a.gl_eo_id
           and d.eo_bhav = 'C'

         group by a.gl_cld_id, a.gl_org_id, a.gl_coa_id) op1,
       (select a.gl_cld_id cld_id,
               a.ap_org_id org_id,
               a.ap_coa_id coa_id,
               count(*) cnt
          from ap a
         where a.gl_cld_id = $P{Cld_Id}
           and a.ap_org_id = nvl($P{Org_Id}, a.ap_org_id)
           and trunc(a.ap_vou_dt) >=
               to_date(NVL($P{From_Date}, '1000-01-01'), 'YYYY-MM-DD')
           and trunc(a.ap_vou_dt) <=
               to_date(NVL($P{To_Date}, SYSDATE), 'YYYY-MM-DD')

           and nvl(ap_amt_sp, 0) * nvl(ap_cc, 1) > 0
           and ap_curr_id_sp = nvl($P{Curr_Id}, a.ap_curr_id_sp)
           and a.ap_coa_id = nvl($P{Coa_Id}, a.ap_coa_id)
         group by a.gl_cld_id, a.ap_org_id, a.ap_coa_id) x1,

       -- add this---

       (select a.tvou_cld_id cld_id,
               a.tvou_org_id org_id,
               a.tvou_coa_id coa_id,
               count(*) cnt
          from tvou_lines a, app.app$eo f
         where a.tvou_cld_id = $P{Cld_Id}
           and a.tvou_org_id = nvl($P{Org_Id}, a.tvou_org_id)
           and a.tvou_id =
               decode($P{PostFlag}, 'P', '1', 'A', a.tvou_id, a.tvou_id)
           and trunc(a.tvou_dt) >=
               to_date(NVL($P{From_Date}, '1000-01-01'), 'YYYY-MM-DD')
           and trunc(a.tvou_dt) <=
               to_date(NVL($P{To_Date}, SYSDATE), 'YYYY-MM-DD')
           and a.tvou_amt_bs > 0

           and a.tvou_curr_id_sp = nvl($P{Curr_Id}, a.tvou_curr_id_sp)
           and a.tvou_coa_id = nvl($P{Coa_Id}, a.tvou_coa_id)

           and f.eo_cld_id = a.tvou_cld_id
           and f.eo_sloc_id = a.tvou_sloc_id
           and f.eo_ho_org_id = a.tvou_ho_org_id
           and f.eo_id = a.tvou_eo_id
           and f.eo_bhav = 'C'
         group by a.tvou_cld_id, a.tvou_org_id, a.tvou_coa_id) x2,
       --comment this to get opening balance
       (select a.gl_cld_id cld_id,
               a.ap_sloc_id sloc_id, -- add this here
               a.ap_org_id org_id,
               a.ap_coa_id coa_id,
               f.glbl_curr_notation_alias curr_nm,
               a.ap_vou_id vou_id,
              substr(b.doc_txn_id_disp, 3) doc_txn_id_disp,
               a.ap_vou_dt voucher_date,
               a.ap_doc_id_ref1 sales_invoice_doc_id,
               a.ap_txn_id_ref1 sales_invoice_id,
               a.ap_txn_dt_ref1 sales_invoice_date,
               decode(a.ap_amt_typ,
                      'Dr',
                      substr(a.ap_ext_doc_no, 3),
                      a.ap_ext_doc_no) debtors_invoice,
               a.ap_ext_doc_dt debtors_invoice_date,
               a.ap_vou_due_dt due_date,
               ((to_date(nvl($P{To_Date}, sysdate), 'YYYY-MM-DD')) -
               a.ap_vou_due_dt) over_due_days,
               a.ap_amt_sp,
               a.ap_amt_typ,
               nvl(a.ap_cc, 1) rate,
               decode(ltrim(rtrim(upper(a.ap_amt_typ))),
                      'DR',
                      nvl(a.ap_amt_sp, 0) * nvl(ap_cc, 1),
                      0) damt,
               decode(ltrim(rtrim(upper(a.ap_amt_typ))),
                      'CR',
                      nvl(a.ap_amt_sp, 0) * nvl(ap_cc, 1),
                      0) camt,
               decode(ltrim(rtrim(upper(a.ap_amt_typ))),
                      'DR',
                      nvl(a.ap_amt_sp, 0) * nvl(ap_cc, 1),
                      'CR',
                      -nvl(a.ap_amt_sp, 0) * nvl(ap_cc, 1)) amt
          from ap a, app.app$doc$txn b, app.app$glbl$curr f
         where a.gl_cld_id = $P{Cld_Id}
           and a.ap_org_id = nvl($P{Org_Id}, a.ap_org_id)
           and trunc(a.ap_vou_dt) >=
               to_date(NVL($P{From_Date}, '1000-01-01'), 'YYYY-MM-DD')
           and trunc(a.ap_vou_dt) <=
               to_date(NVL($P{To_Date}, SYSDATE), 'YYYY-MM-DD')

           and nvl(ap_amt_sp, 0) * nvl(ap_cc, 1) > 0 -- dm 2014-05-09
           and a.ap_curr_id_sp = nvl($P{Curr_Id}, a.ap_curr_id_sp)
           and a.ap_coa_id = nvl($P{Coa_Id}, a.ap_coa_id)
           and b.doc_cld_id = a.gl_cld_id
           and b.doc_sloc_id = a.ap_sloc_id
           and b.doc_org_id = a.ap_org_id
           and b.doc_txn_id = a.ap_vou_id
           and f.glbl_curr_id = a.ap_curr_id_sp
		   and a.ap_eomst_id = nvl($P{Entity_Group},a.ap_eomst_id)


        union all

        select a.tvou_cld_id cld_id,
               a.tvou_sloc_id sloc_id, -- add this here
               a.tvou_org_id org_id,
               a.tvou_coa_id coa_id,
               g.glbl_curr_notation_alias curr_nm,
               a.tvou_id vou_id,
               substr(b.doc_txn_id_disp, 3) || '(-interim)' doc_txn_id_disp,
               a.tvou_dt voucher_date,
               null purchase_invoice_doc_id,
               null purchase_invoice_id,
               null purchase_invoice_date,
               a.tvou_bill_no debtors_invoice,
               a.tvou_bill_date debtors_invoice_date,
               a.tvou_due_dt due_date,
               (a.tvou_due_dt -
               (to_date(nvl($P{To_Date}, SYSDATE), 'YYYY-MM-DD'))) over_due_days,
               a.tvou_amt_bs,
               a.tvou_amt_typ,
               nvl(a.tvou_cc, 1) rate,
               decode(ltrim(rtrim(upper(a.tvou_amt_typ))),
                      'DR',
                      nvl(a.tvou_amt_bs, 0),
                      0) damt,
               decode(ltrim(rtrim(upper(a.tvou_amt_typ))),
                      'CR',
                      nvl(a.tvou_amt_bs, 0),
                      0) camt,
               decode(ltrim(rtrim(upper(a.tvou_amt_typ))),
                      'DR',
                      nvl(a.tvou_amt_bs, 0),
                      'CR',
                      -nvl(a.tvou_amt_bs, 0)) amt
          from tvou_lines        a,
               app.app$doc$txn   b,
               app.app$glbl$curr g,
               app.app$eo        f
         where a.tvou_cld_id = $P{Cld_Id}
           and a.tvou_org_id = nvl($P{Org_Id}, a.tvou_org_id)
           and a.tvou_id =
               decode($P{PostFlag}, 'P', '1', 'A', a.tvou_id, a.tvou_id)
           and trunc(a.tvou_dt) >=
               to_date(NVL($P{From_Date}, '1000-01-01'), 'YYYY-MM-DD')
           and trunc(a.tvou_dt) <=
               to_date(NVL($P{To_Date}, SYSDATE), 'YYYY-MM-DD')
           and a.tvou_amt_bs > 0

           and a.tvou_curr_id_sp = nvl($P{Curr_Id}, a.tvou_curr_id_sp)
           and a.tvou_coa_id = nvl($P{Coa_Id}, a.tvou_coa_id)
           and b.doc_cld_id = a.tvou_cld_id
           and b.doc_sloc_id = a.tvou_sloc_id
           and b.doc_org_id = a.tvou_org_id
           and b.doc_txn_id = a.tvou_id
           and f.eo_cld_id = a.tvou_cld_id
           and f.eo_sloc_id = a.tvou_sloc_id
           and f.eo_ho_org_id = a.tvou_ho_org_id
           and f.eo_id = a.tvou_eo_id
           and f.eo_bhav = 'C'
           and g.glbl_curr_id = a.tvou_curr_id_sp

		   and a.tvou_eomst_id = nvl($P{Entity_Group},a.tvou_eomst_id)

		   ) e
where a.org_cld_id = $P{Cld_Id}
   and a.org_id = nvl($P{Org_Id}, a.org_id)
   and b.coa_cld_id = a.org_cld_id
   and b.coa_ho_org_id = nvl(a.org_id_parent_l0, a.org_id)
   and b.coa_id = nvl($P{Coa_Id}, b.coa_id)

  and b.coa_cog_id = nvl($P{Cog_Id},b.coa_cog_id)

   and b1.cld_id = b.coa_cld_id
   and b1.org_sloc_id = b.coa_sloc_id
   and b1.ho_org_id = b.coa_ho_org_id
   and b1.org_id = a.org_id
   and b1.org_coa_id = b.coa_id

   and c.acc_cld_id = b.coa_cld_id
   and c.acc_sloc_id = b.coa_sloc_id
   and c.ho_org_id = b.coa_ho_org_id
   and c.acc_id = b.coa_acc_id

   and d.eo_cld_id = c.acc_cld_id
   and d.eo_sloc_id = c.acc_sloc_id
   and d.eo_ho_org_id = c.ho_org_id
   and d.eo_id = c.acc_type_id
   and d.eo_bhav = 'C'
   and d.eo_mst_id = nvl($P{Entity_Group},d.eo_mst_id)

   and b.coa_cld_id = op.org_coa_cld_id
   and a.org_id = op.org_id
   and b.coa_id = op.org_coa_id

   and b1.cld_id = op1.gl_cld_id(+)
   and b1.org_id = op1.gl_org_id(+)
   and b1.org_coa_id = op1.gl_coa_id(+)

      --and (nvl(op.org_coa_op_bal,0) + nvl(op1.gl_amt,0)) <> 0

      --comment this to get opening balance

   and b1.cld_id = e.cld_id(+)
   and b1.org_sloc_id = e.sloc_id(+) -- add this here
   and b1.org_id = e.org_id(+)
   and b1.org_coa_id = e.coa_id(+)

   and not (((nvl(op.org_coa_op_bal, 0) + nvl(op1.gl_amt, 0)) = 0) and
        (nvl(e.damt, 0) = 0) and (nvl(e.camt, 0) = 0))
      --and d.eo_cld_id = h.eo_cld_id(+)
      --and d.eo_sloc_id = h.eo_sloc_id(+)
      --and d.eo_ho_org_id = h.eo_ho_org_id(+)
      --and d.eo_id = h.eo_id(+)
      -------dhirendra ---------------
   and d.eo_cld_id = i.eo_cld_id(+)
   and d.eo_sloc_id = i.eo_sloc_id(+)
   and d.eo_ho_org_id = i.eo_ho_org_id(+)
   and d.eo_id = i.eo_id(+)
      -------dhirendra ---------------
      -- add this---

   and b1.cld_id = x1.cld_id(+)
   and b1.org_id = x1.org_id(+)
   and b1.org_coa_id = x1.coa_id(+)

      -- add this---

   and b1.cld_id = x2.cld_id(+)
   and b1.org_id = x2.org_id(+)
   and b1.org_coa_id = x2.coa_id(+)

      -- add this---
   and not (((nvl(op.org_coa_op_bal, 0) + nvl(op1.gl_amt, 0)) = 0) and
        (nvl(x1.cnt, 0) = 0) and (nvl(x2.cnt, 0) = 0))]]>
	</queryString>
	<field name="ORG_DESC" class="java.lang.String"/>
	<field name="COA_CLD_ID" class="java.lang.String"/>
	<field name="ORG_ID" class="java.lang.String"/>
	<field name="COA_ID" class="java.math.BigDecimal"/>
	<field name="COA_NM" class="java.lang.String"/>
	<field name="EO_PHONE" class="java.lang.String"/>
	<field name="EO_EMAIL_ID" class="java.lang.String"/>
	<field name="EO_FAX_NO" class="java.lang.String"/>
	<field name="AVG_CR_DAYS" class="java.math.BigDecimal"/>
	<field name="CR_LIMIT" class="java.math.BigDecimal"/>
	<field name="OPEN_BAL" class="java.math.BigDecimal"/>
	<field name="ORG_COA_OP_BAL_TYP" class="java.lang.String"/>
