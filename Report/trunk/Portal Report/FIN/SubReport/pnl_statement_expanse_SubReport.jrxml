	<queryString>
		<![CDATA[SELECT
COG_ID,
COG_ID_GRP,
COG_NM_TREE cog_ind,
COA_ID,
z.COA_NM,
NVL(decode(cl_BAL_type,'Dr',CL_BAL_2014,'Cr',-CL_BAL_2014,CL_BAL_2014),0) CL_BAL_2014,
NVL(y.CL_BAL_2013,0) CL_BAL_2013
FROM	(SELECT	c.ORG_COA_CLD_ID CLD_ID,
		c.ORG_ID ORG_ID,
		a.ORG_DESC ORG_DESC,
		c.ORG_FY_ID ORG_FY_ID,
		c.ORG_COA_ID COA_ID,
		b.COA_NM COA_NM,
		d.COG_ID,
    SUBSTR(d.COG_ID,1,1) COG_ID_GRP,
		d.COG_NM COG_NM,
		d.COG_NM_TREE,
		d.COG_NM_INDENT,
