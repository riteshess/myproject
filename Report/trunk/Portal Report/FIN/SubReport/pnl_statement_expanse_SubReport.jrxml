 		AND	B.COA_HO_ORG_ID = NVL(A.ORG_ID_PARENT_L0,A.ORG_ID)
		AND 	B.COA_COG_ID LIKE decode($P{Include_Grp},'Y', NVL($P{COGID}, COA_COG_ID)||'%', NVL($P{COGID}, COA_COG_ID))
