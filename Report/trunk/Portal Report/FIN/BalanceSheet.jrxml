    (select fn_get_op_bal_diff_fr_dt($P{Cld_Id},$P{Org_Id},nvl(to_date($P{AsOnDate},'YYYY-MM-DD'),to_date(trunc(sysdate),'YYYY-MM-DD'))) op_bal_diff
    from dual) d
where a.org_id = $P{Org_Id}
and a.ORG_CURR_ID_BS=b.GLBL_CURR_ID
order by 2]]>
	</queryString>
