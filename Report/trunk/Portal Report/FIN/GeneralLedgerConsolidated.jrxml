	<queryString>
		<![CDATA[SELECT  CLD_ID,
		ORG_ID ,
		ORGANISATION,
		Orgnaisation_ShortNm,
		ORG_FY_ID,
		COA_ID,
		COA_NM COANAME,
		DTL_ORG_ID,
		DTL_ORG_DESC,
		DTL_ORG_ALIAS,
		VOUCHER_DATE VOUCHER_DATE,
		VOUCHER_NO VOUCHER_ID,
		VOUCHER_NO VOUCHER_DISP_ID,
		NARRATION,
		'Opening Balance as on '|| $P{From_Date} OP_BAL_STR,
		decode(OP_BAL_TYPE,'Dr',OP_BAL,'Cr',-OP_BAL) OP_BAL_SIGN,
		OP_BAL  OP_BALANCE,
		OP_BAL_TYPE  OP_BALANCE_TYPE,
		decode(OP_BAL_TYPE,'Dr',OP_BAL,0) OP_BAL_DR,
		decode(OP_BAL_TYPE,'Cr',OP_BAL,0) OP_BAL_CR,
		DR_AMT DebitAmount,
		nvl(DR_AMT,0) deb_amt,
		CR_AMT CreditAmount,
		nvl(CR_AMT,0) cre_amt,
		(select glbl_curr_notation_alias from app.app$glbl$curr where glbl_curr_id = CURR_ID_SP) curr_desc,
		nvl(CURR_CONV,1) CURR_CONV,
		voucher_id_hex,
		AMT_SP,
		CURR_ID_SP,
		CURR_DESC_SP,
		1 running_balance
FROM   (
        SELECT  eh.CLD_ID CLD_ID,
                eh.ORG_ID ORG_ID,
				a.org_alias Orgnaisation_ShortNm,
				a.ORG_DESC  ORGANISATION,
				c.ORG_FY_ID ORG_FY_ID,
				c.ORG_COA_ID COA_ID,
				b.COA_NM COA_NM,
				EH.ORG_ID DTL_ORG_ID,
				EH.ORG_DESC DTL_ORG_DESC,
				EH.ORG_ALIAS DTL_ORG_ALIAS,

				DECODE($P{PostFlag},
				'A',DECODE(LTRIM(RTRIM(UPPER(NVL(c.ORG_COA_OP_BAL_TYP,'Dr')))),
								'DR',ABS(NVL(c.ORG_COA_OP_BAL, 0) + NVL(e1.gl_dr_amt_op, 0) - NVL(e1.gl_cr_amt_op, 0) + NVL(f1.tmp_dr_amt_op, 0) - NVL(f1.tmp_cr_amt_op, 0)),
								'CR',ABS(NVL(c.ORG_COA_OP_BAL, 0) - NVL(e1.gl_dr_amt_op, 0) + NVL(e1.gl_cr_amt_op, 0) - NVL(f1.tmp_dr_amt_op, 0) + NVL(f1.tmp_cr_amt_op, 0))),
				'P',DECODE(LTRIM(RTRIM(UPPER(NVL(c.ORG_COA_OP_BAL_TYP,'Dr')))),
								'DR',ABS(NVL(c.ORG_COA_OP_BAL, 0) + NVL(e1.gl_dr_amt_op, 0) - NVL(e1.gl_cr_amt_op, 0)),
								'CR',ABS(NVL(c.ORG_COA_OP_BAL, 0) - NVL(e1.gl_dr_amt_op, 0) + NVL(e1.gl_cr_amt_op, 0))))
												OP_BAL,
				DECODE($P{PostFlag},
				'A',DECODE(LTRIM(RTRIM(UPPER(NVL(c.ORG_COA_OP_BAL_TYP,'Dr')))),
								'DR',DECODE(SIGN(NVL(c.ORG_COA_OP_BAL, 0) + NVL(e1.gl_dr_amt_op, 0) - NVL(e1.gl_cr_amt_op, 0) + NVL(f1.tmp_dr_amt_op, 0) - NVL(f1.tmp_cr_amt_op, 0)),0,'Dr',1,'Dr',-1,'Cr'),
								'CR',DECODE(SIGN(NVL(c.ORG_COA_OP_BAL, 0) - NVL(e1.gl_dr_amt_op, 0) + NVL(e1.gl_cr_amt_op, 0) - NVL(f1.tmp_dr_amt_op, 0) + NVL(f1.tmp_cr_amt_op, 0)),0,'Dr',1,'Cr',-1,'Dr')),
				'P',DECODE(LTRIM(RTRIM(UPPER(NVL(c.ORG_COA_OP_BAL_TYP,'Dr')))),
				'DR',DECODE(SIGN(NVL(c.ORG_COA_OP_BAL,0)+ NVL(e1.gl_dr_amt_op,0)- NVL(e1.gl_cr_amt_op,0)),0,'Dr',1,'Dr',-1,'Cr'),
				'CR',DECODE(SIGN(NVL(c.ORG_COA_OP_BAL,0)- NVL(e1.gl_dr_amt_op,0)+ NVL(e1.gl_cr_amt_op,0)),0,'Dr',1,'Cr',-1,'Dr')))
				OP_BAL_type,
				eh.VOUCHER_NO,
				eh.VOUCHER_DATE,
				eh.gl_dr_amt DR_AMT,
				eh.gl_cr_amt CR_AMT,
				eh.curr_id_bs, --here
				eh.curr_desc,
				CURR_CONV,
				eh.amt_sp, --here
				eh.curr_id_sp, --here
				eh.curr_desc_sp, --here,
				eh.voucher_id voucher_id_hex,
				NARRATION
      FROM     (select  A.ORG_CLD_ID,
								ORG_ID_L0 ORG_ID,
								b.org_desc,
								b.org_alias
						from (SELECT  ORG_CLD_ID,
									SUBSTR(SYS_CONNECT_BY_PATH(ORG_ID,'->'),3) ORG_TREE,
									ORG_ID,
									ORG_ID_PARENT,
									ORG_DESC,
									SUBSTR(SYS_CONNECT_BY_PATH(ORG_ID,'->'),3,2) ORG_ID_L0
								FROM   app.ORG
								START WITH nvl(ORG_ID_PARENT,'!') in ('!','00')
								CONNECT BY PRIOR ORG_ID = ORG_ID_PARENT) a,
								app.org b
							WHERE a.ORG_CLD_ID = NVL($P{Cld_Id} , a.ORG_CLD_ID)
							AND                       a.ORG_ID = NVL($P{Ho_Org_Id} , A.ORG_ID)
							and                        b.ORG_ID = a.ORG_ID_L0
            ) a,
            (SELECT  DISTINCT a.COA_CLD_ID ORG_CLD_ID,
                    a.COA_HO_ORG_ID HO_ORG_ID,
					c.ORG_ID,
					a.COA_ID,
					a.COA_NM,
					a.COA_COG_ID,
					a.COA_ACC_ID
			FROM    fin$coa a,
					(select nvl(app.FN_GET_PARENT_ORG_L0($P{Cld_Id}, $P{Org_Id}),$P{Org_Id}) ho_org_id
					from dual) b,
					app.org c
			WHERE COA_CLD_ID = NVL($P{Cld_Id},COA_CLD_ID)
			AND   COA_HO_ORG_ID = b.ho_org_id
			AND   COA_ID = NVL($P{Coa_Id}, COA_ID)
			AND   COA_COG_ID like decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, COA_COG_ID)||'%', NVL($P{Cog_Id}, COA_COG_ID))

			AND A.COA_CLD_ID = C.ORG_CLD_ID
			AND A.COA_HO_ORG_ID = NVL(C.ORG_ID_PARENT_L0,C.ORG_ID)
            )b,
            (SELECT a.ORG_COA_CLD_ID,
					a.ORG_COA_HO_ORG_ID HO_ORG_ID,
					a.ORG_ID,
					a.ORG_FY_ID,
					a.ORG_COA_COG_ID,
					a.ORG_COA_ID,
					ABS(SUM(DECODE(a.ORG_COA_OP_BAL_TYP,'Dr',a.ORG_COA_OP_BAL,-a.ORG_COA_OP_BAL))) ORG_COA_OP_BAL,
					DECODE(SIGN(SUM(DECODE(a.ORG_COA_OP_BAL_TYP,'Dr',a.ORG_COA_OP_BAL,-a.ORG_COA_OP_BAL))),1,'Dr',0,'Dr','Cr') ORG_COA_OP_BAL_TYP
			FROM     FIN.ORG$COA$FY a,
					 (select nvl(app.FN_GET_PARENT_ORG_L0($P{Cld_Id}, $P{Org_Id}),$P{Org_Id}) ho_org_id
					from dual) b,
					(select app.fn_get_fy_id($P{Cld_Id}, $P{Org_Id},TO_DATE($P{From_Date}, 'YYYY-MM-DD'),'FY') fy
					from dual) c

			WHERE    ORG_COA_CLD_ID = NVL($P{Cld_Id} , ORG_COA_CLD_ID)
			AND      ORG_COA_HO_ORG_ID = b.ho_org_id
			AND      ORG_FY_ID = c.fy
			AND      ORG_COA_ID = NVL($P{Coa_Id} , ORG_COA_ID)
			AND      ORG_COA_COG_ID like decode($P{Include_Grp},'Y', NVL($P{Cog_Id} ,ORG_COA_COG_ID)||'%', NVL($P{Cog_Id} ,ORG_COA_COG_ID))
			AND      ORG_COA_NA_ID = NVL($P{Na_Id} , ORG_COA_NA_ID)
			GROUP BY ORG_COA_CLD_ID,
					ORG_COA_HO_ORG_ID,
					ORG_ID,
					ORG_FY_ID,
					ORG_COA_COG_ID,
					ORG_COA_ID
            )c,
            (SELECT  A.GL_CLD_ID,
					A.GL_HO_ORG_ID GL_HO_ORG_ID,
					A.GL_ORG_ID,
					A.GL_COA_ID,


					app.fn_get_fy_id($P{Cld_Id}, $P{Org_Id},TO_DATE($P{To_Date}, 'YYYY-MM-DD'),'FY') FY_ID,
					NVL(SUM(DECODE(GL_AMT_TYP, 'Dr', GL_AMT_BS, 0)),0) gl_dr_amt_op,
					NVL(SUM(DECODE(GL_AMT_TYP, 'Cr', GL_AMT_BS, 0)),0) gl_cr_amt_op
			FROM   GL_LINES A,
				   GL B,
                                   (select nvl(app.FN_GET_PARENT_ORG_L0($P{Cld_Id}, $P{Org_Id}),$P{Org_Id}) ho_org_id
					from dual) c,
				  (SELECT app.fn_get_fy_start_date($P{Cld_Id},
					nvl(app.FN_GET_PARENT_ORG_L0($P{Cld_Id}, $P{Org_Id}),$P{Org_Id}),
					TO_DATE($P{From_Date},'YYYY-mm-dd'),'FY') dt
						FROM DUAL) d

			 WHERE  B.GL_CLD_ID = NVL($P{Cld_Id},B.GL_CLD_ID)
			 AND    B.GL_STAT = DECODE($P{PostFlag}, 'A', B.GL_STAT, 'P', 'FLC')
			 AND    A.GL_CLD_ID = B.GL_CLD_ID
			 AND    A.GL_SLOC_ID = B.GL_SLOC_ID
			 AND    A.GL_VOU_ID = B.GL_VOU_ID
			 AND    a.GL_HO_ORG_ID = c.ho_org_id
			 AND    a.GL_COA_ID = NVL($P{Coa_Id}, a.GL_COA_ID)
			 AND    a.GL_COG_ID like decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, a.GL_COG_ID)||'%', NVL($P{Cog_Id}, a.GL_COG_ID))
			 AND    a.GL_NA_ID = NVL($P{Na_Id}, a.GL_NA_ID)

			 AND    TRUNC(a.GL_VOU_DT) >=  d.dt
			 AND    TRUNC(a.GL_VOU_DT) < TO_DATE($P{From_Date}, 'YYYY-mm-dd')
			-- AND    A.GL_TXN_TYP_MIG not in  ('O','MO')
			 GROUP BY    A.GL_CLD_ID,
						 A.GL_HO_ORG_ID,
						 A.GL_COA_ID,
						  A.GL_ORG_ID

                ) e1,
				(SELECT  a.TVOU_CLD_ID,
						a.TVOU_HO_ORG_ID TVOU_HO_ORG_ID,
						a.TVOU_ORG_ID,
						a.TVOU_COA_ID,

						app.fn_get_fy_id($P{Cld_Id}, $P{Org_Id},TO_DATE($P{To_Date}, 'YYYY-MM-DD'),'FY') FY_ID,
						NVL(SUM(DECODE(a.TVOU_AMT_TYP,'Dr',a.TVOU_AMT_BS,0)),0) tmp_dr_amt_op,
						NVL(SUM(DECODE(a.TVOU_AMT_TYP,'Cr',a.TVOU_AMT_BS,0)),0) tmp_cr_amt_op
				FROM   TVOU_LINES a,
						(select nvl(app.FN_GET_PARENT_ORG_L0($P{Cld_Id}, $P{Org_Id}),$P{Org_Id}) ho_org_id
					from dual) b,
					 (SELECT app.fn_get_fy_start_date($P{Cld_Id},
					nvl(app.FN_GET_PARENT_ORG_L0($P{Cld_Id}, $P{Org_Id}),$P{Org_Id}),
					TO_DATE($P{From_Date},'YYYY-mm-dd'),'FY') dt
						FROM DUAL) d

				WHERE  TVOU_CLD_ID = NVL($P{Cld_Id},TVOU_CLD_ID)
				AND    TVOU_HO_ORG_ID = b.ho_org_id
				AND    tvou_COA_ID = NVL($P{Coa_Id}, tvou_COA_ID)
				AND    tvou_COG_ID like decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, tvou_COG_ID)||'%', NVL($P{Cog_Id}, tvou_COG_ID))
				AND    tvou_NA_ID = NVL($P{Na_Id}, tvou_NA_ID)
				AND    TRUNC(tVOU_DT) >= d.dt
				AND    TRUNC(tVOU_DT) < TO_DATE($P{From_Date}, 'YYYY-mm-dd')
				GROUP BY TVOU_CLD_ID,
						 TVOU_HO_ORG_ID,
						TVOU_COA_ID,
						TVOU_ORG_ID

				) f1,
                (SELECT  A.GL_CLD_ID CLD_ID,
						a.GL_HO_ORG_ID HO_ORG_ID,
						A.GL_ORG_ID ORG_ID,
						E.ORG_DESC,
						E.ORG_ALIAS,
						A.GL_COA_ID  COA_ID,

						app.fn_get_fy_id($P{Cld_Id}, $P{Org_Id},TO_DATE($P{From_Date}, 'YYYY-MM-DD'),'FY') FY_ID,
						A.GL_VOU_ID VOUCHER_ID,
						SUBSTR(D.DOC_TXN_ID_DISP,3) VOUCHER_NO,
						A.GL_VOU_DT VOUCHER_DATE,
						A.GL_SL_NO SL_NO,
						A.GL_AMT_SP AMT_SP,
						A.GL_CURR_ID_SP CURR_ID_SP,
						CX.CURR_DESC CURR_DESC_SP,
						A.GL_CC CURR_CONV,
						A.GL_CURR_ID_BS CURR_ID_BS,
						C.CURR_DESC,
						NVL(DECODE(GL_AMT_TYP, 'Dr', GL_AMT_BS, 0),0) gl_dr_amt,
						NVL(DECODE(GL_AMT_TYP, 'Cr', GL_AMT_BS, 0),0) gl_cr_amt,
						A.GL_NARR NARRATION,
						A.GL_EOMST_ID EO_MST_ID

				FROM    GL_LINES A,
						GL B,

						App.app$curr c,
						App.app$curr cx,
						app.APP$DOC$TXN D,
						APP.ORG E,
						(select nvl(app.FN_GET_PARENT_ORG_L0($P{Cld_Id}, $P{Org_Id}),$P{Org_Id}) ho_org_id
					from dual) f
				WHERE   B.GL_CLD_ID = NVL($P{Cld_Id},A.GL_CLD_ID)
				AND     A.GL_CLD_ID = B.GL_CLD_ID
				AND     A.GL_SLOC_ID = B.GL_SLOC_ID
				AND     A.GL_VOU_ID = B.GL_VOU_ID
				AND     a.GL_HO_ORG_ID = f.ho_org_id
				AND     a.GL_COA_ID = NVL($P{Coa_Id}, a.GL_COA_ID)
				AND     a.GL_COG_ID like decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, a.GL_COG_ID)||'%', NVL($P{Cog_Id}, a.GL_COG_ID))
				AND     a.GL_NA_ID = NVL($P{Na_Id}, a.GL_NA_ID)
				AND     B.GL_STAT = DECODE($P{PostFlag}, 'A', B.GL_STAT, 'P', 'FLC')
				AND     TRUNC(a.GL_VOU_DT) >= TO_DATE($P{From_Date},  'YYYY-mm-dd')
				AND     TRUNC(a.GL_VOU_DT) <= TO_DATE($P{To_Date}, 'YYYY-mm-dd')
			--	AND     A.GL_TXN_TYP_MIG not in  ('O','MO')
				and     a.gl_cld_id = c.cld_id
				and     a.gl_sloc_id = c.curr_sloc_id
				and     a.gl_curr_id_bs = c.curr_id --here
				and     a.gl_cld_id = cx.cld_id --here
				and     a.gl_sloc_id = cx.curr_sloc_id --here
				and     a.gl_curr_id_sp = cx.curr_id --here
				AND     a.gl_cld_id = d.doc_cld_id
				and     A.GL_ORG_ID  = D.DOC_ORG_ID
				AND     A.GL_VOU_ID = D.DOC_TXN_ID
				AND     A.GL_ORG_ID = E.ORG_ID

        UNION
			SELECT  A.TVOU_CLD_ID CLD_ID,
					a.TVOU_HO_ORG_ID HO_ORG_ID,
					A.TVOU_ORG_ID ORG_ID,
					E.ORG_DESC,
					E.ORG_ALIAS,
					A.TVOU_COA_ID COA_ID,

					app.fn_get_fy_id($P{Cld_Id}, $P{Org_Id},TO_DATE($P{From_Date}, 'YYYY-MM-DD'),'FY') FY_ID,
					A.TVOU_ID VOUCHER_ID,
					SUBSTR(D.DOC_TXN_ID_DISP,3)||'(P)' VOUCHER_NO,
					A.TVOU_DT VOUCHER_DATE,
					A.TVOU_SL_NO SL_NO,
					A.TVOU_AMT_SP AMT_SP,
					A.TVOU_CURR_ID_SP CURR_ID_SP,
					CX.CURR_DESC CURR_DESC_SP,
					A.TVOU_CC CURR_CONV,
					A.TVOU_CURR_ID_BS CURR_ID_BS,
					C.CURR_DESC,
					NVL(DECODE(TVOU_AMT_TYP, 'Dr', TVOU_AMT_BS, 0),0) gl_dr_amt,
					NVL(DECODE(TVOU_AMT_TYP, 'Cr', TVOU_AMT_BS, 0),0) gl_cr_amt,
					A.NARR NARRATION,
					A.TVOU_EOMST_ID EO_MST_ID

            FROM   TVOU_LINES A,
                   TVOU B,

				   App.app$curr c,
    			   App.app$curr cx,
				   app.APP$DOC$TXN D,
				   APP.ORG E,
				   (select nvl(app.FN_GET_PARENT_ORG_L0($P{Cld_Id}, $P{Org_Id}),$P{Org_Id}) ho_org_id
					from dual) f
		WHERE A.TVOU_ID = DECODE($P{PostFlag},'P','1','A',A.TVOU_ID,A.TVOU_ID)
		AND                       B.TVOU_CLD_ID = NVL($P{Cld_Id},A.TVOU_CLD_ID)
		AND A.TVOU_CLD_ID = B.TVOU_CLD_ID
		AND A.TVOU_SLOC_ID = B.TVOU_SLOC_ID
		AND A.TVOU_ID = B.TVOU_ID
		AND A.TVOU_HO_ORG_ID = f.ho_org_id
		AND a.TVOU_COA_ID = NVL($P{Coa_Id}, a.TVOU_COA_ID)
		AND a.TVOU_COG_ID like decode($P{Include_Grp},'Y', NVL($P{Cog_Id}, a.TVOU_COG_ID)||'%', NVL($P{Cog_Id}, a.TVOU_COG_ID))
		AND a.TVOU_NA_ID = NVL($P{Na_Id}, a.TVOU_NA_ID)
		AND TRUNC(a.TVOU_DT) >= TO_DATE($P{From_Date},'YYYY-mm-dd')
		AND TRUNC(a.TVOU_DT) <= TO_DATE($P{To_Date}, 'YYYY-mm-dd')
		and a.TVOU_cld_id = c.cld_id
		and a.TVOU_sloc_id = c.curr_sloc_id
		and a.TVOU_curr_id_bs = c.curr_id
		and a.TVOU_cld_id = cx.cld_id
		and a.TVOU_sloc_id = cx.curr_sloc_id
		and a.tvou_curr_id_sp = cx.curr_id
		AND a.TVOU_cld_id = d.doc_cld_id
		and A.TVOU_ORG_ID = D.DOC_ORG_ID
		AND A.TVOU_ID = D.DOC_TXN_ID
		AND A.TVOU_ORG_ID = E.ORG_ID

        ) eh,
		(select nvl(app.FN_GET_PARENT_ORG_L0($P{Cld_Id}, $P{Org_Id}),$P{Org_Id}) ho_org_id
					from dual) ho
        WHERE   A.ORG_CLD_ID = NVL($P{Cld_Id}, A.ORG_CLD_ID)
		AND A.ORG_ID = NVL($P{Ho_Org_Id}, A.ORG_ID)
			AND eh.EO_MST_ID = nvl($P{Entity_Group},eh.EO_MST_ID)

			AND  a.ORG_ID= ho.ho_org_id

			AND  B.ORG_CLD_ID = A.ORG_CLD_ID
			AND  B.ORG_ID = A.ORG_ID

			AND  c.ORG_COA_CLD_ID = B.ORG_CLD_ID
			AND  c.HO_ORG_ID = B.HO_ORG_ID
			AND  c.ORG_COA_ID = b.COA_ID

			AND  c.ORG_COA_CLD_ID = e1.GL_CLD_ID(+)
			AND  c.HO_ORG_ID = e1.GL_HO_ORG_ID(+)
			AND  c.ORG_ID = e1.GL_ORG_ID(+)
			AND  c.ORG_COA_ID = e1.GL_COA_ID(+)
			AND  c.ORG_FY_ID = e1.FY_ID(+)

			AND  c.ORG_COA_CLD_ID = f1.TVOU_CLD_ID(+)
			AND  c.HO_ORG_ID = f1.tvou_HO_ORG_ID(+)
			AND  c.ORG_ID = f1.tvou_org_id(+)
			AND  c.ORG_COA_ID = f1.tvou_COA_ID(+)
			AND  c.ORG_FY_ID = f1.FY_ID(+)

			AND  c.ORG_COA_CLD_ID = eh.CLD_ID
			AND  c.HO_ORG_ID = eh.HO_ORG_ID
			AND c.ORG_ID = eh.ORG_ID
			AND  c.ORG_COA_ID = eh.COA_ID
			AND  c.ORG_FY_ID = eh.FY_ID
               )
where not (OP_BAL = 0 AND DR_AMT = 0 AND CR_AMT = 0)]]>
	</queryString>
