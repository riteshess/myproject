	<queryString>
		<![CDATA[SELECT	D.ORG_DESC,
		a.AR_COA_ID COA_ID,
		a.AR_AMT_SP,
		c.COA_NM,
		F.GLBL_CURR_NOTATION_ALIAS CURR_NM,
		SUBSTR(B.DOC_TXN_ID_DISP,3) DOC_TXN_ID_DISP,
		a.AR_VOU_DT VOUCHER_DATE,
		a.AR_DOC_ID_REF1 SALES_INVOICE_DOC_ID,
		a.AR_TXN_ID_REF1 SALES_INVOICE_ID,
		a.AR_TXN_DT_REF1 SALES_INVOICE_DATE,

		SUBSTR(a.AR_EXT_DOC_NO,3)  DEBTORS_INVOICE,
		a.AR_EXT_DOC_DT  DEBTORS_INVOICE_DATE,

		a.AR_VOU_DUE_DT  DUE_DATE,
		DECODE(LTRIM(RTRIM(UPPER(a.AR_AMT_TYP))),'DR',NVL(a.AR_AMT_SP*AR_CC,0),0) DAMT,
		DECODE(LTRIM(RTRIM(UPPER(a.AR_AMT_TYP))),'CR',NVL(a.AR_AMT_SP*AR_CC,0),0) CAMT,
		DECODE(LTRIM(RTRIM(UPPER(a.AR_AMT_TYP))),'DR',NVL(a.AR_AMT_SP*AR_CC,0),'CR',-NVL(a.AR_AMT_SP*AR_CC,0)) AMT,
		a.AR_AMT_TYP BalanceType,
		H.EO_PHONE,
		H.EO_EMAIL_ID,
		H.EO_FAX_NO,
    op.ORG_COA_OP_BAL Open_bal,
    op.ORG_COA_OP_BAL_TYP,
    a.AR_VOU_DUE_DT DUE_DATE,
    a.AR_CC Rate,
    ((to_date(NVL($P{To_Date},SYSDATE),'YYYY-MM-DD'))-a.AR_VOU_DUE_DT ) OVER_DUE_DAYS
FROM	AR a,
		app.app$doc$txn b,
		fin$coa c,
		app.org d,
		APP.APP$GLBL$CURR f,
		APP.APP$EO$PERS$COMM H,
   ( SELECT	ORG_COA_CLD_ID,
							ORG_ID,
							ORG_FY_ID,
							ORG_COA_ID,
							SUM(DECODE(ORG_COA_OP_BAL_TYP,'Dr',ORG_COA_OP_BAL,-ORG_COA_OP_BAL)) ORG_COA_OP_BAL,
							DECODE(SIGN(SUM(DECODE(ORG_COA_OP_BAL_TYP,'Dr',ORG_COA_OP_BAL,-ORG_COA_OP_BAL))),1,'Dr',0,'Dr','Cr') ORG_COA_OP_BAL_TYP
				FROM ORG$COA$FY
				WHERE ORG_COA_CLD_ID = NVL($P{Cld_Id}, ORG_COA_CLD_ID)
				AND ORG_ID = NVL($P{Org_Id}, ORG_ID)
				AND ORG_FY_ID = app.pkg_APP.get_fy_id(TO_DATE($P{From_Date}, 'YYYY-MM-DD'),'FY',ORG_ID)
				AND ORG_COA_ID LIKE NVL($P{Coa_Id}, ORG_COA_ID)
				GROUP BY ORG_COA_CLD_ID,
							ORG_ID,
							ORG_FY_ID,
							ORG_COA_COG_ID,
							ORG_COA_ID
		 		) op

WHERE	a.AR_COA_ID=op.ORG_COA_ID
and   a.AR_ORG_ID=op.org_id
and   a.GL_CLD_ID=op.ORG_COA_CLD_ID
and   a.GL_CLD_ID = $P{Cld_Id}
and		a.AR_ORG_ID = nvl($P{Org_Id},a.AR_ORG_ID)
and		TRUNC(a.AR_VOU_DT) between to_date(NVL($P{From_Date},'1000-01-01'),'YYYY-MM-DD') and to_date(NVL($P{To_Date},SYSDATE),'YYYY-MM-DD')
and 	(AR_AMT_SP*AR_CC) between nvl($P{AmountX1},0) AND nvl($P{AmountX2},999999999999999999)
and 	AR_CURR_ID_SP = nvl($P{Curr_Id},a.AR_CURR_ID_SP)
and		a.AR_COA_ID = nvl($P{Coa_Id},a.AR_COA_ID)

AND		b.DOC_CLD_ID = a.GL_CLD_ID
and		b.DOC_SLOC_ID = a.AR_SLOC_ID
and		b.DOC_ORG_ID = A.AR_ORG_ID
and		b.DOC_TXN_ID = a.AR_VOU_ID

and		c.COA_CLD_ID = a.GL_CLD_ID
and		c.COA_SLOC_ID = a.AR_SLOC_ID
and		c.COA_HO_ORG_ID = app.FN_GET_PARENT_ORG_L0(a.GL_CLD_ID,A.AR_ORG_ID)
and		c.COA_ID = a.AR_COA_ID

AND 	d.org_id = a.AR_org_id


AND 	A.GL_CLD_ID = H.EO_CLD_ID(+)
AND 	A.AR_SLOC_ID = H.EO_SLOC_ID(+)
AND 	A.AR_ORG_ID = H.EO_ORG_ID(+)
AND 	A.AR_EO_ID = H.EO_ID(+)


AND		F.GLBL_CURR_ID = A.AR_CURR_ID_SP

UNION

SELECT	D.ORG_DESC,
		a.TVOU_COA_ID COA_ID,
a.TVOU_AMT_BS,
		c.COA_NM,
		g.GLBL_CURR_NOTATION_ALIAS CURR_NM,
		SUBSTR(B.DOC_TXN_ID_DISP,3)||'(-Interim)' DOC_TXN_ID_DISP,
		a.TVOU_DT VOUCHER_DATE,
		NULL PURCHASE_INVOICE_DOC_ID,
		NULL PURCHASE_INVOICE_ID,
		NULL PURCHASE_INVOICE_DATE,

		a.TVOU_BILL_NO  CREDITORS_INVOICE,
		a.TVOU_BILL_DATE  CREDITORS_INVOICE_DATE,

		a.TVOU_DUE_DT  DUE_DATE,
		DECODE(LTRIM(RTRIM(UPPER(a.TVOU_AMT_TYP))),'DR',NVL(a.TVOU_AMT_BS,0),0) DAMT,
		DECODE(LTRIM(RTRIM(UPPER(a.TVOU_AMT_TYP))),'CR',NVL(a.TVOU_AMT_BS,0),0) CAMT,
		DECODE(LTRIM(RTRIM(UPPER(a.TVOU_AMT_TYP))),'DR',NVL(a.TVOU_AMT_BS,0),'CR',-NVL(a.TVOU_AMT_BS,0)) AMT,
		a.TVOU_AMT_TYP BalanceType,

		H.EO_PHONE,
		H.EO_EMAIL_ID,
		H.EO_FAX_NO,
    op.ORG_COA_OP_BAL Open_bal,
    op.ORG_COA_OP_BAL_TYP,
    a.TVOU_DUE_DT DUE_DATE,
    a.TVOU_CC Rate,
    (a.TVOU_DUE_DT -(to_date(NVL($P{To_Date},SYSDATE),'YYYY-MM-DD'))) OVER_DUE_DAYS
FROM 	TVOU_LINES A,
		APP.APP$DOC$TXN B,
		FIN$COA C,
		APP.ORG D,
		APP.APP$EO F,
	 	APP.APP$GLBL$CURR G,
		APP.APP$EO$PERS$COMM H,
    ( SELECT	ORG_COA_CLD_ID,
							ORG_ID,
							ORG_FY_ID,
							ORG_COA_ID,
							SUM(DECODE(ORG_COA_OP_BAL_TYP,'Dr',ORG_COA_OP_BAL,-ORG_COA_OP_BAL)) ORG_COA_OP_BAL,
							DECODE(SIGN(SUM(DECODE(ORG_COA_OP_BAL_TYP,'Dr',ORG_COA_OP_BAL,-ORG_COA_OP_BAL))),1,'Dr',0,'Dr','Cr') ORG_COA_OP_BAL_TYP
				FROM ORG$COA$FY
				WHERE ORG_COA_CLD_ID = NVL($P{Cld_Id}, ORG_COA_CLD_ID)
				AND ORG_ID = NVL($P{Org_Id}, ORG_ID)
				AND ORG_FY_ID = app.pkg_APP.get_fy_id(TO_DATE($P{From_Date}, 'YYYY-MM-DD'),'FY',ORG_ID)
				AND ORG_COA_ID LIKE NVL($P{Coa_Id}, ORG_COA_ID)
				GROUP BY ORG_COA_CLD_ID,
							ORG_ID,
							ORG_FY_ID,
							ORG_COA_COG_ID,
							ORG_COA_ID
		 		) op

WHERE	a.TVOU_COA_ID=op.ORG_COA_ID
and   A.TVOU_ORG_ID=op.org_id
and   a.TVOU_CLD_ID=op.ORG_COA_CLD_ID
and   A.TVOU_ID = DECODE($P{PostFlag},'P','1','A',A.TVOU_ID,A.TVOU_ID)
AND	  a.TVOU_CLD_ID = $P{Cld_Id}
and 	A.TVOU_ORG_ID = nvl($P{Org_Id},A.TVOU_ORG_ID)
and		TRUNC(a.TVOU_DT) between to_date(NVL($P{From_Date},'1000-01-01'),'YYYY-MM-DD') and to_date(NVL($P{To_Date},SYSDATE),'YYYY-MM-DD')
and 	A.TVOU_AMT_BS between nvl($P{AmountX1},0) AND nvl($P{AmountX2},999999999999999999)
and 	A.TVOU_CURR_ID_SP = nvl($P{Curr_Id},A.TVOU_CURR_ID_SP)
and		A.TVOU_COA_ID = nvl($P{Coa_Id},A.TVOU_COA_ID)

AND		b.DOC_CLD_ID = a.TVOU_CLD_ID
and		b.DOC_SLOC_ID = a.TVOU_SLOC_ID
and		b.DOC_ORG_ID = A.TVOU_ORG_ID
and		b.DOC_TXN_ID = a.TVOU_ID

and		c.COA_CLD_ID = a.TVOU_CLD_ID
and		c.COA_SLOC_ID = a.TVOU_SLOC_ID
and		c.COA_HO_ORG_ID = app.FN_GET_PARENT_ORG_L0(a.TVOU_CLD_ID,A.TVOU_ORG_ID)
and		c.COA_ID = a.TVOU_COA_ID

AND 	D.ORG_ID = A.TVOU_ORG_ID


AND		f.EO_CLD_ID = a.TVOU_CLD_ID
AND		f.EO_SLOC_ID = a.TVOU_SLOC_ID
AND		f.EO_HO_ORG_ID = app.FN_GET_PARENT_ORG_L0(a.TVOU_CLD_ID,A.TVOU_ORG_ID)
AND 	f.eo_id = a.tvou_eo_id
AND 	f.eo_bhav = 'D'


AND 	A.TVOU_CLD_ID = H.EO_CLD_ID(+)
AND 	A.TVOU_SLOC_ID = H.EO_SLOC_ID(+)
AND 	A.TVOU_ORG_ID = H.EO_ORG_ID(+)
AND 	A.TVOU_EO_ID = H.EO_ID(+)


AND		g.GLBL_CURR_ID = A.TVOU_CURR_ID_SP

order by 1,4,7,6,5]]>
	</queryString>
