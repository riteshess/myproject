		<![CDATA[SELECT A.ORG_ID,A.PO_ID,A.PO_DT,E.ORG_DESC,C.USR_NAME ORDR_BY,D.USR_NAME APPROVED_BY,B.USR_ID_CREATE_DT
FROM MM$DRFT$PO A,MM$DRFT$PO$WF$TXN B,APP.APP$SEC$USR C,APP.APP$SEC$USR D,APP.ORG E
WHERE A.CLD_ID=B.CLD_ID
AND A.SLOC_ID =B.SLOC_ID
AND A.ORG_ID =B.ORG_ID
AND A.DOC_ID=B.TXN_DOC_ID
AND A.USR_ID_CREATE = C.USR_ID
AND A.ORG_ID =E.ORG_ID
AND B.WF_STATUS_ID ='A'
AND B.USR_ID_CREATE =D.USR_ID
AND A.CLD_ID = NVL($P{CldId},A.CLD_ID)
AND A.SLOC_ID = NVL($P{SlocId},A.SLOC_ID)
AND A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
AND A.DOC_ID = NVL($P{DocId},A.DOC_ID)
AND A.PO_DT >= NVL(TO_DATE($P{FromDate},'yyyy-mm-dd'),A.PO_DT)
AND A.PO_DT <= NVL(TO_DATE($P{ToDate},'yyyy-mm-dd'),A.PO_DT)
AND A.USR_ID_CREATE = NVL($P{OrderBy},A.USR_ID_CREATE)
AND B.USR_ID_CREATE = NVL($P{ApprovedBy},B.USR_ID_CREATE)
ORDER BY A.ORG_ID,B.USR_ID_CREATE,A.PO_ID]]>
