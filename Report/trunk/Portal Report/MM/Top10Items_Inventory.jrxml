	<queryString>
		<![CDATA[SELECT * FROM
(SELECT A.ORG_ID,  D.ORG_DESC,   A.ITM_ID, B.ITM_DESC,   A.ITM_UOM_BS,   C.UOM_NM
      ,SUM(A.TOT_STK) TOT_STK,  SUM(A.LND_VAL)LND_VAL, dense_rank() over (order by SUM(A.TOT_STK) desc, SUM(A.LND_VAL) DESC) as rank
FROM MM.MM$STK$SUMM$LOT A, APP.APP$ITM$PRF B, APP.APP$UOM$CONV$STD  C, APP.ORG  D
WHERE   A.CLD_ID  = B.CLD_ID
AND     A.SLOC_ID = B.SLOC_ID
AND     MM.MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID) = B.HO_ORG_ID
AND     A.ITM_ID  = B.ITM_ID
AND     A.CLD_ID  = C.CLD_ID
AND     A.SLOC_ID = C.SLOC_ID
AND     A.ITM_UOM_BS = C.UOM_ID
AND     A.CLD_ID    = D.ORG_CLD_ID
AND     A.SLOC_ID   = D.ORG_CREATE_REF_SLOC_ID
AND     A.ORG_ID    = D.ORG_ID
AND     A.CLD_ID    = $P{CldId}
AND     A.SLOC_ID   = $P{SlocId}
AND     A.ORG_ID    = $P{OrgId}
AND     A.ITM_ID    = NVL($P{ItmId}, A.ITM_ID)
AND     TRUNC(A.LOT_DT) >=NVL(TO_DATE($P{FromDate}, 'YYYY-MM-DD'),TRUNC(A.LOT_DT))
AND     TRUNC(A.LOT_DT) <=NVL(TO_DATE($P{ToDate}, 'YYYY-MM-DD'),TRUNC(A.LOT_DT))
group by A.ORG_ID, D.ORG_DESC, A.ITM_ID, B.ITM_DESC, A.ITM_UOM_BS,
C.UOM_NM
ORDER BY 7 DESC)
WHERE RANK<=10]]>
	</queryString>
