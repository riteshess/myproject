	<queryString>
		<![CDATA[Select K.Coa_Nm,
Decode(A.Tran_Type,'A',A.Oc_Amt_Sp
                     ,'S', To_number('-'||''||A.Oc_Amt_Sp)) Oc_Amt_Sp,
(Select Sum(Oc_Amt_Sp)From MM$QUOT$OC
 Where Doc_Id = (select doc_id from  mm$quot where quot_id =$P{QuotId})
 And Tran_Type ='A') -
(Select nvl(Sum(Oc_Amt_Sp),0) From MM$QUOT$OC
 Where Doc_Id = (select doc_id from  mm$quot where quot_id =$P{QuotId})
 And Tran_Type ='S')tot_oc
FROM FIN.FIN$COA$DTL$VW K,MM$QUOT$OC A
                  WHERE K.CLD_ID =A.CLD_ID
                  AND K.SLOC_ID =A.SLOC_ID
                  AND K.HO_ORG_ID = MM.MM_GET_HO_ORG(A.CLD_ID,A.SLOC_ID,A.ORG_ID)
                  AND K.ORG_ID =A.ORG_ID
                  And K.Coa_Id =A.Coa_Id
                  AND A.DOC_ID in (select doc_id from  mm$quot where quot_id =$P{QuotId})
                  order by tran_type]]>
	</queryString>
