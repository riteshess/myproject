	<queryString>
		<![CDATA[SELECT A.ORG_ID,  D.ORG_DESC, A.EO_ID, C.EO_NM, COUNT(A.PO_ID)TOT_PO_NO,  B.ITM_ID,   E.ITM_DESC, SUM(B.ORD_QTY) ORD_QTY
      ,B.ITM_UOM, F.UOM_NM
FROM MM.MM$DRFT$PO A, MM.MM$DRFT$PO$ITM B, APP.APP$EO C, APP.ORG D, APP.APP$ITM$PRF E, APP.APP$UOM$CONV$STD F
WHERE A.CLD_ID  = B.CLD_ID
AND   A.SLOC_ID = B.SLOC_ID
AND   A.ORG_ID  = B.ORG_ID
AND   A.DOC_ID  = B.DOC_ID
AND   A.CLD_ID  = C.EO_CLD_ID
AND   A.SLOC_ID = C.EO_SLOC_ID
AND   MM.MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID) = C.EO_HO_ORG_ID
AND   A.EO_ID   = C.EO_ID
AND   A.CLD_ID  = D.ORG_CLD_ID
AND   A.SLOC_ID = D.ORG_CREATE_REF_SLOC_ID
AND   A.ORG_ID  = D.ORG_ID
AND   B.CLD_ID  = E.CLD_ID
AND   B.SLOC_ID = E.SLOC_ID
AND   MM.MM_GET_HO_ORG(B.CLD_ID,  B.SLOC_ID, B.ORG_ID) = E.HO_ORG_ID
AND   B.ITM_ID  = E.ITM_ID
AND   B.CLD_ID  = F.CLD_ID
AND   B.SLOC_ID = F.SLOC_ID
AND   B.ITM_UOM = F.UOM_ID
AND   A.CLD_ID  = $P{CldId}
AND   A.SLOC_ID = $P{SlocId}
AND   A.ORG_ID  = $P{OrgId}
AND   A.EO_ID   = NVL($P{EoId},  A.EO_ID)
AND   B.ITM_ID  = NVL($P{ItmId}, B.ITM_ID)
AND   TRUNC(A.PO_DT)  >= NVL(TO_DATE($P{FromDate}, 'yyyy-mm-dd'),TRUNC(A.PO_DT))
AND   TRUNC(A.PO_DT)  <= NVL(TO_DATE($P{ToDate}, 'yyyy-mm-dd'),TRUNC(A.PO_DT))
group by A.ORG_ID, A.EO_ID, B.ITM_ID, D.ORG_DESC, C.EO_NM,
E.ITM_DESC, B.ITM_UOM, F.UOM_NM
ORDER BY C.EO_NM, E.ITM_DESC]]>
	</queryString>
