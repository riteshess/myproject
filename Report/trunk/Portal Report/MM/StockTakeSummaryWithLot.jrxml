	<queryString>
		<![CDATA[SELECT  A.ORG_ID,     C.ORG_DESC,  A.DOC_ID,  A.STK_TAKE_NO,  A.STK_TAKE_DT,       F.ATT_NM STATUS,
        B.ITM_ID,       D.ITM_DESC,       E.UOM_NM,       B.TOT_QTY,       B.PHY_QTY,       B.DIFF_QTY,
       G.WH_NM,         B.WH_ID
      , DECODE( B.DIFF_TYPE , 'A', 'ADDED', 'S', 'SUBTRACTED')DIFF_TYPE
        ,H.LOT_ID,  H.TOT_QTY LOT_QTY, H.PHY_QTY LOT_PHY_QTY, H.DIFF_QTY LOT_DIFF_QTY
FROM MM$STK$TAKE A,   MM$STK$TAKE$ITM B,    APP.ORG C,  APP.APP$ITM$PRF D,  APP.APP$UOM$CONV$STD E, APP.APP$DS$ATT F, APP.APP$WH$ORG G
    ,MM.MM$STK$TAKE$LOT H
WHERE A.CLD_ID =B.CLD_ID
AND A.SLOC_ID =B.SLOC_ID
AND A.ORG_ID =B.ORG_ID
AND A.DOC_ID =B.DOC_ID
AND A.ORG_ID =C.ORG_ID
AND B.CLD_ID =D.CLD_ID
AND B.SLOC_ID =D.SLOC_ID
AND B.ITM_ID =D.ITM_ID
AND B.CLD_ID =E.CLD_ID
AND B.SLOC_ID =E.SLOC_ID
AND B.ITM_UOM=E.UOM_ID
AND A.STK_TAKE_STAT =F.ATT_ID
AND B.CLD_ID = G.CLD_ID
AND B.SLOC_ID =G.SLOC_ID
AND B.ORG_ID =G.ORG_ID
AND B.WH_ID =G.WH_ID
AND B.CLD_ID  = H.CLD_ID
AND   B.SLOC_ID = H.SLOC_ID
AND   B.ORG_ID  = H.ORG_ID
AND   B.DOC_ID  = H.DOC_ID
AND   B.WH_ID   = H.WH_ID
AND   B.ITM_ID  = H.ITM_ID
AND   B.ITM_UOM = H.ITM_UOM
AND A.CLD_ID = NVL($P{CldId},A.CLD_ID)
AND A.SLOC_ID =NVL($P{SlocId},A.SLOC_ID)
AND A.ORG_ID = NVL($P{OrgId},A.ORG_ID)
AND B.WH_ID = NVL($P{WhId},B.WH_ID)
AND A.DOC_ID =NVL($P{DocId},A.DOC_ID)
AND B.ITM_ID = NVL($P{ItmId},B.ITM_ID)
AND A.STK_TAKE_STAT = NVL($P{StkTakeStat},A.STK_TAKE_STAT)
AND   TRUNC(A.STK_TAKE_DT) >=NVL(TO_DATE($P{FromDate},'YYYY-MM-DD'), TRUNC(A.STK_TAKE_DT))
AND  TRUNC(A.STK_TAKE_DT) <=NVL(TO_DATE($P{ToDate},'YYYY-MM-DD'), TRUNC(A.STK_TAKE_DT))
order by a.stk_take_no,b.wh_id,b.itm_id]]>
	</queryString>
