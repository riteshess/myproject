	<queryString>
		<![CDATA[SELECT A.CLD_ID,A.SLOC_ID,A.ORG_ID,A.DOC_ID,B.ORG_DESC,
       A.CPO_ID,A.CPO_DT,A.COA_ID,I.COA_NM,
       DECODE(A.DISC_TYPE,'A','Amount'
                         ,'P','Percentange')DISC_TYPE,
       DECODE(A.DISC_TYPE,'A',DISC_VAL||''||'.00'
                       ,'P',DISC_VAL||''||'%') DISCOUNT,
       A.CPO_AMOUNT_SP
       FROM MM$CPO A,FIN.FIN$COA$DTL$VW I,APP.ORG B
       WHERE A.CLD_ID =I.CLD_ID
       AND A.SLOC_ID =I.SLOC_ID
       AND A.ORG_ID =I.ORG_ID
       AND A.COA_ID =I.COA_ID
       AND MM.MM_GET_HO_ORG(A.CLD_ID, A.SLOC_ID, A.ORG_ID)= I.HO_ORG_ID
       AND A.ORG_ID =B.ORG_ID
       AND A.CLD_ID= NVL( $P{CldId}  ,A.CLD_ID)
       AND A.SLOC_ID = NVL($P{SlocId} ,A.SLOC_ID)
       AND A.ORG_ID = NVL( $P{OrgId} ,A.ORG_ID)
       AND A.DOC_ID =nvl($P{DocId},a.doc_id)
       AND A.CPO_DT >=NVL(TO_DATE($P{FromDate},'yyyy-mm-dd'),A.CPO_DT)
       AND A.CPO_DT <=NVL(TO_DATE($P{ToDate},'yyyy-mm-dd'),A.CPO_DT)
       AND A.COA_ID = NVL($P{CoaId},A.COA_ID)
       ORDER BY A.ORG_ID,A.CPO_ID]]>
