		<![CDATA[select aw.wh_nm,ab.bin_nm,aip.itm_desc,aucs.uom_desc,itm_qty,org.org_desc,org.org_id
from app.app$bin$cap abc,app.app$itm$prf aip,app.APP$UOM$CONV$STD aucs,app.App$Bin ab,app.app$wh$org aw,app.org org
where ab.cld_id = org.org_cld_id
and ab.org_id = org.org_id
and abc.cld_id = org.org_cld_id
and abc.org_id = org.org_id
and abc.itm_id=aip.itm_id
and aip.cld_id=abc.cld_id
and aip.sloc_id=abc.sloc_id
and abc.itm_basic_uom=aucs.uom_id
and aucs.cld_id=abc.cld_id
and aucs.sloc_id=abc.sloc_id
and abc.wh_id=ab.wh_id
and abc.bin_id=ab.bin_id
and ab.cld_id=abc.cld_id
and ab.sloc_id=abc.sloc_id
and ab.org_id=abc.org_id
and abc.wh_id=aw.wh_id
and aw.cld_id=abc.cld_id
and aw.sloc_id=abc.sloc_id
and abc.wh_id=nvl($P{WhId},abc.wh_id)
and abc.cld_id=nvl($P{CldId},abc.cld_id)
and abc.sloc_id=nvl($P{SlocId},abc.sloc_id)
and abc.org_id = nvl($P{OrgId},abc.org_id)
order by org.org_id,abc.wh_id,abc.bin_id,abc.itm_id]]>
