	<queryString>
		<![CDATA[SELECT distinct

C.DOC_ID,c.DOC_DT,c.SC_NO,c.SC_DT,c.quot_no,C.SC_TYPE, C.EO_ID,C.ADDS_ID_EO,C.COA_ID, C.BILL_CYCLE,C.SVC_CYCLE,C.PRD_FM,C.PRD_TO,C.CTC_INIT_TYPE ,C.REPAIR_TYPE,C.DISC_TYPE,i.tax_amt,

C.EO_ID, (SELECT EO_NM FROM APP.APP$EO WHERE EO_ID = C.EO_ID AND EO_HO_ORG_ID = C.HO_ORG_ID) EO_NM,

C.HO_ORG_ID,C.CLD_ID,C.ORG_ID,  (SELECT ORG_DESC  FROM APP.ORG  WHERE ORG_CLD_ID = E.CLD_ID AND ORG_ID = E.ORG_ID) ORG_DESC,(i.ITM_QTY * i.ITM_PRICE) Amt ,C.TOT_AMT_SP,

i.ITM_ID,i.ITM_UOM,i.ITM_QTY,i.ITM_PRICE,p.itm_desc,
(select att_nm from svc$ds$att  where att_id=c.sc_type) service_type,
(select att_nm from svc$ds$att  where att_id=c.repair_type) rep_type,
(select att_nm from svc$ds$att where att_id=c.bill_cycle) billcycle,
(select att_nm from svc$ds$att  where att_id=c.svc_cycle) svccycle,
(select adds from app.app$adds$vw where adds_id = c.adds_id_eo) cust_adds,
 APP.FN_GET_ADDS ($P{Sloc_Id}, $P{Cld_Id}, nvl(o.org_add_id1, o.org_add_id2),'M') ORG_ADDS,oc.oc_amt_sp,


E.EMP_NM,u.uom_nm

FROM SVC$SC C, SVC$SC$SVC$ASGN A, SVC$EXEC E,svc$sc$itm i,app.app$uom$conv$std u,app.app$itm$prf p,app.org$oth$dtl o,svc$sc$oc oc
WHERE
a.cld_id=i.cld_id and
a.sloc_id=i.sloc_id and
a.org_id=i.org_id and
a.ho_org_id=i.ho_org_id and
a.doc_id=i.doc_id and
a.itm_id=i.itm_id and
c.org_id=o.org_id and
i.sloc_id=u.sloc_id and
i.cld_id=u.cld_id and
i.itm_uom=u.uom_id and

i.cld_id=p.cld_id and
i.sloc_id=p.sloc_id and

i.ho_org_id=p.ho_org_id and
i.itm_id=p.itm_id and

i.cld_id=oc.cld_id(+) and
i.sloc_id=oc.sloc_id(+) and
i.org_id=oc.org_id(+) and
i.ho_org_id=oc.ho_org_id(+) and
i.doc_id=oc.doc_id(+) and




C.CLD_ID = NVL($P{Cld_Id},C.CLD_ID) and

C.HO_ORG_ID = NVL($P{Ho_Org_Id}, C.HO_ORG_ID) and

C.ORG_ID = NVL($P{Org_Id}, C.ORG_ID) and

C.EO_ID = NVL($P{Eo_Id}, C.EO_ID) and

C.DOC_STAT = NVL($P{SC_Status},C.DOC_STAT) AND

C.DOC_ID = NVL($P{SC_Doc_Id},C.DOC_ID) AND

TRUNC(c.DOC_DT) >= nvl(TO_DATE($P{From_Date}, 'YYYY-mm-dd'),TRUNC(c.DOC_DT))
 AND TRUNC(c.DOC_DT) <= nvl(TO_DATE($P{To_Date}, 'YYYY-mm-dd'),TRUNC(c.DOC_DT)) and



A.EMP_CODE = E.EMP_CODE AND A.ORG_ID = E.ORG_ID AND A.HO_ORG_ID = E.HO_ORG_ID AND

C.DOC_ID = A.DOC_ID AND C.ORG_ID = A.ORG_ID AND C.HO_ORG_ID = A.HO_ORG_ID AND

C. AUTH_STAT = 'Y'
order by sc_no]]>
