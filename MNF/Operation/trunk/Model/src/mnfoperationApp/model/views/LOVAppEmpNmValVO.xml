    PrecisionRule="true"
    Precision="20"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="CURR_ID_SP"
    Expression="CURR_ID_SP"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="EoId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfoperationApp.model.ModelBundle"/>
