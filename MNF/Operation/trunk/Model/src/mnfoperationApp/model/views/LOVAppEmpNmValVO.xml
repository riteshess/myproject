  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfoperationApp.model.views.LOVAppEmpNmValVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="EoNmBind"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="OrgIdBind"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT A.EO_ID,         
  A.EO_NM,B.SUPP_CURR_ID CURR_ID_SP         
FROM APP.APP$EO A ,       
  APP.APP$EO$PRF B         
WHERE A.EO_TYPE  = 70         
AND A.EO_CLD_ID    = B.EO_CLD_ID         
AND A.EO_SLOC_ID   = B.EO_SLOC_ID         
AND A.EO_HO_ORG_ID = B.EO_HO_ORG_ID         
AND A.EO_TYPE      = B.EO_TYPE         
AND A.EO_ID        = B.EO_ID         
AND A.EO_SLOC_ID   = :SlocIdBind           
AND A.EO_ACTV      = 'Y'            
AND A.EO_ORG_ID    = :OrgIdBind            
AND A.EO_HO_ORG_ID = :HoOrgIdBind            
AND A.EO_CLD_ID    = :CldIdBind    
and A.EO_NM = :EoNmBind]]></SQLQuery>
  <ViewAttribute
    Name="EoId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="EO_ID"
    Expression="EO_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="EoNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EO_NM"
    Expression="EO_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
