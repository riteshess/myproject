<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVSearchDetailVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfproductioncosting.model.views.LOVSearchDetailVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindDocId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindCreateDt"
    Kind="where"
    Type="java.sql.Timestamp"/>
  <Variable
    Name="BindCreateToDt"
    Kind="where"
    Type="java.sql.Timestamp"/>
  <SQLQuery><![CDATA[SELECT CLD_ID,SLOC_ID, HO_ORG_ID, ORG_ID ,DOC_ID ,JE_ID ,DOC_DT ,         
DISP_ID_SRC      ,SRC_DOC_DT,ITM_DESC,OUTPT_ITM_QTY  
   
FROM mnf$je$jc$rc_vw       
WHERE mnf$je$jc$rc_vw.CLD_ID = :BindCldId AND     
      mnf$je$jc$rc_vw.SLOC_ID = :BindSlocId AND     
      mnf$je$jc$rc_vw.HO_ORG_ID = :BindHoOrgId AND     
      mnf$je$jc$rc_vw.ORG_ID = :BindOrgId and   
      mnf$je$jc$rc_vw.DOC_ID = :BindDocId and   
      Trunc(mnf$je$jc$rc_vw.DOC_DT) Between NVL(:BindCreateDt, To_Date('01-JAN-1900') )    
and NVL(:BindCreateToDt, Trunc(SYSDATE))]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SlocId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="HoOrgId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="HO_ORG_ID"
    Expression="HO_ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrgId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DocId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOC_ID"
    Expression="DOC_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="JeId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="JE_ID"
    Expression="JE_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DocDt"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="DOC_DT"
    Expression="DOC_DT"
    SQLType="DATE"/>
  <ViewAttribute
    Name="DispIdSrc"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DISP_ID_SRC"
    Expression="DISP_ID_SRC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SrcDocDt"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="SRC_DOC_DT"
    Expression="SRC_DOC_DT"
    SQLType="DATE"/>
  <ViewAttribute
    Name="ItmDesc"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_DESC"
    Expression="ITM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OutptItmQty"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="26"
    Scale="6"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="OUTPT_ITM_QTY"
    Expression="OUTPT_ITM_QTY"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="CldId"/>
    <Item Value="SlocId"/>
    <Item Value="HoOrgId"/>
    <Item Value="OrgId"/>
    <Item Value="DocId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfproductioncosting.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
