<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PendingForMyApprovalVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfLandingPageApp.model.views.PendingForMyApprovalVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindUsrId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT GLBL_DOC_ID,  
         GLBL_DOC_NM,  
         FN_MNF_GET_DOC_CNT (DOC_CLD_ID, DOC_SLOC_ID, :BindHoOrgId, :BindOrgId, GLBL_DOC_ID, :BindUsrId, 'M') DOC_CNT  
  FROM APP.APP$GLBL$DOC A,  
    APP.APP$DOC B  
  WHERE A.GLBL_DOC_ID = B.DOC_ID  
  AND GLBL_DOC_ID    IN (25506, 25507, 25508, 26001, 26002, 26003, 26004)  
  ORDER BY 1]]></SQLQuery>
  <ViewAttribute
    Name="GlblDocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Long"
    ColumnType="LONGINT"
    AliasName="GLBL_DOC_ID"
    Expression="GLBL_DOC_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="GlblDocNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="GLBL_DOC_NM"
    Expression="GLBL_DOC_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DocCnt"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="DOC_CNT"
    Expression="DOC_CNT"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="GlblDocId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfLandingPageApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
