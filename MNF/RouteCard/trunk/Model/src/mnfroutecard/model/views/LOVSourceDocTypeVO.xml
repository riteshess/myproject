  <SQLQuery><![CDATA[SELECT DISTINCT DISP_DOC_ID as DISP_DOC_ID,                        
CLD_ID,                        
SLOC_ID,                        
HO_ORG_ID,                        
ORG_ID,                        
DOC_ID,                        
SRC_DOC_TYPE,                        
pdo_mpp_type,                        
doc_mode,                        
doc_stat,              
doc_dt,          
REV_NO,  
ITM_BATCH_NO  
FROM                        
mnf.mnf$pdo$mpp$op$rc_vw  a                
WHERE                        
doc_mode IN(43,44) AND                        
doc_stat IN(100,41) AND                        
cld_id = :CldIdBind AND                        
sloc_id = :SlocIdBind AND                        
ho_org_id = :HoOrgIdBind AND                        
org_id = :OrgIdBind AND                        
src_doc_type = :SrcDocTypeBind         
AND ( (pdo_mpp_type = 26001        
        
                AND NOT EXISTS (SELECT 1        
        
                                  FROM mnf$mpp$op$itm b        
        
                                 WHERE a.doc_id = b.pdo_id))        
        
              OR pdo_mpp_type <> 26001)        
ORDER BY DISP_DOC_ID ASC]]></SQLQuery>
