mnf.mnf$pdo$mpp$op$rc_vw   
where   
cld_id = :BindCldId and   
sloc_id = :BindSlocId and   
ho_org_id = :BindHoOrgId and   
org_id = :BindOrgId and   
src_doc_type = :BindPdoMppType and    
