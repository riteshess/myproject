  <SQLQuery><![CDATA[SELECT        
A.CLD_ID,        
A.SLOC_ID,        
A.HO_ORG_ID,        
A.ORG_ID,        
A.WC_ID,        
A.WS_ID,        
A.SHIFT_ID,    
A.OP_ID,  
b.op_desc,        
A.STRT_TM,        
A.END_TM,        
A.OUTPT_ITM_ID,        
SUM(A.OUTPT_ITM_QTY) OUTPT_ITM_QTY,        
ROW_NUMBER()over(order by a.WS_ID ) as RN        
FROM MNF.MNF$CAP$PLN$SCHDL A, mnf.mnf$op b        
WHERE        
A.CLD_ID = b.CLD_ID and A.SLOC_ID = b.SLOC_ID and A.HO_ORG_ID = b.HO_ORG_ID and A.OP_ID = b.DOC_ID   
GROUP BY        
A.CLD_ID,        
A.SLOC_ID,        
A.HO_ORG_ID,        
A.ORG_ID,        
A.WC_ID,        
A.WS_ID,        
A.SHIFT_ID,  
A.OP_ID,  
b.op_desc,        
A.STRT_TM,        
A.END_TM,        
A.OUTPT_ITM_ID]]></SQLQuery>
