<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
    <task-flow-definition id="Machine_Schedule_Gantt_Chart">
        <default-activity>Gantt_Chart_View</default-activity>
        <data-control-scope>
            <shared/>
        </data-control-scope>
        <input-parameter-definition id="__1">
            <name>GLBL_APP_CLD_ID</name>
            <value>#{pageFlowScope.GLBL_APP_CLD_ID}</value>
            <class>java.lang.String</class>
        </input-parameter-definition>
        <input-parameter-definition id="__2">
            <name>GLBL_HO_ORG_ID</name>
            <value>#{pageFlowScope.GLBL_HO_ORG_ID}</value>
            <class>java.lang.String</class>
        </input-parameter-definition>
        <input-parameter-definition id="__3">
            <name>GLBL_APP_SERV_LOC</name>
            <value>#{pageFlowScope.GLBL_APP_SERV_LOC}</value>
            <class>java.lang.Integer</class>
        </input-parameter-definition>
        <input-parameter-definition id="__4">
            <name>GLBL_APP_USR_ORG</name>
            <value>#{pageFlowScope.GLBL_APP_USR_ORG}</value>
            <class>java.lang.String</class>
        </input-parameter-definition>
        <input-parameter-definition id="__5">
            <name>GLBL_APP_USR</name>
            <value>#{pageFlowScope.GLBL_APP_USR}</value>
            <class>java.lang.Integer</class>
        </input-parameter-definition>
        <input-parameter-definition id="__6">
            <name>GLBL_DT_FORMAT</name>
            <value>#{pageFlowScope.GLBL_DT_FORMAT}</value>
            <class>java.lang.String</class>
        </input-parameter-definition>
        <input-parameter-definition id="__7">
            <name>GLBL_AMT_DIGIT</name>
            <value>#{pageFlowScope.GLBL_AMT_DIGIT}</value>
            <class>java.lang.Integer</class>
        </input-parameter-definition>
        <input-parameter-definition id="__8">
            <name>GLBL_NUM_FORMAT</name>
            <value>#{pageFlowScope.GLBL_NUM_FORMAT}</value>
            <class>java.lang.String</class>
        </input-parameter-definition>
        <input-parameter-definition id="__9">
            <name>GLBL_CURR_DIGIT</name>
            <value>#{pageFlowScope.GLBL_CURR_DIGIT}</value>
            <class>java.lang.Integer</class>
        </input-parameter-definition>
        <input-parameter-definition id="__10">
            <name>GLBL_QTY_DIGIT</name>
            <value>#{pageFlowScope.GLBL_QTY_DIGIT}</value>
            <class>java.lang.Integer</class>
        </input-parameter-definition>
        <input-parameter-definition id="__11">
            <name>GLBL_RATE_DIGIT</name>
            <value>#{pageFlowScope.GLBL_RATE_DIGIT}</value>
            <class>java.lang.Integer</class>
        </input-parameter-definition>
        <managed-bean id="__17">
            <managed-bean-name>GanttChartViewBean</managed-bean-name>
            <managed-bean-class>mnfcapplanapp.view.bean.GanttChartViewBean</managed-bean-class>
            <managed-bean-scope>pageFlow</managed-bean-scope>
        </managed-bean>
        <view id="Gantt_Chart_View">
            <page>/Fragments/Gantt_Chart_View.jsff</page>
        </view>
        <task-flow-return id="taskFlowReturn1">
            <outcome>
                <name>taskFlowReturn1</name>
            </outcome>
        </task-flow-return>
        <method-call id="filterProjectGanttChart_Func">
            <method>#{bindings.filterProjectGanttChart_Func.execute}</method>
            <outcome>
                <fixed-outcome>filterProjectGanttChart_Func</fixed-outcome>
            </outcome>
        </method-call>
        <control-flow-rule id="__12">
            <from-activity-id>Gantt_Chart_View</from-activity-id>
            <control-flow-case id="__13">
                <from-outcome>Back_To_Home</from-outcome>
                <to-activity-id>taskFlowReturn1</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
        <control-flow-rule id="__14">
            <from-activity-id>filterProjectGanttChart_Func</from-activity-id>
            <control-flow-case id="__15">
                <from-outcome>filterProjectGanttChart_Func</from-outcome>
                <to-activity-id>Gantt_Chart_View</to-activity-id>
            </control-flow-case>
        </control-flow-rule>
        <use-page-fragments/>
    </task-flow-definition>
</adfc-config>
