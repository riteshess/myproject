<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVJCRCIdVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfProductionOrderApp.model.views.LOVJCRCIdVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindjc"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT att_id,   
       att_nm    
  FROM mnf$ds$att_vw    
WHERE att_type_id = 34    
AND CASE  
    WHEN att_id = 105   
         THEN 'Y'    
    WHEN att_id <> 105    
         THEN :bindjc    
    END = :bindjc]]></SQLQuery>
  <ViewAttribute
    Name="AttId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="ATT_ID"
    Expression="ATT_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="AttNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ATT_NM"
    Expression="ATT_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="AttId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfProductionOrderApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
