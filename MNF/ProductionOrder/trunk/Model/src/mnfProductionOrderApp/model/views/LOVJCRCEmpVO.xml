    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindShftId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT  distinct               
        CLD_ID CLD_ID,                     
        SLOC_ID,                     
        HO_ORG_ID,                 
        ORG_ID,                 
        EMP_ID,                     
        EMP_NM            
FROM mnf$wc$ws$shift$emp_vw                    
WHERE CLD_ID = :BindCldId                       
AND SLOC_ID = :BindSlocId                       
AND HO_ORG_ID = :BindHoOrgId                 
AND ORG_ID = :BindOrgId       
AND WC_ID=   :BindWcId   
AND NVL(WS_ID,'0') = NVL(:BindWsId ,NVL(WS_ID,'0'))  
AND SHIFT_ID = :BindShftId]]></SQLQuery>
