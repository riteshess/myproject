  <SQLQuery><![CDATA[select pdo.itm_batch_no,           
pdo.outpt_itm_id,           
pdo.outpt_itm_qty,           
pdo.pln_end_tm,          
pdo.pln_strt_tm,           
pdo.rt_id,          
pdo.bom_id,           
pdo.doc_id,           
pdo.pdo_id,  
pdo.DOC_DT,  
pdo.PDO_REV_NO,    
bom.bom_desc,          
itmvw.itm_desc          
from mnf$pdo pdo , mnf$bom bom, APP$MNF$ITM_VW itmvw           
where pdo.bom_id = bom.doc_id          
and pdo.outpt_itm_id = itmvw.itm_id(+)          
and pdo.cld_id = itmvw.cld_id          
and pdo.sloc_id = itmvw.sloc_id          
and pdo.org_id = itmvw.org_id          
and pdo.ho_org_id = itmvw.ho_org_id          
and pdo.cld_id = bom.cld_id          
and pdo.sloc_id = bom.sloc_id          
and pdo.ho_org_id = bom.ho_org_id          
and pdo.cld_id = :BindCLdId         
and pdo.sloc_id = :BindSlocId         
and pdo.ho_org_id = :BindHoOrgId         
and pdo.org_id = :BindOrgId     
order by pdo_id asc]]></SQLQuery>
