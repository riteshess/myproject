    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select   
    distinct pd.pdo_id ,   
    pd.doc_id as pdo_doc_id,  
    pd.outpt_itm_id,  
    pd.outpt_itm_qty,     
    pd.bom_id,  
