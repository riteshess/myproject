from mnf$param param, org$mnf$param org                   
where org.ORG_ID = :BindOrgId              
and param.param_id = org.param_id                   
and param.cld_id = org.cld_id                   
and  param.ho_org_id = org.ho_org_id                    
and  param.sloc_id = org.sloc_id                   
and org.org_param_actv = 'Y'    
and param.param_actv   ='Y' 
and param.cld_id = :BindCldId                    
and param.ho_org_id = :BindHoOrgId                     
and param.sloc_id = :BindSlocId           
and param.PARAM_SET_ID = :BindSetID         
