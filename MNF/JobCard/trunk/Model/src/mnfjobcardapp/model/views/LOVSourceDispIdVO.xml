<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVSourceDispIdVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfjobcardapp.model.views.LOVSourceDispIdVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="OrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="DocIDBind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT DISTINCT DISP_DOC_ID as DISP_DOC_ID,                
CLD_ID,                
SLOC_ID,                
HO_ORG_ID,                
ORG_ID,                
DOC_ID,                           
pdo_mpp_type     
FROM                
mnf.mnf$pdo$mpp$op_vw        
where      
cld_id = :CldIdBind     
AND ho_org_id = :HoOrgIdBind     
AND sloc_id = :SlocIdBind     
AND org_id = :OrgIdBind     
AND doc_id = nvl(:DocIDBind, doc_id)]]></SQLQuery>
  <ViewAttribute
    Name="DispDocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
