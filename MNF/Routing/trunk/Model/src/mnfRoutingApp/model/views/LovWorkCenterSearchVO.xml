  <SQLQuery><![CDATA[SELECT Distinct   
       a.WC_ID,           
       b.WC_DESC        
  FROM MNF$OP$WC   a, MNF$WC b        
  WHERE a.CLD_ID = b.CLD_ID        
  AND  a.HO_ORG_ID = b.HO_ORG_ID        
  AND  a.SLOC_ID = b.SLOC_ID      
  AND  a.WC_ID = b.WC_ID        
  AND  a.CLD_ID = :BindCldId           
  AND  a.SLOC_ID = :BindSlcId           
  AND  a.HO_ORG_ID = :BindHoOrgId        
  AND  a.DOC_ID = NVL(:BindOpDocId,a.DOC_ID)   
  AND exists ( 
  select 1 from mnf$rt$op x where a.CLD_ID = x.CLD_ID        
  AND  a.HO_ORG_ID = x.HO_ORG_ID        
  AND  a.SLOC_ID = x.SLOC_ID      
  AND  a.WC_ID = x.WC_ID 
  ) 
  ORDER by 1]]></SQLQuery>
