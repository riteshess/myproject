b.PDO_MODE, m.att_nm as mode_Desc, b.PDO_PRIORITY, p.att_nm as priorty_desc, b.PDO_STAT, s.att_nm as stat_desc, b.DOC_DT, b.USR_ID_CREATE, u.USR_NAME,
t.usr_id_TO,  t.WF_STATUS_ID, w.usr_Name as Approved_BY, b.RT_ID, r.rt_desc
from mnf.mnf$pdo$src a, mnf.mnf$pdo b,  mnf.mnf$bom c, app.APP$ITM$PRF f, mnf.MNF$DS$ATT_VW m,mnf.MNF$DS$ATT_VW s,mnf.MNF$DS$ATT_VW p,  app.app$sec$usr u,
MNF.MNF$PDO$WF$TXN t, app.app$sec$usr w, Mnf.mnf$RT r
where
b.cld_id=r.cld_id
and b.sloc_id = r.sloc_id
and b.Ho_org_id= r.ho_org_id
and b.rt_id=r.doc_id
and t.SLOC_ID = a.sloc_id
and t.CLD_ID = a.cld_id
and t.ORG_ID = a.org_id
and t.TXN_DOC_ID = a.doc_id
and t.wf_status_id ='A'
and t.sloc_id=w.sloc_id
and t.usr_id_to = w.usr_id
and a.cld_id=b.cld_id
and a.sloc_id = b.sloc_id
and a.Ho_org_id= b.ho_org_id
and a.org_id=b.org_id
and a.doc_id=b.DOC_ID
and a.ITM_ID =b.OUTPT_ITM_ID
and b.cld_id=c.cld_id
and b.sloc_id = c.sloc_id
and b.Ho_org_id= c.ho_org_id
and b.bom_id=c.DOC_ID
and a.cld_id=f.cld_id
and a.sloc_id = f.sloc_id
and a.Ho_org_id= f.ho_org_id
and a.itm_id=f.itm_ID
and u.USR_ID = b.USR_ID_CREATE
and u.sloc_id=b.sloc_id
and m.att_id=b.pdo_mode
and p.att_id=b.pdo_Priority
and s.att_id=b.PDO_STAT
and a.CLD_ID = :BindCldId
and a.SLOC_ID = :BindSlocId
and a.HO_ORG_ID = :BindHoOrgId
and a.ORG_ID = :BindOrgId
and a.SRC_DOC_ID =NVL(:BindSlsDocId, a.SRC_DOC_ID)]]></SQLQuery>
