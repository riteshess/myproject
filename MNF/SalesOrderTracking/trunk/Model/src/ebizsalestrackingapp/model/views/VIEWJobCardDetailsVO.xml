a.LOC_TYPE, a.USR_ID_CREATE, a.JC_MODE, a.OUTPT_ITM_ID, c.itm_desc, w.USR_NAME as Approved_by  , u.USR_NAME as created_BY,  a.OUTPT_ITM_QTY, a.OP_ID , o.op_desc, a.OP_SR_NO,a.WH_ID,a.RQMT_AREA_ID,a.ACT_OUTPT_ITM_QTY,a.BAL_OP_QTY 
from mnf.mnf$jc$src b, mnf.mnf$jc a, mnf.mnf$WS ws, mnf.mnf$wc wc , APP.APP$SHIFT s, app.APP$ITM$PRF c, app.app$eo eo, 
app.app$sec$usr w,app.app$sec$usr u, MNF$JC$WF$TXN t, mnf.mnf$op o
where
a.cld_id=o.cld_id and
a.sloc_id=o.sloc_id and
a.ho_org_id = o.ho_org_id and
a.op_id= o.DOC_ID and
a.cld_id=wc.cld_id 
and a.sloc_id=wc.sloc_id
and a.ho_org_id = wc.ho_org_id
and a.wc_id=wc.wc_id
and a.cld_id=ws.cld_id
and a.sloc_id=ws.sloc_id
and a.ho_org_id = ws.ho_org_id
and a.WS_ID = ws.ws_id
and a.cld_id=s.cld_id
and a.sloc_id=s.sloc_id
and a.ho_org_id = s.ho_org_id
and a.SHIFT_ID = s.shift_id
and a.cld_id=c.cld_id
and a.sloc_id=c.sloc_id
and a.ho_org_id = c.ho_org_id
and a.OUTPT_ITM_ID = c.itm_id
and a.cld_id=eo.eo_cld_id
and a.sloc_id=eo.eo_sloc_id
and a.ho_org_id = eo.eo_ho_org_id
and a.emp_id= eo.eo_id
and u.sloc_id=a.sloc_id
and u.usr_id = a.USR_ID_CREATE
and t.TXN_DOC_ID = a.doc_id
and t.wf_status_id ='A'
and t.sloc_id=w.sloc_id
and t.usr_id_to = w.usr_id
and a.sloc_id=u.sloc_id
and u.USR_ID = a.USR_ID_CREATE
and a.cld_id=b.cld_id
and a.sloc_id=b.sloc_id
and a.ho_org_id = b.ho_org_id
and a.org_id=b.org_id
and a.doc_id=b.doc_id
and b.CLD_ID = :BindCldId
and b.SLOC_ID = :BindSlocId
and b.HO_ORG_ID = :BindHoOrgId
and b.ORG_ID = :BindOrgId 
and a.op_sr_no=NVL(:BindOpSrNo, a.op_sr_no)
and b.DOC_ID_SRC =:BindPdoId
and a.op_id = NVL(:BindOpDocId, a.op_id)]]></SQLQuery>
