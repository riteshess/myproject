and a.itm_uom = u.uom_id
and a.cld_id=i.cld_id
and a.sloc_id=i.sloc_id
and a.Ho_org_id= i.ho_org_id
and a.ITM_ID=i.itm_id
and a.ITM_BASIS = b.att_id
and a.ITM_TYPE = t.att_id
order by a.itm_type]]></SQLQuery>
