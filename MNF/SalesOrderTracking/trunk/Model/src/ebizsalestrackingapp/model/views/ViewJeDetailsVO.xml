<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ViewJeDetailsVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="ebizsalestrackingapp.model.views.ViewJeDetailsVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindPdoDocId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select distinct a.DOC_ID, b.je_id, a.PDO_ID, s.att_Nm, b.OUTPT_ITM_ID, i.itm_desc, b.OUTPT_ITM_QTY
from mnf.mnf$je$op$itm a, mnf.mnf$je b,  mnf.mnf$ds$att_vw s, app.APP$ITM$PRF i
where
b.JE_STAT = s.att_id
and i.cld_id=b.cld_id
and i.sloc_id=b.sloc_id
and i.ho_org_id = b.ho_org_id
and b.OUTPT_ITM_ID = i.itm_id
and a.doc_id = b.doc_id
and a.cld_id=b.cld_id
and a.sloc_id = b.sloc_id
and a.ho_org_id = b.ho_org_id
and a.org_id = b.org_id
and A.CLD_ID  = NVL(:BindCldId, A.CLD_ID)
AND A.SLOC_ID = NVL(:BindSlocId, A.SLOC_ID)
AND  A.ORG_ID = NVL(:BindOrgId, A.ORG_ID)
AND A.HO_ORG_ID = NVL(:BindHoOrgId,A.HO_ORG_ID)
AND A.pdo_id= NVL(:BindPdoDocId, A.DOC_ID)
order by je_id desc]]></SQLQuery>
  <ViewAttribute
