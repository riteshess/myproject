  <SQLQuery><![CDATA[SELECT A.CLD_ID, 
A.SLOC_ID, 
A.HO_ORG_ID, 
A.DOC_ID, 
A.OUTPT_ITM_ID, 
B.ITM_ID, 
(SELECT ITM_DESC FROM App.APP$ITM$PRF WHERE CLD_ID = A.CLD_ID AND SLOC_ID = A.SLOC_ID AND HO_ORG_ID = A.HO_ORG_ID AND ITM_ID = B.ITM_ID) ITM_DESC, 
B.INPT_BOM_ID, 
(SELECT BOM_DESC FROM MNF$BOM WHERE CLD_ID = A.CLD_ID AND SLOC_ID = A.SLOC_ID AND HO_ORG_ID = A.HO_ORG_ID AND DOC_ID = B.INPT_BOM_ID) INPT_BOM_DESC, 
B.ITM_BASIS, 
B.RT_ID, 
B.OP_ID, 
B.OP_SR_NO, 
B.ITM_TYPE, 
B.CONSMPTN_FLG, 
B.ITM_UOM, 
B.ISSU_TYPE, 
B.ITM_QTY, 
B.ITM_PRICE, 
B.ITM_TOT_AMT, 
B.ITM_SCRAP_PER, 
B.ITM_RMRK, 
B.ITM_PACK_TYPE, 
B.ITM_PACK_QTY, 
B.ITM_PRICE_TYPE, 
B.ITM_RTRN_FLG, 
B.ITM_RTRN_QTY 
FROM MNF$BOM A, MNF$BOM$Op$ITM B 
WHERE A.CLD_ID = B.CLD_ID 
AND A.SLOC_ID = B.SLOC_ID 
AND A.HO_ORG_ID = B.HO_ORG_ID 
AND A.DOC_ID = B.DOC_ID 
 
AND ITM_TYPE IN (62, 67) 
AND ITM_BASIS = 55]]></SQLQuery>
