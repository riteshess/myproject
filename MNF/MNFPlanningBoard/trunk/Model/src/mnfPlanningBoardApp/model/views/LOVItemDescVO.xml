<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVItemDescVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfPlanningBoardApp.model.views.LOVItemDescVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindItmId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[Select a.itm_id, a.itm_desc, A.SERIALIZED_FLG
from app.app$itm$prf a
where a.cld_id = :BindCldId
and a.Sloc_id = :BindSlocId
and A.HO_ORG_ID = :BindHoOrgId
and a.itm_id =Nvl(:BindItmId, itm_id)]]></SQLQuery>
  <ViewAttribute
    Name="ItmId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_ID"
    Expression="ITM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ItmDesc"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_DESC"
    Expression="ITM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SerializedFlg"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SERIALIZED_FLG"
    Expression="SERIALIZED_FLG"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="ItmId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfPlanningBoardApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
