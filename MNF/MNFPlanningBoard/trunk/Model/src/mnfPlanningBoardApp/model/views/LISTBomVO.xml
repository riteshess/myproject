<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LISTBomVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfPlanningBoardApp.model.views.LISTBomVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT DISTINCT A.BOM_ID DOC_ID,  B.BOM_ID,  B.BOM_DESC, B.BOM_REV_NO
FROM MNF$PLN$BOM A, MNF$BOM B
WHERE A.CLD_ID    = B.CLD_ID
AND   A.SLOC_ID   = B.SLOC_ID
AND   A.HO_ORG_ID = B.HO_ORG_ID
AND   A.BOM_ID    = B.DOC_ID
AND   A.CLD_ID    = :BindCldId
AND   A.SLOC_ID   = :BindSlocId
AND   A.ORG_ID    = :BindOrgId
AND   A.HO_ORG_ID = :BindHoOrgId
order by B.BOM_DESC]]></SQLQuery>
  <ViewAttribute
    Name="DocId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOC_ID"
    Expression="DOC_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="BomId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BOM_ID"
    Expression="BOM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="BomDesc"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BOM_DESC"
    Expression="BOM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="BomRevNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Scale="0"
    Type="java.lang.Long"
    ColumnType="NUMBER"
    AliasName="BOM_REV_NO"
    Expression="BOM_REV_NO"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="DocId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfPlanningBoardApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
