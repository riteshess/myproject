<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVSupplierNameVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfPlanningBoardApp.model.views.LOVSupplierNameVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindEoId"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <SQLQuery><![CDATA[SELECT Q.EO_ID EO_ID,Q.eo_nm eo_nm
FROM APP.APP$EO Q
WHERE Q.EO_CLD_ID =:BindCldId
AND   Q.EO_SLOC_ID  = :BindSlocId
--AND   Q.EO_ORG_ID = :BindOrgId
AND   Q.EO_HO_ORG_ID  = :BindHoOrgId
AND   Q.EO_ID   = NVL(:BindEoId,  Q.EO_ID)
union all
select 0 ,'No Supplier' from dual]]></SQLQuery>
  <ViewAttribute
    Name="EoId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="EO_ID"
    Expression="EO_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="EoNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EO_NM"
    Expression="EO_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="EoId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfPlanningBoardApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
