<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVUomVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfPlanningBoardApp.model.views.LOVUomVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindUomId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT
    a.UOM_ID UOM_ID,   
    a.UOM_NM UOM_NM ,  
 a.UOM_DESC UOM_DESC, 
 a.UOM_CLASS UOM_CLASS  
FROM  
    APP.APP$UOM$CONV$STD a  
WHERE a.CLD_ID = :BindCldId  
AND a.SLOC_ID = :BindSlocId  
AND a.UOM_ID = :BindUomId]]></SQLQuery>
  <ViewAttribute
    Name="UomId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="UOM_ID"
    Expression="UOM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="UomNm"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="UOM_NM"
    Expression="UOM_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="5"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="UomDesc"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="UOM_DESC"
    Expression="UOM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="UomClass"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="UOM_CLASS"
    Expression="UOM_CLASS"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="UomId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfPlanningBoardApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
