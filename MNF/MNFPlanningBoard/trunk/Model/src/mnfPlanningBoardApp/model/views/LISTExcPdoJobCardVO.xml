  <SQLQuery><![CDATA[SELECT  distinct A.CLD_ID , A.SLOC_ID,  A.ORG_ID,   A.HO_ORG_ID, A.DOC_ID, A.PDO_ID,  B.OP_ID OP_DOC_ID, B.OP_SR_NO
  , C.OP_ID, C.OP_DESC, B.OUTPT_ITM_ID, D.ITM_DESC, NVL(F.JC_ID, 'JC NOT GENERATED')JC_ID
FROM MNF.MNF$PDO A, MNF.MNF$PDO$OP B, MNF.MNF$OP C, APP.APP$ITM$PRF D, MNF.MNF$JC$OP$ITM E, MNF.MNF$JC F
WHERE A.CLD_ID  = B.CLD_ID
AND   A.SLOC_ID = B.SLOC_ID
AND   A.ORG_ID  = B.ORG_ID
AND   A.HO_ORG_ID = B.HO_ORG_ID
AND   A.DOC_ID  = B.DOC_ID
AND   B.CLD_ID  = C.CLD_ID
AND   B.SLOC_ID = C.SLOC_ID
AND   B.HO_ORG_ID = C.HO_ORG_ID
AND   B.OP_ID   = C.DOC_ID
AND   B.CLD_ID  = D.CLD_ID
AND   B.SLOC_ID = D.SLOC_ID
AND   B.HO_ORG_ID = D.HO_ORG_ID
AND   B.OUTPT_ITM_ID  = D.ITM_ID
AND   B.CLD_ID    = E.CLD_ID(+)
AND   B.SLOC_ID   = E.SLOC_ID(+)
AND   B.ORG_ID    = E.ORG_ID(+)
AND   B.HO_ORG_ID = E.HO_ORG_ID(+)
AND   B.DOC_ID    = E.PDO_ID(+)
AND   B.OP_ID     = E.OP_ID(+)
AND   B.OP_SR_NO  = E.OP_SR_NO(+)
AND   E.CLD_ID    = F.CLD_ID(+)
AND   E.SLOC_ID    = F.SLOC_ID(+)
AND   E.ORG_ID     = F.ORG_ID(+)
AND   E.HO_ORG_ID   = F.HO_ORG_ID(+)
AND   E.DOC_ID      = F.DOC_ID(+)
ORDER BY b.OP_SR_NO asc, C.OP_DESC , B.OP_SR_NO]]></SQLQuery>
