<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVDsAttVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfPlanningBoardApp.model.views.LOVDsAttVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindAttId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindAttTypeId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[Select 
      ATT_ID,
      ATT_NM
from MNF$DS$ATT_VW
where Att_ID = nvl(:BindAttId, Att_ID)
and att_type_id = nvl(:BindAttTypeId,att_type_id)]]></SQLQuery>
  <ViewAttribute
    Name="AttId"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="ATT_ID"
    Expression="ATT_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="AttNm"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ATT_NM"
    Expression="ATT_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfPlanningBoardApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
