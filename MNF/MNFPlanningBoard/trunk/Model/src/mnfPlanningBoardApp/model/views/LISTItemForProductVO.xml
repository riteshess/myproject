  <SQLQuery><![CDATA[SELECT distinct
  a.PLN_DOC_ID pln_doc_id,
  a.OUTPT_ITM_ID,
  a.OUTPT_ITM_DLV_QTY,
  a.TXN_DOC_DLV_DT,
  a.INPT_ITM_ID,
  B.ITM_DESC,
  a.INPT_ITM_QTY,
  a.INPT_ITM_REQ_QTY,
  a.INPT_ITM_AVL_QTY,
  c.UOM_DESC UOM_DESC,
  a.INPT_ITM_REQ_DT,
  a.SUPP_EO_ID,
  e.eo_nm,
  a.INPT_ITM_PRICE,
  a.ITM_SUPP_LEAD_TM,
  a.BOM_ID,
  D.BOM_DESC
FROM MNF$PLN$ITM a,
  app.app$itm$prf b,
  APP.APP$UOM$CONV$STD c,
  mnf$BOM d,
   app.app$eo e
WHERE a.cld_id       = :BindCldId
AND a.sloc_id        = :BindSlocId
AND a.HO_ORG_ID      = :BindHoOrgId
AND a.ORG_ID         = :BindOrgId
AND a.PLN_DOC_ID     =:BindPlnDocId
AND a.OUTPT_ITM_ID   =:BindProductId
AND a.TXN_DOC_DLV_DT =:BindDlvDt
AND a.BOM_ID         =nvl(:BindBomDocId,a.BOM_ID)
AND A.CLD_ID         = B.CLD_ID
AND A.SLOC_ID        = B.SLOC_ID
AND A.HO_ORG_ID      = B.HO_ORG_ID
AND A.INPT_ITM_ID   = B.ITM_ID
AND a.cld_id         =c.cld_id
AND a.sloc_id        =c.sloc_id
AND a.itm_UOM        =c.UOM_ID
AND A.CLD_ID         = d.CLD_ID
AND A.SLOC_ID        = d.SLOC_ID
AND A.HO_ORG_ID      = d.HO_ORG_ID
AND A.BOM_ID         = D.DOC_ID
AND a.CLD_ID       = e.eo_cld_id
AND a.SLOC_ID      = e.eo_sloc_id
AND a.HO_ORG_ID    = e.eo_ho_org_id
AND A.SUPP_EO_ID   = e.eo_id]]></SQLQuery>
