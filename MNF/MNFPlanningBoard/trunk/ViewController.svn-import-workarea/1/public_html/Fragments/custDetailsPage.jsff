<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:f="http://java.sun.com/jsf/core">
  <af:panelGridLayout id="pgl1">
    <af:gridRow marginTop="5px" height="auto" id="gr1">
      <af:gridCell marginStart="5px" columnSpan="2" marginEnd="5px" id="gc1" valign="middle" halign="stretch">
        <af:decorativeBox id="db1">
          <f:facet name="center">
            <af:panelGroupLayout id="pgl2">
              <p>
                Customer Details
              </p>
              <p>
                &#160;
              </p>
            </af:panelGroupLayout>
          </f:facet>
          <f:facet name="top">
            <af:panelBox text="PanelBox1" id="pb1" showHeader="never" showDisclosure="false">
              <f:facet name="toolbar"/>
              <af:navigationPane hint="buttons" id="np1">
                <af:commandNavigationItem id="ni1" text="Back" action="detailsToOverview"/>
                <af:commandNavigationItem id="ni3" text="Edit"/>
                <af:commandNavigationItem id="ni2" text="Save"/>
              </af:navigationPane>
            </af:panelBox>
          </f:facet>
        </af:decorativeBox>
      </af:gridCell>
    </af:gridRow>
    <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr2">
      <af:gridCell marginStart="5px" width="50%" id="gc2" valign="top" halign="stretch">
        <af:decorativeBox id="db2">
          <f:facet name="center">
            <af:panelGroupLayout id="pgl3"/>
          </f:facet>
          <f:facet name="top"/>
        </af:decorativeBox>
      </af:gridCell>
      <af:gridCell marginStart="5px" width="50%" marginEnd="5px" id="gc3" halign="stretch">
        <af:decorativeBox id="db3">
          <f:facet name="center">
            <af:panelGroupLayout id="pgl4"/>
          </f:facet>
          <f:facet name="top"/>
        </af:decorativeBox>
      </af:gridCell>
    </af:gridRow>
  </af:panelGridLayout>
</jsp:root>
