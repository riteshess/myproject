<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVParamBasis"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfWorkStationApp.model.views.LOVParamBasis_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindAttId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select vw.att_id, vw.att_nm    
from MNF$DS$ATT_VW vw     
where vw.att_type_id = 12  
and vw.att_id = :BindAttId]]></SQLQuery>
  <ViewAttribute
    Name="AttId"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="ATT_ID"
    Expression="ATT_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="AttNm"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ATT_NM"
    Expression="ATT_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="AttId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfWorkStationApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
