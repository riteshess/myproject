<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVUserVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfWorkStationApp.model.views.LOVUserVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindUserId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT   
   APP$SEC$USR.SLOC_ID SLOC_ID,   
   APP$SEC$USR.USR_ID USR_ID,   
   APP$SEC$USR.USR_NAME USR_NAME,   
   APP$SEC$USR.USR_ALIAS USR_ALIAS   
FROM   
   APP.APP$SEC$USR  
WHERE  
   SLOC_ID = :BindSlocId AND  
   USR_ID = :BindUserId]]></SQLQuery>
  <ViewAttribute
    Name="SlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="UsrId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Scale="0"
    Type="java.lang.Long"
    ColumnType="NUMBER"
    AliasName="USR_ID"
    Expression="USR_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="UsrName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="USR_NAME"
    Expression="USR_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="UsrAlias"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="USR_ALIAS"
    Expression="USR_ALIAS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfWorkStationApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
