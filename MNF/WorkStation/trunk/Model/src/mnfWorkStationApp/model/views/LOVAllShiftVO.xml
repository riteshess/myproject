<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVAllShiftVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfWorkStationApp.model.views.LOVAllShiftVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select  s.shift_id,  
        s.shift_nm,  
        s.shift_strt_tm,             
        s.shift_end_tm  
FROM   app.app$shift s               
WHERE           
      s.cld_id = :BindCldId               
      and s.ho_org_id = :BindHoOrgId               
      and s.sloc_id = :BindSlocId]]></SQLQuery>
  <ViewAttribute
    Name="ShiftId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SHIFT_ID"
    Expression="SHIFT_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ShiftNm"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SHIFT_NM"
    Expression="SHIFT_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ShiftStrtTm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Timestamp"
    ColumnType="TIMESTAMP"
    AliasName="SHIFT_STRT_TM"
    Expression="SHIFT_STRT_TM"
    SQLType="TIMESTAMP"/>
  <ViewAttribute
    Name="ShiftEndTm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Timestamp"
    ColumnType="TIMESTAMP"
    AliasName="SHIFT_END_TM"
    Expression="SHIFT_END_TM"
    SQLType="TIMESTAMP"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="ShiftId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfWorkStationApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
