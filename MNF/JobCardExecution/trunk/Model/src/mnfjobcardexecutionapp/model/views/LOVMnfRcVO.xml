  <SQLQuery><![CDATA[SELECT distinct            
A.CLD_ID,           
A.SLOC_ID,           
A.HO_ORG_ID,           
A.ORG_ID,           
A.DOC_ID,           
A.DOC_DT,           
A.RC_ID,           
A.RC_STAT,           
A.RC_MODE,           
A.WH_ID,           
A.RQMT_AREA_ID,           
A.STRT_DT,           
A.END_DT,           
A.OUTPT_ITM_ID OUTPT_ITM_ID,           
A.OUTPT_ITM_QTY OUTPT_ITM_QTY,           
A.ACT_OUTPT_ITM_QTY ACT_OUTPT_ITM_QTY,    
' ' OP_ID,    
1 OP_SR_NO,    
1 EMP_ID,    
' ' SHIFT_ID,    
A.MRS_ID,  
a.bal_qty-tmp_bal_qty pending_outpt_qty      
FROM           
MNF.MNF$RC A ,MNF.MNF$RC$OP B          
WHERE A.CLD_ID = B.CLD_ID    
AND A.SLOC_ID = B.SLOC_ID    
AND A.ORG_ID = B.ORG_ID    
AND A.HO_ORG_ID = B.HO_ORG_ID    
AND A.DOC_ID = B.DOC_ID    
AND A.CLD_ID = :BindCldId     
AND A.SLOC_ID = :BindSlocId     
AND A.HO_ORG_ID = :BindHoId     
AND A.ORG_ID = :BindOrgId     
AND A.RC_STAT = 72     
AND A.RC_MODE = 76     
AND NOT EXISTS (SELECT           
1           
FROM           
MNF$JC$SRC b           
WHERE           
A.CLD_ID = b.CLD_ID AND A.SLOC_ID = b.SLOC_ID AND A.HO_ORG_ID = b.HO_ORG_ID AND A.ORG_ID = b.ORG_ID AND A.DOC_ID = b.DOC_ID_SRC)]]></SQLQuery>
