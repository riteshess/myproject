    <SchemaBasedProperties>
      <LABEL
        ResId="mnfjobcardexecutionapp.model.views.LOVAllParameterSetForViewModeVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
