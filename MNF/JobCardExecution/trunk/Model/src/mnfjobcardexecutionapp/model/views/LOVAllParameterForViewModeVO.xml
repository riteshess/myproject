<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVAllParameterForViewModeVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnfjobcardexecutionapp.model.views.LOVAllParameterForViewModeVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindHoId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT DISTINCT param.PARAM_ID AS Param_ID,         
  param.PARAM_DESC             AS Param_Nme,         
  param.PARAM_VAL              AS Param_Value,         
  param.PARAM_BASIS            AS Param_Basis,         
  param.PARAM_SET_ID           AS Param_Set_id,         
  vw.ATT_ID                    AS Att_Id,         
  vw.ATT_NM                    AS Att_nme        
FROM MNF$PARAM param,         
  MNF$DS$ATT_VW vw       
WHERE param.PARAM_TYPE = vw.ATT_ID                 
AND param.CLD_ID       = :BindCldId         
AND param.HO_ORG_ID    = :BindHoId         
AND param.SLOC_ID      = :BindSlocId               
ORDER BY 2]]></SQLQuery>
  <ViewAttribute
    Name="ParamId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_ID"
    Expression="PARAM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ParamNme"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_NME"
    Expression="PARAM_NME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ParamValue"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="26"
    Scale="6"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="PARAM_VALUE"
    Expression="PARAM_VALUE"
    SQLType="NCLOB"/>
  <ViewAttribute
    Name="ParamBasis"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="PARAM_BASIS"
    Expression="PARAM_BASIS"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="ParamSetId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_SET_ID"
    Expression="PARAM_SET_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="AttId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="ATT_ID"
    Expression="ATT_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="AttNme"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ATT_NME"
    Expression="ATT_NME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="ParamId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnfjobcardexecutionapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
