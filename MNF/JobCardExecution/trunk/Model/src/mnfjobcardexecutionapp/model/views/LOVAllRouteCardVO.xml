    A.OUTPT_ITM_QTY OUTPT_ITM_QTY,            
    A.ACT_OUTPT_ITM_QTY ACT_OUTPT_ITM_QTY,          
    A.BAL_QTY - A.TMP_BAL_QTY as PendingQty               
FROM            
    MNF.MNF$RC A            
WHERE            
    A.CLD_ID = :BindCldId AND A.SLOC_ID = :BindSlocId AND A.HO_ORG_ID = :BindHoId AND A.ORG_ID = :BindOrgId AND NOT EXISTS (SELECT            
    1            
FROM            
    MNF$JC$SRC b            
WHERE            
    A.CLD_ID = b.CLD_ID AND A.SLOC_ID = b.SLOC_ID AND A.HO_ORG_ID = b.HO_ORG_ID AND A.ORG_ID = b.ORG_ID AND A.DOC_ID = b.DOC_ID_SRC)]]></SQLQuery>
