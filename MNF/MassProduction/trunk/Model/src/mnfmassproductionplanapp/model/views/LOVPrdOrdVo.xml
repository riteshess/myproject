    Kind="where"
    Type="java.sql.Timestamp"/>
  <Variable
    Name="BindToDt"
    Kind="where"
    Type="java.sql.Timestamp"/>
  <SQLQuery><![CDATA[SELECT a.cld_id, a.sloc_id, a.ho_org_id, a.org_id, a.pdo_id, a.doc_id, a.doc_dt,
       a.outpt_itm_id, a.outpt_itm_qty, a.pdo_rev_no, a.pdo_priority, a.pln_end_tm,b.itm_desc
  FROM MNF.mnf$pdo a,app.app$itm$prf b
  where a.cld_id=b.cld_id
  and   a.sloc_id=b.sloc_id
  and   a.ho_org_id=b.ho_org_id
  and   a.outpt_itm_id=b.itm_id
  and   a.cld_id=:BindCldId
  and   a.sloc_id=:BindSlocId
  and   a.ho_org_id=:BindHoOrgId
  and   a.org_id=:BindOrgId
  and   a.outpt_itm_id=nvl(:BindItmId,a.outpt_itm_id)
  and   a.doc_id =nvl(:BindDocId,a.doc_id)
  and   a.doc_dt  between nvl(:BindFrmDt,a.doc_dt) and nvl(:BindToDt,a.doc_dt)]]></SQLQuery>
