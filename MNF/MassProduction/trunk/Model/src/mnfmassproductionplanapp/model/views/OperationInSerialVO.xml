  <SQLQuery><![CDATA[SELECT  distinct a.cld_id cld_id,
        a.sloc_id,
        a.HO_ORG_ID,
        a.org_id,
        a.op_id, 
        a.op_sr_no, 
        a.outpt_itm_id, 
        a.outpt_itm_qty,
        a.OP_DOC_ID,
        b.op_desc

FROM mnf$pdo$common$op_vw a, mnf$op b

WHERE 
a.cld_id = b.CLD_ID
and a.sloc_id = b.sloc_id
and a.HO_ORG_ID = b.HO_ORG_ID
and a.OP_DOC_ID = b.DOC_ID
and a.CLD_ID =:BindCldId
AND  a.SLOC_ID =:BindSlocId
AND  a.HO_ORG_ID =:BindHoOrgId
AND  a.ORG_ID =:BindOrgId
and a.op_sr_no <= :BindOprSrNo 
AND a.doc_id = :BindPdoDocId]]></SQLQuery>
