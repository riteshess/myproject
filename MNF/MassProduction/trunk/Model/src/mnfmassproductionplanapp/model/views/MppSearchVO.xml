  <SQLQuery><![CDATA[SELECT  CLD_ID ,
        SLOC_ID ,
        HO_ORG_ID ,
        ORG_ID ,
        DOC_ID ,
        MPP_ID ,
        MPP_STAT_ID ,
        MPP_MODE_ID ,
        MPP_REV_NO ,
        MPP_REV_DT ,
        MPP_RMRK ,
        USR_ID_CREATE ,
        USR_ID_CREATE_DT ,
        USR_CREATE_NM ,
        MPP_STAT ,
        MPP_MODE,
        OUTPT_ITM_ID,
        OUTPT_ITM_QTY
FROM MNF.SEARCH$MPP_VW a
WHERE               
a.CLD_ID = :BindCldId            
AND a.SLOC_ID = :BindSlocId            
AND a.HO_ORG_ID = :BindHoOrgId
AND a.ORG_ID = :BindOrgId
AND NVL(MPP_STAT_ID, 0) = NVL(:BindMppStat,  NVL(MPP_STAT_ID, 0))            
AND NVL(MPP_MODE_ID,0) = NVL(:BindMode, NVL(MPP_MODE_ID,0))      
AND NVL(MPP_ID,0) = NVL(:BindMppId, NVL(MPP_ID,0))
AND NVL(OUTPT_ITM_ID,0) = NVL(:BindOutItmId, NVL(OUTPT_ITM_ID,0))
AND NVL(USR_ID_CREATE,0)=NVL(:BindUsrId, NVL(USR_ID_CREATE,0))
AND Trunc(USR_ID_CREATE_DT) Between NVL(:BindStrtDt, To_Date ('01-JAN-1900') ) and NVL(:BindEndDt, Trunc(SYSDATE)) 
GROUP BY 
        MPP_ID ,
        DOC_ID ,
        MPP_STAT_ID ,
        MPP_MODE_ID ,
        MPP_REV_NO ,
        MPP_REV_DT ,
        MPP_RMRK ,
        USR_ID_CREATE ,
        USR_ID_CREATE_DT ,
        USR_CREATE_NM ,
        MPP_STAT ,
        MPP_MODE,
        OUTPT_ITM_ID,
        OUTPT_ITM_QTY,
        CLD_ID ,
        SLOC_ID ,
        HO_ORG_ID ,
        ORG_ID
order By 
        USR_ID_CREATE_DT desc]]></SQLQuery>
