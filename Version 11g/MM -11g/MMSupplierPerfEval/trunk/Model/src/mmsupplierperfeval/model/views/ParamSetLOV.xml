  <SQLQuery><![CDATA[SELECT 
    a.ORG_ID ORG_ID, 
    a.PARAM_SET_ID PARAM_SET_ID, 
    a.PARAM_SET_NM PARAM_SET_NM, 
    a.SLOC_ID SLOC_ID, 
    a.CLD_ID CLD_ID,
    a.ACTV ACTV 
FROM 
    MM$EVAL$PARAM$SET a 
where 
a.ACTV ='Y'
and a.CAT_FLG='S'
And Exists (Select 1 From Mm$eval$param Where Param_Set_Id = A.Param_Set_Id And Org_Id=A.Org_Id And Sloc_Id=A.Sloc_Id And Cld_Id=A.Cld_Id)
And Org_id=Nvl(:Bindorgid,Org_Id)
And Sloc_Id=Nvl(:Bindslocid,Sloc_Id)
and cld_id=nvl(:BindCldId,Cld_Id) 
order by 3]]></SQLQuery>
