  <SQLQuery><![CDATA[SELECT ORG_DOC_CLD_ID CLD_ID,
Org_Sloc_Id Sloc_Id,
ORG_ID,ORG_DOC_ID DOC_ID,GLBL_DOC_NM DOC_NM
FROM APP.ORG$DOC A,APP.APP$GLBL$DOC B
WHERE
A.Org_Doc_Id=B.Glbl_Doc_Id
and ORG_ID=:orgIdBindVar
and ORG_DOC_CLD_ID=:cldIdBindVar
and ORG_SLOC_ID=:slocIdBindVar
and B.glbl_doc_id=nvl(:docIdbindvar,B.Glbl_Doc_Id)
And
exists (select C.Glbl_Doc_Type_Id from app.app$glbl$doc$type c
     Where
      C.Glbl_Doc_Id=B.Glbl_Doc_Id
      and glbl_doc_type_id=nvl(:BindDocTypeid,C.Glbl_Doc_Type_Id)
)
And
Nvl(:usrIdBindVar,0)=Nvl(:usrIdBindVar,0)]]></SQLQuery>
