NVL(TO_DATE(:BindFrmDate,'DD-MON-YY'),TO_DATE(MM$QUOT$ANA$RPT$VW.EVAL_DT,'DD-MON-YY'))
AND TO_DATE(MM$QUOT$ANA$RPT$VW.EVAL_DT,'DD-MON-YY') <=
NVL(TO_DATE(:BindToDate,'DD-MON-YY'),TO_DATE(MM$QUOT$ANA$RPT$VW.EVAL_DT,'DD-MON-YY'))]]></SQLQuery>
