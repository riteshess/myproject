MM$QUOT$ANA$RPT$VW.CLD_ID=NVL(:BINDCLDID, MM$QUOT$ANA$RPT$VW.CLD_ID)
AND
MM$QUOT$ANA$RPT$VW.SLOC_ID=NVL(:BINDSLOCID, MM$QUOT$ANA$RPT$VW.SLOC_ID)
