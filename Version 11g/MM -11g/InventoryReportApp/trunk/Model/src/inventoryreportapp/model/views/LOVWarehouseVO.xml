<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVWarehouseVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="inventoryreportapp.model.views.LOVWarehouseVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BINDCLDID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BINDSLOCID"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BINDORGID"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT A.WH_ID,A.WH_NM
FROM APP.APP$WH$ORG A
WHERE
A.CLD_ID = NVL(:BINDCLDID,A.CLD_ID)
AND A.SLOC_ID = NVL(:BINDSLOCID,A.SLOC_ID)
AND A.ORG_ID =NVL(:BINDORGID,A.ORG_ID)]]></SQLQuery>
  <ViewAttribute
    Name="WhId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="WH_ID"
    Expression="WH_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="WhNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="WH_NM"
    Expression="WH_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="inventoryreportapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
