<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovDocIdForPurchaseInvoiceVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="inventoryreportapp.model.views.LovDocIdForPurchaseInvoiceVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BINDCLDID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BINDSLOCID"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BINDORGID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindInvoiceType"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT A.DOC_ID,A.INVC_NO
FROM MM$INVC A
WHERE A.CLD_ID = :BINDCLDID
AND A.SLOC_ID = :BINDSLOCID
AND A.ORG_ID = :BINDORGID
And A.Invc_Type=nvl(:BindInvoiceType,A.Invc_Type)]]></SQLQuery>
  <ViewAttribute
    Name="DocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOC_ID"
    Expression="DOC_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="InvcNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="INVC_NO"
    Expression="INVC_NO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
</ViewObject>
