       c.CUM_TOT_ISSU_QTY,( select IMG_PATH from APP.APP$ITM$IMG
       where CLD_ID = C.CLD_ID
       AND SLOC_ID = C.SLOC_ID
       AND ho_org_id = C.ORG_ID
       AND ITM_ID = C.ITM_ID
       AND NVL(IMG_DFLT,'N')='Y')IMG_PATH
  FROM app.app$itm$org a,
       app.app$itm$prf b,
       mm.mm$stk$summ$itm c
