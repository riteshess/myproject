  <SQLQuery><![CDATA[select 
a.SLOC_ID,
a.ORG_ID,
a.DOC_ID,
a.DOC_DT,
a.CPO_ID,
a.CPO_DT,
a.ATT_ID,
a.COA_ID,
a.TAX_RULE_FLG,
a.DISC_TYPE,
a.DISC_VAL,
a.CURR_ID_SP,
a.CURR_CONV_FCTR,
a.CPO_AMOUNT_BS,
a.CPO_AMOUNT_SP,
a.USR_ID_CREATE,
a.USR_ID_CREATE_DT,
a.USR_ID_MOD,
a.USR_ID_MOD_DT,
a.ENTITY_ID,
d.usr_name,
e.coa_nm
from 
mm$cpo a,
(select  min(usr_id_create_dt) min_date,max(usr_id_create_dt) max_date
from mm$cpo 
) c,
App.App$sec$usr D,
fin.fin$coa e
where a.sloc_id = nvl(:BindSlocId,a.sloc_id)
and a.org_id = nvl(:BindOrgId,a.org_id)
and trunc(a.usr_id_create_dt) between nvl(:BindFromDate , trunc(c.min_date)) and nvl(:BindToDate , trunc(c.max_date))
and a.doc_id = nvl(:BindDocId , a.doc_id)
and a.cpo_id = nvl(upper(:BindCpoId) , a.cpo_id)
and a.usr_id_create = d.usr_id
And A.Coa_Id = E.Coa_Id
and a.cld_id = e.coa_cld_id
And A.Sloc_Id = E.coa_Sloc_Id
And mm.mm_get_ho_org(a.cld_id,a.sloc_id,a.org_id) = e.coa_ho_Org_Id
and exists (select 1 
from MM$CPO$ITM b 
where b.doc_id = a.doc_id 
and b.sloc_id = a.sloc_id 
and b.org_id = a.org_id 
And B.Itm_Id = Nvl(:BindItemId , B.Itm_Id))
order by cpo_id desc]]></SQLQuery>
