  <SQLQuery><![CDATA[SELECT 
    APP$SEARCH$ITM_VW.IMG_ID,
    APP$SEARCH$ITM_VW.ITM_ID,
    APP$SEARCH$ITM_VW.IMG_PATH,
    APP$SEARCH$ITM_VW.ITM_DESC,
    APP$SEARCH$ITM_VW.UOM_BASIC,
    APP$SEARCH$ITM_VW.UOM_DESC,
    APP$SEARCH$ITM_VW.PRICE_BASIC,
    APP$SEARCH$ITM_VW.GRP_ID,
    APP$SEARCH$ITM_VW.GRP_NM,
    APP$SEARCH$ITM_VW.ACTV,
    APP$SEARCH$ITM_VW.STOCKABLE_FLG,
    APP$SEARCH$ITM_VW.SRVC_ITM_FLG,
    APP$SEARCH$ITM_VW.CAPITAL_GD_FLG,
    APP$SEARCH$ITM_VW.PUR_ITM_FLG,
    APP$SEARCH$ITM_VW.SLS_ITM_FLG,
    APP$SEARCH$ITM_VW.TAX_EXMPT_FLG,
    APP$SEARCH$ITM_VW.MODDATE,
    APP$SEARCH$ITM_VW.CLD_ID,
    APP$SEARCH$ITM_VW.SLOC_ID,
    APP$SEARCH$ITM_VW.HO_ORG_ID
 FROM 
   APP$SEARCH$ITM_VW                             
WHERE 
 ITM_ID = NVL(:BindItmId, ITM_ID)
AND ITM_DESC Like NVL('%'||:BindItmDesc||'%',ITM_DESC)
AND UOM_BASIC = NVL(:BindUomId,UOM_BASIC)
AND UOM_DESC = NVL(:BindUomDesc,UOM_DESC)
AND PRICE_BASIC between NVL(:BindAmtFrom,0) AND NVL(:BindAmtTo,PRICE_BASIC)
AND GRP_ID = NVL(:BindGrpId,GRP_ID)
AND GRP_NM = NVL(:BindGrpNm,GRP_NM)
AND STOCKABLE_FLG = NVL(:BindStokable,STOCKABLE_FLG)
AND SRVC_ITM_FLG = NVL(:BindServiceItem,SRVC_ITM_FLG)
AND CAPITAL_GD_FLG = NVL(:BindCapitalGd,CAPITAL_GD_FLG)
AND PUR_ITM_FLG = NVL(:BindPurItem,PUR_ITM_FLG)
AND SLS_ITM_FLG = NVL(:BindSaleItem,SLS_ITM_FLG)
AND TAX_EXMPT_FLG = NVL(:BindTaxExempt,TAX_EXMPT_FLG)
AND MODDATE between nvl(:BindFromDate,MODDATE) and nvl(:BindToDate,MODDATE)
AND ACTV = NVL(:BindActv,ACTV)]]></SQLQuery>
