  <SQLQuery><![CDATA[SELECT 
        A.SLOC_ID,  A.ORG_ID,   A.HO_ORG_ID,    A.CLD_ID, 
        A.DOC_ID,
        SUBSTR (A.QUOT_ID, 3) DISP_DOC_ID, 
        A.DOC_DT,
        A.EO_ID,
        A.DISC_TYPE,
        A.DISC_VAL, 
        A.TAX_RULE_FLG, 
        A.QUOT_AMT_SP, 
        A.QUOT_AMT_BS, 
        A.CURR_ID_SP,
        A.CURR_CONV_FCTR, B.CATG_ID, B.COA_ID,
        NVL (DECODE (A.DISC_TYPE, 'A', DISC_VAL, 'P', DISC_PERC),
            0
           ) DISC_PERC_VAL,
        A.ASSIGNED_TO, 
        A.FY_ID,
        A.PAY_MODE,
        A.QUOT_STAT
  FROM 
        SLS.SLS$QUOT A, APP.APP$CUST_VW B
 WHERE 
        A.EO_ID = B.EO_ID
        AND A.ORG_ID = B.ORG_ID
        AND A.CLD_ID = B.CLD_ID
        AND A.HO_ORG_ID = B.HO_ORG_ID
        AND A.SLOC_ID = B.SLOC_ID
        AND A.QUOT_MODE = 314
        AND A.QUOT_STAT IN (319,321)
        AND A.ORG_ID = :orgIdBind
        AND A.SLOC_ID = :slocIdBind
        AND A.HO_ORG_ID = :hoOrgIdBind
        AND A.FY_ID = :FyIdBind
        AND A.CLD_ID = :cldIdBind
        AND A.EO_ID = :EoIdBind
        AND A.QUOT_STAT NOT IN ('322')
ORDER BY DOC_DT DESC]]></SQLQuery>
