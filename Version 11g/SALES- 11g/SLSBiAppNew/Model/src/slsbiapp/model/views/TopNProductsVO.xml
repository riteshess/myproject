<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TopNProductsVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false"
  FetchSize="10"
  RangeSize="10">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="slsbiapp.model.views.TopNProductsVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="CountBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="MisTypeBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT 
    ITM_DESC ITEM,
    AMOUNT ,
    ITM_ID
  FROM (
        SELECT   
            A.ITM_DESC, 
            SUM (A.ITM_AMT)  AMOUNT,
            A.ITM_ID
        FROM 
            SLS.SLS$TOP$MIS_VW A
        WHERE 
            A.CLD_ID = :CldIdBind
            AND A.SLOC_ID = :SlocIdBind
            AND A.HO_ORG_ID = :HoOrgIdBind
            AND MIS_TYPE = :MisTypeBind
        GROUP BY A.ITM_DESC , A.ITM_ID
        ORDER BY AMOUNT DESC)
 WHERE ROWNUM <= :CountBind]]></SQLQuery>
  <ViewAttribute
    Name="Item"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITEM"
    Expression="ITEM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Amount"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="AMOUNT"
    Expression="AMOUNT"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="ItmId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_ID"
    Expression="ITM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="slsbiapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
