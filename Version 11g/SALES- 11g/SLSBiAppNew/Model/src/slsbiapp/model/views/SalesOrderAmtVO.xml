<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SalesOrderAmtVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="slsbiapp.model.views.SalesOrderAmtVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="OrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="EoIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT 
    A.ORDER_TYPE,
    SUM(A.ORDER_AMT) ORDER_AMOUNT
FROM
    SLS$SO$TYPE$AMT_VW A
WHERE
    A.CLD_ID = :CldIdBind
    AND A.ORG_ID = NVL(:OrgIdBind,A.ORG_ID)
    AND A.HO_ORG_ID = :HoOrgIdBind
    AND A.SLOC_ID = :SlocIdBind
    AND A.EO_ID = NVL(:EoIdBind, A.EO_ID)
GROUP BY 
    ORDER_TYPE]]></SQLQuery>
  <ViewAttribute
    Name="OrderType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORDER_TYPE"
    Expression="ORDER_TYPE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrderAmount"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="ORDER_AMOUNT"
    Expression="ORDER_AMOUNT"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="slsbiapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
