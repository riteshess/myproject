  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="slsquotationapp.model.views.LOVQuotTypeVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="OrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT 'D' ID, 'Direct Quotation' VALUE FROM DUAL
      UNION
SELECT 'E' ID, 'Quotation From An Opportunity' VALUE FROM DUAL
        WHERE 
       EXISTS (
                SELECT 
                    1 
                FROM 
                    APP.ORG$SLS$PRF 
                WHERE 
                    CLD_ID = :CldIdBind AND
                    ORG_ID = :OrgIdBind AND
                    SLOC_ID = :SlocIdBind AND 
                    USE_OPP_ENTRY = 'Y'
               )]]></SQLQuery>
