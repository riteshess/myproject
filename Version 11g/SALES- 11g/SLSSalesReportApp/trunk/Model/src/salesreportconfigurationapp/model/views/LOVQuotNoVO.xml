  <SQLQuery><![CDATA[SELECT 
  CLD_ID,
  SLOC_ID,
  ORG_ID,
  HO_ORG_ID,
  SUBSTR(QUOT_ID, 3) DISP_DOC_ID,
  DOC_ID,
  DOC_DT,
  TO_CHAR(DOC_DT,'DD-MM-RRRR') DISPLAY_DATE
FROM 
  SLS.SLS$QUOT
WHERE
  QUOT_ID is not null
  and CLD_ID = :cld_id
and SLOC_ID=:sloc_id
and ORG_ID=:org_id
and QUOT_STAT =319
and HO_ORG_ID=:ho_org_id]]></SQLQuery>
