    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="DOC_PEND"
    Expression="DOC_PEND"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="DocCancel"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="DOC_CANCEL"
    Expression="DOC_CANCEL"
    SQLType="NUMERIC"/>
