    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT 
    DOC_ID,
    CLD_ID,
    SLOC_ID,
    HO_ORG_ID,
    ORG_ID,
    DOC_MONTH_YEAR, 
    DOC_MNTH,
    DOC_YEAR,
    DOC_CLOSED,
    DOC_PEND,
    DOC_CANCEL,
    DOC_AMT_CLOSED,
    DOC_AMT_PEND, 
    DOC_AMT_CANCEL
FROM 
    SLS$TRAN$CNT_VW A
WHERE
    A.DOC_ID = 21501 AND
    A.CLD_ID = :CldIdBind AND
    A.SLOC_ID = :SlocIdBind AND
    A.HO_ORG_ID = :HoOrgIdBind AND
    A.ORG_ID = :OrgIdBind]]></SQLQuery>
