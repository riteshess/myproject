    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="TIMESTAMP WITH TIME ZONE"
    AliasName="DOC_AMT_CLOSED"
    Expression="DOC_AMT_CLOSED"
