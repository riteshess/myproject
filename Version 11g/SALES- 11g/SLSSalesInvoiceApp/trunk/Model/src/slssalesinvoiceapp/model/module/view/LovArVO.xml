       A.ARAP_EXT_DOC_DT, A.ARAP_VOU_DUE_DT
    FROM FIN.ARAP_OS_VW A,APP.APP$DOC$TXN B
    WHERE
    A.ARAP_FLG='AR'
    AND A.GL_CLD_ID=b.DOC_CLD_ID
    AND A.ARAP_ORG_ID=b.DOC_ORG_ID
