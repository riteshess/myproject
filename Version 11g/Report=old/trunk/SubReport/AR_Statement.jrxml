				<textField isStretchWithOverflow="true" pattern="#,##0.00;-#,##0.00" isBlankWhenNull="true">
					<reportElement x="715" y="0" width="69" height="26" forecolor="#0000CC"/>
					<box>
						<pen lineColor="#000000"/>
