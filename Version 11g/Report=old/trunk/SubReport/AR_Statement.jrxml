	<queryString>
		<![CDATA[select
a.org_desc,
b.coa_id,
b.coa_nm,

null eo_phone, --h.eo_phone,
null eo_email_id, --h.eo_email_id,
null eo_fax_no, --h.eo_fax_no,
null avg_cr_days, --i.avg_cr_days,
null cr_limit, --i.cr_limit,

--op.org_coa_op_bal open_bal,

nvl(op.org_coa_op_bal,0) + nvl(op1.gl_amt,0) open_bal,

--op.org_coa_op_bal_typ org_coa_op_bal_typ,

decode(sign(nvl(op.org_coa_op_bal,0) + nvl(op1.gl_amt,0)),1,'Dr',0,'Dr',-1,'Cr') org_coa_op_bal_typ,

--comment this to get opening balance

e.voucher_date,
e.doc_txn_id_disp,
e.sales_invoice_doc_id,
e.sales_invoice_id,
e.sales_invoice_date,
e.debtors_invoice,
e.debtors_invoice_date,
e.due_date,
e.over_due_days,
e.curr_nm,
e.ar_amt_sp,
e.ar_amt_typ BalanceType,
e.rate,
e.damt,
e.camt,
e.amt
from app.org a,
  fin$coa b,
  org$coa b1,
  fin$acc$na c,
  app.app$eo d,
  --app.app$eo$pers$comm h,
  --app.app$eo$prf i,
  (
  select a.org_coa_cld_id,
    a.org_id,
    a.org_fy_id,
    a.org_coa_id,
    sum(decode(a.org_coa_op_bal_typ,'Dr',a.org_coa_op_bal,-a.org_coa_op_bal)) org_coa_op_bal,
    decode(sign(sum(decode(a.org_coa_op_bal_typ,'Dr',a.org_coa_op_bal,-a.org_coa_op_bal))),1,'Dr',0,'Dr','Cr') org_coa_op_bal_typ
  from org$coa$fy a,
    (select app.fn_get_fy_id('0000',$P{Org_id},to_date($P{FromDate},'yyyy-MM-dd'),'FY') fy_id
    from dual ) b
  where a.org_coa_cld_id = nvl($P{cld_id}, a.org_coa_cld_id)
  and a.org_id = nvl($P{Org_id}, a.org_id)
  and a.org_fy_id = b.fy_id
  and a.org_coa_id = nvl($P{Coa_Id}, a.org_coa_id)
  group by
    org_coa_cld_id,
    org_id,
    org_fy_id,
    org_coa_id
  ) op,
  (
    select    a.gl_cld_id,
            a.gl_org_id,
            a.gl_coa_id,
            nvl(sum(decode(gl_amt_typ, 'Dr', gl_amt_bs, 'Cr',-gl_amt_bs)),0) gl_amt
        from  gl_lines a,
              gl b,
             (select app.fn_get_fy_start_date($P{cld_id}, $P{Org_id}, TO_DATE($P{FromDate},'YYYY-mm-dd'),'FY') fy_start_date
              from dual) c,
       	    app.app$eo d
    where     b.gl_cld_id = nvl($P{cld_id},b.gl_cld_id)
    and    a.gl_org_id = nvl($P{Org_id}, a.gl_org_id)
    and     a.gl_coa_id = nvl($P{Coa_Id}, a.gl_coa_id)
    and     b.gl_stat = 'FLC'
    and     a.gl_cld_id = b.gl_cld_id
    and     a.gl_sloc_id = b.gl_sloc_id
    and     a.gl_vou_id = b.gl_vou_id
    and     trunc(a.gl_vou_dt) >= c.fy_start_date
    and     trunc(a.gl_vou_dt) < to_date($P{FromDate}, 'YYYY-mm-dd')
    and     a.gl_txn_typ_mig not in  ('O','MO')

    and d.eo_cld_id = a.gl_cld_id
    and d.eo_sloc_id = a.gl_sloc_id
    and d.eo_ho_org_id = a.gl_ho_org_id
    and d.eo_id = a.gl_eo_id
    and d.eo_bhav = 'D'

group by     a.gl_cld_id,
    a.gl_org_id,
    a.gl_coa_id
  ) op1,
  --comment this to get opening balance
  (
  select
    a.gl_cld_id cld_id,
    a.ar_sloc_id sloc_id,  -- add this here
   a.ar_org_id org_id,
    a.ar_coa_id coa_id,
    f.glbl_curr_notation_alias curr_nm,
    substr(b.doc_txn_id_disp,3) doc_txn_id_disp,
    a.ar_vou_dt voucher_date,
    a.ar_doc_id_ref1 sales_invoice_doc_id,
    a.ar_txn_id_ref1 sales_invoice_id,
    a.ar_txn_dt_ref1 sales_invoice_date,
    a.ar_ext_doc_no debtors_invoice,
    a.ar_ext_doc_dt  debtors_invoice_date,
    a.ar_vou_due_dt  due_date,
    ((to_date(nvl($P{ToDate},sysdate),'YYYY-MM-DD'))-a.ar_vou_due_dt ) over_due_days,
    a.ar_amt_sp,
    a.ar_amt_typ,
    nvl(a.ar_cc,1) rate,
    decode(ltrim(rtrim(upper(a.ar_amt_typ))),'DR',nvl(a.ar_amt_sp,0)*nvl(ar_cc,1),0) damt,
    decode(ltrim(rtrim(upper(a.ar_amt_typ))),'CR',nvl(a.ar_amt_sp,0)*nvl(ar_cc,1),0) camt,
    decode(ltrim(rtrim(upper(a.ar_amt_typ))),'DR',nvl(a.ar_amt_sp,0)*nvl(ar_cc,1),'CR',-nvl(a.ar_amt_sp,0)*nvl(ar_cc,1)) amt
  from  ar a,
        app.app$doc$txn b,
        app.app$glbl$curr f
  where a.gl_cld_id = $P{cld_id}
  and a.ar_org_id = nvl($P{Org_id},a.ar_org_id)
  and trunc(a.ar_vou_dt) >= to_date(NVL($P{FromDate},'1000-01-01'),'YYYY-MM-DD')
  and trunc(a.ar_vou_dt)  <= to_date(NVL($P{ToDate},SYSDATE),'YYYY-MM-DD')

  and nvl(ar_amt_sp,0)*nvl(ar_cc,1) > 0 -- dm 2014-05-09
  and ar_curr_id_sp = nvl($P{CurrencyID},a.ar_curr_id_sp)
  and a.ar_coa_id = nvl($P{Coa_Id},a.ar_coa_id)
  and b.doc_cld_id = a.gl_cld_id
  and b.doc_sloc_id = a.ar_sloc_id
  and b.doc_org_id = a.ar_org_id
  and b.doc_txn_id = a.ar_vou_id
  and f.glbl_curr_id = a.ar_curr_id_sp

  union all

select
    a.tvou_cld_id cld_id,
    a.tvou_sloc_id sloc_id,  -- add this here
    a.tvou_org_id org_id,
    a.tvou_coa_id coa_id,
    g.glbl_curr_notation_alias curr_nm,
    substr(b.doc_txn_id_disp,3)||'(-interim)' doc_txn_id_disp,
    a.tvou_dt voucher_date,
    null purchase_invoice_doc_id,
    null purchase_invoice_id,
    null purchase_invoice_date,
    a.tvou_bill_no  debtors_invoice,
    a.tvou_bill_date  debtors_invoice_date,
    a.tvou_due_dt  due_date,
    (a.tvou_due_dt -(to_date(nvl($P{ToDate},SYSDATE),'YYYY-MM-DD'))) over_due_days,
    a.tvou_amt_bs,
    a.tvou_amt_typ,
    nvl(a.tvou_cc,1) rate,
    decode(ltrim(rtrim(upper(a.tvou_amt_typ))),'DR',nvl(a.tvou_amt_bs,0),0) damt,
    decode(ltrim(rtrim(upper(a.tvou_amt_typ))),'CR',nvl(a.tvou_amt_bs,0),0) camt,
    decode(ltrim(rtrim(upper(a.tvou_amt_typ))),'DR',nvl(a.tvou_amt_bs,0),'CR',-nvl(a.tvou_amt_bs,0)) amt
  from tvou_lines a,
      app.app$doc$txn b,
      app.app$glbl$curr g,
      app.app$eo f
  where a.tvou_cld_id = $P{cld_id}
  and a.tvou_org_id = nvl($P{Org_id},a.tvou_org_id)
  and a.tvou_id = decode($P{PostFlag},'P','1','A',a.tvou_id,a.tvou_id)
  and trunc(a.tvou_dt) >= to_date(NVL($P{FromDate},'1000-01-01'),'YYYY-MM-DD')
  and trunc(a.tvou_dt) <= to_date(NVL($P{ToDate},SYSDATE),'YYYY-MM-DD')
  and  a.tvou_amt_bs > 0

  and a.tvou_curr_id_sp = nvl($P{CurrencyID},a.tvou_curr_id_sp)
  and a.tvou_coa_id = nvl($P{Coa_Id},a.tvou_coa_id)
  and b.doc_cld_id = a.tvou_cld_id
  and b.doc_sloc_id = a.tvou_sloc_id
  and b.doc_org_id = a.tvou_org_id
  and b.doc_txn_id = a.tvou_id
  and f.eo_cld_id = a.tvou_cld_id
  and f.eo_sloc_id = a.tvou_sloc_id
  and f.eo_ho_org_id = a.tvou_ho_org_id
  and f.eo_id = a.tvou_eo_id
  and f.eo_bhav = 'D'
  and g.glbl_curr_id = a.tvou_curr_id_sp
  ) e
where a.org_cld_id = $P{cld_id}
and   a.org_id = nvl($P{Org_id},a.org_id)
and b.coa_cld_id = a.org_cld_id
and b.coa_ho_org_id = nvl(a.org_id_parent_l0,a.org_id)
and b.coa_id = nvl($P{Coa_Id},b.coa_id)

and b.coa_cog_id = '1002001' -- hard coding

and b1.cld_id = b.coa_cld_id
and b1.org_sloc_id = b.coa_sloc_id
and b1.ho_org_id = b.coa_ho_org_id
and b1.org_id = a.org_id
and b1.org_coa_id = b.coa_id

and c.acc_cld_id = b.coa_cld_id
and c.acc_sloc_id = b.coa_sloc_id
and c.ho_org_id = b.coa_ho_org_id
and c.acc_id = b.coa_acc_id

and d.eo_cld_id = c.acc_cld_id
and d.eo_sloc_id = c.acc_sloc_id
and d.eo_ho_org_id = c.ho_org_id
and d.eo_id = c.acc_type_id
and d.eo_bhav = 'D'

and b.coa_cld_id = op.org_coa_cld_id
and a.org_id = op.org_id
and b.coa_id = op.org_coa_id

and b1.cld_id = op1.gl_cld_id(+)
and b1.org_id = op1.gl_org_id(+)
and b1.org_coa_id = op1.gl_coa_id(+)

--and (nvl(op.org_coa_op_bal,0) + nvl(op1.gl_amt,0)) <> 0

--comment this to get opening balance

and b1.cld_id = e.cld_id(+)
and b1.org_sloc_id = e.sloc_id(+) -- add this here
and b1.org_id = e.org_id(+)
and b1.org_coa_id = e.coa_id(+)

and not (((nvl(op.org_coa_op_bal,0) + nvl(op1.gl_amt,0)) = 0) and (nvl(e.damt,0) = 0) and (nvl(e.camt,0) = 0))
]]>
	</queryString>
