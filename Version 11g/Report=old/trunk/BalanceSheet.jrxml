    (select fn_get_op_bal_diff_fr_dt($P{Cld_Id},$P{Org_Id},nvl(to_date($P{AsOnDate},'YYYY-MM-DD'),to_date(sysdate,'YYYY-MM-DD'))) op_bal_diff
