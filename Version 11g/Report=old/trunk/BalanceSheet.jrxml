a.org_id,
a.org_desc,
a.ORG_CURR_ID_BS,
b.GLBL_CURR_NOTATION_ALIAS,
CASE WHEN
  c.PROFIT_LOSS_VALUE >= 0 then 'P' else 'L'
end PROFIT_LOSS,
c.PROFIT_LOSS_VALUE,
d.op_bal_diff
from app.org a,
	app.app$glbl$curr b,
    (select FN_NET_CALC($P{Cld_Id},$P{Org_Id},to_date(nvl($P{AsOnDate},to_date(sysdate,'YYYY-MM-DD')),'YYYY-MM-DD'),$P{PostFlag}) PROFIT_LOSS_VALUE
