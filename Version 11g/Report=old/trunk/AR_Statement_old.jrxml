	</variable>
	<variable name="TotalSumDebitforOneOrg" class="java.math.BigDecimal" resetType="Group" resetGroup="Org" calculation="Sum">
		<variableExpression><![CDATA[$F{DAMT}]]></variableExpression>
	</variable>
	<variable name="TotalSumCreditForOneOrg" class="java.math.BigDecimal" resetType="Group" resetGroup="Org" calculation="Sum">
		<variableExpression><![CDATA[$F{CAMT}]]></variableExpression>
	</variable>
	<variable name="TotalBalance" class="java.math.BigDecimal" resetType="Group" resetGroup="Org" calculation="Sum">
		<variableExpression><![CDATA[$V{balance}]]></variableExpression>
		<initialValueExpression><![CDATA[0.00]]></initialValueExpression>
	</variable>
	<variable name="balance" class="java.math.BigDecimal" resetType="Group" resetGroup="Coa"/>
	<group name="Org" isStartNewPage="true">
		<groupExpression><![CDATA[$F{ORG_DESC}]]></groupExpression>
		<groupHeader>
			<band/>
		</groupHeader>
		<groupFooter>
			<band height="21">
				<printWhenExpression><![CDATA[$P{Org_Id}!=""]]></printWhenExpression>
				<textField isBlankWhenNull="true">
					<reportElement x="0" y="0" width="635" height="20"/>
					<textElement textAlignment="Right" verticalAlignment="Middle">
						<font fontName="Arial" size="10" isBold="true"/>
					</textElement>
					<textFieldExpression class="java.lang.String"><![CDATA["Total Amount for :"+$F{ORG_DESC}+" (in Base Currency) is:"]]></textFieldExpression>
				</textField>
