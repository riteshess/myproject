    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="GL_CURR_ID_BS"
    Expression="GL_CURR_ID_BS"
    SQLType="INTEGER"/>
</ViewObject>
