  <SQLQuery><![CDATA[select a.SLOC_ID,
a.ORG_ID,
a.FY_ID,
decode(a.MON_ID,1,'January',2,'February',3,'March',4,'April'
,5,'May',6,'June',7,'July',8,'August',9,'September',10,'October',11,'November',12,'December') "Month",
a.COA_ID,
a.CC_ID,
a.COA_CLD_ID,
a.COA_HO_ORG_ID,
a.COA_CC_BUDGET,
a.COA_CC_BUDGET_TYPE,
a.COA_CC_ACT_BGT,
a.COA_CC_ACT_BGT_TYPE,
a.USR_ID_CREATE,
a.USR_ID_CREATE_DT,
a.USR_ID_MOD,
a.USR_ID_MOD_DT,
a.CC_PRF_ID,
a.CC_ENT_ID,
a.CC_ENT_COMP_ID,
a.CC_COL_ID, 
b.cc_value
from org$budget$cc a , CC$BUDGET_VW b where a.cc_id = b.cc_id  and a.sloc_id = b.sloc_id
and a.coa_cld_id = b.cld_id and a.org_id = b.org_id]]></SQLQuery>
