    <SQLQuery><![CDATA[SELECT 
FIN_COA.COA_SLOC_ID,
ORG_COA_FY.ORG_ID,
ORG_COA_FY.ORG_FY_ID,
FIN_COA.COA_ID,
FIN_COA.COA_NM,
FIN_COA.COA_ACTV,
FIN_COA.COA_COG_ID,
FIN_COA.COA_ACC_ID,
FIN_COA.COA_TRF_BAL,
ORG_COA_FY.ORG_COA_EO_ID,
ORG_COA_FY.ORG_COA_EO_MST_ID,
ORG_COA_FY.ORG_COA_CL_BAL,
ORG_COA_FY.ORG_COA_CL_BAL_TYP
FROM FIN$COA FIN_COA, ORG$COA$FY ORG_COA_FY
WHERE
FIN_COA.COA_ID= ORG_COA_FY.ORG_COA_ID
AND 
substr(FIN_COA.COA_COG_ID,1,1) IN(3,4)
AND
FIN_COA.COA_ACTV = 'Y'
AND
FIN_COA.COA_TRF_BAL = 'N']]></SQLQuery>
