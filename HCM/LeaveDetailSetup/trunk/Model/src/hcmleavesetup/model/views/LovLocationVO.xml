<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovLocationVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="hcmleavesetup.model.views.LovLocationVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[Select A.PARAM_ID, B.PARAM_DESC from ORG$HCM$SETUP A, HCM$SETUP B 
WHERE  
A.HO_ORG_ID=B.HO_ORG_ID 
AND 
A.CLD_ID=B.CLD_ID 
AND 
B.PARAM_TYPE=3 
AND 
A.SLOC_ID=B.SLOC_ID 
AND 
A.PARAM_ID=B.PARAM_ID 
AND 
A.ORG_ID=:BindOrgId 
AND 
A.HO_ORG_ID=:BindHoOrgId 
AND  
A.SLOC_ID=:BindSlocId 
AND 
A.CLD_ID=:BindCldId]]></SQLQuery>
  <ViewAttribute
    Name="ParamId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_ID"
    Expression="PARAM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ParamDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_DESC"
    Expression="PARAM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="ParamId"/>
  </AttrArray>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="hcmleavesetup.view.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
