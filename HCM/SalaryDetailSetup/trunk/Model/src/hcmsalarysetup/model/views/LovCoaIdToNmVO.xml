<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovCoaIdToNmVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="hcmsalarysetup.model.views.LovCoaIdToNmVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindCoaId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[select fc.coa_id,fc.coa_nm           
from FIN.fin$coa fc, FIN.fin$acc$na fan           
where fc.coa_acc_id=fan.acc_id           
and fc.coa_cld_id=fan.acc_cld_id           
and fc.coa_ho_org_id=fan.ho_org_id           
and fc.coa_sloc_id=fan.acc_sloc_id           
and fan.acc_type=0           
and fc.coa_actv='Y'           
and fan.acc_actv='Y'  
and fan.acc_cld_id=:BindCldId     
and fan.ho_org_id=:BindHoOrgId 
and fan.ACC_SLOC_ID =:BindSlocId 
and fc.coa_id =:BindCoaId]]></SQLQuery>
  <ViewAttribute
    Name="CoaId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="BIGINT"
    AliasName="COA_ID"
    Expression="COA_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="CoaNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="COA_NM"
    Expression="COA_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="CoaId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="hcmsalarysetup.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
