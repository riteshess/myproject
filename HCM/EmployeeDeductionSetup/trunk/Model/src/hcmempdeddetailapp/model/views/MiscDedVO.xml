  <SQLQuery><![CDATA[SELECT      
    CLD_ID,      
    SLOC_ID,      
    HO_ORG_ID,      
    ORG_ID,      
    DOC_ID DED_ID,      
    DED_DESC,      
    TAX_NATURE,      
    TAX_COMP_MTHD,      
    MISC_DED_LIMIT,      
    HCM$DED.VALID_STRT_DT VALID_STRT_DT,      
    HCM$DED.VALID_END_DT VALID_END_DT,    
     DED_PRF,  
     DED_TYPE   
FROM      
    HCM$DED      
WHERE      
    CLD_ID = :cldIdBindVar  
    AND SLOC_ID = :slocIdBindVar  
    AND HO_ORG_ID = :hoOrgIdBindVar  
    AND ORG_ID = :orgIdBindVar  
    AND DED_PRF = :dedPrfBindVar 
    and 
    (TRUNC(sysdate) between TRUNC(VALID_STRT_DT) and TRUNC(NVL(VALID_END_DT,SYSDATE)))]]></SQLQuery>
