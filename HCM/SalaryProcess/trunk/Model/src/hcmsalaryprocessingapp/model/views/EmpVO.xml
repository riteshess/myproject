<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
    xmlns="http://xmlns.oracle.com/bc4j"
    Name="EmpVO"
    Version="12.1.3.10.8"
    InheritPersonalization="merge"
    BindingStyle="OracleName"
    CustomQuery="true"
    PageIterMode="Full">
    <DesignTime>
        <Attr Name="_isExpertMode" Value="true"/>
    </DesignTime>
    <Properties>
        <SchemaBasedProperties>
            <LABEL
                ResId="hcmsalaryprocessingapp.model.views.EmpVO_LABEL"/>
        </SchemaBasedProperties>
    </Properties>
    <Variable
        Name="BindOrgId"
        Kind="where"
        Type="java.lang.String"/>
    <Variable
        Name="BindSlocId"
        Kind="where"
        Type="java.lang.Integer"/>
    <Variable
        Name="BindHoOrgId"
        Kind="where"
        Type="java.lang.String"/>
    <Variable
        Name="BindCldId"
        Kind="where"
        Type="java.lang.String"/>
    <Variable
        Name="BindDeptId"
        Kind="where"
        Type="java.lang.String"/>
    <Variable
        Name="BindLocId"
        Kind="where"
        Type="java.lang.String"/>
    <Variable
        Name="BindGrpId"
        Kind="where"
        Type="java.lang.String"/>
    <Variable
        Name="BindDocId"
        Kind="where"
        Type="java.lang.String"/>
    <SQLQuery><![CDATA[Select Doc_Id,Emp_Nm,Emp_Id,Emp_Code,        
Emp_Dept_Id,Emp_Grp_Id, Emp_Loc_Id, Emp_Doj       
From        
Org$hcm$emp$prf         
Where Org_Id=:BindOrgId         
And         
Sloc_Id=:BindSlocId         
And         
Ho_Org_Id=:BindHoOrgId         
And         
Cld_Id=:BindCldId       
And       
Emp_Dept_Id=NVL(:BindDeptId,Emp_Dept_Id)       
And       
Emp_Loc_Id=NVL(:BindLocId,Emp_Loc_Id)       
And       
Emp_Grp_Id=NVL(:BindGrpId, Emp_Grp_Id)       
And       
Doc_Id=NVL(:BindDocId,Doc_Id)]]></SQLQuery>
    <ViewAttribute
        Name="DocId"
        IsPersistent="false"
        PrecisionRule="true"
        Precision="40"
        Type="java.lang.String"
        ColumnType="VARCHAR2"
        AliasName="DOC_ID"
        Expression="DOC_ID"
        SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="40"/>
        </DesignTime>
    </ViewAttribute>
    <ViewAttribute
        Name="EmpNm"
        IsPersistent="false"
        PrecisionRule="true"
        Precision="200"
        Type="java.lang.String"
        ColumnType="VARCHAR2"
        AliasName="EMP_NM"
        Expression="EMP_NM"
        SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="200"/>
        </DesignTime>
    </ViewAttribute>
    <ViewAttribute
        Name="EmpId"
        IsPersistent="false"
        PrecisionRule="true"
        Precision="20"
        Type="java.lang.String"
        ColumnType="VARCHAR2"
        AliasName="EMP_ID"
        Expression="EMP_ID"
        SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="20"/>
        </DesignTime>
    </ViewAttribute>
    <ViewAttribute
        Name="EmpCode"
        IsPersistent="false"
        PrecisionRule="true"
        Type="oracle.jbo.domain.Number"
        ColumnType="BIGINT"
        AliasName="EMP_CODE"
        Expression="EMP_CODE"
        SQLType="NUMERIC"/>
    <ViewAttribute
        Name="EmpDeptId"
        IsPersistent="false"
        PrecisionRule="true"
        Precision="20"
        Type="java.lang.String"
        ColumnType="VARCHAR2"
        AliasName="EMP_DEPT_ID"
        Expression="EMP_DEPT_ID"
        SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="20"/>
        </DesignTime>
    </ViewAttribute>
    <ViewAttribute
        Name="EmpGrpId"
        IsPersistent="false"
        PrecisionRule="true"
        Precision="20"
        Type="java.lang.String"
        ColumnType="VARCHAR2"
        AliasName="EMP_GRP_ID"
        Expression="EMP_GRP_ID"
        SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="20"/>
        </DesignTime>
    </ViewAttribute>
    <ViewAttribute
        Name="EmpLocId"
        IsPersistent="false"
        PrecisionRule="true"
        Precision="20"
        Type="java.lang.String"
        ColumnType="VARCHAR2"
        AliasName="EMP_LOC_ID"
        Expression="EMP_LOC_ID"
        SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="20"/>
        </DesignTime>
    </ViewAttribute>
    <ViewAttribute
        Name="EmpDoj"
        IsUpdateable="false"
        IsPersistent="false"
        PrecisionRule="true"
        Type="oracle.jbo.domain.Timestamp"
        ColumnType="DATE"
        AliasName="EMP_DOJ"
        Expression="EMP_DOJ"
        SQLType="DATE"/>
    <AttrArray Name="KeyAttributes">
        <Item Value="DocId"/>
    </AttrArray>
</ViewObject>
