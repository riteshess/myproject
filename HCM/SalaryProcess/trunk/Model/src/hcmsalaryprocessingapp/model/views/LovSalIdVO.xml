<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovSalIdVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="hcmsalaryprocessingapp.model.views.LovSalIdVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="cldIdBindVar"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="slocIdBindVar"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="hoOrgIdBindVar"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="orgIdBindVar"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="salBehavBindVar"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT A.CLD_ID,                    
A.SLOC_ID,                    
A.HO_ORG_ID,                    
A.ORG_ID,                    
A.SAL_ID,                  
A.SAL_BEHAV,                  
B.PARAM_DESC SAL_DESC,                
A.VALID_STRT_DT,              
A.VALID_END_DT,         
A.OTHR_DED_CHK               
FROM ORG$HCM$SALARY A, HCM$SETUP B                    
WHERE                     
B.PARAM_TYPE = 22                    
AND A.CLD_ID = B.CLD_ID                    
AND A.SLOC_ID = B.SLOC_ID                    
AND A.HO_ORG_ID  = B.HO_ORG_ID                    
AND A.SAL_ID = B.PARAM_ID                   
AND A.CLD_ID = :cldIdBindVar                   
AND A.SLOC_ID = :slocIdBindVar                   
AND A.HO_ORG_ID= :hoOrgIdBindVar                   
AND A.ORG_ID= :orgIdBindVar     
AND A.SAL_TYPE = 'T'       
AND       
TRUNC(sysdate) between TRUNC(A.VALID_STRT_DT) and TRUNC(NVL(A.VALID_END_DT,SYSDATE))     
AND A.SAL_BEHAV = NVL(:salBehavBindVar,A.SAL_BEHAV)]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="HoOrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="HO_ORG_ID"
    Expression="HO_ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SalId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SAL_ID"
    Expression="SAL_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SalBehav"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SAL_BEHAV"
    Expression="SAL_BEHAV"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="SalDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SAL_DESC"
    Expression="SAL_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ValidStrtDt"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="VALID_STRT_DT"
    Expression="VALID_STRT_DT"
    SQLType="DATE"/>
  <ViewAttribute
    Name="ValidEndDt"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="VALID_END_DT"
    Expression="VALID_END_DT"
    SQLType="DATE"/>
  <ViewAttribute
    Name="OthrDedChk"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="OTHR_DED_CHK"
    Expression="OTHR_DED_CHK"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="CldId"/>
    <Item Value="SlocId"/>
    <Item Value="HoOrgId"/>
    <Item Value="OrgId"/>
    <Item Value="SalId"/>
  </AttrArray>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="hcmsalaryprocessingapp.view.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
