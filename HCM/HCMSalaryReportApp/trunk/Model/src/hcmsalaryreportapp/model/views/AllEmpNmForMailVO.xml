A.CLD_ID, 
A.ORG_ID, 
A.SLOC_ID, 
A.HO_ORG_ID 
From HCM.Org$hcm$emp$prf  A                           
Where                          
A.Wrk_Stat In (30,32,33)                         
And                         
A.Org_Id=:BindOrgId                               
And                             
A.Cld_Id=:BindCldId                             
And                             
A.Sloc_id=:BindSlocId                            
And                            
A.Ho_Org_Id=:BindHoOrgId                           
AND                            
A.EMP_GRP_ID=NVL(:BindGrpId,A.EMP_GRP_ID)                      
and                      
A.EMP_DEPT_ID=NVL(:BindDeptId,A.EMP_DEPT_ID)                      
AND                      
A.EMP_LOC_ID=NVL(:BindLocId,A.EMP_LOC_ID)     
AND     
A.DOC_ID = NVL(:BindEmpDocId,A.DOC_ID)]]></SQLQuery>
