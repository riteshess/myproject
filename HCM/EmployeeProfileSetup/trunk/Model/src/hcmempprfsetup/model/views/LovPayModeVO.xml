FROM HCM.ORG$HCM$ATT$LNK A, HCM.HCM$DS$ATT B   
WHERE A.ATT_ID=B.ATT_ID    
AND     
B.ATT_TYPE_ID=11  
AND    
A.CNTRY_ID=:cntryIdBindVar    
AND    
A.ORG_ID=:bindOrgId    
AND    
A.HO_ORG_ID=:bindHoOrgId    
AND    
A.SLOC_ID=:bindSlocId    
AND    
a.CLD_ID=:bindCldId]]></SQLQuery>
