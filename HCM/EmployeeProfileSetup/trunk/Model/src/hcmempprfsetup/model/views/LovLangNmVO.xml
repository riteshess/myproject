<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovLangNmVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="hcmempprfsetup.model.views.LovLangNmVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BIND_ORG_ID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BIND_CLD_ID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BIND_SLOC_ID"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BIND_HO_ORG_ID"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT A.PARAM_ID,               
A.PARAM_DESC,            
trunc(B.PARAM_START_DT) PARAM_START_DT,        
trunc(NVL(B.PARAM_END_DT,SYSDATE)) PARAM_END_DT,        
(case when (TRUNC(sysdate) between TRUNC(B.PARAM_START_DT) and TRUNC(NVL(B.PARAM_END_DT,SYSDATE))) then 'Y'            
     ELSE 'N'            
     END) ACTV_FLG            
FROM HCM$SETUP A, ORG$HCM$SETUP B              
WHERE                
A.CLD_ID = B.CLD_ID              
AND A.SLOC_ID = B.SLOC_ID              
AND A.HO_ORG_ID = B.HO_ORG_ID              
AND B.ORG_ID = :BIND_ORG_ID              
AND B.CLD_ID = :BIND_CLD_ID              
AND B.SLOC_ID = :BIND_SLOC_ID              
AND B.HO_ORG_ID = :BIND_HO_ORG_ID              
AND A.PARAM_ID = B.PARAM_ID              
AND A.PARAM_TYPE = 99]]></SQLQuery>
  <ViewAttribute
    Name="ParamId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_ID"
    Expression="PARAM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ParamDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_DESC"
    Expression="PARAM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ParamStartDt"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="PARAM_START_DT"
    Expression="PARAM_START_DT"
    SQLType="DATE"/>
  <ViewAttribute
    Name="ParamEndDt"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="PARAM_END_DT"
    Expression="PARAM_END_DT"
    SQLType="DATE"/>
  <ViewAttribute
    Name="ActvFlg"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="CHAR"
    AliasName="ACTV_FLG"
    Expression="ACTV_FLG"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="ParamId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="hcmempprfsetup.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
