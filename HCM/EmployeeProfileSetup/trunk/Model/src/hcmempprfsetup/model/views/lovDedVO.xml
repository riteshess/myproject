  <SQLQuery><![CDATA[SELECT A.ATT_ID, B.ATT_NM, A.CNTRY_ID            
FROM ORG$HCM$ATT$LNK A, HCM$DS$ATT B           
WHERE      
B.ATT_TYPE_ID=18        
AND       
A.ATT_ID=B.ATT_ID           
AND               
A.CNTRY_ID=:cntryIdBindVar           
AND           
A.ORG_ID=:bindOrgId           
AND           
A.HO_ORG_ID=:bindHoOrgId           
AND           
A.SLOC_ID=:bindSlocId           
AND           
a.CLD_ID=:bindCldId]]></SQLQuery>
