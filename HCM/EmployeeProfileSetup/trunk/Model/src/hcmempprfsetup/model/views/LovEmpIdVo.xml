  <SQLQuery><![CDATA[SELECT EO_CLD_ID,              
EO_SLOC_ID,              
EO_HO_ORG_ID,             
EO_ID,              
NVL(EMP_NM,EO_NM) EO_NM,        
B.EMP_GRP_ID,     
B.EMP_LOC_ID,     
B.EMP_DESG_ID,     
B.EMP_DEPT_ID,     
NVL((SELECT DISTINCT EMP_ID FROM ORG$HCM$EMP$PRF WHERE EMP_CODE = EO_ID           
                AND HO_ORG_ID = EO_HO_ORG_ID           
                AND SLOC_ID = EO_SLOC_ID           
                AND CLD_ID = EO_CLD_ID          
                ), 'Profile Not Created') EMP_ID_DISP,       
(SELECT DISTINCT EMP_ID FROM ORG$HCM$EMP$PRF WHERE EMP_CODE = EO_ID           
                AND HO_ORG_ID = EO_HO_ORG_ID           
                AND SLOC_ID = EO_SLOC_ID           
                AND CLD_ID = EO_CLD_ID) EMP_ID                   
FROM APP.APP$EO A, HCM.ORG$HCM$EMP$PRF B           
WHERE           
EO_TYPE = 80              
AND EO_ACTV = 'Y'     
AND EO_ID = B.EMP_CODE(+)]]></SQLQuery>
