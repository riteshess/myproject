<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovDesigNmVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="hcmgradesetuapp.model.views.LovDesigNmVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BIND_HO_ORG_ID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BIND_CLD_ID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BIND_SLOC_ID"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BIND_ORG_ID"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT                                    
    a.CLD_ID CLD_ID,                                    
    a.SLOC_ID SLOC_ID,                                      
    a.HO_ORG_ID HO_ORG_ID,                            
    b.org_id ORG_ID,                            
    a.PARAM_TYPE PARAM_TYPE,                                    
    a.PARAM_ID PARAM_ID,                                    
    a.PARAM_DESC PARAM_DESC,                            
    b.param_start_dt,                            
    b.param_end_dt,                           
    (case when (TRUNC(sysdate) between TRUNC(B.PARAM_START_DT) and TRUNC(NVL(B.PARAM_END_DT,SYSDATE))) then 'Y'                             
     ELSE 'N'                             
     END) ACTV_FLG                   
FROM                                    
    HCM.HCM$SETUP A , ORG$HCM$SETUP B                         
    where a.param_type = 4        
    AND b.org_id=:BIND_HO_ORG_ID      
    AND b.CLD_ID = :BIND_CLD_ID       
    AND b.SLOC_ID = :BIND_SLOC_ID                              
    and a.cld_id = b.cld_id                              
    AND A.SLOC_ID = B.SLOC_ID                               
    AND A.HO_ORG_ID = B.HO_ORG_ID                               
    and a.param_id = b.param_id                              
    AND B.ORG_ID = :BIND_ORG_ID]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
