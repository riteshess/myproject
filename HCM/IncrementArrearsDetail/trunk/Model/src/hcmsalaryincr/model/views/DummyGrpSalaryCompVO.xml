<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DummyGrpSalaryCompVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="hcmsalaryincr.model.views.DummyGrpSalaryCompVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BIND_HOORG_ID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BIND_CLD_D"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BIND_SLOC_ID"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BIND_GRP_ID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BIND_ORG_ID"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT P.PARAM_ID,P.PARAM_DESC,O.SAL_BEHAV             
  FROM HCM.HCM$SETUP P,HCM.ORG$HCM$SALARY O            
WHERE O.HO_ORG_ID=:BIND_HOORG_ID            
AND O.CLD_ID = :BIND_CLD_D              
AND O.SLOC_ID = :BIND_SLOC_ID            
AND O.ORG_ID=:BIND_ORG_ID 
AND P.CLD_ID = O.CLD_ID            
  AND P.SLOC_ID = O.SLOC_ID            
  AND P.HO_ORG_ID = O.HO_ORG_ID            
  AND P.PARAM_ID = O.SAL_ID            
AND NOT EXISTS (            
  SELECT 1            
FROM            
  HCM.ORG$HCM$EMP$PRF H,HCM.HCM$EMP$SAL$REF S            
WHERE H.CLD_ID = S.CLD_ID            
  AND H.SLOC_ID = S.SLOC_ID            
  AND H.HO_ORG_ID = S.HO_ORG_ID         
  AND H.ORG_ID = S.ORG_ID          
  AND H.DOC_ID = S.DOC_ID           
  AND S.SAL_ID=P.PARAM_ID             
  AND H.CLD_ID = P.CLD_ID            
  AND H.SLOC_ID = P.SLOC_ID            
  AND H.HO_ORG_ID = P.HO_ORG_ID            
  AND H.EMP_GRP_ID=:BIND_GRP_ID         
  )   
AND EXISTS (            
  SELECT 1   
FROM            
  HCM.ORG$HCM$EMP$PRF H,HCM.HCM$EMP$SAL S            
WHERE H.CLD_ID = S.CLD_ID            
  AND H.SLOC_ID = S.SLOC_ID            
  AND H.HO_ORG_ID = S.HO_ORG_ID         
  AND H.ORG_ID = S.ORG_ID             
  AND H.DOC_ID=S.DOC_ID   
  AND S.SAL_ID=P.PARAM_ID             
  AND H.CLD_ID = P.CLD_ID            
  AND H.SLOC_ID = P.SLOC_ID            
  AND H.HO_ORG_ID = P.HO_ORG_ID   
  AND H.EMP_GRP_ID= :BIND_GRP_ID   
)]]></SQLQuery>
  <ViewAttribute
    Name="ParamId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_ID"
    Expression="PARAM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ParamDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_DESC"
    Expression="PARAM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SalBehav"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SAL_BEHAV"
    Expression="SAL_BEHAV"
    SQLType="INTEGER"/>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="hcmsalaryincr.view.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
