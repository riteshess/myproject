  <SQLQuery><![CDATA[SELECT A.CLD_ID,          
A.SLOC_ID,          
A.HO_ORG_ID,          
A.ORG_ID,          
A.DOC_ID,          
A.INCR_DOC_ID,          
A.GRP_ID,          
A.EMP_DOC_ID,          
A.SAL_ID,         
(SELECT H.INCR_CRITERA FROM HCM$INCR H WHERE A.INCR_DOC_ID = H.DOC_ID) INCR_CRITERA,  
(SELECT B.EMP_NM FROM ORG$HCM$EMP$PRF B WHERE B.DOC_ID = A.EMP_DOC_ID) EMP_NM,          
(SELECT B.PARAM_DESC FROM HCM$SETUP B WHERE B.PARAM_ID = A.GRP_ID AND B.HO_ORG_ID = A.HO_ORG_ID) GRP_NM,          
(SELECT B.PARAM_DESC FROM HCM$SETUP B WHERE B.PARAM_ID = A.SAL_ID AND B.HO_ORG_ID = A.HO_ORG_ID) SAL_NM,        
(CASE (SELECT WRK_STAT FROM ORG$HCM$EMP$PRF  B WHERE B.DOC_ID = A.EMP_DOC_ID)             
  WHEN 32 THEN 'Working'             
  WHEN 33 THEN 'Suspended'            
  WHEN 31 THEN 'Resigned'            
  ELSE 'OTHERS' END)  WorkStatus,        
A.ARR_STRT_DT,          
A.ARR_END_DT,          
A.PAY_DAYS,         
(SELECT USR_NAME FROM APP.APP$SEC$USR U WHERE U.USR_ID = A.USR_ID_CREATE ) USER_CREATED,         
A.ARR_AMT          
FROM              
    HCM$INCR$ARR A ORDER BY A.USR_ID_CREATE_DT DESC]]></SQLQuery>
