<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="GrpShiftUsageVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="hcmgroupprfapp.model.views.GrpShiftUsageVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT DISTINCT A.CLD_ID, A.SLOC_ID , A.HO_ORG_ID , A.ORG_ID, B.EMP_GRP_ID, A.SHIFT_ID FROM  
HCM$EMP$SHIFT A, ORG$HCM$EMP$PRF B 
WHERE TRUNC(SYSDATE) BETWEEN TRUNC(A.VALID_STRT_DT) AND TRUNC(NVL(A.VALID_END_DT,SYSDATE)) 
AND  
A.CLD_ID = B.CLD_ID 
AND A.SLOC_ID = B.SLOC_ID 
AND A.HO_ORG_ID = B.HO_ORG_ID 
AND A.ORG_ID = B.ORG_ID 
AND A.DOC_ID = B.DOC_ID]]></SQLQuery>
