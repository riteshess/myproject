<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovUserNameVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="hcmparameterapp.model.views.LovUserNameVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[Select Usr_Id, Usr_Name From APP.App$sec$usr  
Where   
--Usr_Actv='Y'  
--And  
Sloc_Id=:BindSlocId]]></SQLQuery>
  <ViewAttribute
    Name="UsrId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="LONGINT"
    AliasName="USR_ID"
    Expression="USR_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="UsrName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="USR_NAME"
    Expression="USR_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="hcmparameterapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
