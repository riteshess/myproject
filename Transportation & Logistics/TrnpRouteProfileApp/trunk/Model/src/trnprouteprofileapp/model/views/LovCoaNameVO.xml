a.CLD_ID = b.COA_CLD_ID and              
a.HO_ORG_ID = b.COA_HO_ORG_ID and              
a.ORG_SLOC_ID = b.COA_SLOC_ID and              
a.ORG_COA_ID = b.COA_ID and               
a.ORG_COA_ACTV = 'Y' and          
     
a.CLD_ID = :BindCldId and      
a.ORG_SLOC_ID = :BindSlocId AND        
a.HO_ORG_ID = :BindHoOrgId AND           
a.ORG_ID = :BindOrgId AND         
a.ORG_COA_ID = :BindCoaId]]></SQLQuery>
