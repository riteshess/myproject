    AliasName="EO_CLD_ID"
    Expression="EO_CLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="EoSlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="EO_SLOC_ID"
    Expression="EO_SLOC_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="EoHoOrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EO_HO_ORG_ID"
    Expression="EO_HO_ORG_ID"
    SQLType="VARCHAR">
