  </ViewAttribute>
  <ViewAttribute
    Name="EoType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="EO_TYPE"
    Expression="EO_TYPE"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="EoId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="EO_ID"
    Expression="EO_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="EoNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
