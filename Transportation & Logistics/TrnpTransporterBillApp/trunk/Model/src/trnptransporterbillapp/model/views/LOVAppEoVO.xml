<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVAppEoVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="trnptransporterbillapp.model.views.LOVAppEoVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindEoId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT EO_CLD_ID,        
EO_SLOC_ID,        
EO_HO_ORG_ID,        
EO_TYPE,        
EO_ID,       
EO_NM        
FROM APP.APP$EO        
WHERE EO_CLD_ID = :BindCldId        
AND EO_SLOC_ID = :BindSlocId        
AND EO_HO_ORG_ID = :BindHoOrgId        
AND EO_ID = NVL(:BindEoId, EO_ID)]]></SQLQuery>
