      Kind="where"
      Type="oracle.jbo.domain.Timestamp"/>
    <SQLQuery><![CDATA[SELECT A.CLD_ID,                             
A.SLOC_ID,                             
A.ORG_ID,                             
A.HO_ORG_ID,                             
A.DOC_ID,                             
A.TRNSP_ID,                             
B.CURR_ID,                             
B.CURR_RATE,                          
B.COA_ID,                             
B.TRAN_TYPE,                             
B.OC_ID,                             
B.OC_CAT,                             
SUM(B.AMT_SP_OC) AMT_SP_OC,                             
SUM(B.AMT_SP) AMT_SP,                             
SUM(B.AMT_BS) AMT_BS                             
FROM SLS$TRNP$OL A, SLS$TRNP$OL$OC B                             
WHERE A.CLD_ID = B.CLD_ID                             
AND A.SLOC_ID = B.SLOC_ID                             
AND A.HO_ORG_ID = B.HO_ORG_ID                             
AND A.ORG_ID = B.ORG_ID                             
AND A.DOC_ID = B.DOC_ID                             
                             
AND A.CLD_ID = :BindCldId                             
AND A.SLOC_ID = :BindSlocId                             
AND A.ORG_ID = :BindOrgId                             
AND A.HO_ORG_ID = :BindHoOrgId                             
AND A.TRNSP_ID = NVL(:BindTranspId, TRNSP_ID)                             
AND A.CURR_ID_SP = NVL(:BindCurrIdSp, CURR_ID_SP)                             
AND trunc(A.OL_DT) >= trunc(:BindStDt)              
AND trunc(A.OL_DT) <= trunc(:BindEndDt)                 
AND A.OL_STATUS = 12               
AND REQ_TYP = 'T'       
   
AND A.DOC_ID NOT IN (SELECT LI_ID FROM SLS$TRNP$TB$DTL   
                    WHERE A.CLD_ID = :BindCldId                                   
                    AND A.SLOC_ID = :BindSlocId                                   
                    AND A.ORG_ID = :BindOrgId                                   
                    AND A.HO_ORG_ID = :BindHoOrgId                                   
                    )   
GROUP BY A.CLD_ID,                             
A.SLOC_ID,                             
A.ORG_ID,                             
A.HO_ORG_ID,                             
A.DOC_ID,                             
A.TRNSP_ID,                             
B.CURR_ID,                             
B.CURR_RATE,                          
B.COA_ID,                             
B.TRAN_TYPE,                             
B.OC_ID,                             
B.OC_CAT]]></SQLQuery>
