<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TrnpUnselectedTickerListVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="trnpdashboardapp.model.views.TrnpUnselectedTickerListVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="OrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="UsrIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT      
  A.TKR_ID,     
  A.TKR_NM,     
  B.TKR_POS SR_NO     
FROM      
  MM.MM$TKR A,     
  MM.MM$TKR$USR B     
WHERE     
  A.CLD_ID = B.CLD_ID     
  AND A.SLOC_ID = B.SLOC_ID     
  AND A.ORG_ID = B.ORG_ID     
  AND A.TKR_ID = B.TKR_ID     
  AND A.CLD_ID = :CldIdBind     
  AND A.SLOC_ID = :SlocIdBind     
  AND A.ORG_ID = :OrgIdBind     
  AND A.ACTV = 'Y'     
  AND B.USR_ID = :UsrIdBind     
  AND B.TKR_POS = 0     
  AND A.GLBL_APPLI_STRUCT_ID = '00309'     
ORDER BY SR_NO ASC]]></SQLQuery>
  <ViewAttribute
    Name="TkrId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TKR_ID"
    Expression="TKR_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="TkrNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TKR_NM"
    Expression="TKR_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SrNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SR_NO"
    Expression="SR_NO"
    SQLType="INTEGER"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="trnpdashboardapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
