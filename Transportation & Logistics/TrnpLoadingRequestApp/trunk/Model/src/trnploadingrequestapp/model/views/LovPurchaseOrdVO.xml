  <Variable
    Name="PurchaseOrder"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT DISTINCT      
A.CLD_ID CLD_ID,    
A.SLOC_ID,    
A.ORG_ID,    
A.DOC_ID,    
B.AUTH_PO_NO,    
A.DLV_DT    
FROM MM.MM$DRFT$PO$DLV$SCHDL A, MM.MM$DRFT$PO B    
WHERE  B.PO_TYPE IN (170,171,172,741)         
AND A.CLD_ID = B.CLD_ID    
AND A.SLOC_ID = B.SLOC_ID    
AND A.ORG_ID = B.ORG_ID    
AND A.DOC_ID = B.DOC_ID    
AND A.BAL_QTY - A.TMP_RCPT_QTY > 0    
AND A.CLD_ID = :cldIdBind    
AND A.SLOC_ID = :slocIdBind    
AND A.ORG_ID = :orgIdBind   
AND B.DOC_ID = NVL(:bindDocId, B.DOC_ID)         
AND B.EO_ID = NVL(:bindEoId, B.EO_ID)      
AND B.CURR_ID_SP = NVL(:bindCurrIdSp, B.CURR_ID_SP)     
AND B.PO_MODE = NVL(:poModeBind, B.PO_MODE)  
ORDER BY DLV_DT DESC]]></SQLQuery>
