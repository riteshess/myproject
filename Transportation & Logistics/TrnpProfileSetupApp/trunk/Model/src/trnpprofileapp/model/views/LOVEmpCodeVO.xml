<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVEmpCodeVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="trnpprofileapp.model.views.LOVEmpCodeVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="cldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="slocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="orgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="hoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT B.CLD_ID, B.SLOC_ID, B.ORG_ID, B.HO_ORG_ID, B.PARAM_ID, A.PARAM_DESC 
FROM HCM.HCM$SETUP A, HCM.ORG$HCM$SETUP B 
WHERE A.PARAM_TYPE = 4 
AND A.PARAM_ID = B.PARAM_ID 
AND A.HO_ORG_ID = B.HO_ORG_ID 
AND A.CLD_ID = B.CLD_ID 
AND A.SLOC_ID = B.SLOC_ID 
AND B.CLD_ID = :cldIdBind 
AND B.SLOC_ID = :slocIdBind 
AND B.ORG_ID = :orgIdBind 
AND B.HO_ORG_ID = :hoOrgIdBind]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="HoOrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="HO_ORG_ID"
    Expression="HO_ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ParamId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_ID"
    Expression="PARAM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ParamDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARAM_DESC"
    Expression="PARAM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="ebizresources.resource.bundle.Resource"/>
  </ResourceBundle>

</ViewObject>
