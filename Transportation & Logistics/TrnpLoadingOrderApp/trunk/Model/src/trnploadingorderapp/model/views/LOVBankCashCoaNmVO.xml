  <SQLQuery><![CDATA[SELECT C.CLD_ID,  
      C.ORG_SLOC_ID,  
      C.HO_ORG_ID,  
      C.ORG_ID,  
      B.COA_ID,   
      B.COA_NM  
	FROM FIN.FIN$ACC$NA A,  
			 FIN.FIN$COA B,  
			 FIN.ORG$COA C  
	WHERE  
		A.ACC_CLD_ID = B.COA_CLD_ID  
		AND A.ACC_SLOC_ID = B.COA_SLOC_ID   
		AND A.HO_ORG_ID = B.COA_HO_ORG_ID   
		AND A.ACC_ID = B.COA_ACC_ID  
		  
		AND B.COA_CLD_ID = C.CLD_ID  
		AND B.COA_SLOC_ID = C.ORG_SLOC_ID  
		AND B.COA_HO_ORG_ID = C.HO_ORG_ID  
		AND B.COA_ID = C.ORG_COA_ID  
		  
                AND A.ACC_CLD_ID = :BindCldId  
		AND A.ACC_SLOC_ID = :BindSlocId  
		AND A.HO_ORG_ID = :BindHoOrgId  
		AND C.ORG_ID = :BindOrgId  
                AND A.ACC_TYPE IN (78, 79)  
                AND B.COA_ID = NVL(:BindCoaId, B.COA_ID)]]></SQLQuery>
