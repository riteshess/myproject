    <SQLQuery><![CDATA[SELECT                    
A.CLD_ID,                    
A.SLOC_ID,                    
A.ORG_ID,                    
A.HO_ORG_ID,                    
A.DOC_ID,                    
A.LR_ID,       
A.ROUTE_DOC_ID,           
A.LR_DT,           
A.REQ_TYP,           
A.TRNSP_ID,           
A.VEHICLE_TYP,           
A.MULTIPLE_CUST_FLG,           
A.CURR_ID_SP,           
A.RATE_BASIS,           
A.CURR_CONV_FCTR,        
A.LR_BASIS,              
E.VHCL_NO,     
E.VHCL_ITM_ID,   
SUM(C.FUEL_QTY) FUEL_QTY,    
C.UOM_FUEL    
FROM SLS$TRNP$LR A, SLS$TRNP$VHCL$SCHDL E,SLS$TRNP$VHCL$SCHDL$DTL B, SLS$TRNP$ROUTE$PRF$DTL C      
WHERE  
E.DOC_ID = B.DOC_ID  
AND E.CLD_ID = B.CLD_ID  
AND E.HO_ORG_ID = B.HO_ORG_ID  
AND E.ORG_ID =B.ORG_ID  
AND E.SLOC_ID = B.SLOC_ID  
AND A.CLD_ID = B.CLD_ID(+)      
AND A.SLOC_ID = B.SLOC_ID(+)      
AND A.HO_ORG_ID = B.HO_ORG_ID(+)      
AND A.DOC_ID = B.LR_DOC_ID(+)      
    
AND A.CLD_ID = C.CLD_ID      
AND A.SLOC_ID = C.SLOC_ID    
AND A.HO_ORG_ID = C.HO_ORG_ID    
AND A.ORG_ID = C.ORG_ID    
AND A.ROUTE_DOC_ID = C.DOC_ID      
    
AND A.CLD_ID = :BindCldId                    
AND A.SLOC_ID = :BindSlocId                    
AND A.ORG_ID = :BindOrgId                    
AND A.HO_ORG_ID = :BindHoOrgId                    
AND A.DOC_ID = NVL(:BindDocId, A.DOC_ID)         
AND A.LR_STATUS = NVL(:BindLrStatus, A.LR_STATUS)    
    
GROUP BY    
A.CLD_ID,                    
A.SLOC_ID,                    
A.ORG_ID,                    
A.HO_ORG_ID,                    
A.DOC_ID,                    
A.LR_ID,                
A.ROUTE_DOC_ID,           
A.LR_DT,           
A.REQ_TYP,           
A.TRNSP_ID,           
A.VEHICLE_TYP,           
A.MULTIPLE_CUST_FLG,           
A.CURR_ID_SP,           
A.RATE_BASIS,           
A.CURR_CONV_FCTR,        
A.LR_BASIS,       
E.VHCL_NO,      
E.VHCL_ITM_ID,  
C.UOM_FUEL]]></SQLQuery>
