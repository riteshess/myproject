    <SQLQuery><![CDATA[SELECT a.coa_cld_id cld_id,              
               a.coa_sloc_id sloc_id,              
               a.coa_ho_org_id ho_org_id,              
               b.org_id,              
               a.coa_id coa_id,               
               a.coa_nm,              
               a.coa_alias,              
               a.coa_leg_cd,              
               a.coa_cog_id cog_id,              
               c.cog_nm,              
               d.acc_id na_id,              
               d.acc_nm,              
               d.acc_alias,              
               d.acc_leg_code,              
               d.acc_type,              
               na_type acc_type_desc,              
               nvl(e.eo_bhav,'N') eo_bhav,              
               a.part_of_bdgt,              
                Decode(d.acc_type, 68,    '  (CUSTOMER)',              
                                  69,    '  (SUPPLIER)',              
                                  70,    '  (SERVICE PROVIDER)',              
                                  71,    '  (SUB CONTRACTOR)',              
                                  72,    '  (AGENT)',              
                                  73,    '  (BANK)',              
                                  74,    '  (BANK BRANCH)',              
                                  77,    '  (TRANSPORTER)',              
                                  78,    '  (BANK ACCOUNT)',              
                                  79,    '  (CASH ACCOUNT)',              
                                  80,    '  (EMPLOYEE)',              
                                  81,    '  (PROSPECT)',              
                                  82,    '  (SUPPLIER PROSPECT)', '  (OTHERS)' )  acc_suffix,  
              nvl((select avg_cr_days from app.app$eo$prf g where g.eo_cld_id = e.eo_cld_id    
                and g.eo_ho_org_id = e.eo_ho_org_id and g.eo_id = e.eo_id and g.eo_sloc_id = e.eo_sloc_id),0) avg_cr_days  
FROM fin.fin$coa a,               
   fin.org$coa b,              
   fin.fin$cog c,              
fin.fin$acc$na d,              
   app.app$eo e,              
   fin.fin$na$type_vw f              
WHERE  a.coa_cld_id= b.cld_id              
AND a.coa_sloc_id = b.org_sloc_id              
and a.coa_ho_org_id=b.ho_org_id              
AND a.coa_id = b.org_coa_id              
AND a.coa_actv = 'Y'              
AND b.org_coa_actv = 'Y'              
               
AND a.coa_cld_id = c.cog_cld_id              
and a.coa_sloc_id= c.cog_sloc_id              
AND a.coa_ho_org_id = c.cog_org_id              
AND a.coa_cog_id = c.cog_id              
AND c.cog_actv = 'Y'              
               
and a.coa_cld_id = d.acc_cld_id              
and a.coa_sloc_id = d.acc_sloc_id              
AND a.coa_ho_org_id = d.ho_org_id              
AND a.coa_acc_id  = d.acc_id              
AND d.acc_actv = 'Y'              
and d.acc_cld_id = e.eo_cld_id(+)              
and d.acc_sloc_id = e.eo_sloc_id(+)              
AND d.ho_org_id = e.eo_ho_org_id(+)              
AND d.acc_type_id = e.eo_id(+)              
and d.acc_type=e.eo_type(+)             
AND f.na_type_id = d.acc_type             
and a.coa_cld_id = :cldIdBind             
and b.org_id = :orgIdBind             
and a.coa_ho_org_id = :hoOrgIdBind             
AND a.coa_sloc_id = :slocIdBind           
and (case when :typeFlgBind = 'E' and acc_type = 80 then 1     
when :typeFlgBind = 'P' and acc_type = 69 then 1     
else 0     
end     
) = 1     
and acc_type in (69, 80)            
AND  a.coa_id =nvl(:coaIdBind,a.coa_id)        
AND nvl(e.eo_bhav,'N') = 'C']]></SQLQuery>
