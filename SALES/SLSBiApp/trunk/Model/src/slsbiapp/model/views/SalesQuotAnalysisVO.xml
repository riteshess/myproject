<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SalesQuotAnalysisVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="slsbiapp.model.views.SalesQuotAnalysisVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT
    A.SNO,
    A.QUOT_TYP_DESC,
    SUM(A.QUOT_CNT) QUOT_COUNT,
    SUM(A.QUOT_AMT) QUOT_AMT
FROM
    SLS$QUOT$ANA_VW A
WHERE
    A.CLD_ID = :CldIdBind 
    AND A.HO_ORG_ID = :HoOrgIdBind
    AND A.SLOC_ID = :SlocIdBind
GROUP BY
    A.QUOT_TYP_DESC ,A.SNO
ORDER BY
    A.SNO]]></SQLQuery>
  <ViewAttribute
    Name="Sno"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Long"
    ColumnType="LONGINT"
    AliasName="SNO"
    Expression="SNO"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="OppTypDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="19"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="OPP_TYP_DESC"
    Expression="OPP_TYP_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="19"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OppCount"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="OPP_COUNT"
    Expression="OPP_COUNT"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="OppAmt"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="OPP_AMT"
    Expression="OPP_AMT"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="slsbiapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
