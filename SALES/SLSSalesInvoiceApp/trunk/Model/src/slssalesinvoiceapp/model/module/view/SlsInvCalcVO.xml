       NVL(SlsInvCalcEO.COA_NM, B.COA_NM) COA_NM,   
       SlsInvCalcEO.SR_NO,    
       SlsInvCalcEO.TRAN_TYPE,    
       SlsInvCalcEO.AMD_NO,    
       SlsInvCalcEO.AMD_DT,   
       SlsInvCalcEO.CURR_CONV_RATE,   
       SlsInvCalcEO.CC_ID, 
       FIN.FN_CHK_COA_COST_CENTER_FLG(SlsInvCalcEO.CLD_ID,SlsInvCalcEO.SLOC_ID,SlsInvCalcEO.HO_ORG_ID,SlsInvCalcEO.COA_ID) COA_COST_CENTER_FLG 
FROM SLS$INV$CALC SlsInvCalcEO ,FIN.FIN$COA B   
WHERE SlsInvCalcEO.CLD_ID=B.COA_CLD_ID(+)   
AND SlsInvCalcEO.SLOC_ID=B.COA_SLOC_ID(+)   
AND SlsInvCalcEO.HO_ORG_ID=B.COA_HO_ORG_ID(+)   
AND SLSINVCALCEO.COA_ID=B.COA_ID(+)   
order by SlsInvCalcEO.SR_NO]]></SQLQuery>
