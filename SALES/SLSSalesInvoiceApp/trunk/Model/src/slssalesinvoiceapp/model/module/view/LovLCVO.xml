  <SQLQuery><![CDATA[SELECT * FROM (SELECT 
  DOC_ID,
  DOC_DT,
  LC_ID,
  LC_AMT_BS
FROM  
  APP.APP$LC
WHERE
  CLD_ID = :CldIdBind
  AND SLOC_ID = :SlocIdBind
  AND HO_ORG_ID = :HoOrgIdBind
  AND ORG_ID = :OrgIdBind
  AND LC_STATUS = 765-- CLOSED
  AND EO_TYPE = 763
  AND FY_ID = :FyIdBind
  AND VLD_FRM_DT <= :DocDtBind
  AND NVL(VLD_END_DT,:DocDtBind) >= :DocDtBind
  AND DOC_ID = NVL(:DocIdBind,DOC_ID)
  AND EO_ID = :EoIdBind)]]></SQLQuery>
