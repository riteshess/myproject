<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ProductAgeingVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="slsproductageingapp.model.views.ProductAgeingVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="OrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="DocDtFrom"
    Kind="where"
    Type="java.sql.Timestamp"/>
  <Variable
    Name="DocDtTo"
    Kind="where"
    Type="java.sql.Timestamp"/>
  <SQLQuery><![CDATA[SELECT
    A.ORG_ID,
    A.ITM_ID,
    A.ITM_UOM,
    A.ITM_DESC,
    sum(A.LOT_QTY) LOT_QTY,
    sum(A.OSDAYS) OSDAYS,
    sum(A.OS_30_DAYS) OS_30_DAYS,
    sum(A.OS_60_DAYS) OS_60_DAYS,
    sum(A.OS_90_DAYS) OS_90_DAYS,
    sum(A.OS_120_DAYS) OS_120_DAYS,
    TRUNC(A.DOC_DT_SRC) DOC_DT
FROM
    SLS.SLS$REP$PRODUCT$AGEING_VW A
WHERE
    A.ORG_ID = :OrgIdBind AND
    TRUNC(A.DOC_DT_SRC) >= NVL(:DocDtFrom,TRUNC(A.DOC_DT_SRC)) AND
    TRUNC(A.DOC_DT_SRC) <= NVL(:DocDtTo,TRUNC(A.DOC_DT_SRC))
GROUP BY 
    A.ORG_ID,
    A.ITM_ID,
    A.ITM_UOM,
    A.ITM_DESC,
    TRUNC(A.DOC_DT_SRC)]]></SQLQuery>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ItmId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_ID"
    Expression="ITM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ItmUom"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_UOM"
    Expression="ITM_UOM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ItmDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_DESC"
    Expression="ITM_DESC"
    SQLType="VARCHAR"
    Precision="255">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="LotQty"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="26"
    Scale="6"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="LOT_QTY"
    Expression="LOT_QTY"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Osdays"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="OSDAYS"
    Expression="OSDAYS"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Os30Days"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="OS_30_DAYS"
    Expression="OS_30_DAYS"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Os60Days"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="OS_60_DAYS"
    Expression="OS_60_DAYS"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Os90Days"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="OS_90_DAYS"
    Expression="OS_90_DAYS"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Os120Days"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="OS_120_DAYS"
    SQLType="NUMERIC"
    IsQueriable="false"
    Expression="OS_120_DAYS"/>
  <ViewAttribute
    Name="DocDt"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="DOC_DT"
    Expression="DOC_DT"
    SQLType="DATE"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="slsproductageingapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
