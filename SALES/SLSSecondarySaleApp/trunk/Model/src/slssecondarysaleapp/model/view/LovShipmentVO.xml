  SUBSTR(SHIPMNT_ID,3) SHIPMNT_ID,
  SHIPMNT_STAT,
  EO_ID,
  FY_ID
FROM
  SLS$SHIPMNT
WHERE
  SHIPMNT_STAT = 218
  AND SLOC_ID = :SlocIdBind
  AND CLD_ID = :CldIdBind
  AND ORG_ID = :OrgIdBind
  AND HO_ORG_ID = :HoOrgIdBind
  AND EO_ID = :EoIdBind
  AND FY_ID = :FyIdBind
