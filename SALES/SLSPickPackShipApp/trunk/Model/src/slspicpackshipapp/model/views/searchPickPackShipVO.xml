     A.SHIP_FLG SHIP_FLG,   
     A.WH_ID WH_ID ,  
     A.SHIPMNT_DOC_ID SHIPMNT_DOC_ID ,
     A.EXT_DOC_NO
FROM   
    SLS.SEARCH$PICK$PACK$SHIP_VW A  
WHERE  
    A.CLD_ID = :cldIdBind   
    AND A.ORG_ID = :orgIdBind   
    AND A.HO_ORG_ID = :hoOrgIdBind   
    AND A.SLOC_ID = :slocIdBind   
    AND A.EO_ID = NVL(:eoIdBind,A.EO_ID)  
    AND A.SO_DOC_ID = NVL(:soDocIdBind,A.SO_DOC_ID)  
    AND A.PICKDOC_ID = NVL(:pickIdBind,A.PICKDOC_ID)  
    AND A.SHIPMNT_DOC_ID = NVL(:shpmntIdBind,A.SHIPMNT_DOC_ID)  
    AND A.DLV_MODE = NVL(:dlvModeBind,A.DLV_MODE)  
    AND A.SO_ID LIKE '%'||NVL(UPPER(:SoIdDispBind),A.SO_ID)||'%'    
    AND A.PICK_ID LIKE '%'||NVL(UPPER(:PickDispIdBind),A.PICK_ID)||'%'    
    AND A.SHIPMNT_ID LIKE '%'||NVL(UPPER(:ShipmntIdDispBind),A.SHIPMNT_ID)||'%'
    AND NVL(A.EXT_DOC_NO, 1) LIKE '%'||NVL(UPPER(:BindExtDocNo),NVL(A.EXT_DOC_NO,1))||'%' 
    and a.DLV_ADDS_ID = NVL(:BindDlvAdds, a.DLV_ADDS_ID)
ORDER BY A.PICK_DT DESC,A.PICK_ID DESC,A.SHIPMNT_ID DESC,A.SO_ID DESC]]></SQLQuery>
