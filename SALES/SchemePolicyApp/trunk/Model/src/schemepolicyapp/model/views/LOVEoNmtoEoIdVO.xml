      <LABEL
        ResId="schemepolicyapp.model.views.LOVEoNmtoEoIdVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
