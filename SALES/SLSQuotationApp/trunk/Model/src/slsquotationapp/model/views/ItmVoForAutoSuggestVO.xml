<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ItmVoForAutoSuggestVO"
  Version="12.1.3.10.8"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="slsquotationapp.model.views.ItmVoForAutoSuggestVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="OrgIdBind"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="ItmNmBind"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT ITM_DESC ITM_DESC
FROM
  (SELECT A.ITM_DESC
  FROM APP.APP$ITM$PRF A,
    APP.APP$ITM$ORG B
  WHERE A.CLD_ID  = B.CLD_ID
  AND A.SLOC_ID   = B.SLOC_ID
  AND A.ITM_ID    = B.ITM_ID
  AND A.HO_ORG_ID = B.HO_ORG_ID
  AND B.CLD_ID    = :CldIdBind
  AND B.SLOC_ID   = :SlocIdBind
  AND B.ORG_ID    = :OrgIdBind
  AND B.HO_ORG_ID = :HoOrgIdBind
  AND A.SLS_ITM_FLG = 'Y'
  AND A.ACTV = 'Y'
  AND UPPER(A.ITM_DESC) LIKE '%'||UPPER(:ItmNmBind)||'%'
 
  ORDER BY ITM_DESC ASC
  )
WHERE ROWNUM <11]]></SQLQuery>
  <ViewAttribute
    Name="ItmDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_DESC"
    Expression="ITM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
</ViewObject>
