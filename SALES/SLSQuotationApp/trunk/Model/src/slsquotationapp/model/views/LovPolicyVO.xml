    Type="java.lang.Integer"/>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="DocDtBind"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <SQLQuery><![CDATA[SELECT    
    CLD_ID,    
    SLOC_ID,    
    HO_ORG_ID,    
    PLC_ID,    
    PLC_NM   
  FROM    
    APP.APP$PRICE$PLC A 
WHERE A.CLD_ID           = :CldIdBind 
AND A.SLOC_ID            = :SlocIdBind 
AND A.HO_ORG_ID          = :HoOrgIdBind 
AND A.ACTV               = 'Y' 
AND TRUNC(NVL(A.VALID_FROM,:DocDtBind)) <= TRUNC(:DocDtBind) 
AND TRUNC(NVL(A.VALID_UPTO,:DocDtBind)) >=TRUNC(:DocDtBind) 
