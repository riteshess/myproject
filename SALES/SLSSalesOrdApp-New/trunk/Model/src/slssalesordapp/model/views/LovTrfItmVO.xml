  <Variable
    Name="ItmDescBind"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT A.ITM_ID ITM_ID, A.ITM_DESC ITM_DESC,A.UOM_SLS UOM_PUR,
     A.UOM_BASIC UOM_BASIC
FROM APP.APP$SLS$ITM_VW A
WHERE A.SLOC_ID = :SlocIdBind
AND A.ORG_ID    = :OrgIdBind
AND A.HO_ORG_ID = :HoOrgIdBind
AND A.CLD_ID    = :CldIdBind
AND A.ITM_ID  IN
    (
        SELECT B.ITM_ID
        FROM SLS.SLS$SO$ITM B
        WHERE A.CLD_ID  = B.CLD_ID
        AND A.SLOC_ID   = B.SLOC_ID
        AND A.ORG_ID    = B.ORG_ID
        AND A.HO_ORG_ID = B.HO_ORG_ID
        AND B.DOC_ID    = :DocIdBind
        UNION
        SELECT C.ITM_ID
        FROM SLS$SO$ITM$SCHM C
        WHERE A.CLD_ID  = C.CLD_ID
        AND A.SLOC_ID   = C.SLOC_ID
        AND A.ORG_ID    = C.ORG_ID
        AND A.HO_ORG_ID = C.HO_ORG_ID
        AND C.DOC_ID    = :DocIdBind
    )]]></SQLQuery>
