<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SLSInvItmDtlVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="slsrmaapp.model.views.SLSInvItmDtlVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="SLOC_ID"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="CLD_ID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="ORG_ID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="HO_ORG_ID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SHIPIDBIND"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT 
    A.DOC_ID INV_DOC_ID, 
    A.DISP_DOC_ID INV_ID 
FROM  
    SLS$INV A, 
    SLS$INV$DTL B 
WHERE 
    A.SLOC_ID = B.SLOC_ID 
    AND A.CLD_ID = B.CLD_ID 
    AND A.ORG_ID = B.ORG_ID 
    AND A.HO_ORG_ID = B.HO_ORG_ID 
    AND A.DOC_ID = B.DOC_ID 
    AND A.SLOC_ID = :SLOC_ID 
    AND A.CLD_ID = :CLD_ID 
    AND A.ORG_ID = :ORG_ID 
    AND A.HO_ORG_ID = :HO_ORG_ID 
    AND B.SHIP_ID = :SHIPIDBIND 
    AND ROWNUM <=1]]></SQLQuery>
  <ViewAttribute
    Name="InvDocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="INV_DOC_ID"
    Expression="INV_DOC_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="InvId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="INV_ID"
    Expression="INV_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="InvDocId"/>
  </AttrArray>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="slsrmaapp.view.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
