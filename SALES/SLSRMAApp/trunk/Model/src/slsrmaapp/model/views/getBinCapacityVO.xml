<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="getBinCapacityVO"
  Version="12.1.3.10.8"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="slsrmaapp.model.views.getBinCapacityVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="OrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="WhIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BinIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="ItmIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT  
    A.ITM_QTY ITM_QTY  
FROM  
    APP.APP$BIN$CAP A 
WHERE 
    A.CLD_ID = :CldIdBind 
    AND A.SLOC_ID = :SlocIdBind 
    AND A.ORG_ID = :OrgIdBind 
    AND A.WH_ID = :WhIdBind 
    AND A.BIN_ID = :BinIdBind 
    AND A.ITM_ID = :ItmIdBind]]></SQLQuery>
  <ViewAttribute
    Name="ItmQty"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="26"
    Scale="6"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="ITM_QTY"
    Expression="ITM_QTY"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="slsrmaapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
