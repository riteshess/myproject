  <SQLQuery><![CDATA[SELECT A.CLD_ID,       
A.SLOC_ID,       
A.HO_ORG_ID,       
A.DOC_ID,       
A.FREE_GRP_ID,       
B.GRP_NM,       
A.FREE_ITM_ID,       
(SELECT ITM_DESC from APP$ITM$PRF where ITM_ID = A.FREE_ITM_ID) ITM_DESC,       
A.FREE_ITM_UOM,       
(SELECT UOM_DESC from APP$UOM$CONV$STD where UOM_ID = A.FREE_ITM_UOM) UOM_DESC,       
(SELECT UOM_NM from APP$UOM$CONV$STD where UOM_ID = A.FREE_ITM_UOM) UOM_NM,       
A.FREE_ITM_QTY,       
A.FREE_ITM_PRICE,       
decode(A.DISC_TYPE, 'A', 'Amount', 'P', 'Percent', null) DISC_TYPE,      
A.DISC_VAL,       
A.FOC_FLG,       
A.CONV_FCTR,       
A.MIN_AMT,       
A.MAX_AMT,       
A.UNLMT_AMT,       
A.GRP_ITM_FLG       
FROM APP$SCHM$DTL A, APP$GRP B    
WHERE A.CLD_ID = B.CLD_ID       
AND A.SLOC_ID = B.SLOC_ID       
AND A.HO_ORG_ID = B.HO_ORG_ID       
AND A.FREE_GRP_ID = B.GRP_ID]]></SQLQuery>
