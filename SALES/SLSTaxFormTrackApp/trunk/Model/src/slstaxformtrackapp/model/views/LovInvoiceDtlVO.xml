    <SQLQuery><![CDATA[SELECT A.CLD_ID, A.SLOC_ID, A.ORG_ID,                 
  A.HO_ORG_ID, A.DOC_ID, A.DOC_DT,                 
  A.CURR_ID_SP, APP.FN_GET_CURR_DESC_FRM_CURR_ID(A.SLOC_ID,A.CLD_ID,A.CURR_ID_SP) CURR_DESC, A.CURR_RATE,                 
  A.INV_TYPE,(SELECT B.ATT_NM                 
  FROM APP.APP$DS$ATT_VW B                 
  WHERE B.ATT_TYPE_ID = 76                 
  AND B.ATT_ID        = A.INV_TYPE                 
  ) INV_TYPE_DESC, A.EO_ID,SLS.FN_GET_EO_DESC_FRM_EO_ID(A.SLOC_ID,A.CLD_ID,A.HO_ORG_ID,A.EO_ID) EO_NM,                 
  A.DISP_DOC_ID, A.INV_STAT,(DECODE (A.INV_STAT , 217,'OPEN',218,'CLOSED',220,'CANCELLED')) INV_STAT_DESC,                 
  A.INV_MODE, (DECODE (A.INV_MODE , 314,'APPROVED',317,'FORWARDED',313,'INCOMPLETE')) INV_MODE_DESC, A.FY_ID,                 
  A.DRFT_INV_ID, A.TAX_FORM_TYP_ID,NVL(                 
  (SELECT B.ATT_NM                 
  FROM APP.APP$DS$ATT_VW B                 
  WHERE B.ATT_TYPE_ID = 155                 
  AND B.ATT_ID        = A.TAX_FORM_TYP_ID                 
  ),'Form not Applied') FORM_TYP_DESC,                  
  A.TOT_AMT_SP,       
  A.FORM_RCVD,       
  DECODE(A.FORM_RCVD,'Y','Form Recived','N','Form Not Recieved') FORM_RCVD_DESC          
FROM SLS.SLS$INV A                 
WHERE A.CLD_ID               = :CldIdBind                 
AND A.SLOC_ID                = :SlocIdBind                 
AND A.ORG_ID                 = :OrgIdBind                 
AND A.HO_ORG_ID              = :HoOrgIdBind                 
AND A.INV_STAT               = 218          
AND A.TAX_FORM_TYP_ID        IS NOT NULL          
AND A.INV_MODE               = 314                 
AND NVL(A.TAX_FORM_TYP_ID,0) = NVL(:FormTypeIdBind,NVL(A.TAX_FORM_TYP_ID,0))                 
AND A.DOC_ID                 = NVL(:InvDocIdBind,A.DOC_ID)             
AND trunc(A.DOC_DT) BETWEEN NVL(:BindFromDt,trunc(A.DOC_DT)) AND NVL(:BindToDt,trunc(A.DOC_DT))            
AND A.EO_ID = NVL(:BindCustId,A.EO_ID)          
AND A.DISP_DOC_ID = NVL(:BindInvoiceNo,A.DISP_DOC_ID)]]></SQLQuery>
