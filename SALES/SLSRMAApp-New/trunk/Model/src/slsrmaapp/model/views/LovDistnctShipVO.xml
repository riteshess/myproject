    <SQLQuery><![CDATA[SELECT DISTINCT A.SHIPMNT_ID,         
    A.DOC_ID,         
    TRUNC(A.DOC_DT) DOC_DT,         
    A.SLOC_ID,         
    A.ORG_ID,         
    A.CLD_ID,         
    A.HO_ORG_ID,         
    A.EO_ID,         
    A.FY_ID,         
    TO_CHAR(A.DOC_DT,'DD-MM-RRRR') DISPLAY_DATE         
  FROM SLS.SLS$SHIPMNT A,         
    SLS$SHIPMNT$ITM D         
  WHERE A.DOC_ID  = D.DOC_ID         
  AND A.CLD_ID    = D.CLD_ID         
  AND A.SLOC_ID   = D.SLOC_ID         
  AND A.HO_ORG_ID = D.HO_ORG_ID         
  AND A.ORG_ID    = D.ORG_ID         
  AND A.Pick_id   = D.Pick_id         
    --AND D.SHIPMNT_QTY    - NVL(FN_RMA_GET_REJ_QTY(A.SLOC_ID, A.CLD_ID, A.ORG_ID, D.ITM_ID,A.DOC_ID),0) >0         
    AND (A.CLD_ID, A.SLOC_ID, A.ORG_ID, A.DOC_ID,D.Pick_id, D.ITM_ID) NOT IN         
    (SELECT B.CLD_ID,         
      B.SLOC_ID,         
      B.ORG_ID,         
      B.SHIPMNT_ID,         
      B.PICK_ID,         
      B.ITM_ID         
    FROM SLS$RMA$ITM B         
    WHERE A.CLD_ID=B.CLD_ID         
    AND A.SLOC_ID =B.SLOC_ID         
    AND A.ORG_ID  =B.ORG_ID         
    AND A.WH_ID   =B.WH_ID         
    AND A.DOC_ID  =B.SHIPMNT_ID         
    )         
  AND (A.CLD_ID, A.SLOC_ID, A.ORG_ID, A.DOC_ID) IN         
    (SELECT C.CLD_ID,         
      C.SLOC_ID,         
      C.ORG_ID,         
      C.SHIP_ID         
    FROM SLS$INV$DTL C         
    WHERE      
    A.EO_ID = nvl(:BindEo_Id, A.EO_ID)  
    AND A.CLD_ID = nvl(:BindCld_id, A.CLD_ID)   
    AND A.SLOC_ID = nvl(:BindSloc_id, A.SLOC_ID)       
    AND A.ORG_ID = nvl(:BindOrg_id, A.ORG_ID)     
    AND A.HO_ORG_ID = nvl(:BindHoOrg_id, A.HO_ORG_ID) 
    AND A.CLD_ID=C.CLD_ID         
    AND A.SLOC_ID =C.SLOC_ID         
    AND A.ORG_ID  =C.ORG_ID         
    AND A.DOC_ID  =C.SHIP_ID         
    AND EXISTS         
      (SELECT *         
      FROM SLS$INV F         
      WHERE F.DOC_ID  = C.DOC_ID         
      AND F.CLD_ID    = C.CLD_ID         
      AND F.HO_ORG_ID = C.HO_ORG_ID         
      AND F.ORG_ID    = C.ORG_ID         
      AND F.SLOC_ID   = C.SLOC_ID         
      AND F.INV_STAT  = 218         
      )         
    ) ORDER BY A.SHIPMNT_ID]]></SQLQuery>
