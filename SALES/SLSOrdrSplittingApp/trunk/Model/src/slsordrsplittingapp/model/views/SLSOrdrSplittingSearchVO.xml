  A.DISP_DOC_ID DISP_DOC_ID, A.CLD_ID CLD_ID, A.SO_DT SO_DT,   
  A.EO_ID EO_ID, A.ORDER_STAT ORDER_STAT, A.EO_NM EO_NM,   
  A.DOC_DT DOC_DT, A.ORDER_TYPE ORDER_TYPE, A.ATT_NM ATT_NM,   
  A.CURR_ID CURR_ID, A.USR_ID_CREATE USR_ID_CREATE, A.USR_NAME USR_NAME,   
  A.USR_ID_CREATE_DT USR_ID_CREATE_DT, A.TOT_ORD_QTY TOT_ORD_QTY, A.TOT_ORD_AMT TOT_ORD_AMT,   
