  A.ORDER_STAT_DESC ORDER_STAT_DESC, A.HO_ORG_ID HO_ORG_ID, A.SO_MODE SO_MODE,   
  A.SO_MODE_DESC SO_MODE_DESC, A.SHIP_ADDS, A.EXT_DOC_NO,  A.SO_SPLITTED   
FROM SEARCH$SO_VW A   
WHERE A.ORDER_TYPE in (318,308)
AND A.CLD_ID  = :CldIdBind    
AND A.SLOC_ID   = :SlocIdBind   
AND A.ORG_ID    = :OrgIdBind   
AND A.HO_ORG_ID = :HoOrgIdBind   
AND DISP_DOC_ID LIKE '%'   
  ||NVL(UPPER(:DispIdBind),DISP_DOC_ID)   
  ||'%'   
AND NVL(EO_NM,1) LIKE '%'   
  ||NVL(UPPER(:EoNmBind),NVL(EO_NM,1))   
  ||'%'   
ORDER BY SO_DT DESC]]></SQLQuery>
