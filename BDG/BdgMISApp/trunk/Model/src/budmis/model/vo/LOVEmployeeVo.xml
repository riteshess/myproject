<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVEmployeeVo"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="budmis.model.vo.LOVEmployeeVo_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="HoOrgBind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT EO_ID, EO_NM    
  FROM APP.APP$EO    
WHERE EO_TYPE = '80'    
   AND EO_SLOC_ID = :SlocIdBind    
   AND EO_CLD_ID = :CldIdBind  
   AND EO_HO_ORG_ID = :HoOrgBind]]></SQLQuery>
  <ViewAttribute
    Name="EoId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="BIGINT"
    AliasName="EO_ID"
    Expression="EO_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="EoNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EO_NM"
    Expression="EO_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="budmis.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
