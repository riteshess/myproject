<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="BdgMtlBudgetAmtCompVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
    <DesignTime>
        <Attr Name="_isExpertMode" Value="true"/>
    </DesignTime>
    <Properties>
        <SchemaBasedProperties>
            <LABEL
              ResId="bdgfinancebudgetapp.model.views.BdgMtlBudgetAmtCompVO_LABEL"/>
        </SchemaBasedProperties>
    </Properties>
    <Variable
      Name="BindStDate"
      Kind="where"
      Type="oracle.jbo.domain.Timestamp"/>
    <Variable
      Name="BindCldId"
      Kind="where"
      Type="java.lang.String"/>
    <Variable
      Name="BindSlocId"
      Kind="where"
      Type="java.lang.Integer"/>
    <Variable
      Name="BindOrgId"
      Kind="where"
      Type="java.lang.String"/>
    <Variable
      Name="BindHoOrgId"
      Kind="where"
      Type="java.lang.String"/>
    <SQLQuery><![CDATA[SELECT A.CLD_ID CLD_ID,    
A.SLOC_ID SLOC_ID,    
A.ORG_ID ORG_ID,    
A.HO_ORG_ID HO_ORG_ID,    
B.YR YR,    
NVL(A.BUDG_AMT,0) BUDG_AMT    
FROM    
(SELECT A.CLD_ID, A.SLOC_ID, A.ORG_ID, A.HO_ORG_ID, TO_CHAR(A.PRD_START_DT,'YYYY') YR, A.BUDG_AMT, A.BUDGET_STAT,A.BUDGET_ID    
FROM BDG$MTL$BUDGET A    
WHERE A.BUDGET_STAT=3     
AND TO_CHAR(A.PRD_START_DT,'YYYY')<=TO_CHAR((:BindStDate), 'YYYY')     
AND A.CLD_ID = :BindCldId    
AND A.SLOC_ID = :BindSlocId    
AND A.ORG_ID = :BindOrgId    
AND A.HO_ORG_ID = :BindHoOrgId    
)A,    
(SELECT (TO_CHAR((:BindStDate), 'YYYY')-LEVEL)+1 YR    
FROM DUAL    
CONNECT BY LEVEL<=5)B    
WHERE A.YR(+)=B.YR    
ORDER BY B.YR DESC]]></SQLQuery>
    <ViewAttribute
      Name="CldId"
      IsUpdateable="false"
      IsPersistent="false"
      PrecisionRule="true"
      Precision="255"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="CLD_ID"
      Expression="CLD_ID"
      SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="4"/>
        </DesignTime>
    </ViewAttribute>
    <ViewAttribute
      Name="SlocId"
      IsUpdateable="false"
      IsPersistent="false"
      PrecisionRule="true"
      Precision="2"
      Scale="0"
      Type="java.lang.Integer"
      ColumnType="NUMBER"
      AliasName="SLOC_ID"
      Expression="SLOC_ID"
      SQLType="NUMERIC"/>
    <ViewAttribute
      Name="OrgId"
      IsUpdateable="false"
      IsPersistent="false"
      PrecisionRule="true"
      Precision="2"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="ORG_ID"
      Expression="ORG_ID"
      SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="2"/>
        </DesignTime>
    </ViewAttribute>
    <ViewAttribute
      Name="HoOrgId"
      IsUpdateable="false"
      IsPersistent="false"
      PrecisionRule="true"
      Precision="2"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="HO_ORG_ID"
      Expression="HO_ORG_ID"
      SQLType="VARCHAR">
        <DesignTime>
            <Attr Name="_DisplaySize" Value="2"/>
        </DesignTime>
    </ViewAttribute>
    <ViewAttribute
      Name="Yr"
      IsUpdateable="false"
      IsPersistent="false"
      PrecisionRule="true"
      Type="java.lang.Integer"
      ColumnType="NUMBER"
      AliasName="YR"
      Expression="YR"
      SQLType="NUMERIC"/>
    <ViewAttribute
      Name="BudgAmt"
      IsUpdateable="false"
      IsPersistent="false"
      PrecisionRule="true"
      Type="oracle.jbo.domain.Number"
      ColumnType="NUMBER"
      AliasName="BUDG_AMT"
      Expression="BUDG_AMT"
      SQLType="NUMERIC"/>
    <AttrArray Name="KeyAttributes">
        <Item Value="CldId"/>
        <Item Value="SlocId"/>
        <Item Value="OrgId"/>
        <Item Value="HoOrgId"/>
        <Item Value="Yr"/>
    </AttrArray>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="ebizresources.resource.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
