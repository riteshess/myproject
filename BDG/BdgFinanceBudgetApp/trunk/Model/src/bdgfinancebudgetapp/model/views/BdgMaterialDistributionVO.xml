    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Timestamp"
    ColumnType="DATE"
    AliasName="PRD_END_DT"
    Expression="PRD_END_DT"
    SQLType="DATE"/>
  <ViewAttribute
