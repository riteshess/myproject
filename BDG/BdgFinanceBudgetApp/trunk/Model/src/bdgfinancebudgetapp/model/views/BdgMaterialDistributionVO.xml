<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="BdgMaterialDistributionVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="bdgfinancebudgetapp.model.views.BdgMaterialDistributionVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindEoId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindPrdStDt"
    Kind="where"
    Type="oracle.jbo.domain.Timestamp"/>
  <Variable
    Name="BindPrdEndDt"
    Kind="where"
    Type="oracle.jbo.domain.Timestamp"/>
  <SQLQuery><![CDATA[SELECT A.CLD_ID,  
A.SLOC_ID,  
A.ORG_ID,  
A.HO_ORG_ID,  
A.PRD_START_DT,  
A.PRD_END_DT,  
A.DOC_ID,  
A.BUDGET_ID,  
A.EO_ID,  
B.PRD_DTL_START_DT,  
B.PRD_DTL_END_DT,  
SUM(B.ITM_AMT_BS) ITM_AMT_BS,  
CASE C.DISTB_PRD_BASIS WHEN 5 THEN TO_CHAR(B.PRD_DTL_START_DT,'MON') --MONTHLY  
                           WHEN 3 THEN TO_CHAR(B.PRD_DTL_START_DT,'MON' )||'-'||TO_CHAR(B.PRD_DTL_END_DT,'MON')--QUATERLY   
                           WHEN 4 THEN TO_CHAR(B.PRD_DTL_START_DT,'MON' )||'-'||TO_CHAR(B.PRD_DTL_END_DT,'MON')--HALF YEAR  
                           WHEN 1 THEN TO_CHAR(B.PRD_DTL_START_DT,'DD-MON') --DAILY  
                           ELSE TO_CHAR(B.PRD_DTL_START_DT,'MON') END PERD  
FROM BDG$MTL$BUDGET A, BDG$MTL$BUDGET$DTL C, BDG$MTL$BUDGET$PROD$PRD$DTL B  
WHERE A.CLD_ID = C.CLD_ID  
AND A.SLOC_ID = C.SLOC_ID  
AND A.HO_ORG_ID = C.HO_ORG_ID  
AND A.ORG_ID = C.ORG_ID  
AND A.DOC_ID = C.DOC_ID  
  
AND A.CLD_ID = B.CLD_ID  
AND A.SLOC_ID = B.SLOC_ID  
AND A.ORG_ID = B.ORG_ID  
AND A.HO_ORG_ID = B.HO_ORG_ID  
AND A.DOC_ID = B.DOC_ID  
  
AND A.BUDGET_STAT = 3  
  
AND A.CLD_ID = :BindCldId  
AND A.SLOC_ID = :BindSlocId  
AND A.HO_ORG_ID = :BindHoOrgId  
AND A.ORG_ID = :BindOrgId  
AND A.EO_ID = :BindEoId  
AND A.PRD_START_DT <= :BindPrdStDt  
AND A.PRD_END_DT >= :BindPrdEndDt  
  
GROUP BY A.CLD_ID,  
A.SLOC_ID,  
A.ORG_ID,  
A.HO_ORG_ID,  
A.PRD_START_DT,  
A.PRD_END_DT,  
A.DOC_ID,  
A.BUDGET_ID,  
A.EO_ID,  
B.PRD_DTL_START_DT,  
B.PRD_DTL_END_DT,  
C.DISTB_PRD_BASIS  
  
ORDER BY B.PRD_DTL_START_DT]]></SQLQuery>
