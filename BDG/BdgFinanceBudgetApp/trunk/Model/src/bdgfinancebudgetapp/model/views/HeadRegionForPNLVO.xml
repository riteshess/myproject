  <Variable
    Name="BindEoId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindCustEoId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT DISTINCT A.CLD_ID CLD_ID,              
A.SLOC_ID SLOC_ID,              
A.ORG_ID ORG_ID,              
A.HO_ORG_ID HO_ORG_ID,              
A.DOC_ID DOC_ID,              
A.REGION REGION,             
B.PARAM_DESC REGION_NAME,    
:BindEoId EO_ID_FOR_CHILD,  
:BindCustEoId CUST_EO_ID_FOR_CHILD 
FROM BDG$FIN$BUDGET$DTL A, HCM.HCM$SETUP B             
WHERE A.CLD_ID = B.CLD_ID             
AND A.SLOC_ID = B.SLOC_ID             
AND A.HO_ORG_ID = B.HO_ORG_ID             
AND A.REGION = B.PARAM_ID           
           
AND A.REGION IN (SELECT regexp_substr(nvl(:BindRegionId, A.Region), '[^, ]+', 1, LEVEL) token                
        FROM dual                
        CONNECT BY LEVEL <= length(:BindRegionId) - length(REPLACE(:BindRegionId, ',', '')) + 1)]]></SQLQuery>
