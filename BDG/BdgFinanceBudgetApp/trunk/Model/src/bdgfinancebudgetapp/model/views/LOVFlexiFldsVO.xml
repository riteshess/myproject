<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVFlexiFldsVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="bdgfinancebudgetapp.model.views.LOVFlexiFldsVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindDocId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select        
a.cld_id,       
a.fld_id,       
a.fld_nm,       
b.doc_id,        
b.reqd,b.actv,       
a.data_type,       
a.data_len           
from           
app.APP$FLX$FLD a,app.APP$FLX$FLD$DOC b         
where         
a.cld_id=b.cld_id         
and a.fld_id=b.fld_id         
and b.doc_id=:BindDocId         
and a.cld_id=:BindCldId]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="FldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FLD_ID"
    Expression="FLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="FldNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FLD_NM"
    Expression="FLD_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="DOC_ID"
    Expression="DOC_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Reqd"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="REQD"
    Expression="REQD"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Actv"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ACTV"
    Expression="ACTV"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DataType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="DATA_TYPE"
    Expression="DATA_TYPE"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="DataLen"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="DATA_LEN"
    Expression="DATA_LEN"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="CldId"/>
    <Item Value="FldId"/>
    <Item Value="DocId"/>
  </AttrArray>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="ebizresources.resource.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
