<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovOrgIdVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="bdgprofilesetupapp.model.views.LovOrgIdVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="cldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="orgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT     
    ORG.ORG_CLD_ID CLD_ID,     
    nvl(ORG.ORG_ID_PARENT_L0,ORG_ID) HO_ORG_ID,    
    ORG.ORG_ID ORG_ID,     
    ORG.ORG_DESC ORG_DESC    
FROM     
    ORG   
     WHERE ORG_CLD_ID = :cldIdBind   
     AND ORG_ID = :orgIdBind]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="HoOrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="HO_ORG_ID"
    Expression="HO_ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrgDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_DESC"
    Expression="ORG_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="bdgprofilesetupapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
