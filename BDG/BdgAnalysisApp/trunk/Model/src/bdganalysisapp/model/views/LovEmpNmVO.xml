    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="hoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="orgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="slocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="empCodeBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT     
CLD_ID,    
SLOC_ID,    
ORG_ID,    
HO_ORG_ID,    
EMP_CODE,    
EMP_NM     
FROM BDG$EMP$MGR_VW   
WHERE CLD_ID = :cldIdBind   
AND EMP_CODE = nvl(:empCodeBind ,EMP_CODE) 
AND HO_ORG_ID = :hoOrgIdBind   
AND ORG_ID = :orgIdBind   
AND SLOC_ID = :slocIdBind]]></SQLQuery>
