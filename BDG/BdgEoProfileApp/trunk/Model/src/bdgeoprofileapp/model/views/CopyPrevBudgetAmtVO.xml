    <SQLQuery><![CDATA[SELECT CLD_ID,              
SLOC_ID,              
ORG_ID,              
HO_ORG_ID,              
BUDGET_SET,              
FY_ID,              
PRD_START_DT,              
PRD_END_DT,              
DOC_ID,              
BUDGET_ID,              
BUDG_AMT,              
BDG_GRP_ID,         
BUDGET_TYP,         
BUDGET_METHOD,         
REGION,         
CURR_ID_SP,         
CURR_CONV_RATE              
FROM BDG$EO$BUDGET              
WHERE CLD_ID = :BindCldId              
AND SLOC_ID = :BindSlocId              
AND HO_ORG_ID = :BindHoOrgId                       
AND ORG_ID = :BindOrgId              
AND EO_ID = :BindEoId              
AND BDG_FY_ID = :BindBdgFyId-1              
AND trunc(CASE :BindFyFlg     
          When 'P' THEN PRD_START_DT    
          Else FY_START_DT    
          END) = trunc(:BindStartDt)              
AND trunc(CASE :BindFyFlg    
          WHEN 'P' THEN PRD_END_DT    
          Else FY_END_DT    
          END) = trunc(:BindEndDt)              
--AND BDG_GRP_ID = :BindBdgGrpId           
AND BUDGET_SET = (SELECT MAX(BUDGET_SET) BUDGET_SET FROM BDG$EO$BUDGET          
WHERE CLD_ID = :BindCldId              
AND SLOC_ID = :BindSlocId              
AND HO_ORG_ID = :BindHoOrgId                       
AND ORG_ID = :BindOrgId              
AND EO_ID = :BindEoId              
AND BDG_FY_ID = :BindBdgFyId-1              
AND trunc(CASE :BindFyFlg     
          When 'P' THEN PRD_START_DT    
          Else FY_START_DT    
          END) = trunc(:BindStartDt)              
AND trunc(CASE :BindFyFlg    
          WHEN 'P' THEN PRD_END_DT    
          Else FY_END_DT    
          END) = trunc(:BindEndDt)             
--AND BDG_GRP_ID = :BindBdgGrpId           
)]]></SQLQuery>
