    <SQLQuery><![CDATA[SELECT distinct CLD_ID CLD_ID,                                      
SLOC_ID,                                      
ORG_ID,                                      
ORG_DESC,                                      
HO_ORG_ID,                                      
BUDGET_SET,                                      
PRD_START_DT,                                      
PRD_END_DT,                                      
DOC_ID,                                      
BUDGET_ID,                 
BUDGET_NM,                 
EO_ID,                                      
EO_NM,                                      
BUDGET_METHOD,                                      
REGION,                                      
BUDG_AMT,                                      
CURR_ID_SP,                                      
CURR_DESC_TXN,                                      
BDG_GRP_ID,                       
BUDGET_STAT,                       
BUDGET_STAT_VAL,               
PENDING_AT,             
MGR_CODE             
FROM SEARCH$SLS$BDG_VW                              
WHERE CLD_ID = :BindCldId                                      
AND SLOC_ID = :BindSlocId                              
AND HO_ORG_ID  = :BindHoOrgId                              
AND ORG_ID = :BindOrgId           
AND BUDGET_METHOD = :BindBudgMethod           
AND BUDGET_ID LIKE ('%'||:BindBdgId||'%')                                               
AND (PRD_START_DT) BETWEEN (nvl(:BindStDt, PRD_START_DT)) and (nvl(:BindEndDt, PRD_END_DT))                                   
AND (PRD_END_DT) BETWEEN (nvl(:BindStDt, PRD_START_DT)) and (nvl(:BindEndDt, PRD_END_DT))                                   
AND BDG_FY_ID = nvl(:BindBdgFyId, BDG_FY_ID)                           
AND EO_NM LIKE ('%'||:BindEoId||'%')                         
and ((:BindBudgMethod = 1 and EO_ID IN (SELECT A.EMP_CODE FROM HCM.ORG$HCM$EMP$PRF A                            
                                                    START WITH USR_ID = :BindUserId                         
                                                    CONNECT BY PRIOR A.EMP_CODE = A.MGR_CODE))        
                    or :BindBudgMethod <> 1)  
Order BY BUDGET_ID desc]]></SQLQuery>
