  <SQLQuery><![CDATA[SELECT distinct A.CLD_ID CLD_ID,  
A.SLOC_ID,  
A.HO_ORG_ID,  
A.ORG_ID,  
B.PARAM_ID,  
B.PARAM_DESC  
FROM HCM.ORG$HCM$EMP$PRF A, HCM.HCM$SETUP B  
WHERE A.CLD_ID = B.CLD_ID  
AND A.SLOC_ID = B.SLOC_ID  
AND A.HO_ORG_ID = B.HO_ORG_ID  
AND A.EMP_LOC_ID=B.PARAM_ID  
  
AND B.PARAM_TYPE = 3  
  
AND A.CLD_ID = :BindCldId    
AND A.SLOC_ID = :BindSlocId    
AND A.HO_ORG_ID = :BindHoOrgId   
AND B.PARAM_ID = NVL(:BindLocId, B.PARAM_ID)  
  
AND A.EMP_CODE IN (SELECT        
EMP_CODE       
FROM HCM.ORG$HCM$EMP$PRF       
WHERE CLD_ID = :BindCldId       
AND SLOC_ID = :BindSlocId       
AND HO_ORG_ID = :BindHoOrgId       
START WITH EMP_CODE = :BindEmpId       
CONNECT BY PRIOR EMP_CODE = MGR_CODE)]]></SQLQuery>
