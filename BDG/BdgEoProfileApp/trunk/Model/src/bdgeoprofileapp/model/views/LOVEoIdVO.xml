  <SQLQuery><![CDATA[SELECT A.EO_CLD_ID CLD_ID,             
A.EO_SLOC_ID SLOC_ID,             
A.EO_HO_ORG_ID HO_ORG_ID,             
A.EO_ORG_ID ORG_ID,             
A.EO_TYPE,             
A.EO_ID,         
B.EMP_NM EO_NM,         
B.MGR_CODE,         
A.CURR_ID_SP CUST_CURR_ID,         
B.USR_ID,   
B.EMP_LOC_ID   
FROM APP.APP$EO A, HCM.ORG$HCM$EMP$PRF B         
WHERE A.EO_CLD_ID = B.CLD_ID         
AND A.EO_SLOC_ID = B.SLOC_ID         
AND A.EO_HO_ORG_ID = B.HO_ORG_ID         
AND A.EO_ID = B.EMP_CODE         
         
AND A.EO_CLD_ID = :BindCldId             
AND A.EO_SLOC_ID = :BindSlocId             
AND A.EO_HO_ORG_ID = :BindHoOrgId             
AND A.EO_TYPE = 80       
AND A.EO_ID = NVL(:BindEoId, A.EO_ID)       
AND A.EO_ID IN (         
SELECT B.EMP_CODE FROM HCM.ORG$HCM$EMP$PRF B           
START WITH USR_ID = :BindUserId         
CONNECT BY PRIOR B.EMP_CODE = B.MGR_CODE )]]></SQLQuery>
