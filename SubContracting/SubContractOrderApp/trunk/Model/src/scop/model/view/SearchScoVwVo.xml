  <SQLQuery><![CDATA[SELECT           
A.CLD_ID,           
A.SLOC_ID,           
A.HO_ORG_ID,           
A.ORG_ID,           
A.DOC_ID,           
A.DOC_DT,           
A.SCO_ID,           
A.SCO_STAT,           
A.SCO_MODE,           
A.SCO_TYPE,           
A.SCO_BASIS,           
A.SCO_RMRK,           
A.USR_ID_CREATE,           
A.USR_ID_CREATE_DT,           
A.USR_CREATE_NM,           
A.SCO_STAT_NM,           
A.SCO_MODE_NM,           
A.SCO_TYPE_NM,           
A.SCO_BASIS_NM,  
A.AMD_NO,  
A.AMD_DT  
FROM MM.SEARCH$SCO_VW A          
WHERE                     
A.ORG_ID = NVL(:BindOrgId, A.ORG_ID) AND                     
A.HO_ORG_ID = NVL(:BindHoOrgId, A.HO_ORG_ID) AND                     
A.SLOC_ID = NVL(:BindSlocId, A.SLOC_ID) AND                     
A.CLD_ID = NVL(:BindCldId, A.CLD_ID) AND                    
TRUNC (A.DOC_DT) >= NVL (:BindFromDate, A.DOC_DT) AND                    
TRUNC (A.DOC_DT) <= NVL (:BindToDate, A.DOC_DT) AND                   
A.DOC_ID = NVL(:BindScoDocId, A.DOC_ID) AND       
A.SCO_STAT = NVL(:BindStatId, A.SCO_STAT) AND       
A.SCO_MODE = NVL(:BindModeId, A.SCO_MODE) AND       
A.SCO_TYPE = NVL(:BindTypeId, A.SCO_TYPE) AND           
A.SCO_BASIS = NVL(:BindBasisId, A.SCO_BASIS)      
ORDER BY A.SCO_ID DESC]]></SQLQuery>
