  <SQLQuery><![CDATA[/* Formatted on 22-05-2015 10:39:19 (QP5 v5.136.908.31019) */   
SELECT A.SLOC_ID SLOC_ID,   
  a.ITM_ID ITM_ID,   
  A.ITM_DESC ITM_DESC,   
  a.CLD_ID CLD_ID,   
  a.HO_ORG_ID HO_ORG_ID,   
  NVL (A.SERIALIZED_FLG, 'N') SERIALIZED_FLG,   
  A.UOM_BASIC UOM_BASIC,   
  A.PRICE_BASIC,   
  A.ACTV,   
  B.UOM_DESC   
FROM APP.APP$ITM$PRF A,   
  APP.APP$UOM$CONV$STD B   
WHERE A.UOM_BASIC   = B.UOM_ID(+)   
AND A.SLOC_ID       = B.SLOC_ID   
AND A.CLD_ID        = B.CLD_ID   
AND (CAPITAL_GD_FLG = 'N'   
OR SAMPL_ITM_FLG    ='N')   
ORDER BY ITM_DESC]]></SQLQuery>
