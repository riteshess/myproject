<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVDocIdSrcNmVo"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="scop.model.view.LOVDocIdSrcNmVo_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindDocIdSrc"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select doc_id, doc_dt, rc_id  from mnf.mnf$rc  
where  
doc_id = :BindDocIdSrc 
union all  
select doc_id, doc_dt, jc_id  from mnf.mnf$jc  
where   
doc_id = :BindDocIdSrc]]></SQLQuery>
