<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVSumIssueQtyVo"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="scissue.model.views.LOVSumIssueQtyVo_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindDocIdSrc"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindDlvSchdlNo"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select   
a.CLD_ID,  
a.SLOC_ID,  
a.ORG_ID,  
a.DOC_ID_SRC, 
a.ITM_ID,  
SUM(a.ISSU_QTY) ISSUE_QTY,  
a.DLV_SCHDL_NO  
from mm.mm$sc$issu$itm a  
where a.DOC_ID_SRC = nvl(:BindDocIdSrc, a.DOC_ID_SRC)  
and a.DLV_SCHDL_NO = nvl(:BindDlvSchdlNo, a.DLV_SCHDL_NO)  
and a.CLD_ID = :BindCldId  
and a.SLOC_ID = :BindSlocId  
and a.ORG_ID = :BindOrgId  
group by   
a.CLD_ID,  
a.SLOC_ID,  
a.ORG_ID,  
a.DOC_ID_SRC,  
a.ITM_ID,  
a.DLV_SCHDL_NO]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR"/>
  <ViewAttribute
    Name="SlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR"/>
  <ViewAttribute
    Name="DocIdSrc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOC_ID_SRC"
    Expression="DOC_ID_SRC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ItmId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_ID"
    Expression="ITM_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="IssueQty"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="ISSUE_QTY"
    Expression="ISSUE_QTY"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="DlvSchdlNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="DLV_SCHDL_NO"
    Expression="DLV_SCHDL_NO"
    SQLType="NUMERIC"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="scissue.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
