  <SQLQuery><![CDATA[SELECT                          
DISTINCT A.EO_ID EO_ID,                          
A.EO_NM,                        
(select supp_curr_id from app.app$eo$prf             
            where eo_id=a.eo_id            
              and   eo_sloc_id=:BindSlocId            
              and eo_ho_org_id=:BindHoOrgId            
              and eo_cld_id=:BindCldId            
              and rownum=1)  CURR_ID_SP ,              
sls.FN_GET_DFLT_BILL_ADDS_OF_EO_ID(:BindSlocId,:BindCldId,:BindHoOrgId,a.EO_ID) adds_id ,         
sls.FN_GET_COA_ID_FRM_EO_ID(:BindSlocId,:BindCldId,:BindHoOrgId,:BindOrgId,a.EO_ID ) coa_id         
from APP.APP$EO A                       
WHERE                          
A.EO_TYPE = (decode(:BindType,826,68,827,70,A.EO_TYPE))and                           
A.EO_SLOC_ID = NVL(:BindSlocId, A.EO_SLOC_ID) and                          
A.EO_ACTV = 'Y' and                                                
A.EO_HO_ORG_ID = NVL(:BindHoOrgId, A.EO_HO_ORG_ID) and                           
A.EO_CLD_ID = NVL(:BindCldId, A.EO_CLD_ID)]]></SQLQuery>
