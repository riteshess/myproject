  <SQLQuery><![CDATA[SELECT DOC_ID,DOC_DT,RCPT_NO DISP_NO FROM MM$SC$RCPT A            
WHERE              
cld_id=:BindCldId and             
sloc_id=:BindSlocId and              
org_id=:BindOrgId and              
827 =  :BindDocType  and            
DOC_ID= nvl(:BindDocId,DOC_ID) and              
eo_id_src=:BindEoId and         
curr_id_sp=:BindCurrId AND   
 A.PRJ_ID= DECODE(:BindPrjAlw,'Y',:BindPrjId,A.PRJ_ID) and 
 RCPT_STG =  case :BindMode when 'Y' then 383   
                            when 'N' then RCPT_STG   
                            else RCPT_STG  
                            END  
                             
UNION             
SELECT DOC_ID,DOC_DT,ISSU_NO DISP_NO FROM MM$SC$ISSU  a          
WHERE              
cld_id=:BindCldId and             
sloc_id=:BindSlocId and              
org_id=:BindOrgId and              
826 =  :BindDocType  and              
DOC_ID= nvl(:BindDocId,DOC_ID) and            
eo_id=:BindEoId and         
curr_id_sp=:BindCurrId AND   
A.PRJ_ID= DECODE(:BindPrjAlw,'Y',:BindPrjId,A.PRJ_ID) and 
ISSU_STAT =case :BindMode when 'Y' then 851   
                            when 'N' then ISSU_STAT   
                            ELSE ISSU_STAT  
                            end]]></SQLQuery>
