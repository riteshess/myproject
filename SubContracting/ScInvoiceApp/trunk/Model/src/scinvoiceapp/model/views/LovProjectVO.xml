<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovProjectVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="scinvoiceapp.model.views.LovProjectVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindPrjId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT a.PROJ_NM,    
   a.CNTRY_CODE,    
   a.PARENT_PROJ_ID,    
   a.PRJ_DOC_ID,    
   a.ORG_ID,    
   a.HO_ORG_ID,    
   a.SLOC_ID,    
   a.CLD_ID  
FROM APP.APP$PRJ$PRF_VW a   
WHERE  A.Cld_Id =:BindCldId    
AND A.Sloc_Id  =:BindSlocId    
AND A.Ho_Org_Id=:BindHoOrgId    
AND A.Org_Id   =:BindOrgId    
and a.PRJ_DOC_ID=NVL(:BindPrjId, A.PRJ_DOC_ID)]]></SQLQuery>
  <ViewAttribute
    Name="ProjNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PROJ_NM"
    Expression="PROJ_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="CntryCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Scale="0"
    Type="java.math.BigInteger"
    ColumnType="NUMBER"
    AliasName="CNTRY_CODE"
    Expression="CNTRY_CODE"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="ParentProjId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARENT_PROJ_ID"
    Expression="PARENT_PROJ_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="PrjDocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PRJ_DOC_ID"
    Expression="PRJ_DOC_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="HoOrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="HO_ORG_ID"
    Expression="HO_ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="PrjDocId"/>
    <Item Value="OrgId"/>
    <Item Value="HoOrgId"/>
    <Item Value="SlocId"/>
    <Item Value="CldId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="scinvoiceapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
