<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovCoaVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="subcontractreceiptapp.model.views.LovCoaVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindCoaId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT COA_ID,COA_NM,ACC_TYPE FROM FIN.FIN$ACC$NA A, FIN.FIN$COA B       
WHERE ACC_SLOC_ID=B.COA_SLOC_ID       
AND A.HO_ORG_ID=B.COA_HO_ORG_ID       
AND A.ACC_CLD_ID=B.COA_CLD_ID       
AND A.ACC_ID=B.COA_ACC_ID       
AND A.ACC_TYPE IN (68,69,5022)        
AND B.COA_SLOC_ID=:BindSlocId       
AND B.COA_CLD_ID=:BindCldId       
AND B.COA_HO_ORG_ID=:BindHoOrgId     
AND B.COA_ID = nvl(:BindCoaId,B.COA_ID)]]></SQLQuery>
  <ViewAttribute
    Name="CoaId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="BIGINT"
    AliasName="COA_ID"
    Expression="COA_ID"
    SQLType="INTEGER">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CoaNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="COA_NM"
    Expression="COA_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
