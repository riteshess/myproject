  <SQLQuery><![CDATA[SELECT   
b.SLOC_ID SLOC_ID,    
b.ORG_ID ORG_ID,    
b.WH_ID WH_ID,    
b.WH_NM WH_NM,    
b.ADDS_ID ADDS_ID,    
b.ACTV ACTV,    
b.CLD_ID CLD_ID  
FROM APP.WH$SEC$USR A,APP.APP$WH$ORG B  
WHERE  
A.CLD_ID=B.CLD_ID AND  
A.SLOC_ID=B.SLOC_ID AND  
A.ORG_ID=B.ORG_ID AND  
MM.MM_GET_HO_ORG(A.CLD_ID,A.SLOC_ID,A.ORG_ID)=B.HO_ORG_ID AND  
A.WH_ID=B.WH_Id   
and b.sloc_Id=:BindSlocId    
and b.cld_id=:BindCldId    
and b.org_id=:BindOrgId  
and A.USR_ID=nvl(:BindUserId,A.USR_ID) 
and (case when b.ACTV is null then 'Y' else b.ACTV end)='Y']]></SQLQuery>
