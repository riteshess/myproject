  <Variable
    Name="bindDocId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select                
a.cld_id,               
a.fld_id,               
a.fld_nm,                 
b.doc_id,                
b.reqd,b.actv,                
a.data_type,               
a.data_len,  
a.hs_LOV  
from                   
app.APP$FLX$FLD a,app.APP$FLX$FLD$DOC b                 
where                 
a.cld_id=b.cld_id                
and a.fld_id=b.fld_id                
and b.doc_id=:bindDocId              
and a.cld_id=:bindCldId]]></SQLQuery>
