a.itm_desc,       
a.ho_org_id,       
b.org_id,       
b.wh_id,      
a.uom_basic,   
b.avl_stk   
from APP.APP$ITM$PRF a ,mm.mm$stk$summ$itm b, app.app$itm$org c        
where a.cld_id=a.cld_id       
and a.sloc_id=b.sloc_id       
and a.itm_id=b.itm_id       
and a.itm_id=c.itm_id       
and a.ho_org_id=c.ho_org_id       
and b.org_id=c.org_id       
--and b.Tot_Stk>0       
and a.SERIALIZED_FLG='N' 
and a.cld_id=:bindCldId   
and a.sloc_id=:bindSlocId   
and b.org_id=:bindOrgId   
and b.wh_id=nvl(:bindWhId,wh_id)]]></SQLQuery>
