  <SQLQuery><![CDATA[select a.cld_id,a.sloc_id,a.org_id,                 
a.doc_id,a.SWAP_DT,a.fy_id,a.SWAP_ID,a.USR_ID_CREATE ,              
a.doc_stat,(select ATT_NM from app.App$Ds$Att where ATT_TYPE_ID=55 and att_id=a.doc_stat) as doc_nm from mm.MM$STK$SWAP a                
 where                    
        ( (UPPER(a.SWAP_ID) LIKE UPPER('%' || nvl(:binddocno,a.SWAP_ID) || '%')))                    
      And Trunc(a.SWAP_DT) Between Nvl(:binfFrmdt,Trunc(a.SWAP_DT)) And Nvl(:bindTodt,Trunc(a.SWAP_DT))                   
and a.doc_stat=nvl(:bindDocStat,a.doc_stat)         
and a.sloc_id=:bindSlocId                    
and a.cld_id=:bindCldId                   
and a.org_id=:bindOrgId]]></SQLQuery>
