EO_ID_SRC,            
ORG_ID_SRC,            
INVOICE_NO_SRC,            
WH_ID_SRC,            
(select distinct(b.DOC_TYPE_SRC)            
                from MM$MTL$RCPT$SRC b             
                where             
                a.CLD_ID=b.CLD_ID and            
                a.SLOC_ID=b.SLOC_ID and            
                a.ORG_ID=b.ORG_ID and            
                a.WH_ID=b.WH_ID and            
                a.DOC_ID=b.DOC_ID            
                ) as DOC_TYPE             
from MM$MTL$RCPT a            
where             
a.rcpt_src_type in (337,338) and             
a.RCPT_STAT=383           
and a.cld_id=:bindCldId           
and a.sloc_id=:bindSlocId           
and a.org_id=:bindOrgid       
and a.org_id_src = nvl(:bindOrgIdSrc,a.org_id_src)       
and a.wh_id_src = nvl(:bindWhIdSrc,a.wh_id_src)      
and nvl(a.prj_id,'PROJ0000') = nvl(:bindPrjId, nvl(a.prj_id,'PROJ0000'))]]></SQLQuery>
