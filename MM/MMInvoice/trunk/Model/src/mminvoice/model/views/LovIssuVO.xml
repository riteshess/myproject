  <SQLQuery><![CDATA[select       
a.CLD_ID,       
a.SLOC_ID,       
a.ORG_ID,       
a.WH_ID,       
a.DOC_ID,       
a.DOC_DT,       
a.FY_ID,       
a.ISSU_NO,       
a.ISSU_DT,       
a.ISSU_TYPE,       
a.ORG_ID_SRC,       
a.WH_ID_SRC,       
a.ISSU_STAT       
from MM.MM$MTL$ISSU a        
where a.ISSU_STAT=368       
and a.cld_id = nvl(:bindCldId, a.cld_id)       
and a.sloc_id=nvl(:bindSlocId,a.sloc_id)       
and a.org_id= nvl(:bindOrgId,a.org_id)       
and nvl(a.prj_id,'PROJ0000') = nvl(:bindPrjId, nvl(a.prj_id,'PROJ0000'))      
and a.doc_id in (       
select distinct b.doc_id from MM.mm$mtl$issu$src b,MM.mm$trf$ord c       
where b.DOC_ID_SRC=c.DOC_ID        
and b.cld_id=c.cld_id        
and b.sloc_id=c.sloc_id        
and b.org_id=c.org_id       
and c.INV_REQD='Y') 
AND A.DOC_ID NOT IN (SELECT DISTINCT I.DOC_ID_RCPT FROM MM$INVC$SRC I  
                                                   WHERE A.CLD_ID = I.CLD_ID 
                                                   AND A.SLOC_ID = I.SLOC_ID 
                                                   AND A.ORG_ID = I.ORG_ID)]]></SQLQuery>
