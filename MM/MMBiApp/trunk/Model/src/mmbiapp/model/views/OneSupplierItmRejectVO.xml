       WHERE A.CLD_ID = :CldIdBind
      AND A.SLOC_ID =:SlocIdBind
      AND A.HO_ORG_ID = :HoOrgIdBind
      GROUP BY ITM_DESC , ITM_ID ,EO_ID_SRC,eo_NM
         ORDER BY REJECTED_QUANTITY , RECEIPT_QUANTITY DESC)]]></SQLQuery>
