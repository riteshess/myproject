<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TopNSupplersRatingVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mmbiapp.model.views.TopNSupplersRatingVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="CountBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT 
    MM$EO$PERF$SCORE_VW.SCORE SCORE, 
    MM$EO$PERF$SCORE_VW.EO_NM SUPPLIER
FROM 
    MM.MM$EO$PERF$SCORE_VW
 where CLD_ID =:CldIdBind
    and SLOC_ID = :SlocIdBind
    and HO_ORG = :HoOrgIdBind
    and  ROWNUM<= :CountBind]]></SQLQuery>
  <ViewAttribute
    Name="Score"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="SCORE"
    Expression="SCORE"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Supplier"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SUPPLIER"
    Expression="SUPPLIER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mmbiapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
