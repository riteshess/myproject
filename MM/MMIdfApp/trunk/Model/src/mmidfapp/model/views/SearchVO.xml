  <SQLQuery><![CDATA[select a.cld_id,a.sloc_id,a.org_id,               
a.doc_id,a.Doc_dt,a.fy_id,a.IDF_APPL_NO,a.USR_ID_CREATE ,            
a.doc_stat,(select ATT_NM from APP.APP$DS$ATT where ATT_TYPE_ID=131 and att_id=a.doc_stat) as doc_nm from mm.mm$ipo$idf a              
 where                  
        ( (UPPER(a.IDF_APPL_NO) LIKE UPPER('%' || nvl(:binddocno,a.IDF_APPL_NO) || '%')))                  
      And Trunc(a.Doc_Dt) Between Nvl(:binfFrmdt,Trunc(a.Doc_Dt)) And Nvl(:bindTodt,Trunc(a.Doc_Dt))                 
and a.doc_stat=nvl(:bindDocStat,a.doc_stat)       
and a.sloc_id=:bindSlocId                  
and a.cld_id=:bindCldId                 
and a.org_id=:bindOrgId]]></SQLQuery>
