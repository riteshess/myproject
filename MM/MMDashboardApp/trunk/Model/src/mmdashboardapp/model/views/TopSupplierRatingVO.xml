<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TopSupplierRatingVO"
  Version="12.1.3.10.8"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mmdashboardapp.model.views.TopSupplierRatingVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="CountBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT   
    MM$EO$PERF$SCORE_VW.SCORE SCORE,   
    MM$EO$PERF$SCORE_VW.EO_NM EO_NM  
FROM   
    MM$EO$PERF$SCORE_VW  
 where CLD_ID =:BindCldId  
    and SLOC_ID = :BindSlocId  
    and HO_ORG = :BindHoOrgId  
    and  ROWNUM<= :CountBind]]></SQLQuery>
  <ViewAttribute
    Name="Score"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="SCORE"
    Expression="SCORE"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="EoNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="EO_NM"
    Expression="EO_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="mmdashboardapp.view.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
