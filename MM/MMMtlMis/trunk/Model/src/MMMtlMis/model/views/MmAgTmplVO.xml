    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindTempNm"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT     
    MM$AG$TMPL.CLD_ID CLD_ID,     
    MM$AG$TMPL.SLOC_ID SLOC_ID,     
    MM$AG$TMPL.ORG_ID ORG_ID,     
    MM$AG$TMPL.TMPL_ID TMPL_ID,     
    MM$AG$TMPL.TMPL_NM TMPL_NM,     
    MM$AG$TMPL.BKT_RANGE_TYPE BKT_RANGE_TYPE     
FROM     
    MM$AG$TMPL 
    where 
    CLD_ID=:BindCldId 
    and SLOC_ID=:BindSlocId 
    and ORG_ID=:BindOrgId 
    and Upper(TMPL_NM) Like '%'||Upper(:BindTempNm)||'%']]></SQLQuery>
