<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SearchVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mminsuranceapp.model.views.SearchVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="binddocno"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="binfFrmdt"
    Kind="where"
    Type="java.sql.Timestamp"/>
  <Variable
    Name="bindTodt"
    Kind="where"
    Type="java.sql.Timestamp"/>
  <Variable
    Name="bindDocStat"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindPoId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindfileno"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select a.cld_id,a.sloc_id,a.org_id,                    
a.doc_id,a.Doc_dt,a.fy_id,a.INS_NO,a.USR_ID_CREATE ,                 
a.doc_stat,a.FILE_NO,(select ATT_NM from APP.APP$DS$ATT where ATT_TYPE_ID=149 and att_id=a.doc_stat) as doc_nm,    
(select AUTH_PO_NO from MM.MM$DRFT$PO b where b.DOC_ID=a.DOC_ID_PO) as po_no,DLV_SCHDL_NO from mm.mm$ipo$ins a                   
 where                       
        ( (UPPER(a.INS_NO) LIKE UPPER('%' || nvl(:binddocno,a.INS_NO) || '%')))                       
      And Trunc(a.Doc_Dt) Between Nvl(:binfFrmdt,Trunc(a.Doc_Dt)) And Nvl(:bindTodt,Trunc(a.Doc_Dt))                      
and a.doc_stat=nvl(:bindDocStat,a.doc_stat)            
and a.sloc_id=:bindSlocId                       
and a.cld_id=:bindCldId                      
and a.org_id=:bindOrgId 
and (select AUTH_PO_NO from MM.MM$DRFT$PO b where b.DOC_ID=a.DOC_ID_PO)=nvl(:bindPoId,(select AUTH_PO_NO from MM.MM$DRFT$PO b where b.DOC_ID=a.DOC_ID_PO))   
and a.FILE_NO=nvl(:bindfileno,a.FILE_NO)]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"
    Type="java.lang.String"
