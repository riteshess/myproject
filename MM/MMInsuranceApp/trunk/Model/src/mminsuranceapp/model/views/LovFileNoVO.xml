    <SQLQuery><![CDATA[SELECT DISTINCT A.Cld_Id,              
  A.Sloc_Id,              
  A.Org_Id,              
  A.Doc_Id,              
  A.Doc_Dt,              
  A.Po_Mode,              
  A.Po_Type,              
  A.Eo_Id,              
  A.Curr_Id_Sp,              
  A.Curr_Conv_Fctr,              
  A.Po_Status,              
  A.Auth_Po_No,      
  concat(A.FILE_NO ||' ',B.DLV_SCHDL_NO) TransFileDlvryNo,     
  B.DLV_SCHDL_NO,        
  B.DLV_MODE,        
  A.FILE_NO              
FROM mm.MM$DRFT$PO A,              
  mm.MM$DRFT$PO$DLV$SCHDL B              
WHERE A.Po_Type                     ='741'              
AND A.Po_Mode                       ='231'              
AND a.cld_id                        = b.cld_id              
AND a.sloc_id                       = b.sloc_id              
AND a.org_id                        = b.org_id              
AND a.doc_id                        = b.doc_id              
AND b.bal_qty                       > 0              
AND (A.Doc_id, b.DLV_SCHDL_NO) NOT IN              
  (SELECT c.Doc_Id_Po,              
    c.DLV_SCHDL_NO              
  FROM MM.MM$IPO$INS c              
  WHERE c.cld_id =A.cld_ID              
  AND c.Org_id   =A.Org_id              
  AND c.Sloc_id  =A.Sloc_Id              
  )         
    AND A.Cld_Id                        = :BindCldId             
AND A.Sloc_Id                       = :BindSlocId             
AND A.org_id                        =:BindOrgId]]></SQLQuery>
