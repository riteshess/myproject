<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ItemVO"
  Version="12.1.3.10.8"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mmappuom.model.views.ItemVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindItemName"
    Kind="viewcriteria"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="mmappuom.model.views.ItemVO.BindItemName_LABEL"/>
        <DISPLAYWIDTH
          Value="20"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[select 
b.sloc_id,
b.org_id,
b.cld_id,
b.ho_org_id,
b.itm_id ,
a.itm_desc,
a.uom_basic
from 
app$itm$prf a , APP$ITM$ORG b
where 
nvl(actv,'N') = 'Y'
and b.itm_id =a.itm_id
and b.cld_id = a.cld_id
and b.sloc_id =a.sloc_id
and b.ho_org_id = a.ho_org_id]]></SQLQuery>
  <ViewAttribute
    Name="SlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR">
