    <ViewCriteriaRow
      Name="ItemVOCriteria_row_0"
      UpperColumns="1">
      <ViewCriteriaItem
        Name="ItemVOCriteria_ItemVOCriteria_row_0_ItmDesc"
        ViewAttribute="ItmDesc"
        Operator="STARTSWITH"
        Conjunction="AND"
        Value=":BindItemName"
        IsBindVarValue="true"
        Required="Optional"/>
    </ViewCriteriaRow>
  </ViewCriteria>
  <AttrArray Name="KeyAttributes">
    <Item Value="ItmId"/>
  </AttrArray>
  <ViewLinkAccessor
    Name="AppUomConvCls"
    ViewLink="mmappuom.model.views.link.itemToUomclsVL"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
