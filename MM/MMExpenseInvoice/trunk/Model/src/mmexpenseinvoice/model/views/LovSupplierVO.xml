  <Variable
    Name="bindEoNm"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT b.eo_cld_id cld_id, B.EO_SLOC_ID sloc_id, b.eo_ho_org_id ho_org_id, e.ORG_ID org_id,             
       a.eo_type, a.eo_id eo_id, a.eo_nm eo_nm,             
       nvl(b.supp_curr_id, app.get_org_def_curr_bs1(A.EO_CLD_ID,A.EO_SLOC_ID, E.ORG_ID)) supp_curr_id,            
       nvl(MM.MM_GET_CURR_CONV_FCTR(A.EO_CLD_ID,A.EO_SLOC_ID, A.EO_HO_ORG_ID, E.ORG_ID,             
                                    nvl(b.supp_curr_id,        
                                    app.get_org_def_curr_bs1(A.EO_CLD_ID,A.EO_SLOC_ID, E.ORG_ID)))            
           ,0) conv_fctr,            
       (SELECT d.adds_id            
          FROM app.app$eo$adds d            
         WHERE d.eo_id = b.eo_id            
           AND d.sloc_id = b.eo_sloc_id            
           AND d.cld_id = b.eo_cld_id            
           AND d.ho_org_id = b.eo_ho_org_id            
           AND d.bill_adds_dflt = 'Y') dflt_bill_adds            
     FROM  app.app$eo a, app.app$eo$prf b, app.org$eo e            
    WHERE  A.EO_CLD_ID = B.EO_CLD_ID            
      AND A.EO_SLOC_ID = B.EO_SLOC_ID            
      and a.EO_HO_ORG_ID = b.EO_HO_ORG_ID            
      AND A.EO_TYPE = B.EO_TYPE            
      AND a.eo_id = b.eo_id            
       AND a.eo_type NOT IN (68, 73, 74, 78, 79, 80, 81, 82)             
      AND NVL (a.eo_actv, 'N') = 'Y'            
      and A.eo_cld_id=e.cld_id            
      AND a.eo_sloc_id = e.org_sloc_id            
      AND a.eo_type = e.org_eo_type            
      AND a.eo_id = e.org_eo_id            
      and a.eo_ho_org_id = e.ho_org_id     
      and  nvl(MM.MM_GET_CURR_CONV_FCTR(A.EO_CLD_ID,A.EO_SLOC_ID, A.EO_HO_ORG_ID, E.ORG_ID,             
                                    nvl(b.supp_curr_id,        
                                    app.get_org_def_curr_bs1(A.EO_CLD_ID,A.EO_SLOC_ID, E.ORG_ID)))            
           ,0) >0   
and a.eo_CLD_ID=:bindCldId                 
AND e.ORG_ID=:bindOrgId                 
AND a.eo_SLOC_ID=:bindSlocId                 
and a.EO_id=nvl(:bindEoId,a.Eo_Id)]]></SQLQuery>
