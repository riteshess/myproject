  <SQLQuery><![CDATA[SELECT                    
A.CLD_ID,                   
A.SLOC_ID,                    
A.ORG_ID,                    
A.DOC_ID,                    
A.DOC_ID_SRC,                    
A.DLV_SCHDL_NO,                    
A.OC_ID,                    
A.COA_ID,                    
A.OC_CAT,                    
sum(A.OC_AMT_BS)oc_amt_bs,                    
A.CURR_ID_SP_OC,                    
sum(A.OC_AMT_SP)oc_amt_sp,                    
A.EO_ID,                 
A.EXPNS_NM,             
A.txn_doc_no,             
A.eo_nm,          
A.expns_type,          
A.doc_type_src ,   
a.FILE_NO   
from MM$EXPNS$SEARCH$VW             A       
where A.cld_id=:bindCldId                   
and A.sloc_id=:bindSlocId                   
and A.org_id=:bindOrgId               
AND a.oc_id =nvl(:bindExpnsNm,a.oc_id)               
and  Trunc(A.Doc_Dt) Between Nvl(:bindFmDt,Trunc(A.Doc_Dt)) And Nvl(:bindToDt,Trunc(A.Doc_Dt))              
and A.eo_id=:bindEoId        
and a.CURR_ID_SP_OC =:bindEoCurrId 
and NOT EXISTS  (select 1 from MM$EXPNS$INVC$SRC B where B.doc_id_src = A.doc_id)       
group by CLD_ID, SLOC_ID, ORG_ID, DOC_ID, DOC_ID_SRC,          
DLV_SCHDL_NO, OC_ID, COA_ID, OC_CAT, CURR_ID_SP_OC,          
EO_ID, EXPNS_NM, txn_doc_no, eo_nm, expns_type,          
doc_type_src, FILE_NO]]></SQLQuery>
