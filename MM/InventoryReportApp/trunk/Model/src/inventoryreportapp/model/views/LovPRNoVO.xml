  <SQLQuery><![CDATA[SELECT A.DOC_ID, A.PUR_RET_NO, A.PUR_RET_DT , B.WH_NM    
FROM MM.MM$PUR$RETN A, APP.APP$WH$ORG B    
WHERE  A.CLD_ID = B.CLD_ID    
AND A.SLOC_ID = B.SLOC_ID    
AND A.ORG_ID = B.ORG_ID    
AND A.WH_ID = B.WH_ID    
AND A.CLD_ID= :BindCldId     
and A.SLOC_ID= :BindSlocId     
and A.ORG_ID= :BindOrgId     
and A.WH_ID= NVL(:BindWhId, A.WH_ID)     
and A.FY_ID= NVL(:BindFyId, A.FY_ID)  
AND A.PUR_RET_STAT = NVL(:BindPRStat, A.PUR_RET_STAT) 
ORDER BY A.PUR_RET_NO desc]]></SQLQuery>
