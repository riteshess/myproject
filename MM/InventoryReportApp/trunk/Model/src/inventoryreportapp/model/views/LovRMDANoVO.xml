  <SQLQuery><![CDATA[SELECT A.DOC_ID, A.RMDA_NO, B.WH_NM FROM MM$RMDA A, APP.APP$WH$ORG B       
where  A.CLD_ID = B.CLD_ID       
AND A.SLOC_ID = B.SLOC_ID       
AND A.ORG_ID = B.ORG_ID       
AND A.WH_ID = B.WH_ID       
AND A.CLD_ID= :BindCldId        
AND A.SLOC_ID= :BindSlocId        
AND A.ORG_ID= :BindOrgId        
AND A.WH_ID= NVL(:BindWhId,A.WH_ID)      
AND A.RMDA_TYPE = NVL(:BindRmdaType, A.RMDA_TYPE)  
AND A.RETN_FLG = NVL(:BindRmdaStat, A.RETN_FLG) 
ORDER BY A.RMDA_NO DESC]]></SQLQuery>
