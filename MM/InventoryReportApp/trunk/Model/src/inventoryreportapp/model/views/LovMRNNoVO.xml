  <SQLQuery><![CDATA[SELECT DOC_ID,MRN_NO , to_char(doc_dt,'dd-mm-yyyy')doc_dt 
FROM MM$MRN   
WHERE CLD_ID = :BindCldId   
AND SLOC_ID = :BindSlocId   
AND ORG_ID = :BindOrgId 
AND WH_ID_RET_TO = NVL(:BindWhIdRet, WH_ID_RET_TO) 
AND ORG_ID_RET_TO = NVL(:BindOrgIdRet,ORG_ID_RET_TO) 
AND MRN_STAT = NVL(:BindMrnStat, MRN_Stat) 
order by mrn_no desc]]></SQLQuery>
