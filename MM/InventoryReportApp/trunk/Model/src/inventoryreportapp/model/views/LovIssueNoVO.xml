  <SQLQuery><![CDATA[SELECT DISTINCT A.ISSU_NO, TO_CHAR(A.ISSU_DT,'DD-MM-YYYY')ISSU_DT,A.DOC_ID     
FROM MM.MM$MTL$ISSU A,MM$MTL$ISSU$SRC B     
WHERE A.CLD_ID =B.CLD_ID     
AND A.SLOC_ID =B.SLOC_ID     
AND A.ORG_ID =B.ORG_ID     
AND A.WH_ID =B.WH_ID     
AND A.DOC_ID =B.DOC_ID     
AND A.CLD_ID = :BindCldId     
AND A.SLOC_ID = :BindSlocId     
AND A.ORG_ID = :BindOrgId     
AND A.WH_ID = NVL(:BindWhId  ,A.WH_ID)   
AND A.FY_ID = nvl(:BindFyId,A.FY_ID)    
AND B.ISSU_DOC_TYPE = nvl(:BindDocType, B.ISSU_DOC_TYPE)  
AND A.ISSU_TYPE = NVL(:BindIssuType, A.ISSU_TYPE) 
ORDER BY A.ISSU_NO DESC]]></SQLQuery>
