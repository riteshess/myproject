  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="inventoryreportapp.model.views.LovLotIdVo_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindWhId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[select A.LOT_ID , B.WH_NM  
from MM.MM$STK$SUMM$LOT A,  APP.APP$WH$ORG B  
where A.CLD_ID = B.CLD_ID  
AND A.SLOC_ID = B.SLOC_ID  
AND A.ORG_ID = B.ORG_ID  
AND A.WH_ID = B.WH_ID  
AND A.CLD_ID = :BindCldId   
and A.SLOC_ID = :BindSlocId   
and A.ORG_ID = :BindOrgId   
and A.WH_ID = NVL(:BindWhId,A.WH_ID)  
ORDER BY A.LOT_ID]]></SQLQuery>
