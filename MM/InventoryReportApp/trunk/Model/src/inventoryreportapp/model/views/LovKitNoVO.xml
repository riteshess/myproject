  <SQLQuery><![CDATA[select A.DOC_ID, A.KIT_PROD_NO, TO_CHAR(A.DOC_DT,'DD-MM-YYYY')DOC_DT, B.WH_NM  
from MM$KIT$PROD A, APP.APP$WH$ORG B   
where  A.CLD_ID = B.CLD_ID   
AND A.SLOC_ID = B.SLOC_ID   
AND A.ORG_ID = B.ORG_ID   
AND A.WH_ID = B.WH_ID   
AND A.CLD_ID = :BindCldId    
and A.SLOC_ID= :BindSlocId    
and A.ORG_ID= :BindOrgId    
and A.WH_ID = NVL(:BindWhId, A.WH_ID)    
and A.FY_ID= NVL(:BindFyId,A.FY_ID) 
AND A.KIT_ACTN_TYPE = NVL(:BindKitType, A.KIT_ACTN_TYPE) 
order by A.KIT_PROD_NO DESC]]></SQLQuery>
