AND A.BIN_ID = C.BIN_ID  
AND A.CLD_ID = :BindCldId    
and A.SLOC_ID = :BindSlocId    
and A.ORG_ID = :BindOrgId    
and A.LOT_ID = NVL(:BindLotId,A.LOT_ID)    
and A.WH_ID = NVL(:BindWhId,A.WH_ID)]]></SQLQuery>
