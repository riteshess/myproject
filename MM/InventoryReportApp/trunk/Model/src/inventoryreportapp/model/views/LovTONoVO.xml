  <SQLQuery><![CDATA[select A.DOC_ID, A.TRF_NO,to_char( A.trf_dt,'dd-mm-yyyy')trf_dt,  B.WH_NM   
from mm.mm$trf$ord A,APP.APP$WH$ORG B     
where  A.CLD_ID = B.CLD_ID     
AND  A.SLOC_ID = B.SLOC_ID     
AND A.ORG_ID = B.ORG_ID     
AND A.WH_ID = B.WH_ID     
AND A.CLD_ID= :BindCldId      
AND A.SLOC_ID= :BindSlocId      
AND A.ORG_ID= :BindOrgId      
AND A.WH_ID= NVL(:BindWhId,A.WH_ID)     
and A.FY_ID= NVL(:BindFyId,A.FY_ID)     
and a.trf_src_type = nvl(:BindSrcType, a.trf_src_type)   
and a.trf_type = nvl(:BindTrfType, a.trf_type)   
and a.trf_stat = nvl(:BindTrfStat, a.trf_stat)   
ORDER BY A.TRF_NO desc]]></SQLQuery>
