  <SQLQuery><![CDATA[select A.DOC_ID,A.GP_NO, TO_CHAR(A.GP_DT,'DD-MM-YYYY')GP_DT, B.WH_NM from MM$GP A, APP.APP$WH$ORG B      
WHERE A.CLD_ID = B.CLD_ID      
AND A.SLOC_ID = B.SLOC_ID      
AND A.ORG_ID = B.ORG_ID      
AND A.WH_ID = B.WH_ID      
AND A.CLD_ID = :BindCldId       
and A.SLOC_ID= :BindSlocId       
and A.ORG_ID= :BindOrgId       
and A.WH_ID = NVL(:BindWhId,A.WH_ID)       
and A.FY_ID = NVL(:BindFyId,A.FY_ID)    
AND A.DOC_TYPE_SRC = NVL(:BindDocTypeSrc, A.DOC_TYPE_SRC)  
order by a.gp_no desc]]></SQLQuery>
