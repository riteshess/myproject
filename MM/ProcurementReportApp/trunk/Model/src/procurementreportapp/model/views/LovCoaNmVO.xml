  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="procurementreportapp.model.views.LovCoaNmVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
