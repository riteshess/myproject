  <SQLQuery><![CDATA[Select       
A.Cld_Id,      
A.Sloc_Id,      
A.Org_Id,      
A.Doc_Id,      
A.Doc_Dt,      
A.Doc_Type,      
A.DOC_TYPE_NM,      
A.Boe_No,      
A.FY_ID,      
A.Doc_Type_Src,      
A.DOC_TYPE_SRC_NM,      
A.Doc_Id_Src,      
A.AUTH_PO_NO,      
A.Dlv_Schdl_No,      
A.File_No,      
A.Eo_Id,      
A.EO_NM,      
A.Eo_Id_Shp,      
A.Invc_No_Src,      
A.Invc_Dt_Src,      
A.Boe_Stat,      
A.BOE_STAT_NM,      
A.Auth_Stat,      
A.Usr_Id_Create,      
A.Usr_Id_Create_Nm,      
A.Usr_Id_Create_Dt      
From       
Mm.Mm$bill$of$entry$srch$vw A       
WHERE      
A.Sloc_Id = Nvl(:BindSlocId,A.Sloc_Id)         
And A.Org_Id = Nvl( :BindOrgId,A.Org_Id)         
And A.Cld_Id=Nvl(:BindCldId,A.Cld_Id)         
And A.Eo_Id = Nvl(:BindEoId,A.Eo_Id)         
And Upper(A.Auth_Po_No) Like '%'||Upper(:BindAuthPoNo)||'%'         
And Upper(A.File_No) Like '%'||Upper(:BindFileNo)||'%'   
And Upper(A.Boe_No) Like '%'||Upper(:BindBeoNo)||'%'     
And Trunc(A.Doc_Dt) Between Nvl(:BindFromDate,Trunc(A.Doc_Dt)) And Nvl(:BindToDate,Trunc(A.Doc_Dt))       
And A.Doc_Type = Nvl(:BindDocType,A.Doc_Type)         
And Nvl(A.Boe_Stat,0) = Nvl(:BindBeoStat,Nvl (A.Boe_Stat,0))]]></SQLQuery>
