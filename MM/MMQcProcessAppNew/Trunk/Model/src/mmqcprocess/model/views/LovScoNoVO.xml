<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovScoNoVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mmqcprocess.model.views.LovScoNoVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="OrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="DocIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT DOC_ID,SCO_ID FROM MM$SCO a  
where  
A.Cld_Id = :CldIdBind and              
A.Sloc_Id = :SlocIdBind AND              
A.Org_Id = :OrgIdBind AND    
a.doc_id= nvl(:DocIdBind,a.doc_id)]]></SQLQuery>
  <ViewAttribute
    Name="DocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOC_ID"
    Expression="DOC_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ScoId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SCO_ID"
    Expression="SCO_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="mmqcprocess.view.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
