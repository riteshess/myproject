    MM$QUOT.QUOT_ID QUOT_ID,   
    MM$QUOT.SLOC_ID SLOC_ID,   
    MM$QUOT.ORG_ID ORG_ID,   
    MM$QUOT.DOC_ID DOC_ID,   
    MM$QUOT.DOC_DT DOC_DT,   
    MM$QUOT.REF_DOC_ID REF_DOC_ID,   
    MM$QUOT.QUOT_DT QUOT_DT,   
    MM$QUOT.EO_ID EO_ID,   
    MM$QUOT.CURR_ID_SP CURR_ID_SP,   
    MM$QUOT.CURR_CONV_FCTR CURR_CONV_FCTR,   
    MM$QUOT.BILL_ADDS_ID BILL_ADDS_ID,   
    MM$QUOT.DLV_ADDS_ID DLV_ADDS_ID,   
    MM$QUOT.DLV_MODE DLV_MODE,   
    MM$QUOT.DLV_DAYS DLV_DAYS,   
    MM$QUOT.PAY_MODE PAY_MODE,   
    MM$QUOT.VALID_FM_DT VALID_FM_DT,   
    MM$QUOT.VALID_TO_DT VALID_TO_DT,   
    MM$QUOT.VLDT_TLRNC_DAYS VLDT_TLRNC_DAYS,   
    MM$QUOT.TAX_RULE_FLG TAX_RULE_FLG,   
    MM$QUOT.DISC_TYPE DISC_TYPE,   
    MM$QUOT.DISC_VAL DISC_VAL,   
    MM$QUOT.QUOT_AMT_BS QUOT_AMT_BS,   
    MM$QUOT.QUOT_AMT_SP QUOT_AMT_SP,   
    MM$QUOT.USR_ID_CREATE USR_ID_CREATE,   
    MM$QUOT.USR_ID_CREATE_DT USR_ID_CREATE_DT,   
    MM$QUOT.USR_ID_MOD USR_ID_MOD,   
    MM$QUOT.USR_ID_MOD_DT USR_ID_MOD_DT,   
    MM$QUOT.ENTITY_ID ENTITY_ID,   
    MM$QUOT.WH_ID WH_ID,   
    MM$QUOT.TAX_AFTER_DISC_FLG TAX_AFTER_DISC_FLG,  
    MM$QUOT.CLD_ID CLD_ID,  
    MM$QUOT.QUOT_STATUS QUOT_STATUS,  
    MM$QUOT.QUOT_ID||RPAD(' ',20-LENGTH(MM$QUOT.QUOT_ID),' ')||TRUNC(MM$QUOT.DOC_DT)  QUOT_ID_DISP,  
    MM.MM_GET_PR_NO_FRM_QUOT(MM$QUOT.CLD_ID,MM$QUOT.SLOC_ID,MM$QUOT.ORG_ID,MM$QUOT.DOC_ID) PR_NO_DISP, 
     (SELECT EO_NM FROM APP.APP$EO 
       WHERE EO_SLOC_ID = MM$QUOT.SLOC_ID 
       AND EO_CLD_ID = MM$QUOT.CLD_ID 
       AND EO_HO_ORG_ID = MM.MM_GET_HO_ORG(MM$QUOT.CLD_ID,MM$QUOT.SLOC_ID,MM$QUOT.ORG_ID) 
       AND EO_ID =MM$QUOT.EO_ID 
       )EO_NM 
FROM   
    MM$QUOT  
    where (nvl(:p_po_dt,SYSDATE)) between (nvl(MM$QUOT.VALID_FM_DT,MM$QUOT.QUOT_DT)) and (nvl(MM$QUOT.VALID_TO_DT,  
    (SELECT OFY.FY_END FROM APP.ORG$FY OFY  
    WHERE   
        OFY.ORG_ID=MM$QUOT.ORG_ID  
    AND OFY.ORG_FY_REF_SLOC_ID=MM$QUOT.SLOC_ID  
    AND OFY.CLD_ID=MM$QUOT.CLD_ID  
    AND OFY.ORG_FY_ID=MM$QUOT.FY_ID)  
    ))  
and quot_status in (214,215,235)]]></SQLQuery>
