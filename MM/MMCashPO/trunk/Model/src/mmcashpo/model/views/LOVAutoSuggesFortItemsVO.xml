<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LOVAutoSuggesFortItemsVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mmcashpo.model.views.LOVAutoSuggesFortItemsVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CldIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="SlocIdBind"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="HoOrgIdBind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="OrgIdbind"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="ItmNmbind"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT       
    MM$ITM$VW.ITM_DESC ITM_DESC      
From       
    MM.MM$ITM$VW       
    where      
  ACTV ='Y'     
  and CASH_PUR_FLG='Y' 
  and cld_id = :CldIdBind     
  and  Sloc_id =:SlocIdBind     
  and ho_org_id =:HoOrgIdBind     
  And Org_Id =:OrgIdbind     
  And Upper(Itm_Desc) Like '%'||Upper(:ItmNmbind)||'%'     
  and ROWNUM <11]]></SQLQuery>
  <ViewAttribute
    Name="ItmDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_DESC"
    Expression="ITM_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="mmcashpo.view.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
