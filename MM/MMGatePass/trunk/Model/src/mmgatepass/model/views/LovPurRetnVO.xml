  <SQLQuery><![CDATA[SELECT        
    A.DOC_ID DOC_ID,        
    A.EO_ID EO_ID,        
    A.CLD_ID CLD_ID,        
    A.DOC_DT DOC_DT,        
    A.PUR_RET_STAT PUR_RET_STAT,        
    A.ORG_ID ORG_ID,        
    A.PUR_RET_NO PUR_RET_NO,        
    A.WH_ID WH_ID,        
    A.SLOC_ID SLOC_ID,        
    A.FY_ID FY_ID,        
    A.AUTH_STAT AUTH_STAT,        
    A.PUR_RET_DT PUR_RET_DT,       
    AEA.DFLT_BILL_ADDS DFLT_BILL_ADDS       
FROM        
    MM$PUR$RETN A, APP.APP$SUPP_VW AEA       
WHERE A.CLD_ID=AEA.CLD_ID        
AND A.SLOC_ID=AEA.SLOC_ID        
AND A.ORG_ID=AEA.ORG_ID        
AND A.EO_ID=AEA.EO_ID  
AND A.PUR_RET_STAT <> 429  
AND NOT EXISTS (SELECT 1 FROM MM$GP G WHERE G.DOC_ID_SRC = A.DOC_ID)  
AND A.CLD_ID = :BindCldId  
AND A.SLOC_ID = :BindSlocId  
AND A.ORG_ID = :BindOrgId  
AND A.WH_ID = :BindWhId  
ORDER BY A.PUR_RET_NO DESC]]></SQLQuery>
