  <SQLQuery><![CDATA[SELECT           
    a.SLOC_ID,           
    a.DOC_ID,           
    a.ORG_ID,           
    a.CLD_ID,           
    a.WH_ID,           
    a.ITM_ID,     
    (select b.serialized_flg     
    from app.app$itm$prf b     
    where     
    b.cld_id=a.cld_id    
    and b.sloc_id=a.sloc_id    
    and b.ho_org_id=MM.MM_GET_HO_ORG(a.cld_id,a.sloc_id,a.org_id)   
    and b.itm_id=a.itm_id) serialized_flg,    
    a.ITM_UOM,          
    a.ITM_UOM_BS,           
    a.UOM_CONV_FCTR,          
    sum(a.issu_qty) issu_qty,       
    sum(a.issu_qty_bs)issu_qty_bs       
FROM           
    MM$mtl$issu$ITM a          
group by           
(a.SLOC_ID,a.DOC_ID,a.ORG_ID,a.CLD_ID,a.WH_ID,a.ITM_ID,a.ITM_UOM,a.ITM_UOM_BS,a.UOM_CONV_FCTR)]]></SQLQuery>
