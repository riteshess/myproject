<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovNewForDocNameVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="flexifieldsetupapp.model.views.LovNewForDocNameVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="bindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[Select A.DOC_ID,B.GLBL_DOC_NM from APP$DOC A, APP$GLBL$DOC B 
WHERE 
B.GLBL_DOC_ID=A.DOC_ID 
AND 
A.DOC_FLX_FLD='Y' 
AND 
DOC_CLD_ID=:bindCldId 
AND 
DOC_SLOC_ID=:bindSlocId]]></SQLQuery>
  <ViewAttribute
    Name="DocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="DOC_ID"
    Expression="DOC_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="GlblDocNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="GLBL_DOC_NM"
    Expression="GLBL_DOC_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="DocId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="flexifieldsetupapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
