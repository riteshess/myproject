  <SQLQuery><![CDATA[SELECT      
A.Cld_Id,      
A.Sloc_Id,      
A.Org_Id,      
A.Doc_Id,      
A.Doc_Dt,      
A.Amd_No,      
A.Amd_Dt,      
A.Po_Id,      
A.Po_Dt,      
A.Auth_Po_No,      
A.Fy_Id,      
A.Po_Mode,      
A.Po_Mode_Nm,      
A.Po_Type,      
A.Po_Type_Nm,      
A.Po_Basis,      
A.PO_BASIS_NM,      
A.Eo_Id,      
A.Eo_Nm,      
A.Po_Amt_Bs,      
A.Po_Amt_Sp,      
A.Po_Status,      
A.Po_Status_Nm,      
A.Usr_Id_Create,      
A.Usr_Id_Create_Nm      
From       
Mm$po$dlvry$schl$srch$vw A      
Where      
A.Sloc_Id = Nvl(:BindSlocId,A.Sloc_Id)      
And A.Org_Id = Nvl( :BindOrgId,A.Org_Id)      
And A.Cld_Id=Nvl(:BindCldId,A.Cld_Id)      
And A.Eo_Id = Nvl(:BindEoId,A.Eo_Id)     
And Upper(A.Auth_Po_No) Like '%'||Upper(:BindAuthPoNo)||'%'      
And Trunc(A.Doc_Dt) Between Nvl(:BindFromDate,Trunc(A.Doc_Dt)) And Nvl(:BindToDate,Trunc(A.Doc_Dt))    
And A.Po_Type = Nvl(:BindPoType,A.Po_Type)      
AND A.PO_BASIS = NVL(:BindBasis,A.PO_BASIS)     
AND A.PO_STATUS = NVL(:BindPoStatus,A.PO_STATUS)]]></SQLQuery>
