  <SQLQuery><![CDATA[SELECT SLOC_ID, 
       ORG_ID,
       CLD_ID, 
       DOC_ID, 
       DOC_DT, 
       SO_ID, 
       TRUNC(so_dt)SO_DT, 
       SO_BASIS_DT_FRM, 
       SO_BASIS_DT_TO, 
       USR_ID_CREATE, 
       USR_ID_CREATE_DT, 
       USR_ID_MOD, 
       USR_ID_MOD_DT, 
       SO_BASIS,
       ENTITY_ID,
       SO_CLOSE
FROM MM$SO
WHERE CLD_ID = :bindCldId
AND ORG_ID = :bindOrgId
AND SLOC_ID =:bindSlocId
AND TRUNC(SO_DT) >= NVL(:bindFrmDt,TRUNC(SO_DT))
AND TRUNC(SO_DT) <= NVL(:bindToDt,TRUNC(SO_DT))]]></SQLQuery>
