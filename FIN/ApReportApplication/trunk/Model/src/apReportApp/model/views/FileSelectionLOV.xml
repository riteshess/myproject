<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="FileSelectionLOV"
  Version="12.1.3.10.8"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="apReportApp.model.views.FileSelectionLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[select 'PDF' as fileName  from dual 
union  
select 'RTF'   from dual 
union  
select 'XLS'  from dual 
--union  
--select 'XLSX'  from dual 
union  
select 'DOCX'  from dual 
union  
select 'HTML' from dual 
union  
select 'XML' from dual 
order by 1]]></SQLQuery>
  <ViewAttribute
    Name="Filename"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FILENAME"
    Expression="FILENAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="apReportApp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
