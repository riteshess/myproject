select 
A.GRPH_SESS_ID SESSION_ID,
A.GRPH_ORG_ID ORG_ID,
A.GRPH_SLOC_ID SLOC_ID,
A.GRPH_ID GRAPH_ID,
A.GRPH_TRND_ID TREND_ID,
A.GRPH_COL1_DATA VOUCHER_DATE,
TO_NUMBER(A.GRPH_COL2_DATA) INCOME_AMOUNT,
TO_NUMBER(A.GRPH_COL3_DATA) AVERAGE_INCOME,
A.GRPH_COL4_DATA,
A.GRPH_COL5_DATA,
A.GRPH_COL6_DATA,
A.GRPH_COL7_DATA,
A.GRPH_COL8_DATA,
A.GRPH_COL_DOC_ID,
to_date(A.GRPH_COL1_DATA,'dd-MM-yyyy') date_val
FROM app.APP$GRPH$COL$DATA A
WHERE A.GRPH_ID=8
AND A.GRPH_TRND_ID=NVL(:IncomeTrendID,APP.PKG_GRPH.FN_GET_DEF_USR_GRPH_TREND(:CldIdBindVar,:SlocIdVar,:OrgIdVar,:UserIdVar,8))
and A.GRPH_ORG_ID=:OrgIdVar
and A.GRPH_SLOC_ID=:SlocIdVar)
order by date_val]]></SQLQuery>
