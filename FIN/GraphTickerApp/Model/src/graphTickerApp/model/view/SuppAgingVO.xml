  <SQLQuery><![CDATA[SELECT
a.AP_SLOC_ID,
a.AP_APPL_INST_ID,
a.AP_ORG_ID,
a.AP_VOU_ID,
TXN.DOC_TXN_ID_DISP TVOU_NO,
a.AP_VOU_DT,
TRUNC((SYSDATE - a.AP_VOU_DT)) OS_DAYS,
a.AP_COA_ID,

a.AP_AMT_SP,
a.AP_NET_AMT_OT,
a.AP_AMT_TYP,
c.coa_nm
-- voucher no

FROM AP a,
app.APP$DOC$TXN txn,
org$coa b,
fin$coa c
where a.AP_TXN_TYP_ADV = 'N'
and a.AP_NET_AMT_OT > 0
and a.ap_type_id in (8)
and a.AP_ORG_ID = b.org_id
and a.AP_COA_ID = b.org_coa_id
and b.org_coa_id = c.coa_id
AND A.ap_SLOC_ID = TXN.DOC_SLOC_ID(+)
AND A.ap_ORG_ID = TXN.DOC_ORG_ID(+)
--AND APP.PKG_APP.HEX2DEC(SUBSTR(A.ap_vou_ID,12,4)) = TXN.DOC_ID(+)
AND A.AP_VOU_ID = TXN.DOC_TXN_ID(+)]]></SQLQuery>
