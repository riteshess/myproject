<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AppTdsRuleVO"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="tempVoucherList.model.views.AppTdsRuleVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlcId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT 
    A.TAX_RULE_ID TAX_RULE_ID, 
    A.TAX_RULE_DESC TAX_RULE_DESC, 
    A.TAX_RULE_SLOC_ID TAX_RULE_SLOC_ID, 
    A.TAX_RULE_INST TAX_RULE_INST, 
    A.TAX_RULE_STRT_DT TAX_RULE_STRT_DT, 
    A.TAX_TYPE_TDS TAX_TYPE_TDS 
FROM 
    APP.APP$TAX$RULE A  
WHERE NVL(TAX_RULE_ACTV,'N') = 'Y'
AND NVL(TAX_RULE_FINALISED,'N')  = 'Y'
AND NVL(TAX_TYPE_TDS,'!') = 'Y' 
And (Trunc(sysdate) - A.Tax_Rule_Strt_Dt) >= 0
And (Nvl(A.Tax_Rule_Validity_Dt,Trunc(Sysdate)) - Trunc(Sysdate)) >=0
AND A.TAX_HO_ORG_ID = :BindHoOrgId
AND A.TAX_CLD_ID = :BindCldId
And A.TAX_RULE_SLOC_ID =:BindSlcId
AND 0 = (SELECT COUNT(*)
                     FROM APP.APP$TAX$RULE$LINE B
                     WHERE B.TAX_CLD_ID = A.TAX_CLD_ID
                     AND B.TAX_RULE_SLOC_ID = A.TAX_RULE_SLOC_ID
                     AND B.TAX_HO_ORG_ID = A.TAX_HO_ORG_ID
                     AND B.TAX_RULE_ID = A.TAX_RULE_ID
                     AND B.TAX_RULE_COA_ID IS NULL)
and 2 <= (select count(*)
           from app.app$tax$rule$line b
           where b.tax_cld_id = a.tax_cld_id
           and b.tax_rule_sloc_id = a.tax_rule_sloc_id
           and b.tax_ho_org_id = a.tax_ho_org_id
           and b.tax_rule_id = a.tax_rule_id
           and b.tax_rule_coa_id is not null)]]></SQLQuery>
  <ViewAttribute
    Name="TaxRuleId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="TAX_RULE_ID"
    Expression="TAX_RULE_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="TaxRuleDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TAX_RULE_DESC"
    Expression="TAX_RULE_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="TaxRuleSlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="TAX_RULE_SLOC_ID"
    Expression="TAX_RULE_SLOC_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="TaxRuleInst"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="TAX_RULE_INST"
    Expression="TAX_RULE_INST"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="TaxRuleStrtDt"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Date"
    ColumnType="DATE"
    AliasName="TAX_RULE_STRT_DT"
    Expression="TAX_RULE_STRT_DT"
    SQLType="DATE"/>
  <ViewAttribute
    Name="TaxTypeTds"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TAX_TYPE_TDS"
    Expression="TAX_TYPE_TDS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="tempVoucherList.view.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
