FROM     app.APP$CURR_VW_APP a, app.org b      
where a.cld_id=b.org_cld_id      
and a.curr_sloc_id=b.org_create_ref_sloc_id      
and a.ho_org_id=nvl(b.org_id_parent_l0,b.org_id)      
and a.curr_id=b.org_curr_id_bs      
and b.org_id=:BindOrgId      
and b.org_cld_id=:BindCldId      
and b.org_create_ref_sloc_id=:BindSlocid     
order by CURR_DESC]]></SQLQuery>
