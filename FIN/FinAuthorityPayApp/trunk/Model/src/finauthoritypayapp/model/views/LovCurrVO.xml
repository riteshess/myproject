  <SQLQuery><![CDATA[SELECT   a.CURR_ID,      
         a.CURR_DESC,      
         a.CURR_DESC_OLD,      
         a.CURR_NOTATION_ALIAS,      
         a.CURR_NOTATION,      
         a.HO_ORG_ID,      
         a.CLD_ID,      
