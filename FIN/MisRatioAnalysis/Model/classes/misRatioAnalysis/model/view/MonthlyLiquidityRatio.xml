<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="MonthlyLiquidityRatio"
  Version="11.1.2.64.36"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="misRatioAnalysis.model.view.MonthlyLiquidityRatio_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindFyId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select  AGCD.GRPH_COL1_DATA    "FINANCIAL YEAR",
        to_number(AGCD.GRPH_COL2_DATA)    "Current Ratio",
        to_number(AGCD.GRPH_COL3_DATA)    "Quick Ratio",
        AGCD.GRPH_CLD_ID        "GlCldId",
        AGCD.GRPH_ORG_ID        "GlOrgId",
        to_number(AGCD.GRPH_COL5_DATA)   "FyId"
from
       app.app$grph$col$data agcd
where  agcd.grph_ORG_ID=:BindOrgId
and agcd.grph_cld_id=:BindCldId
and agcd.grph_id=31
and AGCD.GRPH_COL6_DATA=:BindFyId
order by to_number(grph_col4_data)]]></SQLQuery>
  <ViewAttribute
    Name="FinancialYear"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName='"FINANCIAL YEAR"'
    Expression='"FINANCIAL YEAR"'
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="255"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="CurrentRatio"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName='"Current Ratio"'
    Expression='"Current Ratio"'
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="QuickRatio"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName='"Quick Ratio"'
    Expression='"Quick Ratio"'
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Glcldid"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName='"GlCldId"'
    Expression='"GlCldId"'
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Glorgid"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName='"GlOrgId"'
    Expression='"GlOrgId"'
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Fyid"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName='"FyId"'
    Expression='"FyId"'
    SQLType="INTEGER"/>
  <ViewCriteria
    Name="MonthlyLiquidityRatioCriteria"
    ViewObjectName="misRatioAnalysis.model.view.MonthlyLiquidityRatio"
    Conjunction="AND">
    <Properties>
      <CustomProperties>
        <Property
          Name="displayOperators"
          Value="InAdvancedMode"/>
        <Property
          Name="autoExecute"
          Value="true"/>
        <Property
          Name="allowConjunctionOverride"
          Value="true"/>
        <Property
          Name="showInList"
          Value="true"/>
        <Property
          Name="mode"
          Value="Basic"/>
      </CustomProperties>
    </Properties>
  </ViewCriteria>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="misRatioAnalysis.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
