<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="UserLOV"
  Version="12.1.3.10.8"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="appCostCenter.model.View.UserLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindUsrId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[SELECT 
   a.USR_ID, 
   a.USR_NAME 
FROM 
   app.APP$SEC$USR a 
where a.usr_id=:BindUsrId 
   order by 2]]></SQLQuery>
  <ViewAttribute
    Name="UsrId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="USR_ID"
    Expression="USR_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="UsrName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="USR_NAME"
    Expression="USR_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="appCostCenter.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
