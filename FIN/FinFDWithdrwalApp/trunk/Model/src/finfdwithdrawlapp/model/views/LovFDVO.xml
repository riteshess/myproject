<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovFDVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="finfdwithdrawlapp.model.views.LovFDVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindCoaId"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select coa_id coa_id,  
coa_nm coa_nm  
from fin.fin$coa a,fin.fin$acc$na b      
where a.coa_acc_id=b.acc_id      
and a.coa_ho_org_id=b.ho_org_id      
and a.coa_cld_id=b.acc_cld_id      
and coa_cld_id=:BindCldId       
and coa_ho_org_id=:BindHoOrgId      
and coa_id=:BindCoaId  
and acc_type=0   
order by coa_nm]]></SQLQuery>
  <ViewAttribute
    Name="CoaId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigInteger"
    ColumnType="BIGINT"
    AliasName="COA_ID"
    Expression="COA_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="CoaNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="COA_NM"
    Expression="COA_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="CoaId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="finfdwithdrawlapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
