  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="chartofgroup.model.view.LovOrgLogoVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
