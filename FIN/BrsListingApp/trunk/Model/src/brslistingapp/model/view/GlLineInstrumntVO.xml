  <SQLQuery><![CDATA[SELECT                     
gli.GL_CLD_ID GL_CLD_ID,                     
gli.GL_SLOC_ID GL_SLOC_ID,                     
gli.GL_APPL_INST_ID GL_APPL_INST_ID,                     
gli.GL_HO_ORG_ID GL_HO_ORG_ID,                     
gli.GL_ORG_ID GL_ORG_ID,                     
gli.GL_REV_VOU_ID,                     
nvl(substr(adt2.doc_txn_id_disp,3,10),'-') doc_txn_id_disp_rev,                     
gli.GL_VOU_ID GL_VOU_ID,                     
substr(adt.doc_txn_id_disp,3,10) doc_txn_id_disp, gli.GL_VOU_DT GL_VOU_DT,                     
gli.GL_TYPE_ID GL_TYPE_ID, gli.GL_SUB_TYPE_ID GL_SUB_TYPE_ID, gli.GL_SL_NO                     
GL_SL_NO, gli.GL_COA_ID GL_COA_ID,                     
Initcap(fc.coa_nm) coa_nm,                     
gli.GL_COA_BNK_ID GL_COA_BNK_ID,                     
Initcap(fc2.coa_nm) bank_coa_nm,                     
gli.GL_CURR_ID_SP GL_CURR_ID_SP,                     
Initcap(cv.CURR_desc) Currency,                     
gli.GL_CHQ_SL_NO GL_CHQ_SL_NO,                     
gli.GL_INSTRMNT_NM GL_INSTRMNT_NM,                     
gli.GL_INSTRMNT_TYP GL_INSTRMNT_TYP,                     
gli.GL_INSTRMNT_NO GL_INSTRMNT_NO,                     
gli.GL_INSTRMNT_DT GL_INSTRMNT_DT,                     
nvl(gli.GL_INSTRMNT_STAT,0) GL_INSTRMNT_STAT, NVL((select att_nm from                     
app.app$ds$att where att_type_id=24 and att_actv='Y'                     
and att_id= gli.GL_INSTRMNT_STAT),'RECEIVED') STATUS,                     
NVL(gli.GL_INSTRMNT_STAT_DT,nvl(gli.usr_id_mod_dt,gli.usr_id_create_Dt))                     
GL_INSTRMNT_STAT_DT,                     
gli.GL_AMT_SP GL_AMT_SP,                     
gli.USR_ID_ARCH USR_ID_ARCH,                     
gli.USR_ID_ARCH_DT USR_ID_ARCH_DT,                     
gli.GL_INSTRMNT_BUK_SL_NO GL_INSTRMNT_BUK_SL_NO, gli.GL_INSTRMNT_TYP_STAT                     
GL_INSTRMNT_TYP_STAT,                     
gli.USR_ID_POST USR_ID_POST, gli.USR_ID_POST_DT USR_ID_POST_DT,                     
Gli.Usr_Id_Create Usr_Id_Create, Gli.Usr_Id_Create_Dt Usr_Id_Create_Dt,                      
gli.USR_ID_MOD USR_ID_MOD, gli.USR_ID_MOD_DT USR_ID_MOD_DT ,glnarr.gl_narr NARRATION, 
gli.gl_prj_id                     
FROM FIN.GL_LINE_INSTRUMNT gli,                     
      app.app$doc$txn adt,                     
      app.app$doc$txn adt2,                     
      fin.fin$coa fc,                     
      Fin.Fin$coa Fc2,                     
     App.App$curr_Vw Cv,                     
      (Select Gl.Gl_Desc,G.Gl_Narr,                     
              gl.gl_cld_id,                     
              gl.gl_sloc_id,                     
              gl.gl_ho_org_id,                     
              gl.gl_vou_id                      
        from fin.gl_lines g, fin.gl                     
       where gl.gl_Vou_id=g.gl_vou_id                     
       and gl.gl_cld_id=g.gl_cld_id                     
       and gl.gl_sloc_id=g.gl_sloc_id                     
       and gl.gl_ho_org_id=g.gl_ho_org_id                     
       and g.gl_sl_no=1) glnarr                     
where  gli.gl_cld_id=:BindCldId                     
and gli.gl_sloc_id=:BindSlocId                     
and gli.gl_ho_org_id=:BindHoOrgId                     
and gli.gl_org_id=:BindOrgId    
and gli.gl_prj_id=nvl(:BindPrjId,gli.gl_prj_id)                  
and gli.gl_Instrmnt_no=NVL(:BindInstmentNo,gli.gl_Instrmnt_no)                     
AND TO_DATE(gli.GL_VOU_DT,'DD-MM-YYYY')  >=NVL(TO_DATE(:BindVoucherDateFrm,'DD-MM-YYYY'),TO_DATE(gli.GL_VOU_DT,'DD-MM-YYYY'))                     
AND TO_DATE(gli.GL_VOU_DT,'DD-MM-YYYY') <=NVL(TO_DATE(:BindVoucherDateTo,'DD-MM-YYYY'),TO_DATE(gli.GL_VOU_DT,'DD-MM-YYYY'))                     
AND TO_DATE(gli.GL_INSTRMNT_DT,'DD-MM-YYYY')  >= NVL(TO_DATE(:BindInstrDateFrom,'DD-MM-YYYY'),TO_DATE(gli.GL_INSTRMNT_DT,'DD-MM-YYYY'))                     
AND TO_DATE(gli.GL_INSTRMNT_DT,'DD-MM-YYYY') <= NVL(TO_DATE(:BindInstrDateTo,'DD-MM-YYYY'),TO_DATE(gli.GL_INSTRMNT_DT,'DD-MM-YYYY'))                     
and gli.gl_cld_id=adt.doc_cld_id                     
and gli.gl_sloc_id=adt.doc_sloc_id                     
and gli.gl_org_id=adt.doc_org_id                     
and gli.gl_vou_id=adt.doc_txn_id                     
and gli.gl_cld_id=fc.coa_cld_id                     
and gli.gl_sloc_id=fc.coa_sloc_id                     
and gli.gl_ho_org_id=fc.coa_ho_org_id                     
and gli.gl_coa_id=fc.coa_id                     
and adt.doc_id='56'                     
and cv.CURR_ID=gli.gl_curr_id_sp                     
and gli.gl_cld_id=fc2.coa_cld_id                     
and gli.gl_sloc_id=fc2.coa_sloc_id                     
and gli.gl_ho_org_id=fc2.coa_ho_org_id                     
and gli.gl_coa_bnk_id=fc2.coa_id                     
and gli.gl_cld_id=adt2.doc_cld_id(+)                     
and gli.gl_sloc_id=adt2.doc_sloc_id(+)                     
and gli.gl_org_id=adt2.doc_org_id(+)                     
and gli.GL_REV_VOU_ID=adt2.doc_txn_id(+)                     
and glnarr.gl_cld_id=gli.gl_cld_id                     
and glnarr.gl_sloc_id=gli.gl_sloc_id                     
and glnarr.gl_ho_org_id=gli.gl_ho_org_id                     
and glnarr.gl_vou_id =gli.gl_vou_id                     
--And Trim(Upper(Glnarr.Gl_Narr)) Like                     
--trim(upper(nvl(:BindNarr,glnarr.gl_narr)))                     
AND upper(nvl(Glnarr.Gl_Narr, 'x')) like '%'||upper(nvl(:BindNarr, nvl(Glnarr.Gl_Narr, 'x')))||'%'              
and exists (select 1 from fin.gl g                     
where g.gl_cld_id=gli.gl_cld_id                     
and g.gl_sloc_id=gli.gl_sloc_id                     
And G.Gl_Ho_Org_Id=Gli.Gl_Ho_Org_Id                     
and g.gl_vou_id=gli.gl_vou_id )            
order by GL_VOU_DT desc]]></SQLQuery>
