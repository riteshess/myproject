  <SQLQuery><![CDATA[SELECT b.gl_cld_id,            
            b.gl_org_id org,            
            b.gl_instrmnt_dt,            
            gl_vou_dt,            
            b.gl_amt_sp,            
            gl_coa_bnk_id,             
            substr(c.DOC_TXN_ID_DISP, 3) DOC_TXN_ID_DISP            
	  FROM  gl_line_instrumnt b,	  app.app$doc$txn c            
	  WHERE b.gl_cld_id = nvl(:BindCldId,b.gl_cld_id)            
      and b.gl_sloc_id = nvl(:BindSlocId,b.gl_sloc_id)            
      and b.gl_org_id = nvl(:BindOrgId,b.gl_org_id)            
	  and b.gl_coa_bnk_id = nvl(:BindBankCoaId,b.gl_coa_bnk_id)            
           
           AND trunc(b.gl_vou_dt) >= nvl(:BindFromDate,trunc(b.gl_vou_dt))             
      and  trunc(b.gl_vou_dt) <=nvl(:BindToDate,trunc(b.gl_vou_dt))     
          
      AND B.GL_TYPE_ID in (2, 6)            
  	  and b.gl_cld_id = c.DOC_CLD_ID            
      and b.gl_sloc_id = c.DOC_SLOC_ID            
      and b.gl_org_id = c.DOC_ORG_ID            
      and b.gl_vou_id = c.DOC_TXN_ID]]></SQLQuery>
