  <SQLQuery><![CDATA[select
  f.coa_id,
  f.coa_nm,
  s.ap_pay_sum_amt1,
  s.ap_pay_sum_amt2,
  s.ap_pay_sum_amt3,
  s.ap_pay_sum_amt4
  from fin.fin$coa f, fin.Ap_Pay_Sum s 
  where 
   f.coa_id = s.ap_coa_id 
  and f.coa_cld_id = s.ap_cld_id
  and f.coa_sloc_id =s.ap_sloc_id
  and f.coa_ho_org_id = s.ap_ho_org_id
  
  and s.AP_PAY_ID=:pay_id_Bind
  and f.coa_sloc_id =:sloc_id_Bind
  and f.coa_ho_org_id =:ho_org_Bind
  and f.coa_cld_id=:cld_id_Bind
  
  and (s.ap_pay_sum_amt1 != nvl(:Amt1bind, s.ap_pay_sum_amt1)
  and s.ap_pay_sum_amt2 != nvl(:Amt2bind, s.ap_pay_sum_amt2)
  and s.ap_pay_sum_amt3 != nvl(:Amt3bind, s.ap_pay_sum_amt3)
  and s.ap_pay_sum_amt4 != nvl(:Amt4bind, s.ap_pay_sum_amt4))]]></SQLQuery>
