<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="MultiOrgVO"
  Version="12.1.3.10.8"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  RowClass="tempVoucherList.model.views.MultiOrgVORowImpl">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_codeGenFlag2" Value="Access"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="tempVoucherList.model.views.MultiOrgVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindTvouId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT A.GL_VOU_ID,  
substr(b.DOC_TXN_ID_DISP,3) DOC_TXN_ID_DISP,  
a.GL_ORG_ID,  
O.ORG_DESC,  
'Posted' gl_pdc_flg  
from fin.gl a, app.app$doc$txn b,app.org o  
where a.gl_sloc_id=b.doc_sloc_id  
and a.gl_cld_id=b.doc_cld_id  
AND A.GL_VOU_ID=B.DOC_TXN_ID  
and a.gl_org_id=b.doc_org_id  
AND B.DOC_CLD_ID=O.ORG_CLD_ID  
AND B.DOC_ID=56  
AND B.DOC_SLOC_ID=O.ORG_CREATE_REF_SLOC_ID  
AND B.DOC_ORG_ID=O.ORG_ID  
AND A.GL_CLD_ID=:BindCldId  
and A.GL_SLOC_ID=:BindSlocId  
and gl_ref_tvou_id=:BindTvouId  
and A.GL_ORG_ID=:BindOrgId  
union  
  
SELECT aa.tvou_id GL_VOU_ID,  
substr(b.DOC_TXN_ID_DISP,3) DOC_TXN_ID_DISP,  
aa.TVOU_ORG_ID GL_ORG_ID,  
O.ORG_DESC,  
'PDC Clearing' gl_pdc_flg  
from fin.tvou aa, app.app$doc$txn b,app.org o  
where aa.tvou_sloc_id=b.doc_sloc_id  
and aa.tvou_cld_id=b.doc_cld_id  
AND AA.tvou_ID=B.DOC_TXN_ID  
and aa.tvou_org_id=b.doc_org_id  
AND B.DOC_CLD_ID=O.ORG_CLD_ID  
AND AA.TVOU_PDC_FLG='Y'  
AND B.DOC_ID=55  
AND B.DOC_SLOC_ID=O.ORG_CREATE_REF_SLOC_ID  
AND B.DOC_ORG_ID=O.ORG_ID  
AND Aa.tvou_CLD_ID=:BindCldId  
and Aa.tvou_SLOC_ID=:BindSlocId  
and aa.tvou_id=:BindTvouId  
and Aa.TVOU_ORG_ID=:BindOrgId]]></SQLQuery>
  <ViewAttribute
    Name="GlVouId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="GL_VOU_ID"
    Expression="GL_VOU_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DocTxnIdDisp"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="40"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DOC_TXN_ID_DISP"
    Expression="DOC_TXN_ID_DISP"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="40"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="GlOrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="2"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="GL_ORG_ID"
    Expression="GL_ORG_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrgDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_DESC"
    Expression="ORG_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="GlPdcFlg"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="GL_PDC_FLG"
    Expression="GL_PDC_FLG"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="TransOrgLogoDesc"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VIEW_ATTR"
    SQLType="VARCHAR"/>
  <ResourceBundle>
    <JavaResourceBundle
      MsgBundleClass="tempVoucherList.view.bundle.Resource"/>
  </ResourceBundle>
</ViewObject>
