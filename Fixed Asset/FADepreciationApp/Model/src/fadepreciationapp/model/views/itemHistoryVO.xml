xa.cld_id,xa.ho_org_id,xa.sloc_id,xa.org_id,xa.fy_id, xa.prd_id, xa.dep_exe_id,xa.itm_grp_id,       
xa.itm_id, xa.itm_lbl_id, xa.itm_seq_id, xa.dep_law_typ,       
xa.dep_shft_co_law, xa.itm_cost_co_law,   xa.itm_cost_co_law_op_bal , xa.dep_acc_co_law, xa.dep_proc_exec_freq,      
xa.dep_efft_st_dt, xa.dep_efft_end_dt,xa.itm_st_dt, XA.itm_end_dt,       
xa.fa_ln_status,xa.doc_id,yb.dep_exe_disp_id      
from mm.mm$fa$dep$dtl xa,      
     mm.mm$fa$dep yb      
where dep_law_typ=:BindLawTyp      
and xa.cld_id=:BindCldId      
and xa.ho_org_id=:BindHoorgId      
and xa.sloc_id=:BindSlocId      
and xa.doc_id=nvl(:BindDocId ,xa.doc_id)     
and xa.cld_id=yb.cld_id(+)      
and xa.sloc_id=yb.sloc_id(+)      
and xa.ho_org_id=yb.ho_org_id(+)      
and xa.dep_exe_id=yb.dep_exe_id(+)      
AND XA.FY_ID<=nvl(:BindFyId,XA.FY_ID)      
AND XA.PRD_ID<=nvl(:BindPrdId,XA.PRD_ID)      
and xa.itm_lbl_id=nvl(:BindLblId,xa.itm_lbl_id)   
and xa.itm_grp_id=nvl(:BindItmGrpId,xa.itm_grp_id)  
and xa.itm_id=nvl(:BindItmId,xa.itm_id)  
order by doc_id,itm_id,itm_lbl_id,fy_id,prd_id]]></SQLQuery>
