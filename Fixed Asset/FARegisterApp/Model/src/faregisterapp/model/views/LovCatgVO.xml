<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovCatgVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="faregisterapp.model.views.LovCatgVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindCatMstId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select cat_id cat_id,    
cat_nm cat_nm,    
cat_desc cat_desc     
from app.app$cat    
where cat_cld_id=:BindCldId     
and Cat_Sloc_Id=:BindSlocId     
and Cat_Ho_Org_Id=:BindHoOrgId     
and cat_mst_id=nvl(:BindCatMstId,cat_mst_id)     
and cat_actv='Y']]></SQLQuery>
  <ViewAttribute
    Name="CatId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="CAT_ID"
    Expression="CAT_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="CatNm"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CAT_NM"
    Expression="CAT_NM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="CatDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CAT_DESC"
    Expression="CAT_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="faregisterapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
