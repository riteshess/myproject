<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="LovTrgtOrgVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="faregisterapp.model.views.LovTrgtOrgVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[select o.org_id org_id,       
o.org_desc org_desc,     
'INDIA' ADDS     
from app.org o, app.org$sloc p     
where o.org_cld_id=:BindCldId     
and o.org_id <> :BindOrgId     
and nvl(o.org_id_parent_L0,O.ORG_ID) = app.FN_GET_PARENT_ORG_L0 (:BindCldId,:BindOrgId)     
     
AND o.org_cld_id= P.org_cld_id     
and o.org_id = P.org_id]]></SQLQuery>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR"
    IsQueriable="false">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="2"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrgDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_DESC"
    Expression="ORG_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Adds"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4000"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ADDS"
    Expression="ADDS"
    SQLType="VARCHAR"
    IsQueriable="false">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4000"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="faregisterapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
