<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="GroupWiseVO"
  Version="12.1.3.10.8"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="falandingapp.model.views.GroupWiseVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="BindCldId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindHoOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="BindSlocId"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="BindOrgId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT   
l.cld_id cld_id,l.sloc_id sloc_id,l.ho_org_id ho_org_id,l.org_id org_id,l.fy_id fy_id,l.prd_id prd_id,l.itm_grp_id itm_grp_id,f.fa_grp_desc fa_grp_desc,  
sum(nvl(l.itm_cost_co_law,0)) ass_amt_tog,   
sum(nvl(l.dep_acc_co_law,0)) acc_dep,   
sum(nvl(l.itm_cost_co_law,0))-sum(nvl(l.dep_acc_co_law,0)) net_asst_value  
FROM Mm$fa$dep$dtl l,app.org$fa$itm$rule i,app.app$fa$grp$prf f  
where l.cld_id=:BindCldId  
and l.ho_org_id=:BindHoOrgId  
and l.sloc_id=:BindSlocId  
and l.org_id=:BindOrgId  
and l.cld_id=i.cld_id  
and l.sloc_id=i.sloc_id  
and l.ho_org_id=i.ho_org_id  
and l.org_id=i.org_id  
and l.itm_grp_id=i.fa_grp_id  
and l.itm_id=i.mm_itm_id  
and i.dep_shift_no=1  
and f.cld_id=i.cld_id  
and f.sloc_id=i.sloc_id  
and f.ho_org_id=i.ho_org_id  
and f.fa_prf_id=i.fa_prf_id  
and f.fa_grp_id=i.mm_itm_grp_id  
group by l.cld_id,l.sloc_id,l.ho_org_id,l.org_id,l.fy_id,l.prd_id,l.itm_grp_id,f.fa_grp_desc  
order by l.cld_id,l.sloc_id,l.ho_org_id,l.org_id,l.fy_id,l.prd_id,l.itm_grp_id]]></SQLQuery>
  <ViewAttribute
    Name="CldId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CLD_ID"
    Expression="CLD_ID"
    SQLType="VARCHAR"/>
  <ViewAttribute
    Name="SlocId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="SLOC_ID"
    Expression="SLOC_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="HoOrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="HO_ORG_ID"
    Expression="HO_ORG_ID"
    SQLType="VARCHAR"/>
  <ViewAttribute
    Name="OrgId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ORG_ID"
    Expression="ORG_ID"
    SQLType="VARCHAR"/>
  <ViewAttribute
    Name="FyId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="FY_ID"
    Expression="FY_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="PrdId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="5"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="PRD_ID"
    Expression="PRD_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="ItmGrpId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ITM_GRP_ID"
    Expression="ITM_GRP_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="FaGrpDesc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="150"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FA_GRP_DESC"
    Expression="FA_GRP_DESC"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="150"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="AssAmtTog"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="ASS_AMT_TOG"
    Expression="ASS_AMT_TOG"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="AccDep"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="ACC_DEP"
    Expression="ACC_DEP"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="NetAsstValue"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="NET_ASST_VALUE"
    Expression="NET_ASST_VALUE"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="CldId"/>
    <Item Value="SlocId"/>
    <Item Value="HoOrgId"/>
    <Item Value="OrgId"/>
    <Item Value="FyId"/>
    <Item Value="PrdId"/>
    <Item Value="ItmGrpId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="falandingapp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
